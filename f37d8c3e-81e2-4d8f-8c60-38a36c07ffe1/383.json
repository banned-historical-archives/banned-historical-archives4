1.5],[93.02,141.5]]},{"text":"富治”","box":[[72,148.03999999999996],[103.5,148.03999999999996],[103.5,158.53999999999996],[72,158.53999999999996]]},{"text":"已经成为事实，","box":[[100.077,148.03999999999996],[173.577,148.03999999999996],[173.577,158.53999999999996],[100.077,158.53999999999996]]},{"text":"在新北大公社成员中也普遍地产生了对谢富治","box":[[170.154,148.03999999999996],[380.154,148.03999999999996],[380.154,158.53999999999996],[170.154,158.53999999999996]]},{"text":"的不满情绪，人们真的产生了怀疑。","box":[[72,165.07999999999998],[240,165.07999999999998],[240,175.57999999999998],[72,175.57999999999998]]},{"text":"在","box":[[93.02,182.06],[103.52,182.06],[103.52,192.56],[93.02,192.56]]},{"text":"","box":[[103.52,182.06],[106.16,182.06],[106.16,182.06],[103.52,182.06]]},{"text":"4","box":[[106.16,182.06],[111.41,182.06],[111.41,192.56],[106.16,192.56]]},{"text":"","box":[[111.41,182.06],[114.08,182.06],[114.08,182.06],[111.41,182.06]]},{"text":"月","box":[[114.08,182.06],[124.58,182.06],[124.58,192.56],[114.08,192.56]]},{"text":"","box":[[124.58,182.06],[127.16,182.06],[127.16,182.06],[124.58,182.06]]},{"text":"14","box":[[127.16,182.06],[137.63,182.06],[137.63,192.56],[127.16,192.56]]},{"text":"","box":[[137.63,182.06],[140.3,182.06],[140.3,182.06],[137.63,182.06]]},{"text":"日中央领导人接见红代会各院校代表的会上，谢富治","box":[[140.3,182.06],[380.12,182.06],[380.12,192.56],[140.3,192.56]]},{"text":"是这样说的","box":[[72,199.10000000000002],[124.5,199.10000000000002],[124.5,209.60000000000002],[72,209.60000000000002]]},{"text":"：","box":[[124.52,199.10000000000002],[135.01999999999998,199.10000000000002],[135.01999999999998,209.60000000000002],[124.52,209.60000000000002]]},{"text":"“办事要调查清楚。","box":[[127.46,199.10000000000002],[221.95999999999998,199.10000000000002],[221.95999999999998,209.60000000000002],[127.46,209.60000000000002]]},{"text":"昨天我事先没调查，","box":[[219.68149999999997,199.10000000000002],[314.18149999999997,199.10000000000002],[314.18149999999997,209.60000000000002],[219.68149999999997,209.60000000000002]]},{"text":"情况不大了解，","box":[[311.90299999999996,199.10000000000002],[385.40299999999996,199.10000000000002],[385.40299999999996,209.60000000000002],[311.90299999999996,209.60000000000002]]},{"text":"还很急躁，","box":[[72,216.14],[124.5,216.14],[124.5,226.64],[72,226.64]]},{"text":"主要地是批评了地院东方红，","box":[[122.2215,216.14],[258.7215,216.14],[258.7215,226.64],[122.2215,226.64]]},{"text":"而且发了脾气，","box":[[256.443,216.14],[329.943,216.14],[329.943,226.64],[256.443,226.64]]},{"text":"也批评了聂","box":[[327.6645,216.14],[380.1645,216.14],[380.1645,226.64],[327.6645,226.64]]},{"text":"元梓，最后走的时候，也没有和聂元梓打招呼。","box":[[72,233.18],[292.5,233.18],[292.5,243.68],[72,243.68]]},{"text":"”","box":[[287.2185,233.18],[297.7185,233.18],[297.7185,243.68],[287.2185,243.68]]},{"text":"26","box":[[297.76,229.16000000000003],[304.80999999999995,229.16000000000003],[304.80999999999995,236.18000000000004],[297.76,236.18000000000004]]},{"text":"谢富治是","box":[[93.02,250.16000000000003],[135.01999999999998,250.16000000000003],[135.01999999999998,260.66],[93.02,260.66]]},{"text":"“事先没调查”","box":[[132.74149999999997,250.16000000000003],[206.24149999999997,250.16000000000003],[206.24149999999997,260.66],[132.74149999999997,260.66]]},{"text":"吗？他根本不让我们说话，","box":[[203.96299999999997,250.16000000000003],[329.96299999999997,250.16000000000003],[329.96299999999997,260.66],[203.96299999999997,260.66]]},{"text":"谈何调查？","box":[[327.68449999999996,250.16000000000003],[380.18449999999996,250.16000000000003],[380.18449999999996,260.66],[327.68449999999996,260.66]]},{"text":"谢富治自称","box":[[72,267.2],[124.5,267.2],[124.5,277.7],[72,277.7]]},{"text":"“主要地是批评了地院东方红，","box":[[120.4785,267.2],[267.4785,267.2],[267.4785,277.7],[120.4785,277.7]]},{"text":"而且发了脾","box":[[263.457,267.2],[315.957,267.2],[315.957,277.7],[263.457,277.7]]},{"text":"气”","box":[[316,267.2],[337,267.2],[337,277.7],[316,277.7]]},{"text":"，","box":[[331.7815,267.2],[342.2815,267.2],[342.2815,277.7],[331.7815,277.7]]},{"text":"真的吗？","box":[[338.26,267.2],[380.197,267.2],[380.197,277.7],[338.26,277.7]]},{"text":"聂元梓","box":[[72,284.24],[103.5,284.24],[103.5,294.74],[72,294.74]]},{"text":"、","box":[[103.52,284.24],[114.02,284.24],[114.02,294.74],[103.52,294.74]]},{"text":"孙蓬一","box":[[111.74,284.24],[143.24,284.24],[143.24,294.74],[111.74,294.74]]},{"text":"、","box":[[143.24,284.24],[153.74,284.24],[153.74,294.74],[143.24,294.74]]},{"text":"胡宗式三人在场，","box":[[151.46,284.24],[235.46,284.24],[235.46,294.74],[151.46,294.74]]},{"text":"既没有听到谢有片言只语批评地","box":[[233.1815,284.24],[380.1815,284.24],[380.1815,294.74],[233.1815,294.74]]},{"text":"院，","box":[[72,301.22],[93,301.22],[93,311.72],[72,311.72]]},{"text":"更没有见到他对地院方面的人发脾气。","box":[[90.7215,301.22],[269.2215,301.22],[269.2215,311.72],[90.7215,311.72]]},{"text":"否则，","box":[[266.943,301.22],[298.443,301.22],[298.443,311.72],[266.943,311.72]]},{"text":"孙蓬一绝对不会有","box":[[296.1645,301.22],[380.1645,301.22],[380.1645,311.72],[296.1645,311.72]]},{"text":"4.13","box":[[72,318.26],[93,318.26],[93,328.76],[72,328.76]]},{"text":"","box":[[93,318.26],[95.72,318.26],[95.72,318.26],[93,318.26]]},{"text":"那样的讲话。","box":[[95.72,318.26],[158.65699999999998,318.26],[158.65699999999998,328.76],[95.72,328.76]]},{"text":"江青在","box":[[93.02,335.3],[124.52,335.3],[124.52,345.8],[93.02,345.8]]},{"text":"","box":[[124.52,335.3],[128.96,335.3],[128.96,335.3],[124.52,335.3]]},{"text":"4.14","box":[[128.96,335.3],[150.01250000000002,335.3],[150.01250000000002,345.8],[128.96,345.8]]},{"text":"","box":[[150.01250000000002,335.3],[154.4,335.3],[154.4,335.3],[150.01250000000002,335.3]]},{"text":"讲话中也提到这件事","box":[[154.4,335.3],[248.9,335.3],[248.9,345.8],[154.4,345.8]]},{"text":"：","box":[[248.92,335.3],[259.41999999999996,335.3],[259.41999999999996,345.8],[248.92,345.8]]},{"text":"“谢富治副总理告诉我，昨","box":[[254.2,335.3],[380.137,335.3],[380.137,345.8],[254.2,345.8]]},{"text":"天搞了一天","box":[[72,352.28],[124.5,352.28],[124.5,362.78],[72,362.78]]},{"text":"（指调解北大和地院冲突的事","box":[[122.2215,352.28],[258.7215,352.28],[258.7215,362.78],[122.2215,362.78]]},{"text":"—","box":[[258.76,352.28],[269.26,352.28],[269.26,362.78],[258.76,362.78]]},{"text":"–","box":[[269.26,352.28],[274.51,352.28],[274.51,362.78],[269.26,362.78]]},{"text":"引者）","box":[[274.54,352.28],[305.97700000000003,352.28],[305.97700000000003,362.78],[274.54,362.78]]},{"text":"，","box":[[300.7585,352.28],[311.2585,352.28],[311.2585,362.78],[300.7585,362.78]]},{"text":"要一起谈，","box":[[308.98,352.28],[361.41700000000003,352.28],[361.41700000000003,362.78],[308.98,362.78]]},{"text":"地院","box":[[359.1385,352.28],[380.1385,352.28],[380.1385,362.78],[359.1385,362.78]]},{"text":"东方红不干；要单独谈，单独谈也崩了。","box":[[72,369.32],[259.32,369.32],[259.32,379.82],[72,379.82]]},{"text":"”","box":[[254.1015,369.32],[264.6015,369.32],[264.6015,379.82],[254.1015,379.82]]},{"text":"27","box":[[263.8,365.3],[270.84999999999997,365.3],[270.84999999999997,372.32],[263.8,372.32]]},{"text":"","box":[[270.84999999999997,365.3],[276.16,365.3],[276.16,365.3],[270.84999999999997,365.3]]},{"text":"看来，谢富治还真的和","box":[[276.16,369.32],[380.257,369.32],[380.257,379.82],[276.16,379.82]]},{"text":"地院东方红单独谈过，但是谈崩了。","box":[[72,386.38],[240,386.38],[240,396.88],[72,396.88]]},{"text":"康生在","box":[[93.02,403.36],[124.52,403.36],[124.52,413.86],[93.02,413.86]]},{"text":"","box":[[124.52,403.36],[128.96,403.36],[128.96,403.36],[124.52,403.36]]},{"text":"4.14","box":[[128.96,403.36],[150.01250000000002,403.36],[150.01250000000002,413.86],[128.96,413.86]]},{"text":"","box":[[150.01250000000002,403.36],[154.4,403.36],[154.4,403.36],[150.01250000000002,403.36]]},{"text":"讲话中也说了这件事","box":[[154.4,403.36],[248.9,403.36],[248.9,413.86],[154.4,413.86]]},{"text":"：","box":[[248.92,403.36],[259.41999999999996,403.36],[259.41999999999996,413.86],[248.92,413.86]]},{"text":"“昨晚谢副总理向我汇报了","box":[[254.2,403.36],[380.137,403.36],[380.137,413.86],[254.2,413.86]]},{"text":"昨天的情况，准备解决问题，地院东方红没有遵守谢副总理的指示，","box":[[72,420.4],[385.446,420.4],[385.446,430.9],[72,430.9]]},{"text":"出了","box":[[72,437.44],[93,437.44],[93,447.94],[72,447.94]]},{"text":"","box":[[93,437.44],[95.66,437.44],[95.66,437.44],[93,437.44]]},{"text":"26","box":[[95.66,437.44],[106.13,437.44],[106.13,447.94],[95.66,447.94]]},{"text":"","box":[[106.13,437.44],[108.8,437.44],[108.8,437.44],[106.13,437.44]]},{"text":"人，这种态度是不对的。谢副总理气量够大的，好些地方冲","box":[[108.8,437.44],[380.1935,437.44],[380.1935,447.94],[108.8,447.94]]},{"text":"公安部都没发脾气，","box":[[72,454.48],[166.5,454.48],[166.5,464.98],[72,464.98]]},{"text":"气量很大。","box":[[164.2215,454.48],[216.7215,454.48],[216.7215,464.98],[164.2215,464.98]]},{"text":"昨天谢副总理不是退兵","box":[[214.44299999999998,454.48],[319.443,454.48],[319.443,464.98],[214.44299999999998,464.98]]},{"text":"","box":[[319.443,454.48],[322.12,454.48],[322.12,454.48],[319.443,454.48]]},{"text":"30","box":[[322.12,454.48],[332.65,454.48],[332.65,464.98],[322.12,464.98]]},{"text":"，","box":[[332.68,454.48],[343.18,454.48],[343.18,464.98],[332.68,464.98]]},{"text":"而是","box":[[340.9015,454.48],[361.8385,454.48],[361.8385,464.98],[340.9015,464.98]]},{"text":"","box":[[361.8385,454.48],[364.48,454.48],[364.48,454.48],[361.8385,454.48]]},{"text":"50","box":[[364.48,454.48],[375.01,454.48],[375.01,464.98],[364.48,464.98]]},{"text":"，","box":[[374.98,454.48],[385.48,454.48],[385.48,464.98],[374.98,464.98]]},{"text":"地院还不谅解，这一点，地院应当自我批评。","box":[[72,471.46000000000004],[282,471.46000000000004],[282,481.96000000000004],[72,481.96000000000004]]},{"text":"”","box":[[276.7815,471.46000000000004],[287.2815,471.46000000000004],[287.2815,481.96000000000004],[276.7815,481.96000000000004]]},{"text":"28","box":[[287.26,467.44],[294.30999999999995,467.44],[294.30999999999995,474.46],[287.26,474.46]]},{"text":"看来，","box":[[93.02,488.5],[124.52,488.5],[124.52,499],[93.02,499]]},{"text":"谢富治确实有一些我们不知道的隐情和难处。","box":[[121.097,488.5],[331.097,488.5],[331.097,499],[121.097,499]]},{"text":"关于调解的","box":[[327.674,488.5],[380.174,488.5],[380.174,499],[327.674,499]]},{"text":"情况，他至少向江青和康生做了汇报。","box":[[72,505.53999999999996],[250.5,505.53999999999996],[250.5,516.04],[72,516.04]]},{"text":"显然，地院“东方红”在和谢富治“单独谈”的时候提出了某种","box":[[93.02,522.52],[380.14250000000004,522.52],[380.14250000000004,533.02],[93.02,533.02]]},{"text":"连谢富治也不能接受的条件，于是“谈崩了”","box":[[72,539.56],[280.929,539.56],[280.929,550.06],[72,550.06]]},{"text":"。为什么连谢富治也谈","box":[[275.71049999999997,539.56],[380.17499999999995,539.56],[380.17499999999995,550.06],[275.71049999999997,550.06]]},{"text":"26","box":[[72,571.72],[81,571.72],[81,580.72],[72,580.72]]},{"text":"","box":[[81,571.72],[85.52,571.72],[85.52,571.72],[81,571.72]]},{"text":"胡宗式、章铎编","box":[[85.52,571.72],[148.52,571.72],[148.52,580.72],[85.52,580.72]]},{"text":"：","box":[[148.52,571.72],[157.52,571.72],[157.52,580.72],[148.52,580.72]]},{"text":"《北京大学文革资料选编》","box":[[157.52,571.72],[265.52000000000004,571.72],[265.52000000000004,580.72],[157.52,580.72]]},{"text":"（","box":[[265.54,571.72],[274.54,571.72],[274.54,580.72],[265.54,580.72]]},{"text":"下","box":[[274.54,571.72],[283.54,571.72],[283.54,580.72],[274.54,580.72]]},{"text":")","box":[[283.54,571.72],[288.04,571.72],[288.04,580.72],[283.54,580.72]]},{"text":"，第","box":[[288.04,571.72],[306.04,571.72],[306.04,580.72],[288.04,580.72]]},{"text":"","box":[[306.04,571.72],[308.26,571.72],[308.26,571.72],[306.04,571.72]]},{"text":"79","box":[[308.26,571.72],[317.26,571.72],[317.26,580.72],[308.26,580.72]]},{"text":"","box":[[317.26,571.72],[319.48,571.72],[319.48,571.72],[317.26,571.72]]},{"text":"页。","box":[[319.48,571.72],[337.48,571.72],[337.48,580.72],[319.48,580.72]]},{"text":"27","box":[[72,583.36],[81,583.36],[81,592.36],[72,592.36]]},{"text":"","box":[[81,583.36],[85.52,583.36],[85.52,583.36],[81,583.36]]},{"text":"胡宗式、章铎编","box":[[85.52,583.36],[148.52,583.36],[148.52,592.36],[85.52,592.36]]},{"text":"：","box":[[148.52,583.36],[157.52,583.36],[157.52,592.36],[148.52,592.36]]},{"text":"《北京大学文革资料选编》","box":[[157.52,583.36],[265.52000000000004,583.36],[265.52000000000004,592.36],[157.52,