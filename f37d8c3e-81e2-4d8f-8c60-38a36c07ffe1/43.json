96]]},{"text":"反”","box":[[72,140.53999999999996],[93,140.53999999999996],[93,151.03999999999996],[72,151.03999999999996]]},{"text":"运动纳入统一的社会主义教育运动，","box":[[90.7215,140.53999999999996],[258.7215,140.53999999999996],[258.7215,151.03999999999996],[90.7215,151.03999999999996]]},{"text":"分三批进行。","box":[[256.443,140.53999999999996],[319.443,140.53999999999996],[319.443,151.03999999999996],[256.443,151.03999999999996]]},{"text":"高等学校被列","box":[[317.1645,140.53999999999996],[380.1645,140.53999999999996],[380.1645,151.03999999999996],[317.1645,151.03999999999996]]},{"text":"入第一批。北京市有三所高等学校作为运动试点，其中之一就是北","box":[[72,157.57999999999998],[379.7550000000001,157.57999999999998],[379.7550000000001,168.07999999999998],[72,168.07999999999998]]},{"text":"大。","box":[[72,174.62],[93,174.62],[93,185.12],[72,185.12]]},{"text":"3","box":[[93.02,169.10000000000002],[95.78,169.10000000000002],[95.78,174.62000000000003],[93.02,174.62000000000003]]},{"text":"1964","box":[[93.02,191.60000000000002],[114.02,191.60000000000002],[114.02,202.10000000000002],[93.02,202.10000000000002]]},{"text":"","box":[[114.02,191.60000000000002],[116.66,191.60000000000002],[116.66,191.60000000000002],[114.02,191.60000000000002]]},{"text":"年","box":[[116.66,191.60000000000002],[127.16,191.60000000000002],[127.16,202.10000000000002],[116.66,202.10000000000002]]},{"text":"","box":[[127.16,191.60000000000002],[129.8,191.60000000000002],[129.8,191.60000000000002],[127.16,191.60000000000002]]},{"text":"7","box":[[129.8,191.60000000000002],[135.05,191.60000000000002],[135.05,202.10000000000002],[129.8,202.10000000000002]]},{"text":"","box":[[135.05,191.60000000000002],[137.66,191.60000000000002],[137.66,191.60000000000002],[135.05,191.60000000000002]]},{"text":"月","box":[[137.66,191.60000000000002],[148.16,191.60000000000002],[148.16,202.10000000000002],[137.66,202.10000000000002]]},{"text":"","box":[[148.16,191.60000000000002],[150.74,191.60000000000002],[150.74,191.60000000000002],[148.16,191.60000000000002]]},{"text":"2","box":[[150.74,191.60000000000002],[155.99,191.60000000000002],[155.99,202.10000000000002],[150.74,202.10000000000002]]},{"text":"","box":[[155.99,191.60000000000002],[158.66,191.60000000000002],[158.66,191.60000000000002],[155.99,191.60000000000002]]},{"text":"日，","box":[[158.66,191.60000000000002],[179.66,191.60000000000002],[179.66,202.10000000000002],[158.66,202.10000000000002]]},{"text":"以中宣部副部长张磐石为组长的调查组","box":[[175.46,191.60000000000002],[353.89700000000005,191.60000000000002],[353.89700000000005,202.10000000000002],[175.46,202.10000000000002]]},{"text":"","box":[[353.89700000000005,191.60000000000002],[356.56,191.60000000000002],[356.56,191.60000000000002],[353.89700000000005,191.60000000000002]]},{"text":"10","box":[[356.56,191.60000000000002],[367.03000000000003,191.60000000000002],[367.03000000000003,202.10000000000002],[356.56,202.10000000000002]]},{"text":"","box":[[367.03000000000003,191.60000000000002],[369.64,191.60000000000002],[369.64,191.60000000000002],[367.03000000000003,191.60000000000002]]},{"text":"人","box":[[369.64,191.60000000000002],[380.14,191.60000000000002],[380.14,202.10000000000002],[369.64,202.10000000000002]]},{"text":"进入北大进行调查研究。","box":[[72,208.64],[187.5,208.64],[187.5,219.14],[72,219.14]]},{"text":"调查组开始叫","box":[[184.497,208.64],[247.497,208.64],[247.497,219.14],[184.497,219.14]]},{"text":"“中央调查组”","box":[[244.49400000000003,208.64],[317.994,208.64],[317.994,219.14],[244.49400000000003,219.14]]},{"text":"，","box":[[312.7755,208.64],[323.2755,208.64],[323.2755,219.14],[312.7755,219.14]]},{"text":"后来改为","box":[[320.27250000000004,208.64],[362.20950000000005,208.64],[362.20950000000005,219.14],[320.27250000000004,219.14]]},{"text":"“中","box":[[359.20650000000006,208.64],[380.20650000000006,208.64],[380.20650000000006,219.14],[359.20650000000006,219.14]]},{"text":"宣部调查组”","box":[[72,225.68],[135,225.68],[135,236.18],[72,236.18]]},{"text":"。派遣调查组的，是中宣部领导层。但这样的事，显然","box":[[129.7185,225.68],[380.091,225.68],[380.091,236.18],[129.7185,236.18]]},{"text":"不是中宣部几个领导说了可以算的。据《北大百年》的说法，调查组","box":[[72,242.66000000000003],[380.08050000000003,242.66000000000003],[380.08050000000003,253.16000000000003],[72,253.16000000000003]]},{"text":"就是“受党中央","box":[[72,259.7],[145.5,259.7],[145.5,270.2],[72,270.2]]},{"text":"、","box":[[145.52,259.7],[156.02,259.7],[156.02,270.2],[145.52,270.2]]},{"text":"毛主席委派”的。","box":[[156.02,259.7],[240.02,259.7],[240.02,270.2],[156.02,270.2]]},{"text":"4","box":[[240.02,254.18],[242.78,254.18],[242.78,259.7],[240.02,259.7]]},{"text":"随后，","box":[[93.02,276.74],[124.52,276.74],[124.52,287.24],[93.02,287.24]]},{"text":"高教部副部长刘仰峤也率人加入调查组，","box":[[119.7845,276.74],[308.7845,276.74],[308.7845,287.24],[119.7845,287.24]]},{"text":"调查组增加到","box":[[304.049,276.74],[367.049,276.74],[367.049,287.24],[304.049,287.24]]},{"text":"","box":[[367.049,276.74],[369.7,276.74],[369.7,276.74],[367.049,276.74]]},{"text":"30","box":[[369.7,276.74],[380.17,276.74],[380.17,287.24],[369.7,287.24]]},{"text":"人。","box":[[72,293.78],[93,293.78],[93,304.28],[72,304.28]]},{"text":"调查组进驻北大，北京市委大学科学工作部（简称市委大学部）","box":[[93.02,310.76],[385.466,310.76],[385.466,321.26],[93.02,321.26]]},{"text":"便深感紧张。","box":[[72,327.8],[135,327.8],[135,338.3],[72,338.3]]},{"text":"北大是市委大学部直接领导的，","box":[[132.7215,327.8],[279.7215,327.8],[279.7215,338.3],[132.7215,338.3]]},{"text":"那是他们的地盘。","box":[[277.443,327.8],[361.443,327.8],[361.443,338.3],[277.443,338.3]]},{"text":"对于","box":[[359.1645,327.8],[380.1645,327.8],[380.1645,338.3],[359.1645,338.3]]},{"text":"中宣部调查组，","box":[[72,344.84],[145.5,344.84],[145.5,355.34],[72,355.34]]},{"text":"他们有着天然的抵触情绪。","box":[[142.077,344.84],[268.077,344.84],[268.077,355.34],[142.077,355.34]]},{"text":"他们认为张磐石到北大来","box":[[264.654,344.84],[380.154,344.84],[380.154,355.34],[264.654,355.34]]},{"text":"是“不怀好意”","box":[[72,361.82],[145.5,361.82],[145.5,372.32],[72,372.32]]},{"text":"，","box":[[140.2815,361.82],[150.7815,361.82],[150.7815,372.32],[140.2815,372.32]]},{"text":"“来者不善”","box":[[145.563,361.82],[208.5,361.82],[208.5,372.32],[145.563,372.32]]},{"text":"，是来“整人”的，是来","box":[[203.2815,361.82],[318.7185,361.82],[318.7185,372.32],[203.2815,372.32]]},{"text":"“找岔”的。","box":[[318.76,361.82],[381.76,361.82],[381.76,372.32],[318.76,372.32]]},{"text":"他们特别关照陆平要“多加小心”","box":[[72,378.86],[227.757,378.86],[227.757,389.36],[72,389.36]]},{"text":"，还建议他回学校后“安排”一下。","box":[[222.5385,378.86],[385.3725,378.86],[385.3725,389.36],[222.5385,389.36]]},{"text":"5","box":[[72,390.4],[74.76,390.4],[74.76,395.91999999999996],[72,395.91999999999996]]},{"text":"陆平的子女回忆说，","box":[[93.02,412.9],[187.51999999999998,412.9],[187.51999999999998,423.4],[93.02,423.4]]},{"text":"“几天后，父亲到北京市委，向刘仁同志汇","box":[[181.75549999999998,412.9],[380.18449999999996,412.9],[380.18449999999996,423.4],[181.75549999999998,423.4]]},{"text":"报了张磐石和","box":[[72,429.94],[135,429.94],[135,440.44],[72,440.44]]},{"text":"‘中央调查组’","box":[[131.997,429.94],[205.497,429.94],[205.497,440.44],[131.997,440.44]]},{"text":"到北大的情况。","box":[[202.49400000000003,429.94],[275.994,429.94],[275.994,440.44],[202.49400000000003,440.44]]},{"text":"刘仁同志回答说","box":[[272.99100000000004,429.94],[346.49100000000004,429.94],[346.49100000000004,440.44],[272.99100000000004,440.44]]},{"text":"‘他来，","box":[[343.48800000000006,429.94],[385.48800000000006,429.94],[385.48800000000006,440.44],[343.48800000000006,440.44]]},{"text":"我们不知道，他来干什么，我们也不知道。","box":[[72,446.98],[270.429,446.98],[270.429,457.48],[72,457.48]]},{"text":"’这就更加令父亲感到疑","box":[[265.21049999999997,446.98],[380.17499999999995,446.98],[380.17499999999995,457.48],[265.21049999999997,457.48]]},{"text":"虑和奇怪","box":[[72,463.96000000000004],[114,463.96000000000004],[114,474.46000000000004],[72,474.46000000000004]]},{"text":"：","box":[[114.02,463.96000000000004],[124.52,463.96000000000004],[124.52,474.46000000000004],[114.02,474.46000000000004]]},{"text":"北大是双重领导单位，党的工作","box":[[124.1,463.96000000000004],[270.68,463.96000000000004],[270.68,474.46000000000004],[124.1,474.46000000000004]]},{"text":"、","box":[[270.7,463.96000000000004],[281.2,463.96000000000004],[281.2,474.46000000000004],[270.7,474.46000000000004]]},{"text":"干部工作","box":[[280.78,463.96000000000004],[322.78,463.96000000000004],[322.78,474.46000000000004],[280.78,474.46000000000004]]},{"text":"、","box":[[322.78,463.96000000000004],[333.28,463.96000000000004],[333.28,474.46000000000004],[322.78,474.46000000000004]]},{"text":"政治运动","box":[[332.86,463.96000000000004],[374.86,463.96000000000004],[374.86,474.46000000000004],[332.86,474.46000000000004]]},{"text":"、","box":[[374.92,463.96000000000004],[385.42,463.96000000000004],[385.42,474.46000000000004],[374.92,474.46000000000004]]},{"text":"政治思想工作接受市委领导，","box":[[72,481],[208.5,481],[208.5,491.5],[72,491.5]]},{"text":"教学等业务工作接受高教部领导，","box":[[205.077,481],[362.577,481],[362.577,491.5],[205.077,491.5]]},{"text":"是高","box":[[359.154,481],[380.154,481],[380.154,491.5],[359.154,491.5]]},{"text":"教部直属学校。","box":[[72,498.03999999999996],[145.5,498.03999999999996],[145.5,508.53999999999996],[72,508.53999999999996]]},{"text":"现在，","box":[[142.497,498.03999999999996],[173.997,498.03999999999996],[173.997,508.53999999999996],[142.497,508.53999999999996]]},{"text":"中宣部插手北大，","box":[[170.99400000000003,498.03999999999996],[254.93100000000004,498.03999999999996],[254.93100000000004,508.53999999999996],[170.99400000000003,508.53999999999996]]},{"text":"派来调查组，","box":[[251.92800000000005,498.03999999999996],[314.92800000000005,498.03999999999996],[314.92800000000005,508.53999999999996],[251.92800000000005,508.53999999999996]]},{"text":"市委都不知道，","box":[[311.92500000000007,498.03999999999996],[385.42500000000007,498.03999999999996],[385.42500000000007,508.53999999999996],[311.92500000000007,508.53999999999996]]},{"text":"意味着什么？刘仁同志问","box":[[72,515.08],[187.5,515.08],[187.5,525.58],[72,525.58]]},{"text":"：","box":[[187.52,515.08],[198.02,515.08],[198.02,525.58],[187.52,525.58]]},{"text":"‘他在人大的情况你听说了吧？’父亲清","box":[[192.02,515.08],[380.180