4.5]]},{"text":"不要你领导》的大字报，说工作组犯了右倾路线错误。","box":[[72,131],[324,131],[324,141.5],[72,141.5]]},{"text":"25","box":[[324.04,126.96000000000004],[331.09,126.96000000000004],[331.09,133.98000000000005],[324.04,133.98000000000005]]},{"text":"从","box":[[93.02,148.03999999999996],[103.52,148.03999999999996],[103.52,158.53999999999996],[93.02,158.53999999999996]]},{"text":"","box":[[103.52,148.03999999999996],[106.58,148.03999999999996],[106.58,148.03999999999996],[103.52,148.03999999999996]]},{"text":"7","box":[[106.58,148.03999999999996],[111.83,148.03999999999996],[111.83,158.53999999999996],[106.58,158.53999999999996]]},{"text":"","box":[[111.83,148.03999999999996],[114.92,148.03999999999996],[114.92,148.03999999999996],[111.83,148.03999999999996]]},{"text":"月","box":[[114.92,148.03999999999996],[125.42,148.03999999999996],[125.42,158.53999999999996],[114.92,158.53999999999996]]},{"text":"","box":[[125.42,148.03999999999996],[128.48,148.03999999999996],[128.48,148.03999999999996],[125.42,148.03999999999996]]},{"text":"12","box":[[128.48,148.03999999999996],[139.01,148.03999999999996],[139.01,158.53999999999996],[128.48,158.53999999999996]]},{"text":"","box":[[139.01,148.03999999999996],[142.1,148.03999999999996],[142.1,148.03999999999996],[139.01,148.03999999999996]]},{"text":"日陈必陶等人大字报贴出，到","box":[[142.1,148.03999999999996],[278.6,148.03999999999996],[278.6,158.53999999999996],[142.1,158.53999999999996]]},{"text":"","box":[[278.6,148.03999999999996],[281.68,148.03999999999996],[281.68,148.03999999999996],[278.6,148.03999999999996]]},{"text":"7","box":[[281.68,148.03999999999996],[286.93,148.03999999999996],[286.93,158.53999999999996],[281.68,158.53999999999996]]},{"text":"","box":[[286.93,148.03999999999996],[290.02,148.03999999999996],[290.02,148.03999999999996],[286.93,148.03999999999996]]},{"text":"月","box":[[290.02,148.03999999999996],[300.52,148.03999999999996],[300.52,158.53999999999996],[290.02,158.53999999999996]]},{"text":"","box":[[300.52,148.03999999999996],[303.58,148.03999999999996],[303.58,148.03999999999996],[300.52,148.03999999999996]]},{"text":"19","box":[[303.58,148.03999999999996],[314.10999999999996,148.03999999999996],[314.10999999999996,158.53999999999996],[303.58,158.53999999999996]]},{"text":"","box":[[314.10999999999996,148.03999999999996],[317.2,148.03999999999996],[317.2,148.03999999999996],[314.10999999999996,148.03999999999996]]},{"text":"日聂元梓发表","box":[[317.2,148.03999999999996],[380.2,148.03999999999996],[380.2,158.53999999999996],[317.2,158.53999999999996]]},{"text":"讲话，在一周的时间里，北大同学围绕工作组问题展开了辩论。当","box":[[72,165.07999999999998],[379.7550000000001,165.07999999999998],[379.7550000000001,175.57999999999998],[72,175.57999999999998]]},{"text":"时，认为工作组犯了“右倾”错误的大字报已经很多，聂元梓因为身","box":[[72,182.06],[380.08050000000003,182.06],[380.08050000000003,192.56],[72,192.56]]},{"text":"份特殊，尤其在学生中有影响力，她的讲话无疑起了推波助澜的作","box":[[72,199.10000000000002],[379.7550000000001,199.10000000000002],[379.7550000000001,209.60000000000002],[72,209.60000000000002]]},{"text":"用。","box":[[72,216.14],[93,216.14],[93,226.64],[72,226.64]]},{"text":"对于聂元梓的讲话，","box":[[93.02,233.18],[187.51999999999998,233.18],[187.51999999999998,243.68],[93.02,243.68]]},{"text":"张承先及其工作组做出了很强烈的反应。","box":[[184.09699999999998,233.18],[373.097,233.18],[373.097,243.68],[184.09699999999998,243.68]]},{"text":"张","box":[[369.674,233.18],[380.174,233.18],[380.174,243.68],[369.674,243.68]]},{"text":"承先连夜召开了领导小组紧急会议，","box":[[72,250.16000000000003],[240,250.16000000000003],[240,260.66],[72,260.66]]},{"text":"“张文”写道","box":[[234.74,250.16000000000003],[297.74,250.16000000000003],[297.74,260.66],[234.74,260.66]]},{"text":"：","box":[[297.76,250.16000000000003],[308.26,250.16000000000003],[308.26,260.66],[297.76,260.66]]},{"text":"大家对聂元梓的做法非常气愤，说聂曾任领导小组办公室主任，","box":[[93.02,273.2],[385.403,273.2],[385.403,283.7],[93.02,283.7]]},{"text":"工作组所有工作情况她是清楚的，处理","box":[[72,290.24],[252.36899999999994,290.24],[252.36899999999994,300.74],[72,300.74]]},{"text":"‘","box":[[252.76,290.24],[263.26,290.24],[263.26,300.74],[252.76,300.74]]},{"text":"六一八事件","box":[[263.38,290.24],[316.36300000000006,290.24],[316.36300000000006,300.74],[263.38,300.74]]},{"text":"’","box":[[316.54,290.24],[327.04,290.24],[327.04,300.74],[316.54,300.74]]},{"text":"时她也是表","box":[[327.16,290.24],[380.1430000000001,290.24],[380.1430000000001,300.74],[327.16,300.74]]},{"text":"示同意了的，为什么又跳出来反对工作组。","box":[[72,307.22],[271.5,307.22],[271.5,317.72],[72,317.72]]},{"text":"那么，聂元梓当过“领导小组办公室主任”吗？","box":[[93.02,330.26],[313.52,330.26],[313.52,340.76],[93.02,340.76]]},{"text":"为了弄清楚这个问题，","box":[[93.02,347.3],[198.01999999999998,347.3],[198.01999999999998,357.8],[93.02,357.8]]},{"text":"笔者先后询问了聂元梓","box":[[194.59699999999998,347.3],[299.597,347.3],[299.597,357.8],[194.59699999999998,357.8]]},{"text":"、","box":[[299.62,347.3],[310.12,347.3],[310.12,357.8],[299.62,357.8]]},{"text":"王茂湘和李清崑","box":[[306.7,347.3],[380.2,347.3],[380.2,357.8],[306.7,357.8]]},{"text":"等人。","box":[[72,364.28],[103.5,364.28],[103.5,374.78],[72,374.78]]},{"text":"聂元梓说","box":[[93.02,381.32],[135.01999999999998,381.32],[135.01999999999998,391.82],[93.02,391.82]]},{"text":"：","box":[[135.02,381.32],[145.52,381.32],[145.52,391.82],[135.02,391.82]]},{"text":"“我又不是工作组成员，说我是工作组办公室主任，","box":[[140.3,381.32],[381.737,381.32],[381.737,391.82],[140.3,391.82]]},{"text":"我的办公室在哪儿？”","box":[[72,398.38],[177,398.38],[177,408.88],[72,408.88]]},{"text":"笔者问","box":[[174.7215,398.38],[206.2215,398.38],[206.2215,408.88],[174.7215,408.88]]},{"text":"：","box":[[206.24,398.38],[216.74,398.38],[216.74,408.88],[206.24,408.88]]},{"text":"“那个时间你在哪里上班呢？”","box":[[209.24,398.38],[356.177,398.38],[356.177,408.88],[209.24,408.88]]},{"text":"她说","box":[[353.8985,398.38],[374.8985,398.38],[374.8985,408.88],[353.8985,408.88]]},{"text":"：","box":[[374.92,398.38],[385.42,398.38],[385.42,408.88],[374.92,408.88]]},{"text":"“我一直在哲学系总支办公室”","box":[[72,415.36],[219,415.36],[219,425.86],[72,425.86]]},{"text":"。","box":[[213.7815,415.36],[224.2815,415.36],[224.2815,425.86],[213.7815,425.86]]},{"text":"王茂湘说","box":[[93.02,432.4],[135.01999999999998,432.4],[135.01999999999998,442.9],[93.02,442.9]]},{"text":"：","box":[[135.02,432.4],[145.52,432.4],[145.52,442.9],[135.02,442.9]]},{"text":"“工作组进校后，从各系抽调了十个人进入工作组。","box":[[140.3,432.4],[381.737,432.4],[381.737,442.9],[140.3,442.9]]},{"text":"经济系是我，","box":[[72,449.44],[135,449.44],[135,459.94],[72,459.94]]},{"text":"哲学系有李清崑，","box":[[132.7215,449.44],[216.7215,449.44],[216.7215,459.94],[132.7215,459.94]]},{"text":"没有聂元梓。","box":[[214.44299999999998,449.44],[277.443,449.44],[277.443,459.94],[214.44299999999998,459.94]]},{"text":"好像工作组对聂的印象","box":[[275.1645,449.44],[380.1645,449.44],[380.1645,459.94],[275.1645,459.94]]},{"text":"不大好，说她陷入‘两派’太深。","box":[[72,466.48],[229.5,466.48],[229.5,476.98],[72,476.98]]},{"text":"”","box":[[224.2815,466.48],[234.7815,466.48],[234.7815,476.98],[224.2815,476.98]]},{"text":"2020","box":[[93.02,483.46000000000004],[114.02,483.46000000000004],[114.02,493.96000000000004],[93.02,493.96000000000004]]},{"text":"","box":[[114.02,483.46000000000004],[116.72,483.46000000000004],[116.72,483.46000000000004],[114.02,483.46000000000004]]},{"text":"年初，","box":[[116.72,483.46000000000004],[148.157,483.46000000000004],[148.157,493.96000000000004],[116.72,493.96000000000004]]},{"text":"90","box":[[147.38,483.46000000000004],[157.85,483.46000000000004],[157.85,493.96000000000004],[147.38,493.96000000000004]]},{"text":"","box":[[157.85,483.46000000000004],[160.52,483.46000000000004],[160.52,483.46000000000004],[157.85,483.46000000000004]]},{"text":"岁高龄的李清崑老师将他写的一篇短文《聂元梓","box":[[160.52,483.46000000000004],[380.18000000000006,483.46000000000004],[380.18000000000006,493.96000000000004],[160.52,493.96000000000004]]},{"text":"当过张承先工作组办公室主任吗？》发给笔者。文中写道","box":[[72,500.5],[334.5,500.5],[334.5,511],[72,511]]},{"text":"：","box":[[334.54,500.5],[345.04,500.5],[345.04,511],[334.54,511]]},{"text":"聂元梓当过张承先工作组办公室主任吗？关于这一问题，有两","box":[[93.02,523.54],[379.8800000