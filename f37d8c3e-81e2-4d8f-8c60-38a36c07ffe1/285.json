4.5],[258.76,124.5]]},{"text":"吴传启的同伙，","box":[[266.98,114],[340.48,114],[340.48,124.5],[266.98,124.5]]},{"text":"没有引起","box":[[338.2015,114],[380.2015,114],[380.2015,124.5],[338.2015,124.5]]},{"text":"足够的警惕，是她的不足。","box":[[72,131],[198,131],[198,141.5],[72,141.5]]},{"text":"陈伯达和戚本禹的谈话，","box":[[93.02,148.03999999999996],[208.51999999999998,148.03999999999996],[208.51999999999998,158.53999999999996],[93.02,158.53999999999996]]},{"text":"聂元梓有没有向别人传达过，","box":[[206.24,148.03999999999996],[342.74,148.03999999999996],[342.74,158.53999999999996],[206.24,158.53999999999996]]},{"text":"不详，","box":[[340.4615,148.03999999999996],[371.9615,148.03999999999996],[371.9615,158.53999999999996],[340.4615,158.53999999999996]]},{"text":"反","box":[[369.683,148.03999999999996],[380.183,148.03999999999996],[380.183,158.53999999999996],[369.683,158.53999999999996]]},{"text":"正笔者毫不知情。","box":[[72,165.07999999999998],[156,165.07999999999998],[156,175.57999999999998],[72,175.57999999999998]]},{"text":"到","box":[[93.02,182.06],[103.52,182.06],[103.52,192.56],[93.02,192.56]]},{"text":"","box":[[103.52,182.06],[106.16,182.06],[106.16,182.06],[103.52,182.06]]},{"text":"3","box":[[106.16,182.06],[111.41,182.06],[111.41,192.56],[106.16,192.56]]},{"text":"","box":[[111.41,182.06],[114.08,182.06],[114.08,182.06],[111.41,182.06]]},{"text":"月初，","box":[[114.08,182.06],[145.57999999999998,182.06],[145.57999999999998,192.56],[114.08,192.56]]},{"text":"吴传启团伙在","box":[[143.30149999999998,182.06],[206.2385,182.06],[206.2385,192.56],[143.30149999999998,192.56]]},{"text":"“反二月逆流”","box":[[203.95999999999998,182.06],[277.46,182.06],[277.46,192.56],[203.95999999999998,192.56]]},{"text":"狂飙中又作了引人注目","box":[[275.18149999999997,182.06],[380.18149999999997,182.06],[380.18149999999997,192.56],[275.18149999999997,192.56]]},{"text":"的表演。由此，聂元梓和新北大公社对这条“又粗又长的黑线”又有","box":[[72,199.10000000000002],[380.08050000000003,199.10000000000002],[380.08050000000003,209.60000000000002],[72,209.60000000000002]]},{"text":"了更深的认识。","box":[[72,216.14],[145.5,216.14],[145.5,226.64],[72,226.64]]},{"text":"一","box":[[96.02,253.27999999999997],[108.02,253.27999999999997],[108.02,265.28],[96.02,265.28]]},{"text":"、","box":[[108.02,253.27999999999997],[120.02,253.27999999999997],[120.02,265.28],[108.02,265.28]]},{"text":"陶铸被打倒和吴传启团","box":[[120.02,253.27999999999997],[240.01999999999998,253.27999999999997],[240.01999999999998,265.28],[120.02,265.28]]},{"text":"伙的崛起","box":[[240.02,253.27999999999997],[288.02,253.27999999999997],[288.02,265.28],[240.02,265.28]]},{"text":"新年伊始，","box":[[93.02,281.12],[145.51999999999998,281.12],[145.51999999999998,291.62],[93.02,291.62]]},{"text":"","box":[[145.51999999999998,281.12],[149.72,281.12],[149.72,281.12],[145.51999999999998,281.12]]},{"text":"1967","box":[[149.72,281.12],[170.72,281.12],[170.72,291.62],[149.72,291.62]]},{"text":"","box":[[170.72,281.12],[173.36,281.12],[173.36,281.12],[170.72,281.12]]},{"text":"年","box":[[173.36,281.12],[183.86,281.12],[183.86,291.62],[173.36,291.62]]},{"text":"","box":[[183.86,281.12],[186.5,281.12],[186.5,281.12],[183.86,281.12]]},{"text":"1","box":[[186.5,281.12],[191.75,281.12],[191.75,291.62],[186.5,291.62]]},{"text":"","box":[[191.75,281.12],[194.36,281.12],[194.36,281.12],[191.75,281.12]]},{"text":"月","box":[[194.36,281.12],[204.86,281.12],[204.86,291.62],[194.36,291.62]]},{"text":"","box":[[204.86,281.12],[207.44,281.12],[207.44,281.12],[204.86,281.12]]},{"text":"4","box":[[207.44,281.12],[212.69,281.12],[212.69,291.62],[207.44,291.62]]},{"text":"","box":[[212.69,281.12],[215.36,281.12],[215.36,281.12],[212.69,281.12]]},{"text":"日，江青","box":[[215.36,281.12],[256.2785,281.12],[256.2785,291.62],[215.36,291.62]]},{"text":"、","box":[[256.3,281.12],[266.8,281.12],[266.8,291.62],[256.3,291.62]]},{"text":"陈伯达发动突然袭击，公","box":[[265.72,281.12],[380.1385,281.12],[380.1385,291.62],[265.72,291.62]]},{"text":"开讲话把陶铸打倒了。","box":[[72,298.16],[177,298.16],[177,308.66],[72,308.66]]},{"text":"陶铸是在中共八届十一中全会上被选为中央政治局常委的，是","box":[[93.02,315.2],[379.8800000000001,315.2],[379.8800000000001,325.7],[93.02,325.7]]},{"text":"第四号人物。陶铸的思想跟不上毛泽东“文革”的节拍，处境困难而","box":[[72,332.18],[380.08050000000003,332.18],[380.08050000000003,342.68],[72,342.68]]},{"text":"被动。到","box":[[72,349.22],[114,349.22],[114,359.72],[72,359.72]]},{"text":"","box":[[114,349.22],[117.56,349.22],[117.56,349.22],[114,349.22]]},{"text":"1966","box":[[117.56,349.22],[138.56,349.22],[138.56,359.72],[117.56,359.72]]},{"text":"","box":[[138.56,349.22],[142.1,349.22],[142.1,349.22],[138.56,349.22]]},{"text":"年","box":[[142.1,349.22],[152.6,349.22],[152.6,359.72],[142.1,359.72]]},{"text":"","box":[[152.6,349.22],[156.14,349.22],[156.14,349.22],[152.6,349.22]]},{"text":"12","box":[[156.14,349.22],[166.60999999999999,349.22],[166.60999999999999,359.72],[156.14,359.72]]},{"text":"","box":[[166.60999999999999,349.22],[170.18,349.22],[170.18,349.22],[166.60999999999999,349.22]]},{"text":"月，他在中央已经处在靠边站的地位了。而这","box":[[170.18,349.22],[380.117,349.22],[380.117,359.72],[170.18,359.72]]},{"text":"种状况，给关锋","box":[[72,366.26],[143.757,366.26],[143.757,376.76],[72,376.76]]},{"text":"、","box":[[143.78,366.26],[154.28,366.26],[154.28,376.76],[143.78,376.76]]},{"text":"戚本禹","box":[[152.54,366.26],[184.04,366.26],[184.04,376.76],[152.54,376.76]]},{"text":"、","box":[[184.04,366.26],[194.54,366.26],[194.54,376.76],[184.04,376.76]]},{"text":"吴传启","box":[[192.86,366.26],[224.36,366.26],[224.36,376.76],[192.86,376.76]]},{"text":"、","box":[[224.36,366.26],[234.86,366.26],[234.86,376.76],[224.36,376.76]]},{"text":"林聿时团伙提供了一个绝好的机","box":[[233.12,366.26],[380.12,366.26],[380.12,376.76],[233.12,376.76]]},{"text":"会，他们控制的“学部联队”借“批陶”之机打垮了对立面，以一支","box":[[72,383.24],[380.13300000000004,383.24],[380.13300000000004,393.74],[72,393.74]]},{"text":"重要的力量出现在北京“文革”舞台上。","box":[[72,400.3],[261,400.3],[261,410.8],[72,410.8]]},{"text":"在“文革”前，关锋是《红旗》杂志的编委，吴传启是中国","box":[[93.02,449.91999999999996],[359.153,449.91999999999996],[359.153,460.41999999999996],[93.02,460.41999999999996]]},{"text":"科学","box":[[359.2,449.91999999999996],[380.2,449.91999999999996],[380.2,460.41999999999996],[359.2,460.41999999999996]]},{"text":"院哲学社会科学部（简称“学部”","box":[[72,466.9],[228.66000000000003,466.9],[228.66000000000003,477.4],[72,477.4]]},{"text":"）哲学所的学术秘书，林聿时是该","box":[[223.44150000000002,466.9],[380.1015,466.9],[380.1015,477.4],[223.44150000000002,477.4]]},{"text":"所刊物《哲学研究》的负责人。","box":[[72,483.94],[219,483.94],[219,494.44],[72,494.44]]},{"text":"关锋","box":[[93.02,500.98],[114.02,500.98],[114.02,511.48],[93.02,511.48]]},{"text":"、","box":[[114.02,500.98],[124.52,500.98],[124.52,511.48],[114.02,511.48]]},{"text":"吴传启","box":[[124.52,500.98],[156.01999999999998,500.98],[156.01999999999998,511.48],[124.52,511.48]]},{"text":"、","box":[[156.02,500.98],[166.52,500.98],[166.52,511.48],[156.02,511.48]]},{"text":"林聿时很早就有合作，以“撒仁兴”","box":[[166.52,500.98],[334.52,500.98],[334.52,511.48],[166.52,511.48]]},{"text":"（","box":[[329.32,500.98],[339.82,500.98],[339.82,511.48],[329.32,511.48]]},{"text":"三人行","box":[[339.82,500.98],[371.257,500.98],[371.257,511.48],[339.82,511.48]]},{"text":"）","box":[[371.26,500.98],[381.76,500.98],[381.76,511.48],[371.26,511.48]]},{"text":"的笔名在《哲学研究","box":[[72,517.96],[166.07999999999998,517.96],[166.07999999999998,528.46],[72,528.46]]},{"text":"》","box":[[166.1,517.96],[176.6,517.96],[176.6,528.46],[166.1,528.46]]},{"text":"《","box":[[170.96,517.96],[181.46,517.96],[181.46,528.46],[170.96,528.46]]},{"text":"新建设》等刊物上发表了不少文章。他们的","box":[[181.46,517.96],[380.12,517.96],[380.12,528.46],[181.46,528.46]]},{"text":"文章“口气大而霸气足”","box":[[72,535],[186.3555,535],[186.3555,545.5],[72,545.5]]},{"text":"（孟祥才语）","box":[[179.94,535],[242.94,535],[242.94,545.5],[179.94,545.5]]},{"text":"，名气很大。关锋","box":[[237.6585,535],[319.3695,535],[319.3695,545.5],[237.6585,545.5]]},{"text":"、","box":[[319.42,535],[329.92,535],[329.92,545.5],[319.42,545.5]]},{"text":"吴传启","box":[[328.78,535],[360.28,535],[360.28,545.5],[328.78,545.5]]},{"text":"、","box":[[360.28,535],[370.78,535],[370.78,545.5],[360.28,545.5]]},{"text":"林","box":[[369.64,535],[380.14,535],[380.14,545.5],[369.64,545.5]]},{"text":"聿时还是康生手下","box":[[72,552.04],[156,552.04],[156,562.54],[72,562.54]]},{"text":"“哲学反修资料编写组”","box":[[153.7215,552.04],[269.2215,552.04],[269.2215,562.54],[153.7215,562.54]]},{"text":"的核心组成员，","box":[[266.943,552.04],[340.443,552.04],[340.443,562.54],[266.943,562.54]]},{"text":"同康生熟","box":[[338.1645,552.04],[380.1645,552.04],[380.1645,562.54],[338.1645,562.54]]},{"text":"得很。","box":[[72,569.02],[103.5,569.02],[103.5,579.52],[72,579.52]]},{"text":"几个人合作做学术研究，","box":[[93.02,586.06],[208.51999999999998,586.06],[208.51999999999998,596.56],[93.02,596.56]]},{"text":"写学术论文，","box":[[205.04449999999997,586.06],[268.04449999999997,586.06],[268.04449999999997,596.56],[205.04449999999997,596.56]]},{"text":"无可厚非。","box":[[264.56899999999996,586.06],[317.06899999999996,586.06],[317.06899999999996,596.56],[264.56899999999996,596.56]]},{"text":"但是，","box":[[313.59349999999995,586.06],[345.09349999999995,586.06],[345.09349999999995,596.56],[313.59349999999995,596.56]]},{"text":"随着","box":[[341.61799999999994,586.06],[362.61799999999994,586.06],[362.61799999999994,596.56],[341.61799999999994,596.56]]},{"text":"“文","box":[[359.1424999999999,586.06],[380.1424999999999,586.06],[380.1424999999999,596.56],[359.1424999999999,596.56]]}]}