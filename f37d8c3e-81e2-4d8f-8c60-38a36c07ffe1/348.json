,{"text":"尖锐地批","box":[[327.354,544.06],[369.354,544.06],[369.354,554.56],[327.354,554.56]]},{"text":"评我就是对我的关心，就是支持我，真正做到“","box":[[61.2,561.1],[284.05199999999996,561.1],[284.05199999999996,571.6],[61.2,571.6]]},{"text":"知无不言，言无不","box":[[284.5,561.1],[369.2980000000001,561.1],[369.2980000000001,571.6],[284.5,571.6]]},{"text":"55","box":[[61.2,595.06],[70.2,595.06],[70.2,604.06],[61.2,604.06]]},{"text":"","box":[[70.2,595.06],[74.7,595.06],[74.7,595.06],[70.2,595.06]]},{"text":"《校内动态》，","box":[[74.7,595.06],[137.70000000000002,595.06],[137.70000000000002,604.06],[74.7,604.06]]},{"text":"1967","box":[[137.72,595.06],[155.72,595.06],[155.72,604.06],[137.72,604.06]]},{"text":"","box":[[155.72,595.06],[157.94,595.06],[157.94,595.06],[155.72,595.06]]},{"text":"年","box":[[157.94,595.06],[166.94,595.06],[166.94,604.06],[157.94,604.06]]},{"text":"","box":[[166.94,595.06],[169.16,595.06],[169.16,595.06],[166.94,595.06]]},{"text":"3","box":[[169.16,595.06],[173.66,595.06],[173.66,604.06],[169.16,604.06]]},{"text":"","box":[[173.66,595.06],[175.94,595.06],[175.94,595.06],[173.66,595.06]]},{"text":"月","box":[[175.94,595.06],[184.94,595.06],[184.94,604.06],[175.94,604.06]]},{"text":"","box":[[184.94,595.06],[187.22,595.06],[187.22,595.06],[184.94,595.06]]},{"text":"6","box":[[187.22,595.06],[191.72,595.06],[191.72,604.06],[187.22,604.06]]},{"text":"","box":[[191.72,595.06],[193.94,595.06],[193.94,595.06],[191.72,595.06]]},{"text":"日。","box":[[193.94,595.06],[211.94,595.06],[211.94,604.06],[193.94,604.06]]}]}