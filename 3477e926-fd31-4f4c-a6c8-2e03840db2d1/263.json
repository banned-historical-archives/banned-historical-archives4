317.2,141.5]]},{"text":"扬声中经不起考验","box":[[72,148.03999999999996],[156,148.03999999999996],[156,158.53999999999996],[72,158.53999999999996]]},{"text":"，","box":[[156.02,148.03999999999996],[166.52,148.03999999999996],[166.52,158.53999999999996],[156.02,158.53999999999996]]},{"text":"没有自知之明","box":[[164.24,148.03999999999996],[227.24,148.03999999999996],[227.24,158.53999999999996],[164.24,158.53999999999996]]},{"text":"，","box":[[227.24,148.03999999999996],[237.74,148.03999999999996],[237.74,158.53999999999996],[227.24,158.53999999999996]]},{"text":"也没有知人之明","box":[[235.46,148.03999999999996],[308.96000000000004,148.03999999999996],[308.96000000000004,158.53999999999996],[235.46,158.53999999999996]]},{"text":"，","box":[[308.98,148.03999999999996],[319.48,148.03999999999996],[319.48,158.53999999999996],[308.98,158.53999999999996]]},{"text":"思想和作风上","box":[[317.2,148.03999999999996],[380.2,148.03999999999996],[380.2,158.53999999999996],[317.2,158.53999999999996]]},{"text":"起了严重变化。官做大了个人野心也大了","box":[[72,165.07999999999998],[262.97399999999993,165.07999999999998],[262.97399999999993,175.57999999999998],[72,175.57999999999998]]},{"text":"，","box":[[263.38,165.07999999999998],[273.88,165.07999999999998],[273.88,175.57999999999998],[263.38,175.57999999999998]]},{"text":"飞扬跋扈","box":[[274,165.07999999999998],[316.37800000000004,165.07999999999998],[316.37800000000004,175.57999999999998],[274,175.57999999999998]]},{"text":"，","box":[[316.54,165.07999999999998],[327.04,165.07999999999998],[327.04,175.57999999999998],[316.54,175.57999999999998]]},{"text":"毫无党的观","box":[[327.16,165.07999999999998],[380.1430000000001,165.07999999999998],[380.1430000000001,175.57999999999998],[327.16,175.57999999999998]]},{"text":"念","box":[[72,182.06],[82.5,182.06],[82.5,192.56],[72,192.56]]},{"text":"，","box":[[82.52,182.06],[93.02,182.06],[93.02,192.56],[82.52,192.56]]},{"text":"搞一言堂","box":[[91.28,182.06],[133.28,182.06],[133.28,192.56],[91.28,192.56]]},{"text":"，","box":[[133.28,182.06],[143.78,182.06],[143.78,192.56],[133.28,192.56]]},{"text":"搞家长制","box":[[142.04,182.06],[184.04,182.06],[184.04,192.56],[142.04,192.56]]},{"text":"，","box":[[184.04,182.06],[194.54,182.06],[194.54,192.56],[184.04,192.56]]},{"text":"资产阶级生活作风越来越重","box":[[192.86,182.06],[318.86,182.06],[318.86,192.56],[192.86,192.56]]},{"text":"，","box":[[318.88,182.06],[329.38,182.06],[329.38,192.56],[318.88,192.56]]},{"text":"背离了主席","box":[[327.64,182.06],[380.14,182.06],[380.14,192.56],[327.64,192.56]]},{"text":"接班人五项条件的要求。”","box":[[72,199.10000000000002],[197.223,199.10000000000002],[197.223,209.60000000000002],[72,209.60000000000002]]},{"text":"","box":[[197.223,199.10000000000002],[201.74,199.10000000000002],[201.74,199.10000000000002],[197.223,199.10000000000002]]},{"text":"刘冰等人在信中揭发了迟群五个方面的","box":[[201.74,199.10000000000002],[380.177,199.10000000000002],[380.177,209.60000000000002],[201.74,209.60000000000002]]},{"text":"问题：","box":[[72,216.14],[103.5,216.14],[103.5,226.64],[72,226.64]]},{"text":"一","box":[[94.04,239.72000000000003],[105.02000000000001,239.72000000000003],[105.02000000000001,250.70000000000002],[94.04,250.70000000000002]]},{"text":"、","box":[[105.02,239.72000000000003],[116,239.72000000000003],[116,250.70000000000002],[105.02,250.70000000000002]]},{"text":"资产阶级个人野心严重","box":[[116,239.72000000000003],[225.97568000000007,239.72000000000003],[225.97568000000007,250.70000000000002],[116,250.70000000000002]]},{"text":"他经常在谢静宜同志不在场的时候炫耀自己：","box":[[93.02,257],[303.02,257],[303.02,267.5],[93.02,267.5]]},{"text":"“我和小谢是主席","box":[[296.18449999999996,257],[380.18449999999996,257],[380.18449999999996,267.5],[296.18449999999996,267.5]]},{"text":"的两个兵”","box":[[72,273.98],[124.5,273.98],[124.5,284.48],[72,284.48]]},{"text":"，","box":[[123.74,273.98],[134.24,273.98],[134.24,284.48],[123.74,284.48]]},{"text":"“我可以到省里干一干”","box":[[132.68,273.98],[248.23250000000002,273.98],[248.23250000000002,284.48],[132.68,284.48]]},{"text":"，","box":[[247.48,273.98],[257.98,273.98],[257.98,284.48],[247.48,284.48]]},{"text":"“到军区干一干”","box":[[256.42,273.98],[340.4725,273.98],[340.4725,284.48],[256.42,284.48]]},{"text":"，","box":[[339.7,273.98],[350.2,273.98],[350.2,284.48],[339.7,284.48]]},{"text":"“我要","box":[[348.64,273.98],[380.14,273.98],[380.14,284.48],[348.64,284.48]]},{"text":"当官有的是","box":[[72,291.02],[124.5,291.02],[124.5,301.52],[72,301.52]]},{"text":"，","box":[[124.52,291.02],[135.01999999999998,291.02],[135.01999999999998,301.52],[124.52,301.52]]},{"text":"让我当国防科委主任","box":[[133.64,291.02],[228.14,291.02],[228.14,301.52],[133.64,301.52]]},{"text":"，","box":[[228.14,291.02],[238.64,291.02],[238.64,301.52],[228.14,301.52]]},{"text":"我都不干”","box":[[237.26,291.02],[289.76,291.02],[289.76,301.52],[237.26,301.52]]},{"text":"，","box":[[288.4,291.02],[298.9,291.02],[298.9,301.52],[288.4,301.52]]},{"text":"“清华大学是个社","box":[[296.14,291.02],[380.14,291.02],[380.14,301.52],[296.14,301.52]]},{"text":"会","box":[[72,308.06],[82.5,308.06],[82.5,318.56],[72,318.56]]},{"text":"，","box":[[82.52,308.06],[93.02,308.06],[93.02,318.56],[82.52,318.56]]},{"text":"当好一个部长不一定能管好我这个清华大学”","box":[[90.74,308.06],[300.74,308.06],[300.74,318.56],[90.74,318.56]]},{"text":"，","box":[[298.48,308.06],[308.98,308.06],[308.98,318.56],[298.48,318.56]]},{"text":"还说什么一个清","box":[[306.7,308.06],[380.2,308.06],[380.2,318.56],[306.7,318.56]]},{"text":"华的校长","box":[[72,325.04],[114,325.04],[114,335.54],[72,335.54]]},{"text":"，","box":[[114.02,325.04],[124.52,325.04],[124.52,335.54],[114.02,335.54]]},{"text":"就能当总理。","box":[[124.52,325.04],[187.51999999999998,325.04],[187.51999999999998,335.54],[124.52,335.54]]},{"text":"十大和四届人大后","box":[[93.02,342.08],[177.01999999999998,342.08],[177.01999999999998,352.58],[93.02,352.58]]},{"text":"，","box":[[177.02,342.08],[187.52,342.08],[187.52,352.58],[177.02,352.58]]},{"text":"他没有当上中央委员","box":[[185.24,342.08],[279.74,342.08],[279.74,352.58],[185.24,352.58]]},{"text":"，","box":[[279.76,342.08],[290.26,342.08],[290.26,352.58],[279.76,352.58]]},{"text