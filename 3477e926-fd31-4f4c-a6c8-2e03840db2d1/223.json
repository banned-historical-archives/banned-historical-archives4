14,272.66],[154.64,272.66]]},{"text":"还有河北的代表","box":[[163.76,262.16],[237.26,262.16],[237.26,272.66],[163.76,272.66]]},{"text":"，","box":[[237.26,262.16],[247.76,262.16],[247.76,272.66],[237.26,272.66]]},{"text":"但没讲出事实","box":[[246.38,262.16],[309.38,262.16],[309.38,272.66],[246.38,272.66]]},{"text":"，","box":[[309.4,262.16],[319.9,262.16],[319.9,272.66],[309.4,272.66]]},{"text":"没有点名","box":[[318.52,262.16],[360.52,262.16],[360.52,272.66],[318.52,272.66]]},{"text":"，","box":[[360.52,262.16],[371.02,262.16],[371.02,272.66],[360.52,272.66]]},{"text":"也","box":[[369.64,262.16],[380.14,262.16],[380.14,272.66],[369.64,272.66]]},{"text":"没有说我串联到他头上","box":[[72,279.2],[177,279.2],[177,289.7],[72,289.7]]},{"text":"，","box":[[177.02,279.2],[187.52,279.2],[187.52,289.7],[177.02,289.7]]},{"text":"也没说我对他说揪出反对毛主席的人。","box":[[184.1,279.2],[362.6,279.2],[362.6,289.7],[184.1,289.7]]},{"text":"倪志","box":[[359.177,279.2],[380.177,279.2],[380.177,289.7],[359.177,289.7]]},{"text":"福发言也就是这么说的","box":[[72,296.24],[177,296.24],[177,306.74],[72,306.74]]},{"text":"，","box":[[177.02,296.24],[187.52,296.24],[187.52,306.74],[177.02,306.74]]},{"text":"说我搞串联","box":[[185.24,296.24],[237.74,296.24],[237.74,306.74],[185.24,306.74]]},{"text":"，","box":[[237.74,296.24],[248.24,296.24],[248.24,306.74],[237.74,306.74]]},{"text":"但没有讲出什么事实来。","box":[[245.96,296.24],[361.46000000000004,296.24],[361.46000000000004,306.74],[245.96,306.74]]},{"text":"使我","box":[[359.1815,296.24],[380.1815,296.24],[380.1815,306.74],[359.1815,306.74]]},{"text":"感到倪志福是跟领导精神的。刘锡昌的态度使我感到他好像很为难","box":[[72,313.22],[374.88300000000004,313.22],[374.88300000000004,323.72],[72,323.72]]},{"text":"，","box":[[374.92,313.22],[385.42,313.22],[385.42,323.72],[374.92,323.72]]},{"text":"不批评我好像也不行","box":[[72,330.26],[166.5,330.26],[166.5,340.76],[72,340.76]]},{"text":"，","box":[[166.52,330.26],[177.02,330.26],[177.02,340.76],[166.52,340.76]]},{"text":"嘟嘟囔囔两句什么话","box":[[174.74,330.26],[269.24,330.26],[269.24,340.76],[174.74,340.76]]},{"text":"，","box":[[269.26,330.26],[279.76,330.26],[279.76,340.76],[269.26,340.76]]},{"text":"我也没有听清楚．","box":[[277.48,330.26],[361.48,330.26],[361.48,340.76],[277.48,340.76]]},{"text":"张世","box":[[359.2015,330.26],[380.2015,330.26],[380.2015,340.76],[359.2015,340.76]]},{"text":"忠没有发言。","box":[[72,347.3],[135,347.3],[135,357.8],[72,357.8]]},{"text":"会上我没有接受这种批评","box":[[93.02,364.28],[208.51999999999998,364.28],[208.51999999999998,374.78],[93.02,374.78]]},{"text":"，","box":[[208.52,364.28],[219.02,364.28],[219.02,374.78],[208.52,374.78]]},{"text":"没有承认我搞串联","box":[[215.6,364.28],[299.6,364.28],[299.6,374.78],[215.6,374.78]]},{"text":"，","box":[[299.62,364.28],[310.12,364.28],[310.12,374.78],[299.62,374.78]]},{"text":"我讲了刘锡昌他","box":[[306.7,364.28],[380.2,364.28],[380.2,374.78],[306.7,374.78]]},{"text":"们三个人曾找过我","box":[[72,381.32],[156,381.32],[156,391.82],[72,391.82]]},{"text":"，","box":[[156.02,381.32],[166.52,381.32],[166.52,391.82],[156.02,391.82]]},{"text":"但这不是串联","box":[[164.24,381.32],[227.24,381.32],[227.24,391.82],[164.24,391.82]]},{"text":"，","box":[[227.24,381.32],[237.74,381.32],[237.74,391.82],[227.24,391.82]]},{"text":"其他人我没有接触。","box":[[235.46,381.32],[329.96000000000004,381.32],[329.96000000000004,391.82],[235.46,391.82]]},{"text":"所以我也没","box":[[327.6815,381.32],[380.1815,381.32],[380.1815,391.82],[327.6815,391.82]]},{"text":"有检讨。这次会开完后","box":[[72,398.38],[178.00799999999998,398.38],[178.00799999999998,408.88],[72,408.88]]},{"text":"，","box":[[178.34,398.38],[188.84,398.38],[188.84,408.88],[178.34,408.88]]},{"text":"我认为我没有接受这种批判","box":[[188.96,398.38],[316.24100000000004,398.38],[316.24100000000004,408.88],[188.96,408.88]]},{"text":"，","box":[[316.54,398.38],[327.04,398.38],[327.04,408.88],[316.54,408.88]]},{"text":"事情就算完","box":[[327.16,398.38],[380.1430000000001,398.38],[380.1430000000001,408.88],[327.16,408.88]]},{"text":"了","box":[[72,415.36],[82.5,415.36],[82.5,425.86],[72,425.86]]},{"text":"，","box":[[82.52,415.36],[93.02,415.36],[93.02,425.86],[82.52,425.86]]},{"text":"只是心情十分不愉快","box":[[93.02,415.36],[187.51999999999998,415.36],[187.51999999999998,425.86],[93.02,425.86]]},{"text":"，","box":[[187.52,415.36],[198.02,415.36],[198.02,425.86],[187.52,425.86]]},{"text":"想不通为什么批我搞串联。","box":[[198.02,415.36],[324.02,415.36],[324.02,425.86],[198.02,425.86]]},{"text":"当九届二中全会要结束时的一次大会上","box":[[93.02,432.4],[273.57800000000003,432.4],[273.57800000000003,442.9],[93.02,442.9]]},{"text":"，","box":[[273.94,432.4],[284.44,432.4],[284.44,442.9],[273.94,442.9]]},{"text":"周总理讲话批评有的","box":[[284.56,432.4],[380.1520000000001,432.4],[380.1520000000001,442.9],[284.56,442.9]]},{"text":"人不了解情况","box":[[72,449.44],[135,449.44],[135,459.94],[72,459.94]]},{"text":"，","box":[[135.02,449.44],[145.52,449.44],[145.52,459.94],[135.02,459.94]]},{"text":"就乱表态","box":[[142.1,449.44],[184.1,449.44],[184.1,459.94],[142.1,459.94]]},{"text":"，","box":[[184.1,449.44],[194.6,449.44],[194.6,459.94],[184.1,459.94]]},{"text":"还有咱们的中央候补委员聂元梓同志也在","box":[[191.18,449.44],[380.18,449.44],[380.18,459.94],[191.18,459.94]]},{"text":"下边搞串联","box":[[72,466.48],[124.5,466.48],[124.5,476.98],[72,476.98]]},{"text":"，","box":[[124.52,466.48],[135.01999999999998,466.48],[135.01999999999998,476.98],[124.52,476.98]]},{"text":"等","box":[[133.64,466.48],[144.14,466.48],[144.14,476.98],[133.64,476.98]]},{"text":"等。我感到问题严重","box":[[144.14,466.48],[237.2645,466.48],[237.2645,476.98],[144.14,476.98]]},{"text":"，","box":[[237.26,466.48],[247.76,466.48],[247.76,476.98],[237.26,476.98]]},{"text":"我都听懵了","box":[[246.38,466.48],[298.88,466.48],[298.88,476.98],[246.38,476.98]]},{"text":"，","box":[[298.9,466.48],[309.4,466.48],[309.4,476.98],[298.9,476.98]]},{"text":"这是谁","box":[[308.02,466.48],[339.52,466.48],[339.52,476.98],[308.02,476.98]]},{"text":"、","box":[[339.52,466.48],[350.02,466.48],[350.02,476.98],[339.52,476.98]]},{"text":"怎么向","box":[[348.64,466.48],[380.14,466.48],[380.14,476.98],[348.64,476.98]]},{"text":"周总理汇报我什么呢？我真有口难辩","box":[[72,483.46000000000004],[240,483.46000000000004],[240,493.96000000000004],[72,493.96000000000004]]},{"text":"，","box":[[240.02,483.46000000000004],[250.52,483.46000000000004],[250.52,493.96000000000004],[240.02,493.96000000000004]]},{"text":"怎么才能向周总理说清楚呢。","box":[[248.92,483.46000000000004],[385.41999999999996,483.46000000000004],[385.41999999999996,493.96000000000004],[248.92,493.96000000000004]]},{"text":"会后","box":[[72,500.5],[93,500.5],[93,511],[72,511]]},{"text":"，","box":[[93.02,500.5],[103.52,500.5],[103.52,511],[93.02,511]]},{"text":"我仔细地想","box":[[100.52,500.5],[153.01999999999998,500.5],[153.01999999999998,511],[100.52,511]]},{"text":"，","box":[[153.02,500.5],[163.52,500.5],[163.52,511],[153.02,511]]},{"text":"这是怎么回事","box":[[160.52,500.5],[223.52,500.5],[223.52,511],[160.52,511]]},{"text":"，","box":[[223.52,500.5],[234.02,500.5],[234.02,511],[223.52,511]]},{"text":"怎么想也想不通。","box":[[231.02,500.5],[315.02,500.5],[315.02,511],[231.02,511]]},{"text":"当时停止开会","box":[[312.017,500.5],[375.017,500.5],[375.017,511],[312.017,511]]},{"text":"，","box":[[375.04,500.5],[385.54,500.5],[385.54,511],[375.04,511]]},{"text":"大家反映的思想情况都是一样么","box":[[72,517.54],[220.49099999999996,517.54],[220.49099999999996,528.04],[72,528.04]]},{"text":"，","box":[[220.82,517.54],[231.32,517.54],[231.32,528.04],[220.82,528.04]]},{"text":"怎么他们互相问会议怎么停止不","box":[[231.5,517.54],[379.99100000000016,517.54],[379.99100000000016,528.04],[231.5,528.04]]},{"text":"开了","box":[[72,534.52],[93,534.52],[93,545.02],[72,545.02]]},{"text":"，","box":[[93.02,534.52],[103.52,534.52],[103.52,545.02],[93.02,545.02]]},{"text":"以及说些谁反对毛主席当国家主席就把他揪出来的话","box":[[100.1,534.52],[341.6,534.52],[341.6,545.02],[100.1,545.02]]},{"text":"，","box":[[341.62,534.52],[352.12,534.52],[352.12,545.02],[341.62,545.02]]},{"text":"就没有","box":[[348.7,534.52],[380.2,534.52],[380.2,545.02],[348.7,545.02]]},{"text":"事","box":[[72,551.56],[82.5,551.56],[82.5,562.06],[72,562.06]]},{"text":"，","box":[[82.64,551.56],[93.14,551.56],[93.14,562.06],[82.64,562.06]]},{"text":"而我和河北代表说了那两句话","box":[[93.26,551.56],[231.14599999999996,551.56],[231.14599999999996,562.06],[93.26,562.06]]},{"text":"，","box":[[231.5,551.56],[242,551.56],[242,562.06],[231.5,562.06]]},{"text":"就是串联呢？我还拒绝了倪志","box":[[242.12,551.56],[380.0060000000002,551.56],[380.0060000000002,562.06],[242.12,562.06]]},{"text":"福","box":[[72,568.6],[82.5,568.6],[82.5,579.1],[72,579.1]]},{"text":"、","box":[[82.64,568.6],[93.14,568.6],[93.14,579.1],[82.64,579.1]]},{"text":"刘锡昌","box":[[93.26,568.6],[125.00000000000001,568.6],[125.00000000000001,579.1],[93.26,579.1]]},{"text":"、","box":[[125.18,568.6],[135.68,568.6],[135.68,579.1],[125.18,579.1]]},{"text":"张世忠他们问我的那些话呢","box":[[135.8,568.6],[263.0809999999999,568.6],[263.0809999999999,579.1],[135.8,579.1]]},{"text":"，","box":[[263.38,568.6],[273.88,568.6],[273.88,579.1],[263.38,579.1]]},{"text":"他们怎么就不是串联我","box":[[274,568.6],[380.07100000000014,568.6],[380.07100000000014,579.1],[274,579.1]]},{"text":"呢？你吴德和吴忠以及吴德和李雪峰的问话","box":[[72,585.58],[273.57899999999995,585.58],[273.57899999999995,596.08],[72,596.08]]},{"text":"，","box":[[274,585.58],[284.5,585.58],[284.5,596.08],[274,596.08]]},{"text":"不是一样的内容吗？","box":[[284.68,585.58],[380.08300000000014,585.58],[380.08300000000014,596.08],[284.68,596.08]]}]}