24.5]]},{"text":"但是她太天真了","box":[[264.76,114],[338.26,114],[338.26,124.5],[264.76,124.5]]},{"text":"，","box":[[338.26,114],[348.76,114],[348.76,124.5],[338.26,124.5]]},{"text":"居然","box":[[348.34,114],[369.34,114],[369.34,124.5],[348.34,124.5]]},{"text":"相信谢富治真的要“整风”了","box":[[61.2,131],[194.214,131],[194.214,141.5],[61.2,141.5]]},{"text":"，","box":[[194.24,131],[204.74,131],[204.74,141.5],[194.24,141.5]]},{"text":"便提了些意见。这使谢富治及其支持","box":[[203,131],[369.3095,131],[369.3095,141.5],[203,141.5]]},{"text":"者非常愤怒","box":[[61.2,148.03999999999996],[113.7,148.03999999999996],[113.7,158.53999999999996],[61.2,158.53999999999996]]},{"text":"，","box":[[113.72,148.03999999999996],[124.22,148.03999999999996],[124.22,158.53999999999996],[113.72,158.53999999999996]]},{"text":"他们不仅在会议上对聂元梓大加批判","box":[[120.8,148.03999999999996],[288.8,148.03999999999996],[288.8,158.53999999999996],[120.8,158.53999999999996]]},{"text":"，","box":[[288.82,148.03999999999996],[299.32,148.03999999999996],[299.32,158.53999999999996],[288.82,158.53999999999996]]},{"text":"还把这场批判扩","box":[[295.9,148.03999999999996],[369.4,148.03999999999996],[369.4,158.53999999999996],[295.9,158.53999999999996]]},{"text":"大到北京市基层。","box":[[61.2,165.07999999999998],[145.2,165.07999999999998],[145.2,175.57999999999998],[61.2,175.57999999999998]]},{"text":"关于事情的经过","box":[[82.2,182.06],[156.0675,182.06],[156.0675,192.56],[82.2,192.56]]},{"text":"，","box":[[156.2,182.06],[166.7,182.06],[166.7,192.56],[156.2,192.56]]},{"text":"聂元梓有详细回忆","box":[[166.76,182.06],[251.23250000000002,182.06],[251.23250000000002,192.56],[166.76,192.56]]},{"text":"，","box":[[251.38,182.06],[261.88,182.06],[261.88,192.56],[251.38,192.56]]},{"text":"70","box":[[261.94,178.04000000000002],[268.98999999999995,178.04000000000002],[268.98999999999995,185.06000000000003],[261.94,185.06000000000003]]},{"text":"","box":[[268.98999999999995,178.04000000000002],[274.42,178.04000000000002],[274.42,178.04000000000002],[268.98999999999995,178.04000000000002]]},{"text":"但她对市革委会开会","box":[[274.42,182.06],[369.3925000000001,182.06],[369.3925000000001,192.56],[274.42,192.56]]},{"text":"时间的记述有误","box":[[61.2,199.10000000000002],[134.7,199.10000000000002],[134.7,209.60000000000002],[61.2,209.60000000000002]]},{"text":"，","box":[[134.72,199.10000000000002],[145.22,199.10000000000002],[145.22,209.60000000000002],[134.72,209.60000000000002]]},{"text":"会议应该是在","box":[[143.12,199.10000000000002],[206.12,199.10000000000002],[206.12,209.60000000000002],[143.12,209.60000000000002]]},{"text":"","box":[[206.12,199.10000000000002],[208.76,199.10000000000002],[208.76,199.10000000000002],[206.12,199.10000000000002]]},{"text":"3","box":[[208.76,199.10000000000002],[214.01,199.10000000000002],[214.01,209.60000000000002],[208.76,209.60000000000002]]},{"text":"","box":[[214.01,199.10000000000002],[216.68,199.10000000000002],[216.68,199.10000000000002],[214.01,199.10000000000002]]},{"text":"月中旬的末尾开始的","box":[[216.68,199.10000000000002],[311.117,199.10000000000002],[311.117,209.60000000000002],[216.68,209.60000000000002]]},{"text":"，","box":[[311.14,199.10000000000002],[321.64,199.10000000000002],[321.64,209.60000000000002],[311.14,209.60000000000002]]},{"text":"而不是在","box":[[319.54,199.10000000000002],[361.54,199.10000000000002],[361.54,209.60000000000002],[319.54,209.60000000000002]]},{"text":"","box":[[361.54,199.10000000000002],[364.18,199.10000000000002],[364.18,199.10000000000002],[361.54,199.10000000000002]]},{"text":"3","box":[[364.18,199.10000000000002],[369.43,199.10000000000002],[369.43,209.60000000000002],[364.18,209.60000000000002]]},{"text":"月下旬。这次会议","box":[[61.2,216.14],[145.2,216.14],[145.2,226.64],[61.2,226.64]]},{"text":"，","box":[[145.22,216.14],[155.72,216.14],[155.72,226.64],[145.22,226.64]]},{"text":"应该就是","box":[[155.72,216.14],[197.72,216.14],[197.72,226.64],[155.72,226.64]]},{"text":"","box":[[197.72,216.14],[200.96,216.14],[200.96,216.14],[197.72,216.14]]},{"text":"3","box":[[200.96,216.14],[206.21,216.14],[206.21,226.64],[200.96,226.64]]},{"text":"","box":[[206.21,216.14],[209.48,216.14],[209.48,216.14],[206.21,216.14]]},{"text":"月","box":[[209.48,216.14],[219.98,216.14],[219.98,226.64],[209.48,226.64]]},{"text":"","box":[[219.98,216.14],[223.16,216.14],[223.16,216.14],[219.98,216.14]]},{"text":"18","box":[[223.16,216.14],[233.69,216.14],[233.69,226.64],[223.16,226.64]]},{"text":"","box":[[233.69,216.14],[236.96,216.14],[236.96,216.14],[233.69,216.14]]},{"text":"日至","box":[[236.96,216.14],[257.96000000000004,216.14],[257.96000000000004,226.64],[236.96,226.64]]},{"text":"","box":[[257.96000000000004,216.14],[261.16,216.14],[261.16,216.14],[257.96000000000004,216.14]]},{"text":"25","box":[[261.16,216.14],[271.69,216.14],[271.69,226.64],[261.16,226.64]]},{"text":"","box":[[271.69,216.14],[274.9,216.14],[274.9,216.14],[271.69,216.14]]},{"text":"日召开的北京市革委","box":[[274.9,216.14],[369.4,216.14],[369.4,226.64],[274.9,226.64]]},{"text":"会第十二次全体委员会议。","box":[[61.2,233.18],[187.2,233.18],[187.2,243.68],[61.2,243.68]]},{"text":"据聂元梓回忆","box":[[82.2,250.16000000000003],[145.2,250.16000000000003],[145.2,260.66],[82.2,260.66]]},{"text":"，","box":[[145.22,250.16000000000003],[155.72,250.16000000000003],[155.72,260.66],[145.22,260.66]]},{"text":"谢富治在市革委会会议上说","box":[[152.3,250.16000000000003],[278.3,250.16000000000003],[278.3,260.66],[152.3,260.66]]},{"text":"，","box":[[278.32,250.16000000000003],[288.82,250.16000000000003],[288.82,260.66],[278.32,260.66]]},{"text":"各级革命委员会都","box":[[285.4,250.16000000000003],[369.4,250.16000000000003],[369.4,260.66],[285.4,260.66]]},{"text":"要革命化","box":[[61.2,267.2],[103.2,267.2],[103.2,277.7],[61.2,277.7]]},{"text":"，","box":[[103.22,267.2],[113.72,267.2],[113.72,277.7],[103.22,277.7]]},{"text":"先制定一个革命化的条例。","box":[[111.44,267.2],[237.44,267.2],[237.44,277.7],[111.44,277.7]]},{"text":"为此要先进行整风","box":[[235.1615,267.2],[319.1615,267.2],[319.1615,277.7],[235.1615,277.7]]},{"text":"，","box":[[319.18,267.2],[329.68,267.2],[329.68,277.7],[319.18,277.7]]},{"text":"各级革委","box":[[327.4,267.2],[369.4,267.2],[369.4,277.7],[327.4,277.7]]},{"text":"会都要整风","box":[[61.2,284.24],[113.7,284.24],[113.7,294.74],[61.2,294.74]]},{"text":"，","box":[[113.72,284.24],[124.22,284.24],[124.22,294.74],[113.72,294.74]]},{"text":"要真正实现“革命化”","box":[[123.68,284.24],[228.1445,284.24],[228.1445,294.74],[123.68,294.74]]},{"text":"，","box":[[222.92,284.24],[233.42,284.24],[233.42,294.74],[222.92,294.74]]},{"text":"市革委会要专门开几天会进行","box":[[232.88,284.24],[369.38,284.24],[369.38,294.74],[232.88,294.74]]},{"text":"整风。","box":[[61.2,301.22],[92.7,301.22],[92.7,311.72],[61.2,311.72]]},{"text":"聂元梓在发言中表示同意谢富治的意见","box":[[82.2,318.26],[262.758,318.26],[262.758,328.76],[82.2,328.76]]},{"text":"，","box":[[263.14,318.26],[273.64,318.26],[273.64,328.76],[263.14,328.76]]},{"text":"并说市革委会需要进","box":[[273.76,318.26],[369.3520000000001,318.26],[369.3520000000001,328.76],[273.76,328.76]]},{"text":"行整风","box":[[61.2,335.3],[92.7,335.3],[92.7,345.8],[61.2,345.8]]},{"text":"，","box":[[92.72,335.3],[103.22,335.3],[103.22,345.8],[92.72,345.8]]},{"text":"但不是只制定几项革命化的条例","box":[[99.8,335.3],[246.8,335.3],[246.8,345.8],[99.8,345.8]]},{"text":"，","box":[[246.8,335.3],[257.3,335.3],[257.3,345.8],[246.8,345.8]]},{"text":"而是结合实际认真进行整","box":[[253.9,335.3],[369.4,335.3],[369.4,345.8],[253.9,345.8]]},{"text":"风。","box":[[61.2,352.28],[82.2,352.28],[82.2,362.78],[61.2,362.78]]},{"text":"首先要整谢副总理的风","box":[[79.92150000000001,352.28],[184.9215,352.28],[184.9215,362.78],[79.92150000000001,362.78]]},{"text":"，","box":[[184.94,352.28],[195.44,352.28],[195.44,362.78],[184.94,362.78]]},{"text":"要一级一级地从上往下整","box":[[193.16,352.28],[308.65999999999997,352.28],[308.65999999999997,362.78],[193.16,362.78]]},{"text":"，","box":[[308.68,352.28],[319.18,352.28],[319.18,362.78],[308.68,362.78]]},{"text":"先从谢副总","box":[[316.9,352.28],[369.4,352.28],[369.4,362.78],[316.9,362.78]]},{"text":"理开始。隨后","box":[[61.2,369.32],[124.2,369.32],[124.2,379.82],[61.2,379.82]]},{"text":"，","box":[[124.22,369.32],[134.72,369.32],[134.72,379.82],[124.22,379.82]]},{"text":"聂元梓讲了几条意见。","box":[[134.72,369.32],[239.72,369.32],[239.72,379.82],[134.72,379.82]]},{"text":"所谓“整风”","box":[[82.2,386.38],[144.6645,386.38],[144.6645,396.88],[82.2,396.88]]},{"text":"，","box":[[139.46,386.38],[149.96,386.38],[149.96,396.88],[139.46,396.88]]},{"text":"谢富治不过是说说而已。他没有想到聂元梓会当","box":[[149.42,386.38],[369.3845,386.38],[369.3845,396.88],[149.42,396.88]]},{"text":"真","box":[[61.2,403.36],[71.7,403.36],[71.7,413.86],[61.2,413.86]]},{"text":"，","box":[[71.7,403.36],[82.2,403.36],[82.2,413.86],[71.7,413.86]]},{"text":"会把火烧到他的头上。听了聂元梓提的意见","box":[[80.46,403.36],[278.217,403.36],[278.217,413.86],[80.46,413.86]]},{"text":"，","box":[[278.26,403.36],[288.76,403.36],[288.76,413.86],[278.26,413.86]]},{"text":"谢富治竟然“气得","box":[[287.02,403.36],[369.3295,403.36],[369.3295,413.86],[287.02,413.86]]},{"text":"手都直哆嗦”","box":[[61.2,420.4],[124.2,420.4],[124.2,430.9],[61.2,430.9]]},{"text":"。","box":[[118.91850000000001,420.4],[129.4185,420.4],[129.4185,430.9],[118.91850000000001,430.9]]},{"text":"聂元梓对谢富治也有一个认识过程。因为参加北京市革委会的","box":[[82.2,437.44],[369.0600000000001,437.44],[369.0600000000001,447.94],[82.2,447.94]]},{"text":"工作","box":[[61.2,454.48],[82.2,454.48],[82.2,464.98],[61.2,464.98]]},{"text":"，","box":[[82.2,454.48],[92.7,454.48],[92.7,464.98],[82.2,464.98]]},{"text":"她比新北大公社的师生知道得多一些。","box":[[89.3,454.48],[267.8,454.48],[267.8,464.98],[89.3,464.98]]},{"text":"比如谢富治在市革委会","box":[[264.377,454.48],[369.377,454.48],[369.377,464.98],[264.377,464.98]]},{"text":"讲话","box":[[61.2,471.46000000000004],[82.2,471.46000000000004],[82.2,481.96000000000004],[61.2,481.96000000000004]]},{"text":"，","box":[[82.2,471.46000000000004],[92.7,471.46000000000004],[92.7,481.96000000000004],[82.2,481.96000000000004]]},{"text":"言必称“我们敬爱的江青同志是高举毛泽东思想伟大红旗的","box":[[91.94,471.46000000000004],[364.163,471.46000000000004],[364.163,481.96000000000004],[91.94,481.96000000000004]]},{"text":"，","box":[[364.18,471.46000000000004],[374.68,471.46000000000004],[374.68,481.96000000000004],[364.18,481.96000000000004]]},{"text":"我们一定要紧跟江青同志”","box":[[61.2,488.5],[187.2,488.5],[187.2,499],[61.2,499]]},{"text":"，","box":[[182,488.5],[192.5,488.5],[192.5,499],[182,499]]},{"text":"还要一一历数中央文革的成员","box":[[191.72,488.5],[328.15700000000004,488.5],[328.15700000000004,499],[191.72,499]]},{"text":"，","box":[[328.18,488.5],[338.68,488.5],[338.68,499],[328.18,499]]},{"text":"不厌其","box":[[337.9,488.5],[369.4,488.5],[369.4,499],[337.9,499]]},{"text":"烦地重复道：","box":[[61.2,505.53999999999996],[124.2,505.53999999999996],[124.2,516.04],[61.2,516.04]]},{"text":"“康老是高举毛泽东思想伟大红旗的”","box":[[118.14150000000001,505.53999999999996],[296.6415,505.53999999999996],[296.6415,516.04],[118.14150000000001,516.04]]},{"text":"，","box":[[291.46,505.53999999999996],[301.96,505.53999999999996],[301.96,516.04],[291.46,516.04]]},{"text":"“伯达同志是高","box":[[295.96,505.53999999999996],[369.397,505.53999999999996],[369.397,516.04],[295.96,516.04]]},{"text":"举毛泽东思想伟大红旗的”","box":[[61.2,522.52],[188.52000000000004,522.52],[188.52000000000004,533.02],[61.2,533.02]]},{"text":"，","box":[[183.44,522.52],[193.94,522.52],[193.94,533.02],[183.44,533.02]]},{"text":"“王力同志是高举毛泽东思想伟大红旗","box":[[188.84,522.52],[369.3125000000001,522.52],[369.3125000000001,533.02],[188.84,533.02]]},{"text":"的”","box":[[61.2,539.56],[82.2,539.56],[82.2,550.06],[61.2,550.06]]},{"text":"，","box":[[76.98,539.56],[87.48,539.56],[87.48,550.06],[76.98,550.06]]},{"text":"“关锋同志是高举毛泽东思想伟大","box":[[78.84,539.56],[236.27700000000002,539.56],[236.27700000000002,550.06],[78.84,550.06]]},{"text":"红旗的”","box":[[236.3,539.56],[278.3,539.56],[278.3,550.06],[236.3,550.06]]},{"text":"，","box":[[273.1,539.56],[283.6,539.56],[283.6,550.06],[273.1,550.06]]},{"text":"“戚本禹同志是高举","box":[[274.96,539.56],[369.397,539.56],[369.397,550.06],[274.96,550.06]]},{"text":"毛泽东思想伟大红旗的”","box":[[61.2,556.6],[176.7,556.6],[176.7,567.1],[61.2,567.1]]},{"text":"，","box":[[171.44,556.6],[181.94,556.6],[181.94,567.1],[171.44,567.1]]},{"text":"等等。","box":[[179.66,556.6],[211.16,556.6],[211.16,567.1],[179.66,567.1]]},{"text":"他还宣布","box":[[208.8815,556.6],[250.8815,556.6],[250.8815,567.1],[208.8815,567.1]]},{"text":"，","box":[[250.9,556.6],[261.4,556.6],[261.4,567.1],[250.9,567.1]]},{"text":"谁要反对哪一个都不行","box":[[259.12,556.6],[364.12,556.6],[364.12,567.1],[259.12,567.1]]},{"text":"，","box":[[364.12,556.6],[374.62,556.6],[374.62,567.1],[364.12,567.1]]},{"text":"70","box":[[61.2,595.06],[70.2,595.06],[70.2,604.06],[61.2,604.06]]},{"text":"","box":[[70.2,595.06],[74.7,595.06],[74.7,595.06],[70.2,595.06]]},{"text":"聂元梓：《聂元梓回忆录》，第","box":[[74.7,595.06],[200.70000000000002,595.06],[200.70000000000002,604.06],[74.7,604.06]]},{"text":"","box":[[200.70000000000002,595.06],[202.94,595.06],[202.94,595.06],[200.70000000000002,595.06]]},{"text":"250","box":[[202.94,595.06],[216.44,595.06],[216.44,604.06],[202.94,604.06]]},{"text":"—","box":[[216.44,595.06],[225.44,595.06],[225.44,604.06],[216.44,604.06]]},{"text":"267","box":[[225.44,595.06],[238.94,595.06],[238.94,604.06],[225.44,604.06]]},{"text":"","box":[[238.94,595.06],[241.22,595.06],[241.22,595.06],[238.94,595.06]]},{"text":"页。","box":[[241.22,595.06],[259.22,595.06],[259.22,604.06],[241.22,604.06]]}]}