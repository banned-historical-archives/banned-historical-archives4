15.36],[218.3,425.86],[207.8,425.86]]},{"text":"是没有资格被利用的。于是","box":[[217.88,415.36],[343.46,415.36],[343.46,425.86],[217.88,425.86]]},{"text":"，","box":[[343.48,415.36],[353.98,415.36],[353.98,425.86],[343.48,425.86]]},{"text":"“被","box":[[348.34,415.36],[369.34,415.36],[369.34,425.86],[348.34,425.86]]},{"text":"利用光荣”的哲学就问世了。唯其被利用","box":[[61.2,432.4],[249.36,432.4],[249.36,442.9],[61.2,442.9]]},{"text":"，","box":[[249.4,432.4],[259.9,432.4],[259.9,442.9],[249.4,442.9]]},{"text":"就更加响当当","box":[[259.48,432.4],[322.48,432.4],[322.48,442.9],[259.48,442.9]]},{"text":"，","box":[[322.48,432.4],[332.98,432.4],[332.98,442.9],[322.48,442.9]]},{"text":"“谁敢否","box":[[327.28,432.4],[369.3325,432.4],[369.3325,442.9],[327.28,442.9]]},{"text":"定我们的大方向","box":[[61.2,449.44],[134.7,449.44],[134.7,459.94],[61.2,459.94]]},{"text":"？”喊得就更加理直气壮。","box":[[134.72,449.44],[260.72,449.44],[260.72,459.94],[134.72,459.94]]},{"text":"、","box":[[105.2,476.98],[116.18,476.98],[116.18,487.96000000000004],[105.2,487.96000000000004]]},{"text":"","box":[[116.18,476.98],[171.2,476.98],[171.2,476.98],[116.18,476.98]]},{"text":"，","box":[[171.2,476.98],[182.17999999999998,476.98],[182.17999999999998,487.96000000000004],[171.2,487.96000000000004]]},{"text":"抛出了关锋","box":[[82.2,499],[134.7,499],[134.7,509.5],[82.2,509.5]]},{"text":"、","box":[[134.72,499],[145.22,499],[145.22,509.5],[134.72,509.5]]},{"text":"王力后","box":[[142.04,499],[173.54,499],[173.54,509.5],[142.04,509.5]]},{"text":"，","box":[[173.54,499],[184.04,499],[184.04,509.5],[173.54,509.5]]},{"text":"毛泽东视察了三大区。","box":[[180.86,499],[285.86,499],[285.86,509.5],[180.86,509.5]]},{"text":"1967","box":[[282.7,499],[303.7525,499],[303.7525,509.5],[282.7,509.5]]},{"text":"","box":[[303.7525,499],[306.34,499],[306.34,499],[303.7525,499]]},{"text":"年","box":[[306.34,499],[316.84,499],[316.84,509.5],[306.34,509.5]]},{"text":"","box":[[316.84,499],[319.48,499],[319.48,499],[316.84,499]]},{"text":"10","box":[[319.48,499],[329.95000000000005,499],[329.95000000000005,509.5],[319.48,509.5]]},{"text":"","box":[[329.95000000000005,499],[332.62,499],[332.62,499],[329.95000000000005,499]]},{"text":"月","box":[[332.62,499],[343.12,499],[343.12,509.5],[332.62,509.5]]},{"text":"","box":[[343.12,499],[345.7,499],[345.7,499],[343.12,499]]},{"text":"7","box":[[345.7,499],[350.95,499],[350.95,509.5],[345.7,509.5]]},{"text":"","box":[[350.95,499],[353.62,499],[353.62,499],[350.95,499]]},{"text":"日","box":[[353.62,499],[364.12,499],[364.12,509.5],[353.62,509.5]]},{"text":"，","box":[[364.12,499],[374.62,499],[374.62,509.5],[364.12,509.5]]},{"text":"中共中央转发了","box":[[61.2,516.04],[134.7,516.04],[134.7,526.54],[61.2,526.54]]},{"text":"《毛主席视察华北","box":[[131.277,516.04],[215.277,516.04],[215.277,526.54],[131.277,526.54]]},{"text":"、","box":[[215.3,516.04],[225.8,516.04],[225.8,526.54],[215.3,526.54]]},{"text":"中南和华东地区时的重要指示》","box":[[222.38,516.04],[369.38,516.04],[369.38,526.54],[222.38,526.54]]},{"text":"。","box":[[364.1615,516.04],[374.6615,516.04],[374.6615,526.54],[364.1615,526.54]]},{"text":"文革史学者王年一指出","box":[[82.2,533.08],[187.2,533.08],[187.2,543.58],[82.2,543.58]]},{"text":"，","box":[[187.22,533.08],[197.72,533.08],[197.72,543.58],[187.22,543.58]]},{"text":"“毛泽东的用意","box":[[191.9,533.08],[265.4,533.08],[265.4,543.58],[191.9,543.58]]},{"text":"，","box":[[265.42,533.08],[275.92,533.08],[275.92,543.58],[265.42,543.58]]},{"text":"是在坚持‘文化大革","box":[[275.38,533.08],[369.3445,533.08],[369.3445,543.58],[275.38,543.58]]},{"text":"命’","box":[[61.2,550.06],[82.2,550.06],[82.2,560.56],[61.2,560.56]]},{"text":"、","box":[[76.98,550.06],[87.48,550.06],[87.48,560.56],[76.98,560.56]]},{"text":"坚持全面夺权的前提下","box":[[87.08,550.06],[192.07999999999998,550.06],[192.07999999999998,560.56],[87.08,560.56]]},{"text":"，","box":[[192.08,550.06],[202.58,550.06],[202.58,560.56],[192.08,560.56]]},{"text":"因势利导","box":[[202.16,550.06],[244.16,550.06],[244.16,560.56],[202.16,560.56]]},{"text":"，","box":[[244.16,550.06],[254.66,550.06],[254.66,560.56],[244.16,560.56]]},{"text":"纠正错误。又要纠正由全","box":[[254.26,550.06],[369.34,550.06],[369.34,560.56],[254.26,560.56]]},{"text":"面夺权派生出来的种种错误","box":[[61.2,567.1],[187.2,567.1],[187.2,577.6],[61.2,577.6]]},{"text":"，","box":[[187.22,567.1],[197.72,567.1],[197.72,577.6],[187.22,577.6]]},{"text":"又要坚持全面夺权","box":[[196.94,567.1],[280.94,567.1],[280.94,577.6],[196.94,577.6]]},{"text":"，","box":[[280.96,567.1],[291.46,567.1],[291.46,577.6],[280.96,577.6]]},{"text":"这是十分矛盾的。","box":[[290.68,567.1],[374.68,567.1],[374.68,577.6],[290.68,577.6]]},{"text":"所有要求纠正错误的内容","box":[[61.2,584.14],[176.7,584.14],[176.7,594.64],[61.2,594.64]]},{"text":"，","box":[[176.72,584.14],[187.22,584.14],[187.22,594.64],[176.72,594.64]]},{"text":"还是只反对现象","box":[[184.94,584.14],[258.44,584.14],[258.44,594.64],[184.94,594.64]]},{"text":"，","box":[[258.46,584.14],[268.96,584.14],[268.96,594.64],[258.46,594.64]]},{"text":"不反对真实的原因。","box":[[266.68,584.14],[361.18,584.14],[361.18,594.64],[266.68,594.64]]},{"text":"因","box":[[358.9015,584.14],[369.4015,584.14],[369.4015,594.64],[358.9015,594.64]]}]}