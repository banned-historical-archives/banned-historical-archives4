5],[61.2,141.5]]},{"text":"，","box":[[270.4,131],[280.9,131],[280.9,141.5],[270.4,141.5]]},{"text":"不过是为“井","box":[[278.86,131],[339.82300000000004,131],[339.82300000000004,141.5],[278.86,141.5]]},{"text":"”","box":[[339.82,131],[350.32,131],[350.32,141.5],[339.82,141.5]]},{"text":"“","box":[[343.06,131],[353.56,131],[353.56,141.5],[343.06,141.5]]},{"text":"红”","box":[[353.56,131],[374.62,131],[374.62,141.5],[353.56,141.5]]},{"text":"翻案制造声势而已。而为“井","box":[[61.2,148.03999999999996],[197.21699999999998,148.03999999999996],[197.21699999999998,158.53999999999996],[61.2,158.53999999999996]]},{"text":"”","box":[[197.24,148.03999999999996],[207.74,148.03999999999996],[207.74,158.53999999999996],[197.24,158.53999999999996]]},{"text":"“","box":[[202.28,148.03999999999996],[212.78,148.03999999999996],[212.78,158.53999999999996],[202.28,158.53999999999996]]},{"text":"红”翻案","box":[[212.78,148.03999999999996],[254.4755,148.03999999999996],[254.4755,158.53999999999996],[212.78,158.53999999999996]]},{"text":"，","box":[[254.5,148.03999999999996],[265,148.03999999999996],[265,158.53999999999996],[254.5,158.53999999999996]]},{"text":"不过是一种手段","box":[[264.76,148.03999999999996],[338.197,148.03999999999996],[338.197,158.53999999999996],[264.76,158.53999999999996]]},{"text":"，","box":[[338.2,148.03999999999996],[348.7,148.03999999999996],[348.7,158.53999999999996],[338.2,158.53999999999996]]},{"text":"其真","box":[[348.46,148.03999999999996],[369.46,148.03999999999996],[369.46,158.53999999999996],[348.46,158.53999999999996]]},{"text":"正目的","box":[[61.2,165.07999999999998],[92.7,165.07999999999998],[92.7,175.57999999999998],[61.2,175.57999999999998]]},{"text":"，","box":[[92.72,165.07999999999998],[103.22,165.07999999999998],[103.22,175.57999999999998],[92.72,175.57999999999998]]},{"text":"是“井冈山”这伙人还想在北大夺权。","box":[[103.22,165.07999999999998],[281.72,165.07999999999998],[281.72,175.57999999999998],[103.22,175.57999999999998]]},{"text":"剩下的就是老调重弹了","box":[[82.2,182.06],[187.2,182.06],[187.2,192.56],[82.2,192.56]]},{"text":"，","box":[[187.22,182.06],[197.72,182.06],[197.72,192.56],[187.22,192.56]]},{"text":"比如“聂","box":[[195.98,182.06],[236.237,182.06],[236.237,192.56],[195.98,192.56]]},{"text":"、","box":[[236.24,182.06],[246.74,182.06],[246.74,192.56],[236.24,192.56]]},{"text":"孙之流是不折不扣的‘二月","box":[[245,182.06],[369.3095,182.06],[369.3095,192.56],[245,192.56]]},{"text":"逆流派’","box":[[61.2,199.10000000000002],[103.2,199.10000000000002],[103.2,209.60000000000002],[61.2,209.60000000000002]]},{"text":"”","box":[[97.9815,199.10000000000002],[108.4815,199.10000000000002],[108.4815,209.60000000000002],[97.9815,209.60000000000002]]},{"text":"，","box":[[103.28,199.10000000000002],[113.78,199.10000000000002],[113.78,209.60000000000002],[103.28,209.60000000000002]]},{"text":"比如“反对谢富治","box":[[113.3,199.10000000000002],[196.88,199.10000000000002],[196.88,209.60000000000002],[113.3,209.60000000000002]]},{"text":"，","box":[[196.88,199.10000000000002],[207.38,199.10000000000002],[207.38,209.60000000000002],[196.88,209.60000000000002]]},{"text":"就是要夺北京市革委会的权”","box":[[206.96,199.10000000000002],[343.46000000000004,199.10000000000002],[343.46000000000004,209.60000000000002],[206.96,209.60000000000002]]},{"text":"，","box":[[338.26,199.10000000000002],[348.76,199.10000000000002],[348.76,209.60000000000002],[338.26,209.60000000000002]]},{"text":"比如","box":[[348.34,199.10000000000002],[369.34,199.10000000000002],[369.34,209.60000000000002],[348.34,209.60000000000002]]},{"text":"“干扰毛主席的伟大战略部署”","box":[[61.2,216.14],[208.2,216.14],[208.2,226.64],[61.2,226.64]]},{"text":"，","box":[[203,216.14],[213.5,216.14],[213.5,226.64],[203,226.64]]},{"text":"等等。北大“井冈山”同校外势力","box":[[213.08,216.14],[369.3725,216.14],[369.3725,226.64],[213.08,226.64]]},{"text":"密切配合","box":[[61.2,233.18],[103.2,233.18],[103.2,243.68],[61.2,243.68]]},{"text":"，","box":[[103.22,233.18],[113.72,233.18],[113.72,243.68],[103.22,243.68]]},{"text":"在这些方面大造舆论","box":[[111.44,233.18],[205.94,233.18],[205.94,243.68],[111.44,243.68]]},{"text":"，","box":[[205.94,233.18],[216.44,233.18],[216.44,243.68],[205.94,243.68]]},{"text":"但这些指责都没有事实根据","box":[[214.16,233.18],[340.15999999999997,233.18],[340.15999999999997,243.68],[214.16,243.68]]},{"text":"，","box":[[340.18,233.18],[350.68,233.18],[350.68,243.68],[340.18,243.68]]},{"text":"现在","box":[[348.4,233.18],[369.4,233.18],[369.4,243.68],[348.4,243.68]]},{"text":"就更加不值一驳了。","box":[[61.2,250.16000000000003],[155.7,250.16000000000003],[155.7,260.66],[61.2,260.66]]},{"text":"北大","box":[[82.2,267.2],[103.2,267.2],[103.2,277.7],[82.2,277.7]]},{"text":"“井冈山”","box":[[99.1785,267.2],[151.67849999999999,267.2],[151.67849999999999,277.7],[99.1785,277.7]]},{"text":"坚决地站在关锋","box":[[147.65699999999998,267.2],[221.15699999999998,267.2],[221.15699999999998,277.7],[147.65699999999998,277.7]]},{"text":"、","box":[[221.18,267.2],[231.68,267.2],[231.68,277.7],[221.18,277.7]]},{"text":"吴传启团伙一边并为其所利用","box":[[227.66,267.2],[364.15999999999997,267.2],[364.15999999999997,277.7],[227.66,277.7]]},{"text":"，","box":[[364.18,267.2],[374.68,267.2],[374.68,277.7],[364.18,277.7]]},{"text":"是众所周知的事情。这个团伙垮台之后","box":[[61.2,284.24],[237.957,284.24],[237.957,294.74],[61.2,294.74]]},{"text":"，","box":[[237.98,284.24],[248.48,284.24],[248.48,294.74],[237.98,294.74]]},{"text":"北大“井冈山”急需找一个","box":[[246.74,284.24],[369.3065,284.24],[369.3065,294.74],[246.74,294.74]]},{"text":"理由为自己辩护。","box":[[61.2,301.22],[145.2,301.22],[145.2,311.72],[61.2,311.72]]},{"text":"9","box":[[145.22,301.22],[150.47,301.22],[150.47,311.72],[145.22,311.72]]},{"text":"","box":[[150.47,301.22],[153.44,301.22],[153.44,301.22],[150.47,301.22]]},{"text":"月","box":[[153.44,301.22],[163.94,301.22],[163.94,311.72],[153.44,311.72]]},{"text":"","box":[[163.94,301.22],[166.88,301.22],[166.88,301.22],[163.94,301.22]]},{"text":"16","box":[[166.88,301.22],[177.41,301.22],[177.41,311.72],[166.88,311.72]]},{"text":"","box":[[177.41,301.22],[180.38,301.22],[180.38,301.22],[177.41,301.22]]},{"text":"日的《新北大报》刊登