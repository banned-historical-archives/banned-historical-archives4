2],[135.02,311.72]]},{"text":"，","box":[[203.3,301.22],[213.8,301.22],[213.8,311.72],[203.3,311.72]]},{"text":"其中有这样一段文字：","box":[[213.8,301.22],[318.737,301.22],[318.737,311.72],[213.8,311.72]]},{"text":"（孙蓬一）","box":[[93.02,324.26],[145.51999999999998,324.26],[145.51999999999998,334.76],[93.02,334.76]]},{"text":"文革中","box":[[143.24149999999997,324.26],[174.74149999999997,324.26],[174.74149999999997,334.76],[143.24149999999997,334.76]]},{"text":"，","box":[[174.74,324.26],[185.24,324.26],[185.24,334.76],[174.74,334.76]]},{"text":"他最早提出中央文革极左路线的问题","box":[[182.96,324.26],[350.96000000000004,324.26],[350.96000000000004,334.76],[182.96,334.76]]},{"text":"，","box":[[350.98,324.26],[361.48,324.26],[361.48,334.76],[350.98,334.76]]},{"text":"公开","box":[[359.2,324.26],[380.2,324.26],[380.2,334.76],[359.2,334.76]]},{"text":"提出反对王","box":[[72,341.3],[124.5,341.3],[124.5,351.8],[72,351.8]]},{"text":"、","box":[[124.52,341.3],[135.01999999999998,341.3],[135.01999999999998,351.8],[124.52,351.8]]},{"text":"关","box":[[135.02,341.3],[145.52,341.3],[145.52,351.8],[135.02,351.8]]},{"text":"、","box":[[145.52,341.3],[156.02,341.3],[156.02,351.8],[145.52,351.8]]},{"text":"戚。我也是在这个过程中跟他成为好朋友的。","box":[[156.02,341.3],[366.02,341.3],[366.02,351.8],[156.02,351.8]]},{"text":"文革后期","box":[[93.02,358.28],[135.01999999999998,358.28],[135.01999999999998,368.78],[93.02,368.78]]},{"text":"，","box":[[135.02,358.28],[145.52,358.28],[145.52,368.78],[135.02,368.78]]},{"text":"工军宣队进校后","box":[[143.78,358.28],[217.28,358.28],[217.28,368.78],[143.78,368.78]]},{"text":"，","box":[[217.28,358.28],[227.78,358.28],[227.78,368.78],[217.28,368.78]]},{"text":"他一直挨整。那时候","box":[[226.04,358.28],[318.797,358.28],[318.797,368.78],[226.04,368.78]]},{"text":"，","box":[[318.82,358.28],[329.32,358.28],[329.32,368.78],[318.82,368.78]]},{"text":"我们偷偷地","box":[[327.64,358.28],[380.14,358.28],[380.14,368.78],[327.64,368.78]]},{"text":"见过面。","box":[[72,375.32],[114,375.32],[114,385.82],[72,385.82]]},{"text":"大概到了","box":[[110.5245,375.32],[152.5245,375.32],[152.5245,385.82],[110.5245,385.82]]},{"text":"“四人帮”","box":[[149.04899999999998,375.32],[201.54899999999998,375.32],[201.54899999999998,385.82],[149.04899999999998,385.82]]},{"text":"垮台不久","box":[[198.07349999999997,375.32],[240.07349999999997,375.32],[240.07349999999997,385.82],[198.07349999999997,385.82]]},{"text":"，","box":[[240.08,375.32],[250.58,375.32],[250.58,385.82],[240.08,385.82]]},{"text":"那是天安门事件一周年","box":[[247.1,375.32],[352.1,375.32],[352.1,385.82],[247.1,385.82]]},{"text":"（","box":[[348.6245,375.32],[359.1245,375.32],[359.1245,385.82],[348.6245,385.82]]},{"text":"1977","box":[[359.14,375.32],[380.14,375.32],[380.14,385.82],[359.14,385.82]]},{"text":"年）","box":[[72,392.38],[93,392.38],[93,402.88],[72,402.88]]},{"text":"，","box":[[93.02,392.38],[103.52,392.38],[103.52,402.88],[93.02,402.88]]},{"text":"","box":[[103.52,392.38],[108.8,392.38],[108.8,392.38],[103.52,392.38]]},{"text":"他还没被解放","box":[[108.8,392.38],[171.73700000000002,392.38],[171.73700000000002,402.88],[108.8,402.88]]},{"text":"，","box":[[171.74,392.38],[182.24,392.38],[182.24,402.88],[171.74,402.88]]},{"text":"他从劳改的地方跑出来","box":[[182.24,392.38],[287.24,392.38],[287.24,402.88],[182.24,402.88]]},{"text":"，","box":[[287.26,392.38],[297.76,392.38],[297.76,402.88],[287.26,402.88]]},{"text":"先到天安门广场","box":[[297.76,392.38],[371.26,392.38],[371.26,402.88],[297.76,402.88]]},{"text":"，","box":[[371.26,392.38],[381.76,392.38],[381.76,402.88],[371.26,402.88]]},{"text":"再到我家。我们一起去看看阮铭","box":[[72,409.36],[217.6245,409.36],[217.6245,419.86],[72,419.86]]},{"text":"，","box":[[217.64,409.36],[228.14,409.36],[228.14,419.86],[217.64,419.86]]},{"text":"他（到阮铭家）就跟阮铭说","box":[[226.76,409.36],[350.009,409.36],[350.009,419.86],[226.76,419.86]]},{"text":"，","box":[[350.02,409.36],[360.52,409.36],[360.52,419.86],[350.02,419.86]]},{"text":"我真","box":[[359.14,409.36],[380.14,409.36],[380.14,419.86],[359.14,419.86]]},{"text":"希望邓小平能够早一天出来工作。阮铭挺冷静的","box":[[72,426.4],[290.757,426.4],