.4,260.66],[295.9,260.66]]},{"text":"案例。","box":[[61.2,267.2],[92.7,267.2],[92.7,277.7],[61.2,277.7]]},{"text":"（七）","box":[[82.2,284.24],[113.7,284.24],[113.7,294.74],[82.2,294.74]]},{"text":"《刑事判决书》称：经法庭调查辩论","box":[[111.42150000000001,284.24],[277.13250000000005,284.24],[277.13250000000005,294.74],[111.42150000000001,294.74]]},{"text":"，","box":[[277.18,284.24],[287.68,284.24],[287.68,294.74],[277.18,294.74]]},{"text":"听取证人证言","box":[[286.54,284.24],[349.54,284.24],[349.54,294.74],[286.54,294.74]]},{"text":"，","box":[[349.54,284.24],[360.04,284.24],[360.04,294.74],[349.54,294.74]]},{"text":"核","box":[[358.9,284.24],[369.4,284.24],[369.4,294.74],[358.9,294.74]]},{"text":"实与本案直接有关的证据","box":[[61.2,301.22],[176.7,301.22],[176.7,311.72],[61.2,311.72]]},{"text":"，","box":[[176.72,301.22],[187.22,301.22],[187.22,311.72],[176.72,311.72]]},{"text":"事实清楚","box":[[186.8,301.22],[228.8,301.22],[228.8,311.72],[186.8,311.72]]},{"text":"，","box":[[228.8,301.22],[239.3,301.22],[239.3,311.72],[228.8,311.72]]},{"text":"证据确实","box":[[238.88,301.22],[280.88,301.22],[280.88,311.72],[238.88,311.72]]},{"text":"、","box":[[280.9,301.22],[291.4,301.22],[291.4,311.72],[280.9,311.72]]},{"text":"充分","box":[[290.98,301.22],[311.98,301.22],[311.98,311.72],[290.98,311.72]]},{"text":"，","box":[[311.98,301.22],[322.48,301.22],[322.48,311.72],[311.98,311.72]]},{"text":"足以认定。","box":[[322.12,301.22],[374.62,301.22],[374.62,311.72],[322.12,311.72]]},{"text":"但事实又是怎样的呢？请看被调查人谢甲林的讲述：","box":[[61.2,318.26],[302.7,318.26],[302.7,328.76],[61.2,328.76]]},{"text":"我原是最高检察院干部","box":[[82.2,335.3],[187.2,335.3],[187.2,345.8],[82.2,345.8]]},{"text":"，","box":[[187.22,335.3],[197.72,335.3],[197.72,345.8],[187.22,345.8]]},{"text":"文革初期","box":[[194.3,335.3],[236.3,335.3],[236.3,345.8],[194.3,345.8]]},{"text":"，","box":[[236.3,335.3],[246.8,335.3],[246.8,345.8],[236.3,345.8]]},{"text":"中组部抽调我参加北大工作","box":[[243.38,335.3],[369.38,335.3],[369.38,345.8],[243.38,345.8]]},{"text":"组","box":[[61.2,352.28],[71.7,352.28],[71.7,362.78],[61.2,362.78]]},{"text":"，","box":[[71.7,352.28],[82.2,352.28],[82.2,362.78],[71.7,362.78]]},{"text":"任保卫组（二组）副组长。工作组撤销走后","box":[[81.06,352.28],[277.1265,352.28],[277.1265,362.78],[81.06,362.78]]},{"text":"，","box":[[277.18,352.28],[287.68,352.28],[287.68,362.78],[277.18,362.78]]},{"text":"任校文革","box":[[286.54,352.28],[328.54,352.28],[328.54,362.78],[286.54,362.78]]},{"text":"、","box":[[328.54,352.28],[339.04,352.28],[339.04,362.78],[328.54,362.78]]},{"text":"工军宣","box":[[337.9,352.28],[369.4,352.28],[369.4,362.78],[337.9,362.78]]},{"text":"队保卫组组长。","box":[[61.2,369.32],[134.7,369.32],[134.7,379.82],[61.2,379.82]]},{"text":"1969","box":[[134.72,369.32],[155.72,369.32],[155.72,379.82],[134.72,379.82]]},{"text":"","box":[[155.72,369.32],[158.72,369.32],[158.72,369.32],[155.72,369.32]]},{"text":"年","box":[[158.72,369.32],[169.22,369.32],[169.22,379.82],[158.72,379.82]]},{"text":"","box":[[169.22,369.32],[172.16,369.32],[172.16,369.32],[169.22,369.32]]},{"text":"1","box":[[172.16,369.32],[177.41,369.32],[177.41,379.82],[172.16,379.82]]},{"text":"","box":[[177.41,369.32],[180.38,369.32],[180.38,369.32],[177.41,369.32]]},{"text":"月","box":[[180.38,369.32],[190.88,369.32],[190.88,379.82],[180.38,379.82]]},{"text":"，","box":[[190.88,369.32],[201.38,369.32],[201.38,379.82],[190.88,379.82]]},{"text":"我被调回高检院。聂元梓被逮捕后","box":[[201.38,369.32],[358.88,369.32],[358.88,379.82],[201.38,379.82]]},{"text":"，","box":[[358.9,369.32],[369.4,369.32],[369.4,379.82],[358.9,379.82]]},{"text":"北京市政法委专案组多次找我调查三个问题：一是聂元梓在文革初","box":[[61.2,386.38],[368.9550000000001,386.38],[368.9550000000001,396.88],[61.2,396.88]]},{"text":"期怎样积极追随林彪","box":[[61.2,403.36],[155.7,403.36],[155.7,413.86],[61.2,413.86]]},{"text":"、","box":[[155.72,403.36],[166.22,403.36],[166.22,413.86],[155.72,413.86]]},{"text":"江青反革命集团的","box":[[162.8,403.36],[246.8,403.36],[246.8,413.86],[162.8,413.86]]},{"text":"，","box":[[246.8,403.36],[257.3,403.36],[257.3,413.86],[246.8,413.86]]},{"text":"他们之间怎么搞阴谋活动","box":[[253.9,403.36],[369.4,403.36],[369.4,413.86],[253.9,413.86]]},{"text":"的；","box":[[61.2,420.4],[82.2,420.4],[82.2,430.9],[61.2,430.9]]},{"text":"二是聂元梓成立北大揪叛徒兵团调查诬陷彭真","box":[[79.92150000000001,420.4],[289.92150000000004,420.4],[289.92150000000004,430.9],[79.92150000000001,430.9]]},{"text":"、","box":[[289.96,420.4],[300.46,420.4],[300.46,430.9],[289.96,430.9]]},{"text":"薄一波","box":[[298.18,420.4],[329.68,420.4],[329.68,430.9],[298.18,430.9]]},{"text":"、","box":[[329.68,420.4],[340.18,420.4],[340.18,430.9],[329.68,430.9]]},{"text":"安子文","box":[[337.9,420.4],[369.4,420.4],[369.4,430.9],[337.9,430.9]]},{"text":"等人；三是聂元梓怎样迫害","box":[[61.2,437.44],[186.78,437.44],[186.78,447.94],[61.2,447.94]]},{"text":"、","box":[[186.8,437.44],[197.3,437.44],[197.3,447.94],[186.8,447.94]]},{"text":"诬陷黑帮","box":[[196.88,437.44],[238.88,437.44],[238.88,447.94],[196.88,447.94]]},{"text":"、","box":[[238.88,437.44],[249.38,437.44],[249.38,447.94],[238.88,447.94]]},{"text":"走资派","box":[[248.98,437.44],[280.48,437.44],[280.48,447.94],[248.98,447.94]]},{"text":"、","box":[[280.48,437.44],[290.98,437.44],[290.98,447.94],[280.48,447.94]]},{"text":"反动学术权威的","box":[[290.56,437.44],[364.06,437.44],[364.06,447.94],[290.56,447.94]]},{"text":"，","box":[[364.12,437.44],[374.62,437.44],[374.62,447.94],[364.12,447.94]]},{"text":"怎样严刑拷打","box":[[61.2,454.48],[124.2,454.48],[124.2,464.98],[61.2,464.98]]},{"text":"，","box":[[124.22,454.48],[134.72,454.48],[134.72,464.98],[124.22,464.98]]},{"text":"致邓朴方“下身瘫痪","box":[[134.72,454.48],[229.22,454.48],[229.22,464.98],[134.72,464.98]]},{"text":"，","box":[[229.22,454.48],[239.72,454.48],[239.72,464.98],[229.22,464.98]]},{"text":"终身残废”的。","box":[[239.72,454.48],[313.22,454.48],[313.22,464.98],[239.72,464.98]]},{"text":"我给专案组实事求是的介绍了在工作组","box":[[82.2,471.46000000000004],[260.7,471.46000000000004],[260.7,481.96000000000004],[82.2,481.96000000000004]]},{"text":"、","box":[[260.74,471.46000000000004],[271.24,471.46000000000004],[271.24,481.96000000000004],[260.74,481.96000000000004]]},{"text":"校文革","box":[[267.82,471.46000000000004],[299.32,471.46000000000004],[299.32,481.96000000000004],[267.82,481.96000000000004]]},{"text":"、","box":[[299.32,471.46000000000004],[309.82,471.46000000000004],[309.82,481.96000000000004],[299.32,481.96000000000004]]},{"text":"宣传队时期所","box":[[306.4,471.46000000000004],[369.4,471.46000000000004],[369.4,481.96000000000004],[306.4,481.96000000000004]]},{"text":"作所为和所见所闻的情况。","box":[[61.2,488.5],[187.2,488.5],[187.2,499],[61.2,499]]},{"text":"我说","box":[[184.92149999999998,488.5],[205.92149999999998,488.5],[205.92149999999998,499],[184.92149999999998,499]]},{"text":"，","box":[[205.94,488.5],[216.44,488.5],[216.44,499],[205.94,499]]},{"text":"文革初期没有林彪","box":[[214.16,488.5],[298.15999999999997,488.5],[298.15999999999997,499],[214.16,499]]},{"text":"、","box":[[298.18,488.5],[308.68,488.5],[308.68,499],[298.18,499]]},{"text":"江青反革命集","box":[[306.4,488.5],[369.4,488.5],[369.4,499],[306.4,499]]},{"text":"团","box":[[61.2,505.53999999999996],[71.7,505.53999999999996],[71.7,516.04],[61.2,516.04]]},{"text":"，","box":[[71.7,505.53999999999996],[82.2,505.53999999999996],[82.2,516.04],[71.7,516.04]]},{"text":"不知道他们之前有什么阴谋","box":[[79.92,505.53999999999996],[205.92000000000002,505.53999999999996],[205.92000000000002,516.04],[79.92,516.04]]},{"text":"，","box":[[205.94,505.53999999999996],[216.44,505.53999999999996],[216.44,516.04],[205.94,516.04]]},{"text":"否则","box":[[214.16,505.53999999999996],[235.16,505.53999999999996],[235.16,516.04],[214.16,516.04]]},{"text":"，","box":[[235.16,505.53999999999996],[245.66,505.53999999999996],[245.66,516.04],[235.16,516.04]]},{"text":"九大选举林彪为党中央副主","box":[[243.38,505.53999999999996],[369.38,505.53999999999996],[369.38,516.04],[243.38,516.04]]},{"text":"席并写入党章","box":[[61.2,522.52],[124.2,522.52],[124.2,533.02],[61.2,533.02]]},{"text":"，","box":[[124.22,522.52],[134.72,522.52],[134.72,533.02],[124.22,533.02]]},{"text":"选举四人帮为党中央成员","box":[[133.94,522.52],[249.44,522.52],[249.44,533.02],[133.94,533.02]]},{"text":"，","box":[[249.46,522.52],[259.96000000000004,522.52],[259.96000000000004,533.02],[249.46,533.02]]},{"text":"不是玷污了伟大的导师","box":[[259.18,522.52],[364.18,522.52],[364.18,533.02],[259.18,533.02]]},{"text":"、","box":[[364.18,522.52],[374.68,522.52],[374.68,533.02],[364.18,533.02]]},{"text":"伟大的领袖","box":[[61.2,539.56],[113.7,539.56],[113.7,550.06],[61.2,550.06]]},{"text":"、","box":[[113.72,539.56],[124.22,539.56],[124.22,550.06],[113.72,550.06]]},{"text":"伟大的统帅","box":[[122.48,539.56],[174.98000000000002,539.56],[174.98000000000002,550.06],[122.48,550.06]]},{"text":"、","box":[[174.98,539.56],[185.48,539.56],[185.48,550.06],[174.98,550.06]]},{"text":"伟大的舵手毛主席和伟大","box":[[183.74,539.56],[299.24,539.56],[299.24,550.06],[183.74,550.06]]},{"text":"、","box":[[299.26,539.56],[309.76,539.56],[309.76,550.06],[299.26,550.06]]},{"text":"光荣","box":[[308.02,539.56],[329.02,539.56],[329.02,550.06],[308.02,550.06]]},{"text":"、","box":[[329.02,539.56],[339.52,539.56],[339.52,550.06],[329.02,550.06]]},{"text":"正确的","box":[[337.78,539.56],[369.3325,539.56],[369.3325,550.06],[337.78,550.06]]},{"text":"中国共产党和九大的全体代表吗！","box":[[61.2,556.6],[218.7,556.6],[218.7,567.1],[61.2,567.1]]},{"text":"？我还说","box":[[215.277,556.6],[257.277,556.6],[257.277,567.1],[215.277,567.1]]},{"text":"，","box":[[257.32,556.6],[267.82,556.6],[267.82,567.1],[257.32,567.1]]},{"text":"揪叛徒是中共中央和最","box":[[264.4,556.6],[369.4,556.6],[369.4,567.1],[264.4,567.1]]},{"text":"高指示在先","box":[[61.2,573.58],[113.7,573.58],[113.7,584.08],[61.2,584.08]]},{"text":"，","box":[[113.72,573.58],[124.22,573.58],[124.22,584.08],[113.72,584.08]]},{"text":"周总理的批示在中","box":[[121.94,573.58],[205.94,573.58],[205.94,584.08],[121.94,584.08]]},{"text":"，","box":[[205.94,573.58],[216.44,573.58],[216.44,584.08],[205.94,584.08]]},{"text":"康生的信在后。","box":[[214.16,573.58],[287.65999999999997,573.58],[287.65999999999997,584.08],[214.16,584.08]]},{"text":"当北大有关组织向","box":[[285.38149999999996,573.58],[369.38149999999996,573.58],[369.38149999999996,584.08],[285.38149999999996,584.08]]},{"text":"我咨询有关外调的事情时","box":[[61.2,590.62],[176.7,590.62],[176.7,601.12],[61.2,601.12]]},{"text":"，","box":[[176.72,590.62],[187.22,590.62],[187.22,601.12],[176.72,601.12]]},{"text":"我到高检院借了","box":[[185.6,590.62],[259.1,590.62],[259.1,601.12],[185.6,601.12]]},{"text":"","box":[[259.1,590.62],[261.76,590.62],[261.76,590.62],[259.1,590.62]]},{"text":"50","box":[[261.76,590.62],[272.23,590.62],[272.23,601.12],[261.76,601.12]]},{"text":"","box":[[272.23,590.62],[274.9,590.62],[274.9,590.62],[272.23,590.62]]},{"text":"年代肃反文件汇编二","box":[[274.9,590.62],[369.337,590.62],[369.337,601.12],[274.9,601.12]]}]}