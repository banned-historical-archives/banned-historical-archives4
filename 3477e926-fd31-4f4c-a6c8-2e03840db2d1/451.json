5.07999999999998],[184.04,175.57999999999998],[173.54,175.57999999999998]]},{"text":"地质等单位参加了这次大会","box":[[182.3,165.07999999999998],[308.3525,165.07999999999998],[308.3525,175.57999999999998],[182.3,175.57999999999998]]},{"text":"，","box":[[308.38,165.07999999999998],[318.88,165.07999999999998],[318.88,175.57999999999998],[308.38,175.57999999999998]]},{"text":"并将这个大会","box":[[317.14,165.07999999999998],[380.14,165.07999999999998],[380.14,175.57999999999998],[317.14,175.57999999999998]]},{"text":"的录音拿到很多机关","box":[[72,182.06],[166.5,182.06],[166.5,192.56],[72,192.56]]},{"text":"、","box":[[166.52,182.06],[177.02,182.06],[177.02,192.56],[166.52,192.56]]},{"text":"学校播放。","box":[[177.02,182.06],[229.52,182.06],[229.52,192.56],[177.02,192.56]]},{"text":"与此同时","box":[[93.02,199.10000000000002],[135.01999999999998,199.10000000000002],[135.01999999999998,209.60000000000002],[93.02,209.60000000000002]]},{"text":"，","box":[[135.02,199.10000000000002],[145.52,199.10000000000002],[145.52,209.60000000000002],[135.02,209.60000000000002]]},{"text":"新北大公社","box":[[143.24,199.10000000000002],[195.74,199.10000000000002],[195.74,209.60000000000002],[143.24,209.60000000000002]]},{"text":"在办公楼召开","box":[[195.74,199.10000000000002],[258.74,199.10000000000002],[258.74,209.60000000000002],[195.74,209.60000000000002]]},{"text":"“揭发批判陆平","box":[[256.4615,199.10000000000002],[329.9615,199.10000000000002],[329.9615,209.60000000000002],[256.4615,209.60000000000002]]},{"text":"、","box":[[329.98,199.10000000000002],[340.48,199.10000000000002],[340.48,209.60000000000002],[329.98,209.60000000000002]]},{"text":"彭珮云黑","box":[[338.2,199.10000000000002],[380.2,199.10000000000002],[380.2,209.60000000000002],[338.2,209.60000000000002]]},{"text":"帮破坏文化大革命罪行大会”","box":[[72,216.14],[208.5,216.14],[208.5,226.64],[72,226.64]]},{"text":"。由于抄林杰办公室的消息传来","box":[[203.2815,216.14],[349.4415,216.14],[349.4415,226.64],[203.2815,226.64]]},{"text":"，","box":[[349.48,216.14],[359.98,216.14],[359.98,226.64],[349.48,226.64]]},{"text":"孙蓬","box":[[359.2,216.14],[380.2,216.14],[380.2,226.64],[359.2,226.64]]},{"text":"一讲话的劲头十足","box":[[72,233.18],[156,233.18],[156,243.68],[72,243.68]]},{"text":"，","box":[[156.02,233.18],[166.52,233.18],[166.52,243.68],[156.02,243.68]]},{"text":"说“谁笑到最后","box":[[166.1,233.18],[239.18,233.18],[239.18,243.68],[166.1,243.68]]},{"text":"，","box":[[239.18,233.18],[249.68,233.18],[249.68,243.68],[239.18,243.68]]},{"text":"谁笑得最好”","box":[[249.28,233.18],[312.28,233.18],[312.28,243.68],[249.28,243.68]]},{"text":"，","box":[[307.06,233.18],[317.56,233.18],[317.56,243.68],[307.06,243.68]]},{"text":"群众也是群情","box":[[317.14,233.18],[380.14,233.18],[380.14,243.68],[317.14,243.68]]},{"text":"激奋。","box":[[72,250.16000000000003],[103.5,250.16000000000003],[103.5,260.66],[72,260.66]]},{"text":"▲清华井冈山","box":[[93.02,267.2],[156.67100000000002,267.2],[156.67100000000002,277.7],[93.02,277.7]]},{"text":"、","box":[[156.86,267.2],[167.36,267.2],[167.36,277.7],[156.86,277.7]]},{"text":"北京体院毛泽东思想兵团在光明日报社抄了穆","box":[[167.48,267.2],[379.97900000000016,267.2],[379.97900000000016,277.7],[167.48,277.7]]},{"text":"欣的家。接着人大三红揪出了肖前。","box":[[72,284.24],[240,284.24],[240,294.74],[72,294.74]]},{"text":"▲《新北大报","box":[[93.02,301.22],[155.4845,301.22],[155.4845,311.72],[93.02,311.72]]},{"text":"","box":[[155.4845,301.22],[160.76,301.22],[160.76,301.22],[155.4845,301.22]]},{"text":"新人大》合刊发表长缨在手战斗队的文章“斩断","box":[[160.76,301.22],[380.18899999999996,301.22],[380.18899999999996,311.72],[160.76,311.72]]},{"text":"伸向中央的这只黑手”","box":[[72,318.26],[177,318.26],[177,328.76],[72,328.76]]},{"text":"。文中写道：","box":[[171.7815,318.26],[234.7185,318.26],[234.7185,328.76],[171.7815,328.76]]},{"text":"她（聂元梓）以“林杰","box":[[93.02,341.3],[192.85399999999998,341.3],[192.85399999999998,351.8],[93.02,351.8]]},{"text":"—","box":[[192.86,341.3],[203.36,341.3],[203.36,351.8],[192.86,351.8]]},{"text":"关锋","box":[[203.36,341.3],[224.36,341.3],[224.36,351.8],[203.36,351.8]]},{"text":"—","box":[[224.36,341.3],[234.86,341.3],[234.86,351.8],[224.36,351.8]]},{"text":"康生”的反动逻辑冲击无产阶级","box":[[234.86,341.3],[380.117,341.3],[380.117,351.8],[234.86,351.8]]},{"text":"司令部。企图从极“左”或极右的方向来动摇中央的领导；她一手炮","box":[[72,358.28],[380.08050000000003,358.28],[380.08050000000003,368.78],[72,368.78]]},{"text":"制了什么","box":[[72,375.32],[114,375.32],[114,385.82],[72,385.82]]},{"text":"“摘桃派”","box":[[111.7215,375.32],[164.2215,375.32],[164.2215,385.82],[111.7215,385.82]]},{"text":"的理论","box":[[161.94299999999998,375.32],[193.44299999999998,375.32],[193.44299999999998,385.82],[161.94299999999998,385.82]]},{"text":"，","box":[[193.46,375.32],[203.96,375.32],[203.96,385.82],[193.46,385.82]]},{"text":"为其全面夺取市革委会大权制造反动理","box":[[201.68,375.32],[380.18,375.32],[380.18,385.82],[201.68,385.82]]},{"text":"论","box":[[72,392.38],[82.5,392.38],[82.5,402.88],[72,402.88]]},{"text":"，","box":[[82.52,392.38],[93.02,392.38],[93.02,402.88],[82.52,402.88]]},{"text":"并直接策划了炮打谢副总理的罪恶活动。","box":[[93.02,392.38],[282.02,392.38],[282.02,402.88],[93.02,402.88]]},{"text":"7","box":[[93.14,415.36],[98.39,415.36],[98.39,425.86],[93.14,425.86]]},{"text":"～","box":[[98.48,415.36],[108.98,415.36],[108.98,425.86],[98.48,425.86]]},{"text":"8","box":[[108.98,415.36],[114.23,415.36],[114.23,425.86],[108.98,425.86]]},{"text":"","box":[[114.23,415.36],[116.96,415.36],[116.96,415.36],[114.23,415.36]]},{"text":"月间","box":[[116.96,415.36],[137.95999999999998,415.36],[137.95999999999998,425.86],[116.96,425.86]]},{"text":"","box":[[137.95999999999998,415.36],[149.18,415.36],[149.18,415.36],[137.95999999999998,415.36]]},{"text":"聂元梓两次提出解散校文革并辞去北大校文革主任","box":[[149.18,415.36],[380.18,415.36],[380.18,425.86],[149.18,425.86]]},{"text":"的请求","box":[[72,432.4],[103.5,432.4],[103.5,442.9],[72,442.9]]},{"text":"，","box":[[103.52,432.4],[114.02,432.4],[114.02,442.9],[103.52,442.9]]},{"text":"第一次被校文革常委会否决","box":[[111.74,432.4],[237.74,432.4],[237.74,442.9],[111.74,442.9]]},{"text":"，","box":[[237.74,432.4],[248.24,432.4],[248.24,442.9],[237.74,442.9]]},{"text":"第二