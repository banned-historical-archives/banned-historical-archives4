35.94],[348.7,446.44],[338.2,446.44]]},{"text":"听取意","box":[[348.7,435.94],[380.2,435.94],[380.2,446.44],[348.7,446.44]]},{"text":"见","box":[[72,452.98],[82.5,452.98],[82.5,463.48],[72,463.48]]},{"text":"，","box":[[82.52,452.98],[93.02,452.98],[93.02,463.48],[82.52,463.48]]},{"text":"宣传队韦教导员主持会议。原新北大公社的“铁杆分子”对宣传","box":[[91.28,452.98],[380.10350000000005,452.98],[380.10350000000005,463.48],[91.28,463.48]]},{"text":"队猛烈炮轰","box":[[72,469.96000000000004],[124.5,469.96000000000004],[124.5,480.46000000000004],[72,480.46000000000004]]},{"text":"，","box":[[124.52,469.96000000000004],[135.01999999999998,469.96000000000004],[135.01999999999998,480.46000000000004],[124.52,480.46000000000004]]},{"text":"说“宣传队进校以来","box":[[133.28,469.96000000000004],[226.037,469.96000000000004],[226.037,480.46000000000004],[133.28,480.46000000000004]]},{"text":"，","box":[[226.04,469.96000000000004],[236.54,469.96000000000004],[236.54,480.46000000000004],[226.04,480.46000000000004]]},{"text":"完全抛开校文革","box":[[234.8,469.96000000000004],[308.3,469.96000000000004],[308.3,480.46000000000004],[234.8,480.46000000000004]]},{"text":"，","box":[[308.32,469.96000000000004],[318.82,469.96000000000004],[318.82,480.46000000000004],[308.32,480.46000000000004]]},{"text":"一切由宣传队","box":[[317.08,469.96000000000004],[380.1325,469.96000000000004],[380.1325,480.46000000000004],[317.08,480.46000000000004]]},{"text":"总指挥部说了算","box":[[72,487],[145.5,487],[145.5,497.5],[72,497.5]]},{"text":"，","box":[[145.52,487],[156.02,487],[156.02,497.5],[145.52,497.5]]},{"text":"虽然得到了原井冈山群众的拥护","box":[[152.6,487],[299.6,487],[299.6,497.5],[152.6,497.5]]},{"text":"，","box":[[299.62,487],[310.12,487],[310.12,497.5],[299.62,497.5]]},{"text":"却严重脱离了原","box":[[306.7,487],[380.2,487],[380.2,497.5],[306.7,497.5]]},{"text":"新北大公社八千群众。最近连续不断发生在北大的学生跟宣传队对","box":[[72,504.03999999999996],[379.7550000000001,504.03999999999996],[379.7550000000001,514.54],[72,514.54]]},{"text":"抗的事件","box":[[72,521.08],[114,521.08],[114,531.58],[72,531.58]]},{"text":"，","box":[[114.02,521.08],[124.52,521.08],[124.52,531.58],[114.02,531.58]]},{"text":"就是宣传队这种做法产生的恶果","box":[[121.1,521.08],[268.1,521.08],[268.1,531.58],[121.1,531.58]]},{"text":"，","box":[[268.12,521.08],[278.62,521.08],[278.62,531.58],[268.12,531.58]]},{"text":"不能一概说成是资产阶","box":[[275.2,521.08],[380.2,521.08],[380.2,531.58],[275.2,531.58]]},{"text":"级知识分子对工人阶级的对抗。","box":[[72,538.06],[220.56000000000006,538.06],[220.56000000000006,548.56],[72,548.56]]},{"text":"”会上","box":[[215.46150000000006,538.06],[247.20150000000007,538.06],[247.20150000000007,548.56],[215.46150000000006,548.56]]},{"text":"，","box":[[247.36,538.06],[257.86,538.06],[257.86,548.56],[247.36,548.56]]},{"text":"韦教导员传达说：","box":[[257.98,538.06],[342.82000000000005,538.06],[342.82000000000005,548.56],[257.98,548.56]]},{"text":"“中央认","box":[[337.72150000000005,538.06],[380.13400000000007,538.06],[380.13400000000007,548.56],[337.72150000000005,548.56]]},{"text":"为","box":[[72,555.1],[82.5,555.1],[82.5,565.6],[72,565.6]]},{"text":"，","box":[[82.52,555.1],[93.02,555.1],[93.02,565.6],[82.52,565.6]]},{"text":"北大校文革在文化革命初期和中期","box":[[90.74,555.1],[248.24,555.1],[248.24,565.6],[90.74,565.6]]},{"text":"，","box":[[248.26,555.1],[258.76,555.1],[258.76,565.6],[248.26,565.6]]},{"text":"都有很大的成绩","box":[[256.48,555.1],[329.98,555.1],[329.98,565.6],[256.48,565.6]]},{"text":"，","box":[[329.98,555.1],[340.48,555.1],[340.48,565.6],[329.98,565.6]]},{"text":"是紧跟毛","box":[[338.2,555.1],[380.2,555.1],[380.2,565.6],[338.2,565.6]]},{"text":"主席的战略部署的。","box":[[72,572.14],[166.5,572.14],[166.5,582.64],[72,582.64]]},{"text":"运动后期也做了不少工作","box":[[164.2215,572.14],[279.7215,572.14],[279.7215,582.64],[164.2215,582.64]]},{"text":"，","box":[[279.76,572.14],[290.26,572.14],[290.26,582.64],[279.76,582.64]]},{"text":"但犯有严重错误","box":[[287.98,572.14],[361.48,572.14],[361.48,582.64],[287.98,582.64]]},{"text":"，","box":[[361.48,572.14],[371.98,572.14],[371.98,582.64],[361.48,582.64]]},{"text":"北","box":[[369.7,572.14],[380.2,572.14],[380.2,582.64],[369.7,582.64]]},{"text":"大校文革是权力机构。","box":[[72,589.12],[177,589.12],[177,599.62],[72,599.62]]},{"text":"”","box":[[171.7815,589.12],[182.2815,589.12],[182.2815,599.62],[171.7815,599.62]]}]}