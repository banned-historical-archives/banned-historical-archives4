5],[82.52,124.5]]},{"text":"长期从事特情工作","box":[[90.74,114],[174.74,114],[174.74,124.5],[90.74,124.5]]},{"text":"，","box":[[174.74,114],[185.24,114],[185.24,124.5],[174.74,124.5]]},{"text":"其革命经历极为曲折且扑朔迷离","box":[[182.96,114],[329.96000000000004,114],[329.96000000000004,124.5],[182.96,124.5]]},{"text":"，","box":[[329.98,114],[340.48,114],[340.48,124.5],[329.98,124.5]]},{"text":"富有传奇","box":[[338.2,114],[380.2,114],[380.2,124.5],[338.2,124.5]]},{"text":"色彩。何奇文章的题目“毛泽东蒋介石座上客","box":[[72,131],[280.446,131],[280.446,141.5],[72,141.5]]},{"text":"","box":[[280.446,131],[285.7,131],[285.7,131],[280.446,131]]},{"text":"根据地国统区穿梭人","box":[[285.7,131],[380.2,131],[380.2,141.5],[285.7,141.5]]},{"text":"——","box":[[72,148.03999999999996],[93,148.03999999999996],[93,158.53999999999996],[72,158.53999999999996]]},{"text":"胡仁奎传奇”表明","box":[[93.02,148.03999999999996],[176.48449999999997,148.03999999999996],[176.48449999999997,158.53999999999996],[93.02,158.53999999999996]]},{"text":"，","box":[[176.48,148.03999999999996],[186.98,148.03999999999996],[186.98,158.53999999999996],[176.48,158.53999999999996]]},{"text":"","box":[[186.98,148.03999999999996],[191.72,148.03999999999996],[191.72,148.03999999999996],[186.98,148.03999999999996]]},{"text":"胡仁奎是一位很高层次的特情人员。何奇","box":[[191.72,148.03999999999996],[380.18449999999996,148.03999999999996],[380.18449999999996,158.53999999999996],[191.72,158.53999999999996]]},{"text":"没有说明其文章的材料来源","box":[[72,165.07999999999998],[198,165.07999999999998],[198,175.57999999999998],[72,175.57999999999998]]},{"text":"，","box":[[198.02,165.07999999999998],[208.52,165.07999999999998],[208.52,175.57999999999998],[198.02,175.57999999999998]]},{"text":"笔者推测他至少见过胡仁奎","box":[[205.1,165.07999999999998],[331.1,165.07999999999998],[331.1,175.57999999999998],[205.1,175.57999999999998]]},{"text":"、","box":[[331.12,165.07999999999998],[341.62,165.07999999999998],[341.62,175.57999999999998],[331.12,175.57999999999998]]},{"text":"李伦所写","box":[[338.2,165.07999999999998],[380.2,165.07999999999998],[380.2,175.57999999999998],[338.2,175.57999999999998]]},{"text":"的自传一类文字","box":[[72,182.06],[145.5,182.06],[145.5,192.56],[72,192.56]]},{"text":"，","box":[[145.52,182.06],[156.02,182.06],[156.02,192.56],[145.52,192.56]]},{"text":"以及李伦晚年的口述资料。","box":[[152.6,182.06],[278.6,182.06],[278.6,192.56],[152.6,192.56]]},{"text":"特情人员的情况确实复","box":[[275.177,182.06],[380.177,182.06],[380.177,192.56],[275.177,192.56]]},{"text":"杂","box":[[72,199.10000000000002],[82.5,199.10000000000002],[82.5,209.60000000000002],[72,209.60000000000002]]},{"text":"，","box":[[82.52,199.10000000000002],[93.02,199.10000000000002],[93.02,209.60000000000002],[82.52,209.60000000000002]]},{"text":"胡不仅是中共地下党员","box":[[90.74,199.10000000000002],[195.74,199.10000000000002],[195.74,209.60000000000002],[90.74,209.60000000000002]]},{"text":"，","box":[[195.74,199.10000000000002],[206.24,199.10000000000002],[206.24,209.60000000000002],[195.74,209.60000000000002]]},{"text":"也是国民党员","box":[[203.96,199.10000000000002],[266.96000000000004,199.10000000000002],[266.96000000000004,209.60000000000002],[203.96,209.60000000000002]]},{"text":"，","box":[[266.98,199.10000000000002],[277.48,199.10000000000002],[277.48,209.60000000000002],[266.98,209.60000000000002]]},{"text":"而且加入了国民党特务","box":[[275.2,199.10000000000002],[380.2,199.10000000000002],[380.2,209.60000000000002],[275.2,209.60000000000002]]},{"text":"机构“中统”","box":[[72,216.14],[135,216.14],[135,226.64],[72,226.64]]},{"text":"。就何奇的文章而言","box":[[129.7185,216.14],[224.2185,216.14],[224.2185,226.64],[129.7185,226.64]]},{"text":"，","box":[[224.24,216.14],[234.74,216.14],[234.74,226.64],[224.24,226.64]]},{"text":"也仍有令常人难以理解的地方。","box":[[234.74,216.14],[381.74,216.14],[381.74,226.64],[234.74,226.64]]},{"text":"如抗战胜利后在南京","box":[[72,233.18],[166.5,233.18],[166.5,243.68],[72,243.68]]},{"text":"，","box":[[166.52,233.18],[177.02,233.18],[177.02,243.68],[166.52,243.68]]},{"text":"胡仁奎被任命为国民党中央党部设计委员","box":[[173.6,233.18],[362.6,233.18],[362.6,243.68],[173.6,243.68]]},{"text":"，","box":[[362.62,233.18],[373.12,233.18],[373.12,243.68],[362.62,243.68]]},{"text":"并","box":[[369.7,233.18],[380.2,233.18],[380.2,243.68],[369.7,243.68]]},{"text":"有国民党中统方面的身份和任务","box":[[72,250.16000000000003],[219,250.16000000000003],[219,260.66],[72,260.66]]},{"text":"，","box":[[219.02,250.16000000000003],[229.52,250.16000000000003],[229.52,260.66],[219.02,260.66]]},{"text":"但这些都向中共领导人汇报过","box":[[226.1,250.16000000000003],[362.6,250.16000000000003],[362.6,260.66],[226.1,260.66]]},{"text":"，","box":[[362.62,250.16000000000003],[373.12,250.16000000000003],[373.12,260.66],[362.62,260.66]]},{"text":"所","box":[[369.7,250.16000000000003],[380.2,250.16000000000003],[380.2,260.66],[369.7,260.66]]},{"text":"以他可以经常出入中共代表团驻地梅园新村","box":[[72,267.2],[271.5,267.2],[271.5,277.7],[72,277.7]]},{"text":"，","box":[[271.54,267.2],[282.04,267.2],[282.04,277.7],[271.54,277.7]]},{"text":"同周恩来","box":[[278.62,267.2],[320.62,267.2],[320.62,277.7],[278.62,277.7]]},{"text":"、","box":[[320.62,267.2],[331.12,267.2],[331.12,277.7],[320.62,277.7]]},{"text":"董必武等人","box":[[327.7,267.2],[380.2,267.2],[380.2,277.7],[327.7,277.7]]},{"text":"都很熟识。","box":[[72,284.24],[124.5,284.24],[124.5,294.74],[72,294.74]]},{"text":"但是","box":[[122.2215,284.24],[143.2215,284.24],[143.2215,294.74],[122.2215,294.74]]},{"text":"，","box":[[143.24,284.24],[153.74,284.24],[153.74,294.74],[143.24,294.74]]},{"text":"中共代表团撤离南京时","box":[[151.46,284.24],[256.46000000000004,284.24],[256.46000000000004,294.74],[151.46,294.74]]},{"text":"，","box":[[256.48,284.24],[266.98,284.24],[266.98,294.74],[256.48,294.74]]},{"text":"并没有告诉胡仁奎","box":[[264.7,284.24],[348.7,284.24],[348.7,294.74],[264.7,294.74]]},{"text":"今后同","box":[[348.7,284.24],[380.2,284.24],[380.2,294.74],[348.7,294.74]]},{"text":"组织上联络的方法","box":[[72,301.22],[156,301.22],[156,311.72],[72,311.72]]},{"text":"，","box":[[156.02,301.22],[166.52,301.22],[166.52,311.72],[156.02,311.72]]},{"text":"实际上切断了同他的联系。","box":[[163.1,301.22],[289.1,301.22],[289.1,311.72],[163.1,311.72]]},{"text":"南京的中共地下党也","box":[[285.677,301.22],[380.177,301.22],[380.177,311.72],[285.677,311.72]]},{"text":"没有联系过他们。","box":[[72,318.26],[156,318.26],[156,328.76],[72,328.76]]},{"text":"胡仁奎","box":[[153.7215,318.26],[185.2215,318.26],[185.2215,328.76],[153.7215,328.76]]},{"text":"、","box":[[185.24,318.26],[195.74,318.26],[195.74,328.76],[185.24,328.76]]},{"text":"李伦夫妇等待数月","box":[[193.46,318.26],[277.46000000000004,318.26],[277.46000000000004,328.76],[193.46,328.76]]},{"text":"，","box":[[277.48,318.26],[287.98,318.26],[287.98,328.76],[277.48,328.76]]},{"text":"一直没有人同他们联","box":[[285.7,318.26],[380.2,318.26],[380.2,328.76],[285.7,328.76]]},{"text":"系。二人遂自行到北京寻找党组织","box":[[72,335.3],[231.09599999999995,335.3],[231.09599999999995,345.8],[72,345.8]]},{"text":"，","box":[[231.5,335.3],[242,335.3],[242,345.8],[231.5,345.8]]},{"text":"北京的地下党不了解他们的情","box":[[242.12,335.3],[380.0060000000002,335.3],[380.0060000000002,345.8],[242.12,345.8]]},{"text":"况","box":[[72,352.28],[82.5,352.28],[82.5,362.78],[72,362.78]]},{"text":"，","box":[[82.52,352.28],[93.02,352.28],[93.02,362.78],[82.52,362.78]]},{"text":"无法接纳他们","box":[[92.72,352.28],[155.72,352.28],[155.72,362.78],[92.72,362.78]]},{"text":"，","box":[[155.72,352.28],[166.22,352.28],[166.22,362.78],[155.72,362.78]]},{"text":"向上级请示","box":[[165.92,352.28],[218.42,352.28],[218.42,362.78],[165.92,362.78]]},{"text":"，","box":[[218.42,352.28],[228.92,352.28],[228.92,362.78],[218.42,362.78]]},{"text":"得到的回复是：","box":[[228.62,352.28],[302.12,352.28],[302.12,362.78],[228.62,362.78]]},{"text":"“中央指示：还是","box":[[296.597,352.28],[380.2295,352.28],[380.2295,362.78],[296.597,362.78]]},{"text":"请你在外边","box":[[72,369.32],[124.5,369.32],[124.5,379.82],[72,379.82]]},{"text":"，","box":[[124.52,369.32],[135.01999999999998,369.32],[135.01999999999998,379.82],[124.52,379.82]]},{"text":"不要回去。","box":[[133.28,369.32],[185.78,369.32],[185.78,379.82],[133.28,379.82]]},{"text":"”","box":[[180.5615,369.32],[191.0615,369.32],[191.0615,379.82],[180.5615,379.82]]},{"text":"","box":[[191.0615,369.32],[194.54,369.32],[194.54,369.32],[191.0615,369.32]]},{"text":"显然","box":[[194.54,369.32],[215.54,369.32],[215.54,379.82],[194.54,379.82]]},{"text":"，","box":[[215.54,369.32],[226.04,369.32],[226.04,379.82],[215.54,379.82]]},{"text":"这是中共情报系统高层的决定。李","box":[[224.3,369.32],[380.12,369.32],[380.12,379.82],[224.3,379.82]]},{"text":"克农等情报系统的领导人","box":[[72,386.38],[187.5,386.38],[187.5,396.88],[72,396.88]]},{"text":"，","box":[[187.52,386.38],[198.02,386.38],[198.02,396.88],[187.52,396.88]]},{"text":"对胡","box":[[198.02,386.38],[219.02,386.38],[219.02,396.88],[198.02,396.88]]},{"text":"、","box":[[219.02,386.38],[229.52,386.38],[229.52,396.88],[219.02,396.88]]},{"text":"李二人的看法是有保","box":[[229.52,386.38],[324.02,386.38],[324.02,396.88],[229.52,396.88]]},{"text":"留的。","box":[[324.04,386.38],[355.54,386.38],[355.54,396.88],[324.04,396.88]]},{"text":"彭真是胡仁奎的","box":[[93.02,403.36],[166.51999999999998,403.36],[166.51999999999998,413.86],[93.02,413.86]]},{"text":"“贵人”","box":[[164.24149999999997,403.36],[206.24149999999997,403.36],[206.24149999999997,413.86],[164.24149999999997,413.86]]},{"text":"。","box":[[200.95999999999998,403.36],[211.45999999999998,403.36],[211.45999999999998,413.86],[200.95999999999998,413.86]]},{"text":"胡","box":[[209.18149999999997,403.36],[219.68149999999997,403.36],[219.68149999999997,413.86],[209.18149999999997,413.86]]},{"text":"","box":[[219.68149999999997,403.36],[222.32,403.36],[222.32,403.36],[219.68149999999997,403.36]]},{"text":"1926","box":[[222.32,403.36],[243.32,403.36],[243.32,413.86],[222.32,413.86]]},{"text":"","box":[[243.32,403.36],[245.96,403.36],[245.96,403.36],[243.32,403.36]]},{"text":"年就加入共产党了","box":[[245.96,403.36],[329.89700000000005,403.36],[329.89700000000005,413.86],[245.96,413.86]]},{"text":"，","box":[[329.92,403.36],[340.42,403.36],[340.42,413.86],[329.92,413.86]]},{"text":"但后来他","box":[[338.14,403.36],[380.14,403.36],[380.14,413.86],[338.14,413.86]]},{"text":"的上级失踪了","box":[[72,420.4],[135,420.4],[135,430.9],[72,430.9]]},{"text":"，","box":[[135.02,420.4],[145.52,420.4],[145.52,430.9],[135.02,430.9]]},{"text":"他和组织失联了。胡一直在寻找党组织","box":[[145.52,420.4],[324.02,420.4],[324.02,430.9],[145.52,430.9]]},{"text":"，","box":[[324.04,420.4],[334.54,420.4],[334.54,430.9],[324.04,430.9]]},{"text":"直到","box":[[334.54,420.4],[355.54,420.4],[355.54,430.9],[334.54,430.9]]},{"text":"","box":[[355.54,420.4],[359.2,420.4],[359.2,420.4],[355.54,420.4]]},{"text":"1937","box":[[359.2,420.4],[380.2,420.4],[380.2,430.9],[359.2,430.9]]},{"text":"年","box":[[72,437.44],[82.5,437.44],[82.5,447.94],[72,447.94]]},{"text":"","box":[[82.5,437.44],[86.96,437.44],[86.96,437.44],[82.5,437.44]]},{"text":"8","box":[[86.96,437.44],[92.21,437.44],[92.21,447.94],[86.96,447.94]]},{"text":"","box":[[92.21,437.44],[96.68,437.44],[96.68,437.44],[92.21,437.44]]},{"text":"月","box":[[96.68,437.44],[107.18,437.44],[107.18,447.94],[96.68,447.94]]},{"text":"下旬","box":[[107.18,437.44],[128.18,437.44],[128.18,447.94],[107.18,447.94]]},{"text":"，","box":[[128.18,437.44],[138.68,437.44],[138.68,447.94],[128.18,447.94]]},{"text":"才由彭真通知他恢复了组织关系。北平解放后的第二","box":[[138.68,437.44],[380.18,437.44],[380.18,447.94],[138.68,447.94]]},{"text":"天","box":[[72,454.48],[82.5,454.48],[82.5,464.98],[72,464.98]]},{"text":"，","box":[[82.52,454.48],[93.02,454.48],[93.02,464.98],[82.52,464.98]]},{"text":"胡直接找到了彭真","box":[[92.78,454.48],[176.78,454.48],[176.78,464.98],[92.78,464.98]]},{"text":"，","box":[[176.78,454.48],[187.28,454.48],[187.28,464.98],[176.78,464.98]]},{"text":"才从“地下”回到“地上”","box":[[187.04,454.48],[312.31550000000004,454.48],[312.31550000000004,464.98],[187.04,464.98]]},{"text":"。这时","box":[[307.09700000000004,454.48],[338.35550000000006,454.48],[338.35550000000006,464.98],[307.09700000000004,464.98]]},{"text":"，","box":[[338.38,454.48],[348.88,454.48],[348.88,464.98],[338.38,464.98]]},{"text":"国民党","box":[[348.64,454.48],[380.14,454.48],[380.14,464.98],[348.64,464.98]]},{"tex