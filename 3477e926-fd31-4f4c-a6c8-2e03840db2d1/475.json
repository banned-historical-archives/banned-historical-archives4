1.12,498.03999999999996],[98.39,498.03999999999996]]},{"text":"月","box":[[101.12,498.03999999999996],[111.62,498.03999999999996],[111.62,508.53999999999996],[101.12,508.53999999999996]]},{"text":"","box":[[111.62,498.03999999999996],[114.26,498.03999999999996],[114.26,498.03999999999996],[111.62,498.03999999999996]]},{"text":"7","box":[[114.26,498.03999999999996],[119.51,498.03999999999996],[119.51,508.53999999999996],[114.26,508.53999999999996]]},{"text":"","box":[[119.51,498.03999999999996],[122.18,498.03999999999996],[122.18,498.03999999999996],[119.51,498.03999999999996]]},{"text":"日","box":[[122.18,498.03999999999996],[132.68,498.03999999999996],[132.68,508.53999999999996],[122.18,508.53999999999996]]},{"text":"","box":[[132.68,498.03999999999996],[143.3,498.03999999999996],[143.3,498.03999999999996],[132.68,498.03999999999996]]},{"text":"上午","box":[[143.3,498.03999999999996],[164.24,498.03999999999996],[164.24,508.53999999999996],[143.3,508.53999999999996]]},{"text":"，","box":[[164.24,498.03999999999996],[174.74,498.03999999999996],[174.74,508.53999999999996],[164.24,508.53999999999996]]},{"text":"校文革召开“热烈庆祝毛主席‘三七指示’发","box":[[173.6,498.03999999999996],[380.16650000000004,498.03999999999996],[380.16650000000004,508.53999999999996],[173.6,508.53999999999996]]},{"text":"表一周年大会”","box":[[72,515.08],[145.5,515.08],[145.5,525.58],[72,525.58]]},{"text":"。","box":[[140.2815,515.08],[150.7815,515.08],[150.7815,525.58],[140.2815,525.58]]},{"text":"驻校解放军全体参加","box":[[148.503,515.08],[242.94,515.08],[242.94,525.58],[148.503,525.58]]},{"text":"，","box":[[242.96,515.08],[253.46,515.08],[253.46,525.58],[242.96,525.58]]},{"text":"但未发言。","box":[[251.2,515.08],[303.7,515.08],[303.7,525.58],[251.2,525.58]]},{"text":"会后召开系文革","box":[[301.4215,515.08],[374.9215,515.08],[374.9215,525.58],[301.4215,525.58]]},{"text":"、","box":[[374.92,515.08],[385.42,515.08],[385.42,525.58],[374.92,525.58]]},{"text":"战斗团长会议","box":[[72,532.06],[135,532.06],[135,542.56],[72,542.56]]},{"text":"，","box":[[135.02,532.06],[145.52,532.06],[145.52,542.56],[135.02,542.56]]},{"text":"聂元梓","box":[[143.24,532.06],[174.74,532.06],[174.74,542.56],[143.24,542.56]]},{"text":"、","box":[[174.74,532.06],[185.24,532.06],[185.24,542.56],[174.74,542.56]]},{"text":"孙蓬一传达昨日接见情况。","box":[[182.96,532.06],[308.96000000000004,532.06],[308.96000000000004,542.56],[182.96,542.56]]},{"text":"说井冈山方面六","box":[[306.6815,532.06],[380.1815,532.06],[380.1815,542.56],[306.6815,542.56]]},{"text":"次举手","box":[[72,549.1],[103.5,549.1],[103.5,559.6],[72,559.6]]},{"text":"，","box":[[103.52,549.1],[114.02,549.1],[114.02,559.6],[103.52,559.6]]},{"text":"也没有让发言。会议期间","box":[[112.28,549.1],[226.037,549.1],[226.037,559.6],[112.28,559.6]]},{"text":"，","box":[[226.04,549.1],[236.54,549.1],[236.54,559.6],[226.04,559.6]]},{"text":"肖力找到孙蓬一说","box":[[234.8,549.1],[318.8,549.1],[318.8,559.6],[234.8,559.6]]},{"text":"，","box":[[318.82,549.1],[329.32,549.1],[329.32,559.6],[318.82,559.6]]},{"text":"应该继续放","box":[[327.58,549.1],[380.1325,549.1],[380.1325,559.6],[327.58,559.6]]},{"text":"炮不要泄气。","box":[[72,566.14],[135,566.14],[135,576.64],[72,576.64]]},{"text":"▲卫戍区司令员","box":[[93.02,583.12],[166.51999999999998,583.12],[166.51999999999998,593.62],[93.02,593.62]]},{"text":"傅崇碧派军人哈斯","box":[[166.52,583.12],[250.52,583.12],[250.52,593.62],[166.52,593.62]]},{"text":"（办公室主任）","box":[[247.097,583.12],[320.597,583.12],[320.597,593.62],[247.097,593.62]]},{"text":"到北大要关于","box":[[317.174,583.12],[380.174,583.12],[380.174,593.62],[317.174,593.62]]}]}