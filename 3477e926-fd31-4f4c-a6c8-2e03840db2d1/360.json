.07999999999998]]},{"text":"《北京大学文化大革命一月情况汇报提纲》","box":[[169.877,157.57999999999998],[369.377,157.57999999999998],[369.377,168.07999999999998],[169.877,168.07999999999998]]},{"text":"。","box":[[364.0955,157.57999999999998],[374.5955,157.57999999999998],[374.5955,168.07999999999998],[364.0955,168.07999999999998]]},{"text":"《提纲》说：","box":[[61.2,174.62],[123.423,174.62],[123.423,185.12],[61.2,185.12]]},{"text":"“北京大学是修正主义的前北京市委的一个重要据点”","box":[[117.3645,174.62],[369.3645,174.62],[369.3645,185.12],[117.3645,185.12]]},{"text":"。","box":[[364.146,174.62],[374.646,174.62],[374.646,185.12],[364.146,185.12]]},{"text":"陆平和","box":[[61.2,191.60000000000002],[92.7,191.60000000000002],[92.7,202.10000000000002],[61.2,202.10000000000002]]},{"text":"“原北大的反动的社会基础结合起来","box":[[90.42150000000001,191.60000000000002],[258.42150000000004,191.60000000000002],[258.42150000000004,202.10000000000002],[90.42150000000001,202.10000000000002]]},{"text":"，","box":[[258.46,191.60000000000002],[268.96,191.60000000000002],[268.96,202.10000000000002],[258.46,202.10000000000002]]},{"text":"依靠","box":[[266.68,191.60000000000002],[287.68,191.60000000000002],[287.68,202.10000000000002],[266.68,202.10000000000002]]},{"text":"、","box":[[287.68,191.60000000000002],[298.18,191.60000000000002],[298.18,202.10000000000002],[287.68,202.10000000000002]]},{"text":"重用大批政治上","box":[[295.9,191.60000000000002],[369.4,191.60000000000002],[369.4,202.10000000000002],[295.9,202.10000000000002]]},{"text":"严重不纯的人","box":[[61.2,208.64],[124.2,208.64],[124.2,219.14],[61.2,219.14]]},{"text":"，","box":[[124.22,208.64],[134.72,208.64],[134.72,219.14],[124.22,219.14]]},{"text":"结成反党的宗派集团","box":[[134.18,208.64],[228.68,208.64],[228.68,219.14],[134.18,219.14]]},{"text":"，","box":[[228.68,208.64],[239.18,208.64],[239.18,219.14],[228.68,219.14]]},{"text":"控制了校","box":[[238.64,208.64],[280.64,208.64],[280.64,219.14],[238.64,219.14]]},{"text":"、","box":[[280.66,208.64],[291.16,208.64],[291.16,219.14],[280.66,219.14]]},{"text":"系两级领导大权","box":[[290.62,208.64],[364.12,208.64],[364.12,219.14],[290.62,219.14]]},{"text":"，","box":[[364.12,208.64],[374.62,208.64],[374.62,219.14],[364.12,219.14]]},{"text":"实行残酷的资产阶级专政”","box":[[61.2,225.68],[187.2,225.68],[187.2,236.18],[61.2,236.18]]},{"text":"。北京大学已“成为一个顽固的反党反社","box":[[181.98149999999998,225.68],[369.3645,225.68],[369.3645,236.18],[181.98149999999998,236.18]]},{"text":"会主义的","box":[[61.2,242.66000000000003],[103.2,242.66000000000003],[103.2,253.16000000000003],[61.2,253.16000000000003]]},{"text":"”","box":[[103.22,242.66000000000003],[113.72,242.66000000000003],[113.72,253.16000000000003],[103.22,253.16000000000003]]},{"text":"“","box":[[106.76,242.66000000000003],[117.26,242.66000000000003],[117.26,253.16000000000003],[106.76,253.16000000000003]]},{"text":"地富反坏右麇集的顽固的反动堡垒”","box":[[117.26,242.66000000000003],[285.26,242.66000000000003],[285.26,253.16000000000003],[117.26,253.16000000000003]]},{"text":"。","box":[[279.9785,242.66000000000003],[290.4785,242.66000000000003],[290.4785,253.16000000000003],[279.9785,253.16000000000003]]},{"text":"《提纲》还说：","box":[[283.517,242.66000000000003],[355.274,242.66000000000003],[355.274,253.16000000000003],[283.517,253.16000000000003]]},{"text":"“六","box":[[348.3125,242.66000000000003],[369.3125,242.66000000000003],[369.3125,253.16000000000003],[348.3125,253.16000000000003]]},{"text":"一八”","box":[[61.2,259.7],[92.7,259.7],[92.7,270.2],[61.2,270.2]]},{"text":"事件","box":[[89.277,259.7],[110.277,259.7],[110.277,270.2],[89.277,270.2]]},{"text":"“是反党反社会主义的阶级敌人在运动发展的关键时刻对","box":[[106.854,259.7],[369.354,259.7],[369.354,270.2],[106.854,270.2]]},{"text":"工作组进行的一次突然袭击”","box":[[61.2,276.74],[197.7,276.74],[197.7,287.24],[61.2,287.24]]},{"text":"，","box":[[192.5,276.74],[203,276.74],[203,287.24],[192.5,287.24]]},{"text":"“制造混乱","box":[[194.36,276.74],[246.79700000000003,276.74],[246.79700000000003,287.24],[194.36,287.24]]},{"text":"，","box":[[246.8,276.74],[257.3,276.74],[257.3,287.24],[246.8,287.24]]},{"text":"企图打乱工作组的作战部","box":[[253.9,276.74],[369.4,276.74],[369.4,287.24],[253.9,287.24]]},{"text":"署”","box":[[61.2,293.78],[82.2,293.78],[82.2,304.28],[61.2,304.28]]},{"text":"，","box":[[76.98,293.78],[87.48,293.78],[87.48,304.28],[76.98,304.28]]},{"text":"把“文化革命引到邪路上去”","box":[[87.5,293.78],[223.937,293.78],[223.937,304.28],[87.5,304.28]]},{"text":"。各系共揪出了","box":[[218.7185,293.78],[292.1555,293.78],[292.1555,304.28],[218.7185,304.28]]},{"text":"","box":[[292.1555,293.78],[294.82,293.78],[294.82,293.78],[292.1555,293.78]]},{"text":"24","box":[[294.82,293.78],[305.29,293.78],[305.29,304.28],[294.82,304.28]]},{"text":"","box":[[305.29,293.78],[307.96,293.78],[307.96,293.78],[305.29,293.78]]},{"text":"个重点人。","box":[[307.96,293.78],[360.397,293.78],[360.397,304.28],[307.96,304.28]]},{"text":"“六一八”事件后","box":[[82.2,310.76],[165.66449999999998,310.76],[165.66449999999998,321.26],[82.2,321.26]]},{"text":"，","box":[[165.68,310.76],[176.18,310.76],[176.18,321.26],[165.68,321.26]]},{"text":"根据工作组的安排","box":[[175.64,310.76],[259.64,310.76],[259.64,321.26],[175.64,321.26]]},{"text":"，","box":[[259.66,310.76],[270.16,310.76],[270.16,321.26],[259.66,321.26]]},{"text":"各单位对群众进行左","box":[[269.62,310.76],[364.12,310.76],[364.12,321.26],[269.62,321.26]]},{"text":"、","box":[[364.12,310.76],[374.62,310.76],[374.62,321.26],[364.12,321.26]]},{"text":"中","box":[[61.2,327.8],[71.7,327.8],[71.7,338.3],[61.2,338.3]]},{"text":"、","box":[[71.7,327.8],[82.2,327.8],[82.2,338.3],[71.7,338.3]]},{"text":"右分类排队。各系对黑帮中的“尖端分子”分别开了批斗大会。","box":[[81.78,327.8],[374.5725,327.8],[374.5725,338.3],[81.78,338.3]]},{"text":"同时","box":[[61.2,344.84],[82.2,344.84],[82.2,355.34],[61.2,355.34]]},{"text":"，","box":[[82.2,344.84],[92.7,344.84],[92.7,355.34],[82.2,355.34]]},{"text":"以班为单位","box":[[91.34,344.84],[143.84,344.84],[143.84,355.34],[91.34,355.34]]},{"text":"，","box":[[143.84,344.84],[154.34,344.84],[154.34,355.34],[143.84,355.34]]},{"text":"由各班文革小组组织大家学习","box":[[152.96,344.84],[289.46000000000004,344.84],[289.46000000000004,355.34],[152.96,355.34]]},{"text":"、","box":[[289.48,344.84],[299.98,344.84],[299.98,355.34],[289.48,355.34]]},{"text":"对照","box":[[298.6,344.84],[319.6,344.84],[319.6,355.34],[298.6,355.34]]},{"text":"、","box":[[319.6,344.84],[330.1,344.84],[330.1,355.34],[319.6,355.34]]},{"text":"检讨","box":[[328.72,344.84],[349.72,344.84],[349.72,355.34],[328.72,355.34]]},{"text":"，","box":[[349.72,344.84],[360.22,344.84],[360.22,355.34],[349.72,355.34]]},{"text":"参","box":[[358.84,344.84],[369.34,344.84],[369.34,355.34],[358.84,355.34]]},{"text":"与乱揪乱斗的人更是个个要作检","box":[[61.2,361.82],[208.2,361.82],[208.2,372.32],[61.2,372.32]]},{"text":"讨。","box":[[208.22,361.82],[229.22,361.82],[229.22,372.32],[208.22,372.32]]},{"text":"北大关起门来","box":[[225.797,361.82],[288.797,361.82],[288.797,372.32],[225.797,372.32]]},{"text":"，","box":[[288.82,361.82],[299.32,361.82],[299.32,372.32],[288.82,372.32]]},{"text":"不准外单位人员","box":[[295.9,361.82],[369.4,361.82],[369.4,372.32],[295.9,372.32]]},{"text":"随便进入北大。同时","box":[[61.2,378.86],[155.7,378.86],[155.7,389.36],[61.2,389.36]]},{"text":"，","box":[[155.72,378.86],[166.22,378.86],[166.22,389.36],[155.72,389.36]]},{"text":"揭","box":[[166.22,378.86],[176.72,378.86],[176.72,389.36],[166.22,389.36]]},{"text":"、","box":[[176.72,378.86],[187.22,378.86],[187.22,389.36],[176.72,389.36]]},{"text":"批了一批“反动学生”","box":[[187.22,378.86],[292.22,378.86],[292.22,389.36],[187.22,389.36]]},{"text":"。","box":[[287.0015,378.86],[297.5015,378.86],[297.5015,389.36],[287.0015,389.36]]},{"text":"7","box":[[82.32,395.92],[87.57,395.92],[87.57,406.42],[82.32,406.42]]},{"text":"","box":[[87.57,395.92],[90.32,395.92],[90.32,395.92],[87.57,395.92]]},{"text":"月","box":[[90.32,395.92],[100.82,395.92],[100.82,406.42],[90.32,406.42]]},{"text":"","box":[[100.82,395.92],[103.46,395.92],[103.46,395.92],[100.82,395.92]]},{"text":"12","box":[[103.46,395.92],[113.99,395.92],[113.99,406.42],[103.46,406.42]]},{"text":"","box":[[113.99,395.92],[116.72,395.92],[116.72,395.92],[113.99,395.92]]},{"text":"日","box":[[116.72,395.92],[127.22,395.92],[127.22,406.42],[116.72,406.42]]},{"text":"","box":[[127.22,395.92],[138.38,395.92],[138.38,395.92],[127.22,395.92]]},{"text":"地球物理系学生陈必陶等五人贴出批评工作组的大","box":[[138.38,395.92],[369.38,395.92],[369.38,406.42],[138.38,406.42]]},{"text":"字报《把运动推向更高阶段》","box":[[61.2,412.9],[197.7,412.9],[197.7,423.4],[61.2,423.4]]},{"text":"。张承先回应","box":[[192.48149999999998,412.9],[255.4185,412.9],[255.4185,423.4],[192.48149999999998,423.4]]},{"text":"，","box":[[255.46,412.9],[265.96000000000004,412.9],[265.96000000000004,423.4],[255.46,423.4]]},{"text":"这是打着红旗反红旗","box":[[265.96,412.9],[360.46,412.9],[360.46,423.4],[265.96,423.4]]},{"text":"，","box":[[360.46,412.9],[370.96,412.9],[370.96,423.4],[360.46,423.4]]},{"text":"向工作组夺领导权。大字报在校内引起强烈反响","box":[[61.2,429.94],[284.05199999999996,429.94],[284.05199999999996,440.44],[61.2,440.44]]},{"text":"，","box":[[284.5,429.94],[295,429.94],[295,440.44],[284.5,440.44]]},{"text":"各种观点纷纷亮","box":[[295.12,429.94],[369.3130000000001,429.94],[369.3130000000001,440.44],[295.12,440.44]]},{"text":"相。一时间","box":[[61.2,446.98],[111.95700000000001,446.98],[111.95700000000001,457.48],[61.2,457.48]]},{"text":"，","box":[[111.98,446.98],[122.48,446.98],[122.48,457.48],[111.98,457.48]]},{"text":"批工作组的大字报纷纷上墙","box":[[120.8,446.98],[246.8,446.98],[246.8,457.48],[120.8,457.48]]},{"text":"，","box":[[246.8,446.98],[257.3,446.98],[257.3,457.48],[246.8,457.48]]},{"text":"只是调门不一","box":[[255.58,446.98],[318.58000000000004,446.98],[318.58000000000004,457.48],[255.58,457.48]]},{"text":"，","box":[[318.58,446.98],[329.08,446.98],[329.08,457.48],[318.58,457.48]]},{"text":"激烈的要","box":[[327.34,446.98],[369.34,446.98],[369.34,457.48],[327.34,457.48]]},{"text":"撤换张承先","box":[[61.2,463.96000000000004],[113.7,463.96000000000004],[113.7,474.46000000000004],[61.2,474.46000000000004]]},{"text":"，","box":[[113.72,463.96000000000004],[124.22,463.96000000000004],[124.22,474.46000000000004],[113.72,474.46000000000004]]},{"text":"一部分认为要分清延安和西安。","box":[[124.22,463.96000000000004],[271.22,463.96000000000004],[271.22,474.46000000000004],[124.22,474.46000000000004]]},{"text":"7","box":[[82.32,481],[87.57,481],[87.57,491.5],[82.32,491.5]]},{"text":"","box":[[87.57,481],[90.32,481],[90.32,481],[87.57,481]]},{"text":"月","box":[[90.32,481],[100.82,481],[100.82,491.5],[90.32,491.5]]},{"text":"","box":[[100.82,481],[103.46,481],[103.46,481],[100.82,481]]},{"text":"15","box":[[103.46,481],[113.99,481],[113.99,491.5],[103.46,491.5]]},{"text":"","box":[[113.99,481],[116.72,481],[116.72,481],[113.99,481]]},{"text":"日","box":[[116.72,481],[127.22,481],[127.22,491.5],[116.72,491.5]]},{"text":"","box":[[127.22,481],[148.94,481],[148.94,481],[127.22,481]]},{"text":"上午","box":[[148.94,481],[169.94,481],[169.94,491.5],[148.94,491.5]]},{"text":"，","box":[[169.94,481],[180.44,481],[180.44,491.5],[169.94,491.5]]},{"text":"在听取北大工作组领导小组的汇报会上","box":[[180.44,481],[358.877,481],[358.877,491.5],[180.44,491.5]]},{"text":"，","box":[[358.9,481],[369.4,481],[369.4,491.5],[358.9,491.5]]},{"text":"吴德说：","box":[[61.2,498.03999999999996],[103.2,498.03999999999996],[103.2,508.53999999999996],[61.2,508.53999999999996]]},{"text":"“六一八”事件中","box":[[97.677,498.03999999999996],[181.3095,498.03999999999996],[181.3095,508.53999999999996],[97.677,508.53999999999996]]},{"text":"，","box":[[181.34,498.03999999999996],[191.84,498.03999999999996],[191.84,508.53999999999996],[181.34,508.53999999999996]]},{"text":"好人是多数","box":[[191.54,498.03999999999996],[244.04,498.03999999999996],[244.04,508.53999999999996],[191.54,508.53999999999996]]},{"text":"，","box":[[244.04,498.03999999999996],[254.54,498.03999999999996],[254.54,508.53999999999996],[244.04,508.53999999999996]]},{"text":"坏人是极少数","box":[[254.26,498.03999999999996],[317.26,498.03999999999996],[317.26,508.53999999999996],[254.26,508.53999999999996]]},{"text":"，","box":[[317.26,498.03999999999996],[327.76,498.03999999999996],[327.76,508.53999999999996],[317.26,508.53999999999996]]},{"text":"但好人也","box":[[327.46,498.03999999999996],[369.46,498.03999999999996],[369.46,508.53999999999996],[327.46,508.53999999999996]]},{"text":"做了检讨。","box":[[61.2,515.08],[113.7,515.08],[113.7,525.58],[61.2,525.58]]},{"text":"好处是警惕性提高了","box":[[110.277,515.08],[204.777,515.08],[204.777,525.58],[110.277,525.58]]},{"text":"，","box":[[204.8,515.08],[215.3,515.08],[215.3,525.58],[204.8,525.58]]},{"text":"副作用是对工作组和左派的意见不","box":[[211.88,515.08],[369.38,515.08],[369.38,525.58],[211.88,525.58]]},{"text":"敢提了。加上工作组控制比较紧了些","box":[[61.2,532.06],[229.2,532.06],[229.2,542.56],[61.2,542.56]]},{"text":"，","box":[[229.22,532.06],[239.72,532.06],[239.72,542.56],[229.22,542.56]]},{"text":"运动就死巴了。","box":[[239.72,532.06],[313.22,532.06],[313.22,542.56],[239.72,542.56]]},{"text":"晚","box":[[82.2,549.1],[92.7,549.1],[92.7,559.6],[82.2,559.6]]},{"text":"","box":[[92.7,549.1],[97.16,549.1],[97.16,549.1],[92.7,549.1]]},{"text":"9","box":[[97.16,549.1],[102.41,549.1],[102.41,559.6],[97.16,559.6]]},{"text":"","box":[[102.41,549.1],[106.88,549.1],[106.88,549.1],[102.41,549.1]]},{"text":"时","box":[[106.88,549.1],[117.38,549.1],[117.38,559.6],[106.88,559.6]]},{"text":"，","box":[[117.38,549.1],[127.88,549.1],[127.88,559.6],[117.38,559.6]]},{"text":"吴德找张承先谈工作","box":[[127.88,549.1],[222.38,549.1],[222.38,559.6],[127.88,559.6]]},{"text":"，","box":[[222.38,549.1],[232.88,549.1],[232.88,559.6],[222.38,559.6]]},{"text":"传达李雪峰对北大工作组的批","box":[[232.88,549.1],[369.38,549.1],[369.38,559.6],[232.88,559.6]]},{"text":"评。","box":[[61.2,566.14],[82.2,566.14],[82.2,576.64],[61.2,576.64]]},{"text":"7","box":[[82.32,583.12],[87.57,583.12],[87.57,593.62],[82.32,593.62]]},{"text":"","box":[[87.57,583.12],[90.32,583.12],[90.32,583.12],[87.57,583.12]]},{"text":"月","box":[[90.32,583.12],[100.82,583.12],[100.82,593.62],[90.32,593.62]]},{"text":"","box":[[100.82,583.12],[103.46,583.12],[103.46,583.12],[100.82,583.12]]},{"text":"16","box":[[103.46,583.12],[113.99,583.12],[113.99,593.62],[103.46,593.62]]},{"text":"","box":[[113.99,583.12],[116.72,583.12],[116.72,583.12],[113.99,583.12]]},{"text":"日","box":[[116.72,583.12],[127.22,583.12],[127.22,593.62],[116.72,593.62]]},{"text":"","box":[[127.22,583.12],[138.38,583.12],[138.38,583.12],[127.22,583.12]]},{"text":"工作组召开全校积极分子大会","box":[[138.38,583.12],[274.88,583.12],[274.88,593.62],[138.38,593.62]]},{"text":"，","box":[[274.9,583.12],[285.4,583.12],[285.4,593.62],[274.9,593.62]]},{"text":"张承先作动员和检","box":[[285.4,583.12],[369.4,583.12],[369.4,593.62],[285.4,593.62]]}]}