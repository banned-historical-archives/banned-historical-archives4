1.5],[220.48999999999998,141.5],[220.48999999999998,152.48],[209.48,152.48]]},{"text":"8341","box":[[93.02,184.16000000000003],[114.02,184.16000000000003],[114.02,194.66000000000003],[93.02,194.66000000000003]]},{"text":"","box":[[114.02,184.16000000000003],[116.72,184.16000000000003],[116.72,184.16000000000003],[114.02,184.16000000000003]]},{"text":"宣传队认为","box":[[116.72,184.16000000000003],[169.157,184.16000000000003],[169.157,194.66000000000003],[116.72,194.66000000000003]]},{"text":"","box":[[169.157,184.16000000000003],[171.8,184.16000000000003],[171.8,184.16000000000003],[169.157,184.16000000000003]]},{"text":"63","box":[[171.8,184.16000000000003],[182.27,184.16000000000003],[182.27,194.66000000000003],[171.8,194.66000000000003]]},{"text":"","box":[[182.27,184.16000000000003],[184.94,184.16000000000003],[184.94,184.16000000000003],[182.27,184.16000000000003]]},{"text":"军宣传队和北京市派到北大的工宣队在","box":[[184.94,184.16000000000003],[363.377,184.16000000000003],[363.377,194.66000000000003],[184.94,194.66000000000003]]},{"text":"“","box":[[359.2,184.16000000000003],[369.7,184.16000000000003],[369.7,194.66000000000003],[359.2,194.66000000000003]]},{"text":"清","box":[[369.7,184.16000000000003],[380.2,184.16000000000003],[380.2,194.66000000000003],[369.7,194.66000000000003]]},{"text":"队","box":[[72,201.14],[82.5,201.14],[82.5,211.64],[72,211.64]]},{"text":"”","box":[[82.52,201.14],[93.02,201.14],[93.02,211.64],[82.52,211.64]]},{"text":"中犯了扩大化的错误","box":[[91.64,201.14],[186.14,201.14],[186.14,211.64],[91.64,211.64]]},{"text":"，","box":[[186.14,201.14],[196.64,201.14],[196.64,211.64],[186.14,211.64]]},{"text":"对此","box":[[195.26,201.14],[216.26,201.14],[216.26,211.64],[195.26,211.64]]},{"text":"，","box":[[216.26,201.14],[226.76,201.14],[226.76,211.64],[216.26,211.64]]},{"text":"他们的","box":[[225.38,201.14],[256.88,201.14],[256.88,211.64],[225.38,211.64]]},{"text":"“经验总结”里有如下一段","box":[[255.52,201.14],[380.1445,201.14],[380.1445,211.64],[255.52,211.64]]},{"text":"叙述：","box":[[72,218.18],[103.5,218.18],[103.5,228.68],[72,228.68]]},{"text":"一派掌权的严重后果","box":[[93.02,241.22000000000003],[187.51999999999998,241.22000000000003],[187.51999999999998,251.72000000000003],[93.02,251.72000000000003]]},{"text":"，","box":[[187.52,241.22000000000003],[198.02,241.22000000000003],[198.02,251.72000000000003],[187.52,251.72000000000003]]},{"text":"主要表现在混淆了两类不同性质的矛盾","box":[[196.4,241.22000000000003],[374.9,241.22000000000003],[374.9,251.72000000000003],[196.4,251.72000000000003]]},{"text":"，","box":[[374.92,241.22000000000003],[385.42,241.22000000000003],[385.42,251.72000000000003],[374.92,251.72000000000003]]},{"text":"把对立面群众组织里的许多人打成“反革命”或“反革命小集团”。","box":[[72,258.2],[385.383,258.2],[385.383,268.7],[72,268.7]]},{"text":"去年宣传队进校后","box":[[93.02,275.24],[177.01999999999998,275.24],[177.01999999999998,285.74],[93.02,285.74]]},{"text":"，","box":[[177.02,275.24],[187.52,275.24],[187.52,285.74],[177.02,285.74]]},{"text":"从十月至今年二月的清队工作中","box":[[184.1,275.24],[331.1,275.24],[331.1,285.74],[184.1,285.74]]},{"text":"，","box":[[331.12,275.24],[341.62,275.24],[341.62,285.74],[331.12,285.74]]},{"text":"继续了这","box":[[338.2,275.24],[380.2,275.24],[380.2,285.74],[338.2,285.74]]},{"text":"个错误","box":[[72,292.28],[103.5,292.28],[103.5,302.78],[72,302.78]]},{"text":"，","box":[[103.52,292.28],[114.02,292.28],[114.02,302.78],[103.52,302.78]]},{"text":"造成了扩大化。搞了许多群众","box":[[112.28,292.28],[247.037,292.28],[247.037,302.78],[112.28,302.78]]},{"text":"，","box":[[247.04,292.28],[257.53999999999996,292.28],[257.53999999999996,302.78],[247.04,302.78]]},{"text":"特别是搞了学生。先后立了","box":[[255.82,292.28],[380.1295,292.28],[380.1295,302.78],[255.82,302.78]]},{"text":"大量的专案","box":[[72,309.32],[124.5,309.32],[124.5,319.82],[72,319.82]]},{"text":"，","box":[[124.52,309.32],[135.01999999999998,309.32],[135.01999999999998,319.82],[124.52,319.82]]},{"text":"打了一批“反革命小集团”","box":[[133.64,309.32],[258.2645,309.32],[258.2645,319.82],[133.64,319.82]]},{"text":"，","box":[[256.9,309.32],[267.4,309.32],[267.4,319.82],[256.9,319.82]]},{"text":"批斗了许多人。有的戴了","box":[[266.02,309.32],[380.1445,309.32],[380.1445,319.82],[266.02,319.82]]},{"text":"反革命分子帽子","box":[[72,326.3],[145.5,326.3],[145.5,336.8],[72,336.8]]},{"text":"，","box":[[145.52,326.3],[156.02,326.3],[156.02,336.8],[145.52,336.8]]},{"text":"还有一部分人根本没有问题也被牵连进去","box":[[152.6,326.3],[341.6,326.3],[341.6,336.8],[152.6,336.8]]},{"text":"，","box":[[341.62,326.3],[352.12,326.3],[352.12,336.8],[341.62,336.8]]},{"text":"被说成","box":[[348.7,326.3],[380.2,326.3],[380.2,336.8],[348.7,336.8]]},{"text":"是“反革命小集团”的“外围”和“外围的外围”","box":[[72,343.34],[298.674,343.34],[298.674,353.84],[72,353.84]]},{"text":"，","box":[[297.88,343.34],[308.38,343.34],[308.38,353.84],[297.88,353.84]]},{"text":"有的被当成“同","box":[[307.48,343.34],[380.14000000000004,343.34],[380.14000000000004,353.84],[307.48,353.84]]},{"text":"伙”和“知情人”打击了","box":[[72,360.38],[184.06650000000002,360.38],[184.06650000000002,370.88],[72,370.88]]},{"text":"，","box":[[184.1,360.38],[194.6,360.38],[194.6,370.88],[184.1,370.88]]},{"text":"有的毕业后还给转去了所谓“反革命”材","box":[[193.46,360.38],[380.17100000000005,360.38],[380.17100000000005,370.88],[193.46,370.88]]},{"text":"料。所以有的同志说：“打击了一批","box":[[72,377.36],[239.2755,377.36],[239.2755,387.86],[72,387.86]]},{"text":"，","box":[[239.3,377.36],[249.8,377.36],[249.8,387.86],[239.3,387.86]]},{"text":"牵扯了一串","box":[[249.58,377.36],[302.08000000000004,377.36],[302.08000000000004,387.86],[249.58,387.86]]},{"text":"，","box":[[302.08,377.36],[312.58,377.36],[312.58,387.86],[302.08,387.86]]},{"text":"压制了一片”","box":[[312.34,377.36],[375.34,377.36],[375.34,387.86],[312.34,387.86]]},{"text":"，","box":[[375.04,377.36],[385.54,377.36],[385.54,387.86],[375.04,387.86]]},{"text":"使很多人背上了包袱。如原“井冈山”派的一个骨干支队","box":[[72,394.42],[329.334,394.42],[329.334,404.92],[72,404.92]]},{"text":"，","box":[[329.38,394.42],[339.88,394.42],[339.88,404.92],[329.38,404.92]]},{"text":"原来对立","box":[[338.14,394.42],[380.14,394.42],[380.14,404.92],[338.14,404.92]]},{"text":"面组织就想搞他们","box":[[72,411.46000000000004],[156,411.46000000000004],[156,421.96000000000004],[72,421.96000000000004]]},{"text":"，","box":[[156.02,411.46000000000004],[166.52,411.46000000000004],[166.52,421.96000000000004],[156.02,421.96000000000004]]},{"text":"宣传队进校后","box":[[163.1,411.46000000000004],[226.1,411.46000000000004],[226.1,421.96000000000004],[163.1,421.96000000000004]]},{"text":"，","box":[[226.1,411.46000000000004],[236.6,411.46000000000004],[236.6,421.96000000000004],[226.1,421.96000000000004]]},{"text":"在没有确凿证据的情况下点了这","box":[[233.18,411.46000000000004],[380.18,411.46000000000004],[380.18,421.96000000000004],[233.18,421.96000000000004]]},{"text":"个支队的名","box":[[72,428.44],[124.5,428.44],[124.5,438.94],[72,438.94]]},{"text":"，","box":[[124.52,428.44],[135.01999999999998,428.44],[135.01999999999998,438.94],[124.52,438.94]]},{"text":"又搞了逼供信","box":[[134.48,428.44],[197.48,428.44],[197.48,438.94],[134.48,438.94]]},{"text":"，","box":[[197.48,428.44],[207.98,428.44],[207.98,438.94],[197.48,438.94]]},{"text":"把个别人的问题上纲","box":[[207.44,428.44],[301.94,428.44],[301.94,438.94],[207.44,438.94]]},{"text":"，","box":[[301.96,428.44],[312.46,428.44],[312.46,438.94],[301.96,438.94]]},{"text":"逐个进行审查","box":[[311.92,428.44],[374.92,428.44],[374.92,438.94],[311.92,438.94]]},{"text":"，","box":[[374.92,428.44],[385.42,428.44],[385.42,438.94],[374.92,438.94]]},{"text":"最后在这个六十多人的支队中搞了一个所谓","box":[[72,445.48],[271.5,445.48],[271.5,455.98],[72,455.98]]},{"text":"“反革命小集团”","box":[[269.2215,445.48],[353.2215,445.48],[353.2215,455.98],[269.2215,455.98]]},{"text":"，","box":[[350.98,445.48],[361.48,445.48],[361.48,455.98],[350.98,455.98]]},{"text":"冲击","box":[[359.2,445.48],[380.2,445.48],[380.2,455.98],[359.2,455.98]]},{"text":"了二十多人。在干部中揪所谓“黑班底”时","box":[[72,462.52],[266.271,462.52],[266.271,473.02],[72,473.02]]},{"text":"，","box":[[266.32,462.52],[276.82,462.52],[276.82,473.02],[266.32,473.02]]},{"text":"干部参加","box":[[275.08,462.52],[317.1325,462.52],[317.1325,473.02],[275.08,473.02]]},{"text":"了一派有的就","box":[[317.14,462.52],[380.14,462.52],[380.14,473.02],[317.14,473.02]]},{"text":"被说成是“挑动群众斗群众的黑手”","box":[[72,479.5],[238.257,479.5],[238.257,490],[72,490]]},{"text":"，","box":[[236.54,479.5],[247.04,479.5],[247.04,490],[236.54,490]]},{"text":"没参加的有的就被说成是“第","box":[[245.36,479.5],[380.117,479.5],[380.117,490],[245.36,490]]},{"text":"三势力”","box":[[72,496.53999999999996],[114,496.53999999999996],[114,507.03999999999996],[72,507.03999999999996]]},{"text":"，","box":[[111.74,496.53999999999996],[122.24,496.53999999999996],[122.24,507.03999999999996],[111.74,507.03999999999996]]},{"text":"搞的干部无所适从。","box":[[119.96,496.53999999999996],[214.45999999999998,496.53999999999996],[214.45999999999998,507.03999999999996],[119.96,507.03999999999996]]},{"text":"有的人有历史问题已经搞清并有了正","box":[[212.18149999999997,496.53999999999996],[380.18149999999997,496.53999999999996],[380.18149999999997,507.03999999999996],[212.18149999999997,507.03999999999996]]},{"text":"确结论","box":[[72,513.58],[103.5,513.58],[103.5,524.08],[72,524.08]]},{"text":"，","box":[[103.52,513.58],[114.02,513.58],[114.02,524.08],[103.52,524.08]]},{"text":"也要老账新算。","box":[[111.74,513.58],[185.24,513.58],[185.24,524.08],[111.74,524.08]]},{"text":"这样做的结果严重地破坏了党的政策","box":[[182.9615,513.58],[350.9615,513.58],[350.9615,524.08],[182.9615,524.08]]},{"text":"，","box":[[350.98,513.58],[361.48,513.58],[361.48,524.08],[350.98,524.08]]},{"text":"加剧","box":[[359.2,513.58],[380.2,513.58],[380.2,524.08],[359.2,524.08]]},{"text":"了原两派群众的对立情绪。","box":[[72,530.62],[198,530.62],[198,541.12],[72,541.12]]},{"text":"25","box":[[198.02,525.1],[203.54000000000002,525.1],[203.54000000000002,530.62],[198.02,530.62]]},{"text":"63","box":[[93.02,553.6],[103.55,553.6],[103.55,564.1],[93.02,564.1]]},{"text":"","box":[[103.55,553.6],[107.24,553.6],[107.24,553.6],[103.55,553.6]]},{"text":"军宣传队和工宣队在“清队”工作中确实有扩大化的错误","box":[[107.24,553.6],[369.677,553.6],[369.677,564.1],[107.24,564.1]]},{"text":"，","box":[[369.7,553.6],[380.2,553.6],[38