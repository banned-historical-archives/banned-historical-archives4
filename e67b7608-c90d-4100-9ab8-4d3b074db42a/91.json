69,338.62780000000004]]},{"text":"","box":[[322.1769,328.62780000000004],[322.4359,328.62780000000004],[322.4359,328.62780000000004],[322.1769,328.62780000000004]]},{"text":"日，国防","box":[[324.76689999999996,329.0278],[365.4869,329.0278],[365.4869,339.0278],[324.76689999999996,339.0278]]},{"text":"委员会副主席贺龙在","box":[[55.486899999999935,346.5278],[147.8868999999999,346.5278],[147.8868999999999,356.5278],[55.486899999999935,356.5278]]},{"text":"《解放军报》","box":[[145.3868999999999,346.5278],[206.28689999999995,346.5278],[206.28689999999995,356.5278],[145.3868999999999,356.5278]]},{"text":"上发表长文","box":[[204.08689999999996,346.5278],[255.2869,346.5278],[255.2869,356.5278],[204.08689999999996,356.5278]]},{"text":"《中国人民解放军的民主","box":[[252.7869,346.5278],[365.4869000000001,346.5278],[365.4869000000001,356.5278],[252.7869,356.5278]]},{"text":"传统》","box":[[55.486899999999935,364.0278],[85.58689999999993,364.0278],[85.58689999999993,374.0278],[55.486899999999935,374.0278]]},{"text":"（该文由唐平铸等人负责起草、修改）","box":[[80.58689999999993,364.0278],[249.38689999999988,364.0278],[249.38689999999988,374.0278],[80.58689999999993,374.0278]]},{"text":"。同日，军报刊登了总参谋","box":[[244.38689999999986,364.0278],[365.3869,364.0278],[365.3869,374.0278],[244.38689999999986,374.0278]]},{"text":"长罗瑞卿在庆祝建军","box":[[55.486899999999935,381.5278],[145.48689999999993,381.5278],[145.48689999999993,391.5278],[55.486899999999935,391.5278]]},{"text":"","box":[[145.48689999999993,381.5278],[145.73689999999993,381.5278],[145.73689999999993,381.5278],[145.48689999999993,381.5278]]},{"text":"38","box":[[147.98689999999993,381.12780000000004],[157.98689999999993,381.12780000000004],[157.98689999999993,391.12780000000004],[147.98689999999993,391.12780000000004]]},{"text":"","box":[[157.98689999999993,381.12780000000004],[158.23689999999993,381.12780000000004],[158.23689999999993,381.12780000000004],[157.98689999999993,381.12780000000004]]},{"text":"周年招待会上的讲话。","box":[[160.48689999999993,381.5278],[260.48689999999993,381.5278],[260.48689999999993,391.5278],[160.48689999999993,391.5278]]},{"text":"9","box":[[76.74689999999993,398.6278],[81.74689999999993,398.6278],[81.74689999999993,408.6278],[76.74689999999993,408.6278]]},{"text":"","box":[[81.74689999999993,398.6278],[82.00889999999993,398.6278],[82.00889999999993,398.6278],[81.74689999999993,398.6278]]},{"text":"月","box":[[84.36689999999993,399.0278],[94.36689999999993,399.0278],[94.36689999999993,409.0278],[84.36689999999993,409.0278]]},{"text":"","box":[[94.36689999999993,399.0278],[94.62889999999993,399.0278],[94.62889999999993,399.0278],[94.36689999999993,399.0278]]},{"text":"3","box":[[96.98689999999993,398.6278],[101.98689999999993,398.6278],[101.98689999999993,408.6278],[96.98689999999993,408.6278]]},{"text":"","box":[[101.98689999999993,398.6278],[102.24889999999994,398.6278],[102.24889999999994,398.6278],[101.98689999999993,398.6278]]},{"text":"日，林彪在","box":[[104.60689999999994,399.0278],[156.07689999999997,399.0278],[156.07689999999997,409.0278],[104.60689999999994,409.0278]]},{"text":"《人民日报》","box":[[153.57689999999997,399.0278],[215.0469,399.0278],[215.0469,409.0278],[153.57689999999997,409.0278]]},{"text":"发表文章","box":[[213.03889999999998,399.0278],[254.5089,399.0278],[254.5089,409.0278],[213.03889999999998,409.0278]]},{"text":"《人民战争胜利万岁——","box":[[252.0089,399.0278],[365.4389000000001,399.0278],[365.4389000000001,409.0278],[252.0089,409.0278]]},{"text":"纪念中国人民抗战胜利","box":[[55.47689999999994,416.5278],[155.47689999999994,416.5278],[155.47689999999994,426.5278],[55.47689999999994,426.5278]]},{"text":"","box":[[155.47689999999994,416.5278],[155.60189999999994,416.5278],[155.60189999999994,416.5278],[155.47689999999994,416.5278]]},{"text":"30","box":[[156.72689999999994,416.1278],[166.72689999999994,416.1278],[166.72689999999994,426.1278],[156.72689999999994,426.1278]]},{"text":"","box":[[166.72689999999994,416.1278],[166.85189999999994,416.1278],[166.85189999999994,416.1278],[166.72689999999994,416.1278]]},{"text":"周年》","box":[[167.97689999999994,416.5278],[197.97689999999994,416.5278],[197.97689999999994,426.5278],[167.97689999999994,426.5278]]},{"text":"，","box":[[192.97689999999994,416.5278],[202.97689999999994,416.5278],[202.97689999999994,426.5278],[192.97689999999994,426.5278]]},{"text":"文中指出赫鲁晓夫是人民战争的叛徒。","box":[[200.47689999999994,416.5278],[370.47689999999994,416.5278],[370.47689999999994,426.5278],[200.47689999999994,426.5278]]},{"text":"该文一部分为由康生牵头的钓鱼台写作班子起草，一部分为军委三座门","box":[[55.476899999999944,434.0278],[365.47689999999994,434.0278],[365.47689999999994,444.0278],[55.476899999999944,444.0278]]},{"text":"写作班子起草，唐平铸是军委写作班子成员。同日，罗瑞卿在首都各界","box":[[55.476899999999944,451.5278],[365.47689999999994,451.5278],[365.47689999999994,461.5278],[55.476899999999944,461.5278]]},{"text":"参加的人民大会堂招待会上，发表了题为","box":[[55.476899999999944,469.0278],[237.07689999999985,469.0278],[237.07689999999985,479.0278],[55.476899999999944,479.0278]]},{"text":"《人民战胜了日本法西斯，人","box":[[234.57689999999985,469.0278],[365.5769,469.0278],[365.5769,479.0278],[234.57689999999985,479.0278]]},{"text":"民也一定能够战胜美帝国主义》","box":[[55.476899999999944,486.5278],[196.55689999999998,486.5278],[196.55689999999998,496.5278],[55.476899999999944,496.5278]]},{"text":"的讲话。罗瑞卿在短短的时间里，在公","box":[[194.1489,486.5278],[365.40889999999973,486.5278],[365.40889999999973,496.5278],[194.1489,496.5278]]},{"text":"众场合、新闻媒体上反复亮相，连续发表多篇讲话和个人署名文章，引","box":[[55.476899999999944,504.0278],[365.47689999999994,504.0278],[365.47689999999994,514.0278000000001],[55.476899999999944,514.0278000000001]]},{"text":"起人们的注目和猜想。","box":[[55.476899999999944,521.5278000000001],[155.47689999999994,521.5278000000001],[155.47689999999994,531.5278000000001],[55.476899999999944,531.5278000000001]]}]}