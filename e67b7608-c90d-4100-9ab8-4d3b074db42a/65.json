304.5278]]},{"text":"","box":[[243.58370000000005,294.5278],[243.84170000000006,294.5278],[243.84170000000006,294.5278],[243.58370000000005,294.5278]]},{"text":"1969","box":[[246.1637,294.12780000000004],[266.1637,294.12780000000004],[266.1637,304.12780000000004],[246.1637,304.12780000000004]]},{"text":"","box":[[266.1637,294.12780000000004],[266.4227,294.12780000000004],[266.4227,294.12780000000004],[266.1637,294.12780000000004]]},{"text":"年，只剩下","box":[[268.7537,294.5278],[319.8337,294.5278],[319.8337,304.5278],[268.7537,304.5278]]},{"text":"《红旗》","box":[[317.3337,294.5278],[357.69370000000004,294.5278],[357.69370000000004,304.5278],[317.3337,304.5278]]},{"text":"杂","box":[[355.55070000000006,294.5278],[365.55070000000006,294.5278],[365.55070000000006,304.5278],[355.55070000000006,304.5278]]},{"text":"志等","box":[[55.54369999999997,312.0278],[75.54369999999997,312.0278],[75.54369999999997,322.0278],[55.54369999999997,322.0278]]},{"text":"","box":[[75.54369999999997,312.0278],[75.79369999999997,312.0278],[75.79369999999997,312.0278],[75.54369999999997,312.0278]]},{"text":"20","box":[[78.04369999999997,311.62780000000004],[88.04369999999997,311.62780000000004],[88.04369999999997,321.62780000000004],[78.04369999999997,321.62780000000004]]},{"text":"","box":[[88.04369999999997,311.62780000000004],[88.29369999999997,311.62780000000004],[88.29369999999997,311.62780000000004],[88.04369999999997,311.62780000000004]]},{"text":"种。几乎所有的印刷厂都在赶印毛泽东著作。","box":[[90.54369999999997,312.0278],[290.54369999999994,312.0278],[290.54369999999994,322.0278],[90.54369999999997,322.0278]]},{"text":"1976","box":[[76.80369999999996,329.12780000000004],[96.80369999999996,329.12780000000004],[96.80369999999996,339.12780000000004],[76.80369999999996,339.12780000000004]]},{"text":"","box":[[96.80369999999996,329.12780000000004],[97.05569999999996,329.12780000000004],[97.05569999999996,329.12780000000004],[96.80369999999996,329.12780000000004]]},{"text":"年，随着毛泽东去世，以及","box":[[99.32369999999996,329.5278],[219.8637,329.5278],[219.8637,339.5278],[99.32369999999996,339.5278]]},{"text":"“四人帮”","box":[[217.3637,329.5278],[267.4837,329.5278],[267.4837,339.5278],[217.3637,339.5278]]},{"text":"被粉碎，十年文革宣告","box":[[265.0447,329.5278],[365.5247,329.5278],[365.5247,339.5278],[265.0447,339.5278]]},{"text":"结束，","box":[[55.54369999999996,347.0278],[85.83369999999996,347.0278],[85.83369999999996,357.0278],[55.54369999999996,357.0278]]},{"text":"《毛主席语录》","box":[[80.83369999999996,347.0278],[151.9937,347.0278],[151.9937,357.0278],[80.83369999999996,357.0278]]},{"text":"这一特定历史时期的产物也就从此退出了历史舞","box":[[149.78169999999997,347.0278],[365.5817000000002,347.0278],[365.5817000000002,357.0278],[149.78169999999997,357.0278]]},{"text":"台，从亿万中国人的生活中逐渐淡出。同时值得一提的是，一直到文革","box":[[55.54369999999996,364.5278],[365.54369999999994,364.5278],[365.54369999999994,374.5278],[55.54369999999996,374.5278]]},{"text":"结束近三年后的","box":[[55.54369999999996,382.0278],[125.54369999999996,382.0278],[125.54369999999996,392.0278],[55.54369999999996,392.0278]]},{"text":"","box":[[125.54369999999996,382.0278],[125.79369999999996,382.0278],[125.79369999999996,382.0278],[125.54369999999996,382.0278]]},{"text":"1979","box":[[128.04369999999994,381.62780000000004],[148.04369999999994,381.62780000000004],[148.04369999999994,391.62780000000004],[128.04369999999994,391.62780000000004]]},{"text":"","box":[[148.04369999999994,381.62780000000004],[148.29369999999994,381.62780000000004],[148.29369999999994,381.62780000000004],[148.04369999999994,381.62780000000004]]},{"text":"年","box":[[150.54369999999994,382.0278],[160.54369999999994,382.0278],[160.54369999999994,392.0278],[150.54369999999994,392.0278]]},{"text":"","box":[[160.54369999999994,382.0278],[160.79369999999994,382.0278],[160.79369999999994,382.0278],[160.54369999999994,382.0278]]},{"text":"2","box":[[163.04369999999994,381.62780000000004],[168.04369999999994,381.62780000000004],[168.04369999999994,391.62780000000004],[163.04369999999994,391.62780000000004]]},{"text":"","box":[[168.04369999999994,381.62780000000004],[168.29369999999994,381.62780000000004],[168.29369999999994,381.62780000000004],[168.04369999999994,381.62780000000004]]},{"text":"月，外文版的","box":[[170.54369999999994,382.0278],[230.54369999999994,382.0278],[230.54369999999994,392.0278],[170.54369999999994,392.0278]]},{"text":"《毛主席语录》","box":[[228.04369999999994,382.0278],[298.04369999999994,382.0278],[298.04369999999994,392.0278],[228.04369999999994,392.0278]]},{"text":"才停止向国外发","box":[[295.54369999999994,382.0278],[365.54369999999994,382.0278],[365.54369999999994,392.0278],[295.54369999999994,392.0278]]},{"text":"行。","box":[[55.543699999999944,399.5278],[75.54369999999994,399.5278],[75.54369999999994,409.5278],[55.543699999999944,409.5278]]},{"text":"《毛主席语录》","box":[[122.032,454.6431],[215.37199999999993,454.6431],[215.37199999999993,467.6431],[122.032,467.6431]]},{"text":"的启示和教训","box":[[212.5119999999999,454.6431],[292.4619999999998,454.6431],[292.4619999999998,467.6431],[212.5119999999999,467.6431]]},{"text":"笔者认为，在研究","box":[[76.7569,487.0278],[159.15690000000004,487.0278],[159.15690000000004,497.0278],[76.7569,497.0278]]},{"text":"《毛主席语录》","box":[[156.65690000000004,487.0278],[228.25690000000006,487.0278],[228.25690000000006,497.0278],[156.65690000000004,497.0278]]},{"text":"的启示、借鉴和教训的问题上，","box":[[226.15390000000005,487.0278],[370.5538999999998,487.0278],[370.5538999999998,497.0278],[226.15390000000005,497.0278]]},{"text":"应当把","box":[[55.496900000000004,504.5278],[85.86690000000002,504.5278],[85.86690000000002,514.5278000000001],[55.496900000000004,514.5278000000001]]},{"text":"《语录》","box":[[83.36690000000002,504.5278],[123.55690000000001,504.5278],[123.55690000000001,514.5278000000001],[83.36690000000002,514.5278000000001]]},{"text":"的这种形式与一些人利用它作为政治工具的目的区别开","box":[[121.24190000000002,504.5278],[365.52189999999996,504.5278],[365.52189999999996,514.5278000000001],[121.24190000000002,514.5278000000001]]},{"text":"来。","box":[[55.496900000000004,522.0278000000001],[75.49690000000001,522.0278000000001],[75.49690000000001,532.0278000000001],[55.496900000000004,532.0278000000001]]},{"text":"纵观历史，","box":[[76.7569,539.5278000000001],[126.90690000000001,539.5278000000001],[126.90690000000001,549.5278000000001],[76.7569,549.5278000000001]]},{"text":"《毛主席语录》","box":[[121.9069,539.5278000000001],[192.10690000000005,539.5278000000001],[192.10690000000005,549.5278000000001],[121.9069,549.5278000000001]]},{"text":"的形式并不是什么新发明。在汗牛充栋、","box":[[189.66090000000005,539.5278000000001],[370.4109000000002,539.5278000000001],[370.4109000000002,549.5278000000001],[189.66090000000005,549.5278000000001]]},{"text":"卷帙浩繁的中国历史文化遗产中，","box":[[55.496900000000004,557.0278000000001],[209.39690000000004,557.0278000000001],[209.39690000000004,567.0278000000001],[55.496900000000004,567.0278000000001]]},{"text":"“语录体”","box":[[204.39690000000007,557.0278000000001],[254.9969000000001,557.0278000000001],[254.9969000000001,567.0278000000001],[204.39690000000007,567.0278000000001]]},{"text":"的著作占有重要位置。自","box":[[252.7969000000001,557.0278000000001],[365.4969000000002,557.0278000000001],[365.4969000000002,567.0278000000001],[252.7969000000001,567.0278000000001]]},{"text":"先秦以至清末近代，结集传世的语录、箴言等文献至少有数百种之多。","box":[[55.496900000000004,574.5278000000001],[370.5218999999999,574.5278000000001],[370.5218999999999,584.5278000000001],[55.496900000000004,584.5278000000001]]},{"text":"其中","box":[[55.496900000000004,592.0278000000001],[75.49690000000001,592.0278000000001],[75.49690000000001,602.0278000000001],[55.496900000000004,602.0278000000001]]},{"text":"《论语》","box":[[72.99690000000001,592.0278000000001],[112.99690000000001,592.0278000000001],[112.99690000000001,602.0278000000001],[72.99690000000001,602.0278000000001]]},{"text":"《孟子》","box":[[107.99690000000001,592.0278000000001],[147.9969,592.0278000000001],[147.9969,602.0278000000001],[107.99690000000001,602.0278000000001]]},{"text":"等经典，就是一种语录体文字，还有南朝刘宋时范","box":[[145.4969,592.0278000000001],[365.4969,592.0278000000001],[365.4969,602.0278000000001],[145.4969,602.0278000000001]]}]}