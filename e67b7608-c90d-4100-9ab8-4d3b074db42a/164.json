6.5278000000001]]},{"text":"1","box":[[358.9627999999999,556.1278],[363.9627999999999,556.1278],[363.9627999999999,566.1278],[358.9627999999999,566.1278]]},{"text":"","box":[[363.9627999999999,556.1278],[364.2177999999999,556.1278],[364.2177999999999,556.1278],[363.9627999999999,556.1278]]},{"text":"月","box":[[366.5127999999999,556.5278000000001],[376.5127999999999,556.5278000000001],[376.5127999999999,566.5278000000001],[366.5127999999999,566.5278000000001]]},{"text":"11","box":[[66.51279999999991,573.6278],[76.51279999999991,573.6278],[76.51279999999991,583.6278],[66.51279999999991,583.6278]]},{"text":"","box":[[76.51279999999991,573.6278],[76.76279999999991,573.6278],[76.76279999999991,573.6278],[76.51279999999991,573.6278]]},{"text":"日，经中央政治局和中央军委常委开会审定，毛泽东批准的","box":[[79.01279999999991,574.0278000000001],[339.0127999999999,574.0278000000001],[339.0127999999999,584.0278000000001],[79.01279999999991,584.0278000000001]]},{"text":"“全军文","box":[[336.5127999999999,574.0278000000001],[376.5127999999999,574.0278000000001],[376.5127999999999,584.0278000000001],[336.5127999999999,584.0278000000001]]}]}