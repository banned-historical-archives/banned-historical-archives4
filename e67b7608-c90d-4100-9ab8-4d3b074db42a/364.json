,171.62779999999998],[339.2728000000001,171.62779999999998],[339.2728000000001,171.62779999999998],[339.0228000000001,171.62779999999998]]},{"text":"月","box":[[341.5228000000001,172.02779999999996],[351.5228000000001,172.02779999999996],[351.5228000000001,182.02779999999996],[341.5228000000001,182.02779999999996]]},{"text":"","box":[[351.5228000000001,172.02779999999996],[351.7728000000001,172.02779999999996],[351.7728000000001,172.02779999999996],[351.5228000000001,172.02779999999996]]},{"text":"1","box":[[354.0228000000001,171.62779999999998],[359.0228000000001,171.62779999999998],[359.0228000000001,181.62779999999998],[354.0228000000001,181.62779999999998]]},{"text":"","box":[[359.0228000000001,171.62779999999998],[359.2728000000001,171.62779999999998],[359.2728000000001,171.62779999999998],[359.0228000000001,171.62779999999998]]},{"text":"日、","box":[[361.5228000000001,172.02779999999996],[381.5228000000001,172.02779999999996],[381.5228000000001,182.02779999999996],[361.5228000000001,182.02779999999996]]},{"text":"1973","box":[[66.52280000000007,189.12779999999998],[86.52280000000007,189.12779999999998],[86.52280000000007,199.12779999999998],[66.52280000000007,199.12779999999998]]},{"text":"","box":[[86.52280000000007,189.12779999999998],[86.77280000000007,189.12779999999998],[86.77280000000007,189.12779999999998],[86.52280000000007,189.12779999999998]]},{"text":"年","box":[[89.02280000000007,189.52779999999996],[99.02280000000007,189.52779999999996],[99.02280000000007,199.52779999999996],[89.02280000000007,199.52779999999996]]},{"text":"","box":[[99.02280000000007,189.52779999999996],[99.27280000000007,189.52779999999996],[99.27280000000007,189.52779999999996],[99.02280000000007,189.52779999999996]]},{"text":"9","box":[[101.52280000000007,189.12779999999998],[106.52280000000007,189.12779999999998],[106.52280000000007,199.12779999999998],[101.52280000000007,199.12779999999998]]},{"text":"","box":[[106.52280000000007,189.12779999999998],[106.77280000000007,189.1277