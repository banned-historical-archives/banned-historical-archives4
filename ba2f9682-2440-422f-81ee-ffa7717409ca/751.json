,[362.58122749904004,539.890673776],[301.934298388528,539.890673776]]},{"text":"在，","box":[[65.26581642152007,545.518706096],[84.96392954152009,545.518706096],[84.96392954152009,555.2222100959999],[65.26581642152007,555.2222100959999]]},{"text":"柴已少，","box":[[80.20921258152008,545.518706096],[119.8965633485281,545.518706096],[119.8965633485281,555.2222100959999],[80.20921258152008,555.2222100959999]]},{"text":"黄花瘦，","box":[[115.14184638852808,545.518706096],[154.82918745203207,545.518706096],[154.82918745203207,555.2222100959999],[115.14184638852808,555.2222100959999]]},{"text":"骨犹傲。怕秋虫夜语，","box":[[150.5596359885281,545.518706096],[252.93163229904005,545.518706096],[252.93163229904005,555.2222100959999],[150.5596359885281,555.2222100959999]]},{"text":"孤灯斜照。蒲柳漫头摇落","box":[[248.7591449860481,545.518706096],[362.5812663130561,545.518706096],[362.5812663130561,555.2222100959999],[248.7591449860481,555.2222100959999]]}]}