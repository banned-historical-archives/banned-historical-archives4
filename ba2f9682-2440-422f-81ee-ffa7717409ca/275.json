8405],[189.85900185160034,347.17904552198405],[189.85900185160034,356.88254952198406],[130.18244254809636,356.88254952198406]]},{"text":"因为这是原则问题。","box":[[185.10430429860838,347.17904552198405],[274.7647200726243,347.17904552198405],[274.7647200726243,356.88254952198406],[185.10430429860838,356.88254952198406]]},{"text":"”","box":[[270.01000311262436,347.17904552198405],[279.71350711262437,347.17904552198405],[279.71350711262437,356.88254952198406],[270.01000311262436,356.88254952198406]]},{"text":"陈伯达做完动员，","box":[[85.25524813860835,362.413546801984],[164.92104508912033,362.413546801984],[164.92104508912033,372.117050801984],[85.25524813860835,372.117050801984]]},{"text":"大家就开始对《","box":[[160.16631842561634,362.413546801984],[231.29298333860834,362.413546801984],[231.29298333860834,372.117050801984],[160.16631842561634,372.117050801984]]},{"text":"二月提纲》","box":[[227.7026771551044,362.413546801984],[278.64608285860834,362.413546801984],[278.64608285860834,372.117050801984],[227.7026771551044,372.117050801984]]},{"text":"逐句逐条地讨论、","box":[[275.2498564586084,362.413546801984],[357.24453318313635,362.413546801984],[357.24453318313635,372.117050801984],[275.2498564586084,372.117050801984]]},{"text":"分","box":[[352.87794667963243,362.413546801984],[362.58145067963244,362.413546801984],[362.58145067963244,372.117050801984],[352.87794667963243,372.117050801984]]},{"text":"析、","box":[[65.26603960211241,377.64808689600005],[84.96415272211243,377.64808689600005],[84.96415272211243,387.35159089600006],[65.26603960211241,387.35159089600006]]},{"text":"批判，","box":[[80.20943576211242,377.64808689600005],[109.90217740912043,377.64808689600005],[109.90217740912043,387.35159089600006],[80.20943576211242,387.35159089600006]]},{"text":"看哪一条错在哪里，","box":[[105.14745074561641,377.64808689600005],[196.4574621996324,377.64808689600005],[196.4574621996324,387.35159089600006],[105.14745074561641,387.35159089600006]]},{"text":"哪一句话错在哪里。这样进行了一天。","box":[[192.09085628912044,377.64808689600005],[367.62729216664036,377.64808689600005],[367.62729216664036,387.35159089600006],[192.09085628912044,387.35159089600006]]},{"text":"因为很多东西说起来也是冠冕堂皇的，","box":[[65.26609782313642,392.882588176],[239.15287009612837,392.882588176],[239.15287009612837,402.586092176],[65.26609782313642,402.586092176]]},{"text":"仅从从字面上很难看出错误。","box":[[234.78628359262447,392.882588176],[367.62723394561647,392.882588176],[367.62723394561647,402.586092176],[234.78628359262447,402.586092176]]},{"text":"这样一句、","box":[[65.26603960211253,408.21412449600007],[114.94798978561657,408.21412449600007],[114.94798978561657,417.9176284960001],[65.26603960211253,417.9176284960001]]},{"text":"一条地分析、","box":[[110.19327282561655,408.21412449600007],[169.86983212912054,408.21412449600007],[169.86983212912054,417.9176284960001],[110.19327282561655,417.9176284960001]]},{"text":"批判就有点繁琐。于是戚本禹在会上提出：","box":[[165.11513457612855,408.21412449600007],[354.6246259171523,408.21412449600007],[354.6246259171523,417.9176284960001],[165.11513457612855,417.9176284960001]]},{"text":"“","box":[[85.25530635963241,423.44862577600003],[94.95881035963241,423.44862577600003],[94.95881035963241,433.15212977600004],[85.25530635963241,433.15212977600004]]},{"text":"我们能不能抓几个主要的问题来进行分析、","box":[[91.17445350313642,423.44862577600003],[281.1691103406563,423.44862577600003],[281.1691103406563,433.15212977600004],[91.17445350313642,433.15212977600004]]},{"text":"批判，","box":[[276.6084731641604,423.44862577600003],[306.49526548416037,423.44862577600003],[306.49526548416037,433.15212977600004],[276.6084731641604,433.15212977600004]]},{"text":"这样可能工作","box":[[301.9346089006564,423.44862577600003],[362.5815768251844,423.44862577600003],[362.5815768251844,433.15212977600004],[301.9346089006564,433.15212977600004]]},{"text":"效率会高点，","box":[[65.26613663715244,438.683127056],[124.94269594065649,438.683127056],[124.94269594065649,448.386631056],[65.26613663715244,448.386631056]]},{"text":"譬如，","box":[[120.18797898065647,438.683127056],[149.88070122065648,438.683127056],[149.88070122065648,448.386631056],[120.18797898065647,448.386631056]]},{"text":"把左派当学阀来反对，","box":[[145.12599396416047,438.683127056],[245.2661649476644,438.683127056],[245.2661649476644,448.386631056],[145.12599396416047,448.386631056]]},{"text":"这条就挺重要。但究竟这是","box":[[240.70552777116848,438.683127056],[362.5815962321924,438.683127056],[362.5815962321924,448.386631056],[240.70552777116848,448.386631056]]},{"text":"不是主要的问题，","box":[[65.26615604416048,453.91764774300805],[144.93195299467254,453.91764774300805],[144.93195299467254,463.62115174300806],[65.26615604416048,463.62115174300806]]},{"text":"我没想好，","box":[[140.1772166276645,453.91764774300805],[189.85916681116845,453.91764774300805],[189.85916681116845,463.62115174300806],[140.1772166276645,463.62115174300806]]},{"text":"要请大家来研究。","box":[[185.1044692581765,453.91764774300805],[264.7702468016804,453.91764774300805],[264.7702468016804,463.62115174300806],[185.1044692581765,463.62115174300806]]},{"text":"”","box":[[260.01552984168046,453.91764774300805],[269.71903384168047,453.91764774300805],[269.71903384168047,463.62115174300806],[260.01552984168046,463.62115174300806]]},{"text":"陈伯达觉得他的意见有点道理，","box":[[85.25541309817646,469.152149023008],[224.88887447219238,469.152149023008],[224.88887447219238,478.855653023008],[85.25541309817646,478.855653023008]]},{"text":"就让大家说说自己的看法。","box":[[220.13415751219247,469.152149023008],[339.77840064620835,469.152149023008],[339.77840064620835,478.855653023008],[220.13415751219247,478.855653023008]]},{"text":"王力说：","box":[[85.25547131920041,484.483665936],[124.94281238270443,484.483665936],[124.94281238270443,494.18716993600003],[85.25547131920041,494.18716993600003]]},{"text":"“","box":[[120.18809542270442,484.483665936],[129.89159942270442,484.483665936],[129.89159942270442,494.18716993600003],[120.18809542270442,494.18716993600003]]},{"text":"《","box":[[126.10723286270442,484.483665936],[135.81073686270443,484.483665936],[135.81073686270443,494.18716993600003],[126.10723286270442,494.18716993600003]]},{"text":"二月提纲》","box":[[132.02638000620843,484.483665936],[181.70833989321636,484.483665936],[181.70833989321636,494.18716993600003],[132.02638000620843,494.18716993600003]]},{"text":"就是个纲要嘛。我们是要抓住要害，","box":[[177.92398303672041,484.483665936],[337.4496082037283,484.483665936],[337.4496082037283,494.18716993600003],[177.92398303672041,494.18716993600003]]},{"text":"抓主要","box":[[332.69491065073635,484.483665936],[362.5817223777443,484.483665936],[362.5817223777443,494.18716993600003],[332.69491065073635,494.18716993600003]]},{"text":"问题来批判，","box":[[65.26631130022434,499.718186623008],[124.9428706037284,499.718186623008],[124.9428706037284,509.42169062300803],[65.26631130022434,509.42169062300803]]},{"text":"一般性的、","box":[[120.18815364372837,499.718186623008],[170.45232377073634,499.718186623008],[170.45232377073634,509.42169062300803],[120.18815364372837,509.42169062300803]]},{"text":"似是而非、","box":[[166.08573726723236,499.718186623008],[216.93210793073632,499.718186623008],[216.93210793073632,509.42169062300803],[166.08573726723236,509.42169062300803]]},{"text":"无关紧要的那些东西就放过去，","box":[[212.56552142723237,499.718186623008],[357.1477601377444,499.718186623008],[357.1477601377444,509.42169062300803],[212.56552142723237,509.42169062300803]]},{"text":"不","box":[[352.87826689526446,499.718186623008],[362.58177089526447,499.718186623008],[362.58177089526447,509.42169062300803],[352.87826689526446,509.42169062300803]]},{"text":"然这个批判文件就弄得太长了。","box":[[65.26635981774444,514.952687903008],[204.89982119176042,514.952687903008],[204.89982119176042,524.656191903008],[65.26635981774444,524.656191903008]]},{"text":"”","box":[[200.14510423176048,514.952687903008],[209.8486082317605,514.952687903008],[209.8486082317605,524.656191903008],[200.14510423176048,524.656191903008]]},{"text":"吴冷西接着王力的话题说：","box":[[85.25560716825647,530.187198886512],[205.48204113526447,530.187198886512],[205.48204113526447,539.8907028865119],[85.25560716825647,539.8907028865119]]},{"text":"“","box":[[201.01843899876852,530.187198886512],[210.72194299876853,530.187198886512],[210.72194299876853,539.8907028865119],[201.01843899876852,539.8907028865119]]},{"text":"评判谁是学阀，","box":[[207.1316562222725,530.187198886512],[278.35535617526455,530.187198886512],[278.35535617526455,539.8907028865119],[207.1316562222725,539.8907028865119]]},{"text":"和所谓‘","box":[[273.98876967176056,530.187198886512],[314.84056032577655,530.187198886512],[314.84056032577655,539.8907028865119],[273.98876967176056,539.8907028865119]]},{"text":"真理面前人","box":[[311.34726007176056,530.187198886512],[362.5818388197925,530.187198886512],[362.5818388197925,539.8907028865119],[311.34726007176056,539.8907028865119]]},{"text":"人平等’","box":[[65.26642774227253,545.518735206512],[104.95375910227256,545.518735206512],[104.95375910227256,555.222239206512],[65.26642774227253,555.222239206512]]},{"text":"的问题，","box":[[101.16940224577655,545.518735206512],[140.85673360577658,545.518735206512],[140.85673360577658,555.222239206512],[101.16940224577655,555.222239206512]]},{"text":"确实是个要害问题，","box":[[136.10202634928058,545.518735206512],[225.76242271628848,545.518735206512],[225.76242271628848,555.222239206512],[136.10202634928058,555.222239206512]]},{"text":"我们首先要把这些问题辩清楚。","box":[[221.00770575628857,545.518735206512],[360.6411865373125,545.518735206512],[360.6411865373125,555.222239206512],[221.00770575628857,555.222239206512]]},{"text":"”","box":[[355.8864501703045,545.518735206512],[365.5899541703045,545.518735206512],[365.5899541703045,555.222239206512],[355.8864501703045,555.222239206512]]}]}