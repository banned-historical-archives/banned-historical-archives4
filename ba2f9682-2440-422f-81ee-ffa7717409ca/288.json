577948354976],[305.71871346604814,255.577948354976],[305.71871346604814,265.281452354976],[276.02597181904014,265.281452354976]]},{"text":"关于社会主义","box":[[301.93433720254416,255.577948354976],[362.5813051270722,255.577948354976],[362.5813051270722,265.281452354976],[301.93433720254416,265.281452354976]]},{"text":"文化大革命”","box":[[65.26586493904023,270.812449634976],[124.94242424254428,270.812449634976],[124.94242424254428,280.51595363497603],[65.26586493904023,280.51595363497603]]},{"text":"的主题报告，","box":[[121.15806738604826,270.812449634976],[180.93167143305624,270.812449634976],[180.93167143305624,280.51595363497603],[121.15806738604826,280.51595363497603]]},{"text":"向与会人员通报了正在全国进行的文化革","box":[[176.56508492955228,270.812449634976],[362.58137305160005,270.812449634976],[362.58137305160005,280.51595363497603],[176.56508492955228,280.51595363497603]]},{"text":"命情况。","box":[[65.26593286356825,286.144044176],[104.95326422356828,286.144044176],[104.95326422356828,295.84754817600003],[65.26593286356825,295.84754817600003]]},{"text":"人大副委员长郭沫若在会上发表了讲话，","box":[[85.25515110356827,301.378506641984],[259.82121717408023,301.378506641984],[259.82121717408023,311.082010641984],[85.25515110356827,311.082010641984]]},{"text":"他说：","box":[[254.7753756870723,301.378506641984],[283.8859070940803,301.378506641984],[283.8859070940803,311.082010641984],[254.7753756870723,311.082010641984]]},{"text":"“","box":[[278.84008501408033,301.378506641984],[288.54358901408034,301.378506641984],[288.54358901408034,311.082010641984],[278.84008501408033,311.082010641984]]},{"text":"石西民同志的报告","box":[[284.4681173340803,301.378506641984],[362.8723714330564,301.378506641984],[362.8723714330564,311.082010641984],[284.4681173340803,311.082010641984]]},{"text":"对我来说，","box":[[65.26594256707244,316.61300792198404],[113.68642752707245,316.61300792198404],[113.68642752707245,326.31651192198404],[65.26594256707244,326.31651192198404]]},{"text":"是有切身的感受。说得沉痛一点，","box":[[108.64060544707243,316.61300792198404],[257.4923762140804,316.61300792198404],[257.4923762140804,326.31651192198404],[108.64060544707243,326.31651192198404]]},{"text":"是有切肤之痛。因为在一","box":[[252.83468459057647,316.61300792198404],[362.87236172955244,316.61300792198404],[362.87236172955244,326.31651192198404],[252.83468459057647,326.31651192198404]]},{"text":"般朋友们、","box":[[65.26593286356848,331.84752860899204],[113.68641782356849,331.84752860899204],[113.68641782356849,341.55103260899205],[65.26593286356848,341.55103260899205]]},{"text":"同志们看来，","box":[[108.64059574356847,331.84752860899204],[166.86160033656046,331.84752860899204],[166.86160033656046,341.55103260899205],[108.64059574356847,341.55103260899205]]},{"text":"我是一个文化人，","box":[[161.81580736707247,331.84752860899204],[239.44383936707248,331.84752860899204],[239.44383936707248,341.55103260899205],[161.81580736707247,341.55103260899205]]},{"text":"甚至于好些人都说我是一个作","box":[[234.39799788006454,331.84752860899204],[362.8723326190406,331.84752860899204],[362.8723326190406,341.55103260899205],[234.39799788006454,341.55103260899205]]},{"text":"家，","box":[[65.2659037530567,347.17904552198405],[84.67291175305671,347.17904552198405],[84.67291175305671,356.88254952198406],[65.2659037530567,356.88254952198406]]},{"text":"还是一个诗人，","box":[[79.6270896730567,347.17904552198405],[147.55161767305674,347.17904552198405],[147.55161767305674,356.88254952198406],[79.6270896730567,356.88254952198406]]},{"text":"又是一个什么历史学家。几十年来，","box":[[142.5057955930567,347.17904552198405],[298.6351943600647,347.17904552198405],[298.6351943600647,356.88254952198406],[142.5057955930567,356.88254952198406]]},{"text":"一直拿着笔杆子","box":[[293.7834229530568,347.17904552198405],[362.87232291553687,347.17904552198405],[362.87232291553687,356.88254952198406],[293.7834229530568,356.88254952198406]]},{"text":"在写东西，","box":[[65.26589404955291,362.413546801984],[113.68637900955292,362.413546801984],[113.68637900955292,372.117050801984],[65.26589404955291,372.117050801984]]},{"text":"也翻译了一些东西。按字数来讲，","box":[[108.6405569295529,362.413546801984],[254.67830183305688,362.413546801984],[254.67830183305688,372.117050801984],[108.6405569295529,372.117050801984]]},{"text":"恐怕有几百万字了。但是，","box":[[249.82653042604898,362.413546801984],[367.918203216561,362.413546801984],[367.918203216561,372.117050801984],[249.82653042604898,372.117050801984]]},{"text":"拿今天的标准来讲，","box":[[65.2658746425451,377.64808689600005],[152.5003756025451,377.64808689600005],[152.5003756025451,387.35159089600006],[65.2658746425451,387.35159089600006]]},{"text":"我以前所写的东西，","box":[[147.45454381904108,377.64808689600005],[237.503090049553,377.64808689600005],[237.503090049553,387.35159089600006],[147.45454381904108,387.35159089600006]]},{"text":"严格地说，","box":[[232.84540812955308,377.64808689600005],[282.52733890604907,377.64808689600005],[282.52733890604907,387.35159089600006],[232.84540812955308,387.35159089600006]]},{"text":"应该全部把它烧掉，","box":[[277.86967639305703,377.64808689600005],[367.91819351305696,377.64808689600005],[367.91819351305696,387.35159089600006],[277.86967639305703,387.35159089600006]]},{"text":"没有一点价值。主要原因是什么呢？就是没有学好毛主席思想，","box":[[65.26586493904102,392.882588176],[338.22546156955303,392.882588176],[338.22546156955303,402.586092176],[65.26586493904102,402.586092176]]},{"text":"没有用","box":[[333.37370956955317,392.882588176],[362.87233261904123,392.882588176],[362.87233261904123,402.586092176],[333.37370956955317,402.586092176]]},{"text":"毛主席思想来武装自己，","box":[[65.2658649390412,408.21412449600007],[172.0044186425452,408.21412449600007],[172.0044186425452,417.9176284960001],[65.2658649390412,417.9176284960001]]},{"text":"所以阶级观点有的时候很模糊......","box":[[166.9585771555372,408.21412449600007],[312.5111565625452,408.21412449600007],[312.5111565625452,417.9176284960001],[166.9585771555372,417.9176284960001]]},{"text":"我虽然已经七十几岁了，","box":[[85.25508317904129,423.44862577600003],[192.2847322990413,423.44862577600003],[192.2847322990413,433.15212977600004],[85.25508317904129,433.15212977600004]]},{"text":"雄心壮志还有一点。就是说，","box":[[187.6270697860493,423.44862577600003],[317.45992419553727,423.44862577600003],[317.45992419553727,433.15212977600004],[187.6270697860493,433.15212977600004]]},{"text":"要滚一身泥","box":[[312.80224227553737,423.44862577600003],[362.8722841015213,423.44862577600003],[362.8722841015213,433.15212977600004],[312.80224227553737,433.15212977600004]]},{"text":"巴，","box":[[65.26585523553734,438.683127056],[84.67286323553735,438.683127056],[84.67286323553735,448.386631056],[65.26585523553734,448.386631056]]},{"text":"我愿意；","box":[[79.72408589904134,438.683127056],[118.73216227553735,438.683127056],[118.73216227553735,448.386631056],[79.72408589904134,448.386631056]]},{"text":"要沾一身油污，","box":[[113.78337523553733,438.683127056],[182.1930784355373,438.683127056],[182.1930784355373,448.386631056],[113.78337523553733,448.386631056]]},{"text":"我愿意；","box":[[177.24431080254533,438.683127056],[216.25237747553732,438.683127056],[216.25237747553732,448.386631056],[177.24431080254533,448.386631056]]},{"text":"甚至于要染一身血迹，","box":[[211.30359043553733,438.683127056],[309.1148913485292,438.683127056],[309.1148913485292,448.386631056],[211.30359043553733,448.386631056]]},{"text":"假使美帝国主","box":[[304.16612371553725,438.683127056],[362.87228410152125,438.683127056],[362.87228410152125,448.386631056],[304.16612371553725,448.386631056]]},{"text":"义要来打我们的话，","box":[[65.26581642152127,453.91764774300805],[152.59735242152126,453.91764774300805],[152.59735242152126,463.62115174300806],[65.26581642152127,463.62115174300806]]},{"text":"向美帝国主义分子投几颗手榴弹，","box":[[147.55153034152121,453.91764774300805],[293.1041000450252,453.91764774300805],[293.1041000450252,463.62115174300806],[147.55153034152121,463.62115174300806]]},{"text":"我也愿意......”","box":[[288.0582779650253,453.91764774300805],[355.9828447790412,453.91764774300805],[355.9828447790412,463.62115174300806],[288.0582779650253,463.62115174300806]]},{"text":"郭沫若的这个讲话，","box":[[85.25504436502524,469.152149023008],[174.91545043553722,469.152149023008],[174.91545043553722,478.855653023008],[85.25504436502524,478.855653023008]]},{"text":"经毛泽东亲笔批示后，","box":[[170.16073347553726,469.152149023008],[270.5920289860492,469.152149023008],[270.5920289860492,478.855653023008],[170.16073347553726,478.855653023008]]},{"text":"于","box":[[266.0313724025453,469.152149023008],[275.7348764025453,469.152149023008],[275.7348764025453,478.855653023008],[266.0313724025453,478.855653023008]]},{"text":"","box":[[275.7348764025453,469.152149023008],[275.97486740254516,469.152149023008],[275.97486740254516,469.152149023008],[275.7348764025453,469.152149023008]]},{"text":"４","box":[[278.063630031008,469.152119912496],[287.767134031008,469.152119912496],[287.767134031008,478.855623912496],[278.063630031008,478.855623912496]]},{"text":"月","box":[[286.117557758016,469.152119912496],[295.821061758016,469.152119912496],[295.821061758016,478.855623912496],[286.117557758016,478.855623912496]]},{"text":"","box":[[295.821061758016,469.152119912496],[296.06105975801603,469.152119912496],[296.06105975801603,469.152119912496],[295.821061758016,469.152119912496]]},{"text":"２","box":[[298.149883311008,469.152119912496],[307.853387311008,469.152119912496],[307.853387311008,478.855623912496],[298.149883311008,478.855623912496]]},{"text":"８","box":[[303.001635311008,469.152119912496],[312.705139311008,469.152119912496],[312.705139311008,478.855623912496],[303.001635311008,478.855623912496]]},{"text":"日，","box":[[311.05556303801603,469.152119912496],[330.75366645451203,469.152119912496],[330.75366645451203,478.855623912496],[311.05556303801603,478.855623912496]]},{"text":"在《","box":[[326.19300987100803,469.152119912496],[346.18223781451206,469.152119912496],[346.18223781451206,478.855623912496],[326.19300987100803,478.855623912496]]},{"text":"光明","box":[[342.5919510380161,469.152119912496],[362.5811789815201,469.152119912496],[362.5811789815201,478.855623912496],[342.5919510380161,478.855623912496]]},{"text":"日报》","box":[[65.26576790400009,484.4