":[[248.273872751008,270.812439931472],[362.5811886850239,270.812439931472],[362.5811886850239,280.515943931472],[248.273872751008,280.515943931472]]},{"text":"《","box":[[65.26577760750394,286.144034472496],[74.96928160750393,286.144034472496],[74.96928160750393,295.847538472496],[65.26577760750394,295.847538472496]]},{"text":"贵州日报》","box":[[71.18491504750394,286.144034472496],[120.86686523100798,286.144034472496],[120.86686523100798,295.847538472496],[71.18491504750394,295.847538472496]]},{"text":"的“","box":[[117.08250837451196,286.144034472496],[136.97469157451195,286.144034472496],[136.97469157451195,295.847538472496],[117.08250837451196,295.847538472496]]},{"text":"现行反革命”","box":[[133.48142043100796,286.144034472496],[195.00165519801595,286.144034472496],[195.00165519801595,295.847538472496],[133.48142043100796,295.847538472496]]},{"text":"。贵州大学的打击面，","box":[[191.21729834152,286.144034472496],[294.17149518852796,286.144034472496],[294.17149518852796,295.847538472496],[191.21729834152,295.847538472496]]},{"text":"占当时在校学生","box":[[289.998988468528,286.144034472496],[362.58123720254395,286.144034472496],[362.58123720254395,295.847538472496],[289.998988468528,295.847538472496]]},{"text":"数（","box":[[65.26582612502398,301.37849693848],[84.963939245024,301.37849693848],[84.963939245024,311.08200093848],[65.26582612502398,311.08200093848]]},{"text":"不含下乡四清尚未返校学生数）","box":[[81.17957268502398,301.37849693848],[220.81303405903992,301.37849693848],[220.81303405903992,311.08200093848],[81.17957268502398,311.08200093848]]},{"text":"的","box":[[217.028677202544,301.37849693848],[226.732181202544,301.37849693848],[226.732181202544,311.08200093848],[217.028677202544,311.08200093848]]},{"text":"","box":[[226.732181202544,301.37849693848],[226.952170202544,301.37849693848],[226.952170202544,301.37849693848],[226.732181202544,301.37849693848]]},{"text":"５","box":[[228.866845344,301.37849693848],[238.57034934400002,301.37849693848],[238.57034934400002,311.08200093848],[228.866845344,311.08200093848]]},{"text":"０","box":[[233.718616751008,301.37849693848],[243.422120751008,301.37849693848],[243.422120751008,311.08200093848],[233.718616751008,311.08200093848]]},{"text":"％","box":[[239.152569287504,301.37849693848],[248.85607328750402,301.37849693848],[248.85607328750402,311.08200093848],[239.152569287504,311.08200093848]]},{"text":"以上。","box":[[249.14717840750401,301.37849693848],[278.839900647504,301.37849693848],[278.839900647504,311.08200093848],[249.14717840750401,311.08200093848]]},{"text":"贵阳师范学院学生杨文华在他日记中的这样写到：","box":[[85.254995847504,316.61299821848],[304.84536899553575,316.61299821848],[304.84536899553575,326.31650221848],[85.254995847504,326.31650221848]]},{"text":"６","box":[[84.67277590400002,331.847528608992],[94.37627990400001,331.847528608992],[94.37627990400001,341.551032608992],[84.67277590400002,341.551032608992]]},{"text":"月","box":[[92.53261414400001,331.847528608992],[102.236118144,331.847528608992],[102.236118144,341.551032608992],[92.53261414400001,341.551032608992]]},{"text":"","box":[[102.236118144,331.847528608992],[102.456119144,331.847528608992],[102.456119144,331.847528608992],[102.236118144,331.847528608992]]},{"text":"８","box":[[104.370898727504,331.847528608992],[114.074402727504,331.847528608992],[114.074402727504,341.551032608992],[104.370898727504,341.551032608992]]},{"text":"日","box":[[112.23072726400001,331.847528608992],[121.934231264,331.847528608992],[121.934231264,341.551032608992],[112.23072726400001,341.551032608992]]},{"text":"晚自习时系上通知要交待重要事情，","box":[[85.25498614400001,347.179045521984],[245.26580591801596,347.179045521984],[245.26580591801596,356.882549521984],[85.25498614400001,356.882549521984]]},{"text":"先通知几个人去开会。我们","box":[[240.70516874152003,347.179045521984],[362.581198388528,347.179045521984],[362.581198388528,356.882549521984],[240.70516874152003,356.882549521984]]},{"text":"等到十时，","box":[[65.26578731100807,362.413546801984],[114.94773749451211,362.413546801984],[114.94773749451211,372.117050801984],[65.26578731100807,372.117050801984]]},{"text":"工作队的老徐非常严肃地到班上宣布","box":[[110.19302053451209,362.413546801984],[270.78609906604805,362.413546801984],[270.78609906604805,372.117050801984],[110.19302053451209,372.117050801984]]},{"text":"","box":[[270.7860990660481,362.413546801984],[271.0260890660481,362.413546801984],[271.0260890660481,362.413546801984],[270.7860990660481,362.413546801984]]},{"text":"６","box":[[273.114842991008,362.41354680198395],[282.818346991008,362.41354680198395],[282.818346991008,372.11705080198396],[273.114842991008,372.11705080198396]]},{"text":"月","box":[[281.168751311008,362.41354680198395],[290.872255311008,362.41354680198395],[290.872255311008,372.11705080198396],[281.168751311008,372.11705080198396]]},{"text":"","box":[[290.872255311008,362.41354680198395],[291.122255311008,362.41354680198395],[291.122255311008,362.41354680198395],[290.872255311008,362.41354680198395]]},{"text":"６","box":[[293.29813131100804,362.41354680198395],[303.00163531100804,362.41354680198395],[303.00163531100804,372.11705080198396],[293.29813131100804,372.11705080198396]]},{"text":"日我院发生的","box":[[301.449074671008,362.41354680198395],[362.58120809203206,362.41354680198395],[362.58120809203206,372.11705080198396],[301.449074671008,372.11705080198396]]},{"text":"事是“","box":[[65.26576790400009,377.648086896],[94.95849014400011,377.648086896],[94.95849014400011,387.351590896],[65.26576790400009,387.351590896]]},{"text":"反革命事件”","box":[[91.1741332875041,377.648086896],[150.8506925910081,377.648086896],[150.8506925910081,387.351590896],[91.1741332875041,387.351590896]]},{"text":"。我一下子蒙了，","box":[[147.06633573451214,377.648086896],[226.63510734852807,377.648086896],[226.63510734852807,387.351590896],[147.06633573451214,387.351590896]]},{"text":"啥？我们热血沸腾参加的是反革","box":[[221.88037098152014,377.648086896],[362.5812663130561,377.648086896],[362.5812663130561,387.351590896],[221.88037098152014,387.351590896]]},{"text":"命事件？还没等我回过神来，","box":[[65.2658261250242,392.88258817599996],[194.71059859553623,392.88258817599996],[194.71059859553623,402.58609217599997],[65.2658261250242,402.58609217599997]]},{"text":"刘某（","box":[[189.95588163553623,392.88258817599996],[219.64860387553622,392.88258817599996],[219.64860387553622,402.58609217599997],[189.95588163553623,402.58609217599997]]},{"text":"我班参加刚才小会的同学）","box":[[215.86422761203224,392.88258817599996],[336.09067128254406,392.88258817599996],[336.09067128254406,402.58609217599997],[215.86422761203224,402.58609217599997]]},{"text":"拿出一","box":[[332.5003845060482,392.88258817599996],[362.58126631305623,392.88258817599996],[362.58126631305623,402.58609217599997],[332.5003845060482,402.58609217599997]]},{"text":"张大字报底稿念起来。这张大字报点明写质问省委大字报的佘开生","box":[[65.26585523553621,408.21412449599995],[362.58137305160005,408.21412449599995],[362.58137305160005,417.91762849599996],[65.26585523553621,417.91762849599996]]},{"text":"是反革命。另外几个参加开会的立即附和。刘某叫大家签名却没人","box":[[65.26593286356825,423.448625776],[362.5814409761282,423.448625776],[362.5814409761282,433.152129776],[65.26593286356825,433.152129776]]},{"text":"响应，","box":[[65.26600078809628,438.68312705599993],[94.9587230280963,438.68312705599993],[94.9587230280963,448.38663105599994],[65.26600078809628,448.38663105599994]]},{"text":"毕竟把一个同学打成反革命可不是件小事。","box":[[90.2040157716003,438.68312705599993],[279.8105421526242,438.68312705599993],[279.8105421526242,448.38663105599994],[90.2040157716003,448.38663105599994]]},{"text":"质问省委的大字报虽然不是我写的（","box":[[85.25526754561633,453.91764774300793],[244.78090241612824,453.91764774300793],[244.78090241612824,463.62115174300794],[85.25526754561633,463.62115174300794]]},{"text":"如果当初的分工是我写质问","box":[[240.99654555963235,453.91764774300793],[362.5815962321923,453.91764774300793],[362.5815962321923,463.62115174300794],[240.99654555963235,463.62115174300794]]},{"text":"省委、","box":[[65.26607841612832,469.15214902300795],[94.95880065612835,469.15214902300795],[94.95880065612835,478.85565302300796],[65.26607841612832,478.85565302300796]]},{"text":"他写顾惠之的大字报，","box":[[90.20409339963234,469.15214902300795],[190.82943958313632,469.15214902300795],[190.82943958313632,478.85565302300796],[90.20409339963234,478.85565302300796]]},{"text":"那当反革命的就是我了，","box":[[186.55990752664036,469.15214902300795],[299.7027932771523,469.15214902300795],[299.7027932771523,478.85565302300796],[186.55990752664036,478.85565302300796]]},{"text":"好险！","box":[[295.4332418136484,469.15214902300795],[325.51411391715243,469.15214902300795],[325.51411391715243,478.85565302300796],[295.4332418136484,478.85565302300796]]},{"text":"）","box":[[320.7593775501444,469.15214902300795],[330.4628815501444,469.15214902300795],[330.4628815501444,478.85565302300796],[320.7593775501444,478.85565302300796]]},{"text":"，","box":[[326.6785538041604,469.15214902300795],[336.3820578041604,469.15214902300795],[336.3820578041604,478.85565302300796],[326.6785538041604,478.85565302300796]]},{"text":"但我积","box":[[332.1124966371524,469.15214902300795],[362.5815380111684,469.15214902300795],[362.5815380111684,478.85565302300796],[332.1124966371524,478.85565302300796]]},{"text":"极地帮着贴、","box":[[65.26612693364837,484.4836659359999],[124.94268623715242,484.4836659359999],[124.94268623715242,494.1871699359999],[65.26612693364837,494.1871699359999]]},{"text":"帮着挂，","box":[[120.18796927715239,484.4836659359999],[159.87531034065637,484.4836659359999],[159.87531034065637,494.1871699359999],[120.18796927715239,494.1871699359999]]},{"text":"干系是少不了的。我冥思苦想了好久，","box":[[155.1205933806564,484.4836659359999],[326.48450313116837,484.4836659359999],[326.48450313116837,494.1871699359999],[155.1205933806564,494.1871699359999]]},{"text":"决定连夜","box":[[322.0209009946724,484.4836659359999],[362.5815865286884,484.4836659359999],[362.5815865286884,494.1871699359999],[322.0209009946724,494.1871699359999]]},{"text":"写一张检查。","box":[[65.2661463406564,499.7181866230079],[124.94270564416045,499.7181866230079],[124.94270564416045,509.4216906230079],[65.2661463406564,509.4216906230079]]},{"text":"６","box":[[84.67277590400002,514.952658792496],[94.37627990400001,514.952658792496],[94.37627990400001,524.6561627924959],[84.67277590400002,524.6561627924959]]},{"text":"月","box":[[92.53261414400001,514.952658792496],[102.236118144,514.952658792496],[102.236118144,524.6561627924959],[92.53261414400001,524.6561627924959]]},{"text":"","box":[[102.236118144,514.952658792496],[102.456119144,514.952658792496],[102.456119144,514.952658792496],[102.236118144,514.952658792496]]},{"text":"９","box":[[104.370898727504,514.952658792496],[114.074402727504,514.952658792496],[114.074402727504,524.6561627924959],[104.370898727504,524.6561627924959]]},{"text":"日","box":[[112.23072726400001,514.952658792496],[121.934231264,514.952658792496],[121.934231264,524.6561627924959],[112.23072726400001,524.6561627924959]]},{"text":"一清早把检查贴了出去，","box":[[85.25498614400001,530.187169776],[195.972005598016,530.187169776],[195.972005598016,539.890673776],[85.25498614400001,539.890673776]]},{"text":"走了十几步又忽然想到，","box":[[191.60541909451203,530.187169776],[304.74834365903996,530.187169776],[304.74834365903996,539.890673776],[191.60541909451203,539.890673776]]},{"text":"这样做岂不是","box":[[300.4787921955361,530.187169776],[362.581237202544,530.187169776],[362.581237202544,539.890673776],[300.4787921955361,539.890673776]]},{"text":"不打自招，","box":[[65.26582612502403,545.518706096],[114.94777630852808,545.518706096],[114.94777630852808,555.2222100959999],[65.26582612502403,555.2222100959999]]},{"text":"自投罗网。管他的，","box":[[110.19305934852805,545.518706096],[201.21195597904006,545.518706096],[201.21195597904006,555.2222100959999],[110.19305934852805,555.2222100959999]]},{"text":"反正贴大字报的不止我一人，","box":[[196.8453694755361,545.518706096],[331.2389678000641,545.518706096],[331.2389678000641,555.2222100959999],[196.8453694755361,555.2222100959999]]},{"text":"等等看。","box":[[326.96936781904014,545.518706096],[367.62707868955215,545.518706096],[367.62707868955215,555.2222100959999],[326.96936781904014,555.2222100959999]]}]}