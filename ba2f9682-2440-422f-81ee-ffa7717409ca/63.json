80198396],[194.22534576750402,372.11705080198396]]},{"text":"年","box":[[202.57033980049601,362.41354680198395],[212.27384380049602,362.41354680198395],[212.27384380049602,372.11705080198396],[202.57033980049601,372.11705080198396]]},{"text":"","box":[[212.27384380049602,362.41354680198395],[212.54384480049603,362.41354680198395],[212.54384480049603,362.41354680198395],[212.27384380049602,362.41354680198395]]},{"text":"４","box":[[214.89379958400002,362.41354680198395],[224.59730358400003,362.41354680198395],[224.59730358400003,372.11705080198396],[214.89379958400002,372.11705080198396]]},{"text":"月，","box":[[223.238832431008,362.41354680198395],[242.93693584750397,362.41354680198395],[242.93693584750397,372.11705080198396],[223.238832431008,372.11705080198396]]},{"text":"上海市委第一书记柯庆施病","box":[[238.66740379100798,362.41354680198395],[362.58124690604785,362.41354680198395],[362.58124690604785,372.11705080198396],[238.66740379100798,372.11705080198396]]},{"text":"逝，","box":[[65.26580671801594,377.648086896],[84.96391983801595,377.648086896],[84.96391983801595,387.351590896],[65.26580671801594,387.351590896]]},{"text":"上海市委的工作由上海市委书记陈丕显负责，","box":[[80.20920287801594,377.648086896],[279.8103577860478,377.648086896],[279.8103577860478,387.351590896],[80.20920287801594,387.351590896]]},{"text":"江青曾经找到他：","box":[[275.055640826048,377.648086896],[354.72139896254384,377.648086896],[354.72139896254384,387.351590896],[275.055640826048,387.351590896]]},{"text":"“","box":[[85.25509288254392,392.88258817599996],[94.95859688254392,392.88258817599996],[94.95859688254392,402.58609217599997],[85.25509288254392,402.58609217599997]]},{"text":"阿丕呀，","box":[[91.17424002604793,392.88258817599996],[130.86158108955195,392.88258817599996],[130.86158108955195,402.58609217599997],[91.17424002604793,402.58609217599997]]},{"text":"我要向你借两个人啊。","box":[[126.10686412955194,392.88258817599996],[225.76186961655986,392.88258817599996],[225.76186961655986,402.58609217599997],[126.10686412955194,402.58609217599997]]},{"text":"”","box":[[221.00715265655995,392.88258817599996],[230.71065665655996,392.88258817599996],[230.71065665655996,402.58609217599997],[221.00715265655995,402.58609217599997]]},{"text":"“","box":[[85.25512199305595,408.21412449599995],[94.95862599305595,408.21412449599995],[94.95862599305595,417.91762849599996],[85.25512199305595,417.91762849599996]]},{"text":"行啊，","box":[[91.17426913655996,408.21412449599995],[120.86699137655998,408.21412449599995],[120.86699137655998,417.91762849599996],[91.17426913655996,417.91762849599996]]},{"text":"借谁？","box":[[116.11228412006398,408.21412449599995],[145.805006360064,408.21412449599995],[145.805006360064,417.91762849599996],[116.11228412006398,417.91762849599996]]},{"text":"”","box":[[141.050269993056,408.21412449599995],[150.753773993056,408.21412449599995],[150.753773993056,417.91762849599996],[141.050269993056,417.91762849599996]]},{"text":"“","box":[[85.25513169656,423.448625776],[94.95863569656,423.448625776],[94.95863569656,433.152129776],[85.25513169656,433.152129776]]},{"text":"张春桥、","box":[[91.174278840064,423.448625776],[130.86161990356803,423.448625776],[130.86161990356803,433.152129776],[91.174278840064,433.152129776]]},{"text":"姚文元。","box":[[126.10690294356802,423.448625776],[165.79423430356803,423.448625776],[165.79423430356803,433.152129776],[126.10690294356802,433.152129776]]},{"text":"”","box":[[161.03951734356804,423.448625776],[170.74302134356805,423.448625776],[170.74302134356805,433.152129776],[161.03951734356804,433.152129776]]},{"text":"“","box":[[85.25515110356804,438.68312705599993],[94.95865510356803,438.68312705599993],[94.95865510356803,448.38663105599994],[85.25515110356804,448.38663105599994]]},{"text":"哦？","box":[[91.17429824707204,438.68312705599993],[110.87241136707206,438.68312705599993],[110.87241136707206,448.38663105599994],[91.17429824707204,448.38663105599994]]},{"text":"”","box":[[106.11768470356805,438.68312705599993],[115.82118870356804,438.68312705599993],[115.82118870356804,448.38663105599994],[106.11768470356805,448.38663105599994]]},{"text":"“","box":[[85.25515110356804,453.91764774300793],[94.95865510356803,453.91764774300793],[94.95865510356803,463.62115174300794],[85.25515110356804,463.62115174300794]]},{"text":"我让他们准备批判《","box":[[91.17429824707204,453.91764774300793],[181.2228444775841,453.91764774300793],[181.2228444775841,463.62115174300794],[91.17429824707204,463.62115174300794]]},{"text":"海瑞罢官》","box":[[177.63253829408012,453.91764774300793],[228.38183510356808,453.91764774300793],[228.38183510356808,463.62115174300794],[177.63253829408012,463.62115174300794]]},{"text":"文章。柯老对此事很支持，","box":[[224.88858336707213,453.91764774300793],[346.95869279758415,453.91764774300793],[346.95869279758415,463.62115174300794],[224.88858336707213,463.62115174300794]]},{"text":"希望","box":[[342.49509066108817,453.91764774300793],[362.58134394108816,453.91764774300793],[362.58134394108816,463.62115174300794],[342.49509066108817,463.62115174300794]]},{"text":"你也能支持我呀。","box":[[65.26593286356814,469.15214902300795],[144.9317298140802,469.15214902300795],[144.9317298140802,478.85565302300796],[65.26593286356814,478.85565302300796]]},{"text":"”","box":[[140.17699344707216,469.15214902300795],[149.88049744707217,469.15214902300795],[149.88049744707217,478.85565302300796],[140.17699344707216,478.85565302300796]]},{"text":"“","box":[[85.25516080707216,484.4836659359999],[94.95866480707215,484.4836659359999],[94.95866480707215,494.1871699359999],[85.25516080707216,494.1871699359999]]},{"text":"我怎么会不支持呀。","box":[[91.17430795057616,484.4836659359999],[180.83471402108816,484.4836659359999],[180.83471402108816,494.1871699359999],[91.17430795057616,494.1871699359999]]},{"text":"”","box":[[176.0799970610882,484.4836659359999],[185.7835010610882,484.4836659359999],[185.7835010610882,494.1871699359999],[176.0799970610882,494.1871699359999]]},{"text":"“","box":[[85.25518021408018,499.7181866230079],[94.95868421408018,499.7181866230079],[94.95868421408018,509.4216906230079],[85.25518021408018,509.4216906230079]]},{"text":"那就谢谢你啦。不过，","box":[[91.17432735758419,499.7181866230079],[191.50856842108817,499.7181866230079],[191.50856842108817,509.4216906230079],[91.17432735758419,509.4216906230079]]},{"text":"这事要保密，","box":[[187.14200132459223,499.7181866230079],[248.37114067510421,499.7181866230079],[248.37114067510421,509.4216906230079],[187.14200132459223,509.4216906230079]]},{"text":"对任何人都不要讲，","box":[[244.00455417160026,499.7181866230079],[336.3819607691202,499.7181866230079],[336.3819607691202,509.4216906230079],[244.00455417160026,509.4216906230079]]},{"text":"特别是","box":[[332.0153742656163,499.7181866230079],[362.5814312726243,499.7181866230079],[362.5814312726243,509.4216906230079],[332.0153742656163,509.4216906230079]]},{"text":"不能让北京市委的人知道。","box":[[65.26602019510426,514.9526879030079],[184.91024392211227,514.9526879030079],[184.91024392211227,524.6561919030079],[65.26602019510426,524.6561919030079]]},{"text":"”","box":[[180.1555269621123,514.9526879030079],[189.8590309621123,514.9526879030079],[189.8590309621123,524.6561919030079],[180.1555269621123,524.6561919030079]]},{"text":"“","box":[[85.2552578421123,530.187198886512],[94.9587618421123,530.187198886512],[94.9587618421123,539.8907028865119],[85.2552578421123,539.8907028865119]]},{"text":"写文章还要保密？","box":[[91.1744049856163,530.187198886512],[170.8401922326243,530.187198886512],[170.8401922326243,539.8907028865119],[91.1744049856163,539.8907028865119]]},{"text":"”","box":[[166.08547527262434,530.187198886512],[175.78897927262435,530.187198886512],[175.78897927262435,539.8907028865119],[166.08547527262434,539.8907028865119]]},{"text":"陈丕显心里想。但是，","box":[[172.00462241612834,530.187198886512],[274.0855039031363,530.187198886512],[274.0855039031363,539.8907028865119],[172.00462241612834,539.8907028865119]]},{"text":"江青这人总喜欢搞点","box":[[269.8159524396324,530.187198886512],[362.5815477146723,530.187198886512],[362.5815477146723,539.8907028865119],[269.8159524396324,539.8907028865119]]},{"text":"新花样，","box":[[65.26610752664033,545.5187352065119],[104.95343888664036,545.5187352065119],[104.95343888664036,555.2222392065119],[65.26610752664033,555.2222392065119]]},{"text":"先对外保密，","box":[[100.19873163014435,545.5187352065119],[159.87529093364836,545.5187352065119],[159.87529093364836,555.2222392065119],[100.19873163014435,555.2222392065119]]},{"text":"待发表时再一鸣惊人，","box":[[155.1205739736484,545.5187352065119],[255.3577897006563,545.5187352065119],[255.3577897006563,555.2222392065119],[155.1205739736484,555.2222392065119]]},{"text":"让大家佩服她，","box":[[250.79715252416037,545.5187352065119],[321.5356772771523,545.5187352065119],[321.5356772771523,555.2222392065119],[250.79715252416037,555.2222392065119]]},{"text":"这也难说。","box":[[317.0720751406563,545.5187352065119],[367.62731157364834,545.5187352065119],[367.62731157364834,555.2222392065119],[317.0720751406563,555.2222392065119]]}]}