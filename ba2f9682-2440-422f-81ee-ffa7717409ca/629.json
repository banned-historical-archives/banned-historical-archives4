55.135928505488],[319.206467584,355.135928505488]]},{"text":"时左右。","box":[[327.357449758016,345.432424505488],[367.626962247504,345.432424505488],[367.626962247504,355.135928505488],[327.357449758016,355.135928505488]]},{"text":"每餐不超过四菜一汤。韶山滴水洞的冬笋、","box":[[65.26576790400003,360.763980232496],[258.55962580502404,360.763980232496],[258.55962580502404,370.467484232496],[65.26576790400003,370.467484232496]]},{"text":"香椿、","box":[[254.19303930152012,360.763980232496],[284.3709464450241,360.763980232496],[284.3709464450241,370.467484232496],[254.19303930152012,370.467484232496]]},{"text":"蕨苗、","box":[[280.1014337955361,360.763980232496],[310.27930212502406,360.763980232496],[310.27930212502406,370.467484232496],[280.1014337955361,370.467484232496]]},{"text":"地菜、","box":[[306.00975066152006,360.763980232496],[336.187638398016,360.763980232496],[336.187638398016,370.467484232496],[306.00975066152006,370.467484232496]]},{"text":"马齿苋","box":[[331.9181451555361,360.763980232496],[362.5812177955361,360.763980232496],[362.5812177955361,370.467484232496],[331.9181451555361,370.467484232496]]},{"text":"这类最具乡土味的菜，","box":[[65.26580671801605,376.192551592496],[165.69709252502412,376.192551592496],[165.69709252502412,385.896055592496],[65.26580671801605,385.896055592496]]},{"text":"毛泽东更喜欢。一次，","box":[[161.33052542852812,376.192551592496],[263.4113972120321,376.192551592496],[263.4113972120321,385.896055592496],[161.33052542852812,385.896055592496]]},{"text":"工作人员采到一点马齿","box":[[259.04484952254415,376.192551592496],[362.5813148305761,376.192551592496],[362.5813148305761,385.896055592496],[259.04484952254415,385.896055592496]]},{"text":"苋，","box":[[65.26587464254413,391.524068505488],[84.96398776254415,391.524068505488],[84.96398776254415,401.227572505488],[65.26587464254413,401.227572505488]]},{"text":"在韶河里捉到几条小鲫鱼，","box":[[80.20927080254414,391.524068505488],[199.85351393656015,391.524068505488],[199.85351393656015,401.227572505488],[80.20927080254414,401.227572505488]]},{"text":"又在塘里捞了些小虾，","box":[[195.0987969765602,391.524068505488],[296.0152676870722,391.524068505488],[296.0152676870722,401.227572505488],[195.0987969765602,401.227572505488]]},{"text":"毛泽东吃了很高","box":[[291.45461110356825,391.524068505488],[362.5813439410883,391.524068505488],[362.5813439410883,401.227572505488],[291.45461110356825,401.227572505488]]},{"text":"兴。还有一次，","box":[[65.2659037530563,406.952639865488],[134.84004684006436,406.952639865488],[134.84004684006436,416.656143865488],[65.2659037530563,416.656143865488]]},{"text":"厨师石荫祥特地为毛泽东做了一盘青椒炒鲜菌，","box":[[130.08532017656032,406.952639865488],[346.66755856707215,406.952639865488],[346.66755856707215,416.656143865488],[130.08532017656032,416.656143865488]]},{"text":"端上","box":[[342.3980653245923,406.952639865488],[362.58137305160034,406.952639865488],[362.58137305160034,416.656143865488],[342.3980653245923,416.656143865488]]},{"text":"桌后才发现菌子里藏有白蛆似的小虫，","box":[[65.26596197408031,422.284195592496],[235.27138116459224,422.284195592496],[235.27138116459224,431.987699592496],[65.26596197408031,431.987699592496]]},{"text":"他想马上端走倒掉。不料毛泽","box":[[230.71074398809634,422.284195592496],[362.58143127262423,422.284195592496],[362.58143127262423,431.987699592496],[230.71074398809634,431.987699592496]]},{"text":"东已走进餐厅，","box":[[65.26599108459232,437.71276695249605],[134.93715950809639,437.71276695249605],[134.93715950809639,447.41627095249606],[65.26599108459232,447.41627095249606]]},{"text":"一切都来不及了。石荫祥歉意地说：","box":[[130.18244254809636,437.71276695249605],[289.70808712211226,437.71276695249605],[289.70808712211226,447.41627095249606],[130.18244254809636,447.41627095249606]]},{"text":"“","box":[[85.25524813860835,453.044283865488],[94.95875213860835,453.044283865488],[94.95875213860835,462.747787865488],[85.25524813860835,462.747787865488]]},{"text":"主席，","box":[[91.17439528211236,453.044283865488],[120.86711752211238,453.044283865488],[120.86711752211238,462.747787865488],[91.17439528211236,462.747787865488]]},{"text":"这个换掉吧！","box":[[116.11241026561638,453.044283865488],[175.78896956912038,453.044283865488],[175.78896956912038,462.747787865488],[116.11241026561638,462.747787865488]]},{"text":"”","box":[[171.03425260912041,453.044283865488],[180.73775660912042,453.044283865488],[180.73775660912042,462.747787865488],[171.03425260912041,462.747787865488]]},{"text":"毛泽东却一边夹菜，","box":[[85.2552675456164,468.47285522548805],[174.91567361612843,468.47285522548805],[174.91567361612843,478.17635922548806],[85.2552675456164,478.17635922548806]]},{"text":"一边说：","box":[[170.16095665612846,468.47285522548805],[209.84829771963243,468.47285522548805],[209.84829771963243,478.17635922548806],[170.16095665612846,478.17635922548806]]},{"text":"“","box":[[205.09358075963246,468.47285522548805],[214.79708475963247,468.47285522548805],[214.79708475963247,478.17635922548806],[205.09358075963246,478.17635922548806]]},{"text":"为什么，","box":[[211.01272790313647,468.47285522548805],[250.70004955963245,468.47285522548805],[250.70004955963245,478.17635922548806],[211.01272790313647,478.17635922548806]]},{"text":"不是蛮香的吗？","box":[[245.94535200664046,468.47285522548805],[315.6165301336484,468.47285522548805],[315.6165301336484,478.17635922548806],[245.94535200664046,478.17635922548806]]},{"text":"”","box":[[310.86181317364844,468.47285522548805],[320.56531717364845,468.47285522548805],[320.56531717364845,478.17635922548806],[310.86181317364844,478.17635922548806]]},{"text":"情急之下，","box":[[85.25531606313643,483.804410952496],[134.84023120664045,483.804410952496],[134.84023120664045,493.50791495249604],[85.25531606313643,493.50791495249604]]},{"text":"石师傅只好以实相告：","box":[[130.08550454313644,483.804410952496],[229.74053914065632,483.804410952496],[229.74053914065632,493.50791495249604],[130.08550454313644,493.50791495249604]]},{"text":"“","box":[[224.98580277364843,483.804410952496],[234.68930677364844,483.804410952496],[234.68930677364844,493.50791495249604],[224.98580277364843,493.50791495249604]]},{"text":"真对不起您，","box":[[230.90494991715244,483.804410952496],[291.3577895406564,483.804410952496],[291.3577895406564,493.50791495249604],[230.90494991715244,493.50791495249604]]},{"text":"菌子没洗干净，","box":[[286.7971523641604,483.804410952496],[357.43868089116836,483.804410952496],[357.43868089116836,493.50791495249604],[286.7971523641604,493.50791495249604]]},{"text":"有","box":[[352.8780243076644,483.804410952496],[362.5815283076644,483.804410952496],[362.5815283076644,493.50791495249604],[352.8780243076644,493.50791495249604]]},{"text":"小虫子。","box":[[65.2661172301444,499.23297260899204],[104.95344859014443,499.23297260899204],[104.95344859014443,508.93647660899205],[65.2661172301444,508.93647660899205]]},{"text":"”","box":[[100.19873163014442,499.23297260899204],[109.90223563014442,499.23297260899204],[109.90223563014442,508.93647660899205],[100.19873163014442,508.93647660899205]]},{"text":"毛泽东笑了笑：","box":[[85.25533547014442,514.564508928992],[154.92653300416043,514.564508928992],[154.92653300416043,524.268012928992],[85.25533547014442,524.268012928992]]},{"text":"“","box":[[150.17179663715243,514.564508928992],[159.87530063715244,514.564508928992],[159.87530063715244,524.268012928992],[150.17179663715243,524.268012928992]]},{"text":"没有关系，","box":[[156.09093407715244,514.564508928992],[205.7728939641604,514.564508928992],[205.7728939641604,524.268012928992],[156.09093407715244,524.268012928992]]},{"text":"不干不净，","box":[[201.01817700416044,514.564508928992],[250.7001174841604,514.564508928992],[250.7001174841604,524.268012928992],[201.01817700416044,524.268012928992]]},{"text":"吃了没病。再说，","box":[[246.23651534766444,514.564508928992],[326.77560825116836,514.564508928992],[326.77560825116836,524.268012928992],[246.23651534766444,524.268012928992]]},{"text":"菌子中有","box":[[322.3120061146724,514.564508928992],[362.5815962321924,514.564508928992],[362.5815962321924,524.268012928992],[322.3120061146724,524.268012928992]]},{"text":"小虫子，","box":[[65.26615604416043,529.993080288992],[104.95348740416046,529.993080288992],[104.95348740416046,539.696584288992],[65.26615604416043,539.696584288992]]},{"text":"说明这菌子没有毒，","box":[[100.19878014766445,529.993080288992],[189.85916681116845,529.993080288992],[189.85916681116845,539.696584288992],[100.19878014766445,539.696584288992]]},{"text":"吃了不会死人，","box":[[185.1044692581765,529.993080288992],[254.77562797817643,529.993080288992],[254.77562797817643,539.696584288992],[185.1044692581765,539.696584288992]]},{"text":"不要换。","box":[[250.0209110181765,529.993080288992],[289.7082520816804,529.993080288992],[289.7082520816804,539.696584288992],[250.0209110181765,539.696584288992]]},{"text":"”","box":[[284.9535351216805,529.993080288992],[294.6570391216805,529.993080288992],[294.6570391216805,539.696584288992],[284.9535351216805,539.696584288992]]},{"text":"石师傅这才松了一口气。","box":[[85.25541309817649,545.3246166089921],[194.90503740868846,545.3246166089921],[194.90503740868846,555.028120608992],[85.25541309817649,555.028120608992]]}]}