[65.266437445777,484.483685343008],[94.95915968577702,484.483685343008],[94.95915968577702,494.187189343008],[65.266437445777,494.187189343008]]},{"text":"不仅可以锻炼身体，","box":[[90.20445242928102,484.483685343008],[181.02927897979305,484.483685343008],[181.02927897979305,494.187189343008],[90.20445242928102,494.187189343008]]},{"text":"而且更能锻炼一个人的意志。大风大浪也","box":[[176.66267306928106,484.483685343008],[362.581906744321,484.483685343008],[362.581906744321,494.187189343008],[176.66267306928106,494.187189343008]]},{"text":"不可怕，","box":[[65.26649566680106,499.718206030016],[104.9538270268011,499.718206030016],[104.9538270268011,509.421710030016],[65.26649566680106,509.421710030016]]},{"text":"人类社会就是从大风大浪中发展起来的！要到大江大海中去","box":[[100.19911977030509,499.718206030016],[362.5819552618409,499.718206030016],[362.5819552618409,509.421710030016],[100.19911977030509,509.421710030016]]},{"text":"游，","box":[[65.26651507380916,514.952707310016],[84.96462819380918,514.952707310016],[84.96462819380918,524.6562113100159],[65.26651507380916,524.6562113100159]]},{"text":"到大风大浪中接受锻炼。","box":[[80.20991123380917,514.952707310016],[189.85953554432118,514.952707310016],[189.85953554432118,524.6562113100159],[80.20991123380917,524.6562113100159]]},{"text":"”","box":[[185.1048379913292,514.952707310016],[194.8083419913292,514.952707310016],[194.8083419913292,524.6562113100159],[185.1048379913292,524.6562113100159]]},{"text":"“","box":[[85.25576242432119,530.18721829352],[94.95926642432119,530.18721829352],[94.95926642432119,539.89072229352],[85.25576242432119,539.89072229352]]},{"text":"跟着毛主席在大风大浪中前进！","box":[[91.1749095678252,530.18721829352],[230.80837094184113,530.18721829352],[230.80837094184113,539.89072229352],[91.1749095678252,539.89072229352]]},{"text":"”","box":[[226.05365398184122,530.18721829352],[235.75715798184123,530.18721829352],[235.75715798184123,539.89072229352],[226.05365398184122,539.89072229352]]},{"text":"这句话很快成为全国家喻户晓","box":[[231.97278171833722,530.18721829352],[362.5820037793612,530.18721829352],[362.5820037793612,539.89072229352],[231.97278171833722,539.89072229352]]},{"text":"的政治口号。","box":[[65.26659270184126,545.51875461352],[124.94315200534531,545.51875461352],[124.94315200534531,555.2222586135199],[65.26659270184126,555.2222586135199]]}]}