0899206],[225.5682944152645,372.11707020899206]]},{"text":"毛主席。发电厂一","box":[[281.4604774552644,362.41356620899205],[362.5818485232964,362.41356620899205],[362.5818485232964,372.11707020899206],[281.4604774552644,372.11707020899206]]},{"text":"停工我们就开不成会了，","box":[[65.26640833526443,377.64810630300803],[174.91603264577645,377.64810630300803],[174.91603264577645,387.35161030300804],[65.26640833526443,387.35161030300804]]},{"text":"我们自己乱了，","box":[[170.16131568577646,377.64810630300803],[240.12359893278443,377.64810630300803],[240.12359893278443,387.35161030300804],[170.16131568577646,387.35161030300804]]},{"text":"对社会主义建设不利，","box":[[235.5629423492805,377.64810630300803],[336.7704793657764,377.64810630300803],[336.7704793657764,387.35161030300804],[235.5629423492805,387.35161030300804]]},{"text":"我们不","box":[[332.3068772292804,377.64810630300803],[362.5818291162885,377.64810630300803],[362.5818291162885,387.35161030300804],[332.3068772292804,387.35161030300804]]},{"text":"能乱，","box":[[65.26641803876845,392.88260758300805],[94.95914027876847,392.88260758300805],[94.95914027876847,402.58611158300806],[65.26641803876845,402.58611158300806]]},{"text":"我们要革敌人的命。因此，","box":[[90.20443302227247,392.88260758300805],[211.5952971727845,392.88260758300805],[211.5952971727845,402.58611158300806],[90.20443302227247,402.58611158300806]]},{"text":"我们要作解释，","box":[[207.22871066928056,392.88260758300805],[278.8405798927845,392.88260758300805],[278.8405798927845,402.58611158300806],[207.22871066928056,402.58611158300806]]},{"text":"组织工作组，","box":[[274.57102842928055,392.88260758300805],[336.1882885327845,392.88260758300805],[336.1882885327845,402.58611158300806],[274.57102842928055,402.58611158300806]]},{"text":"派人到","box":[[331.91879529030456,392.88260758300805],[362.5818679303045,392.88260758300805],[362.5818679303045,402.58611158300806],[331.91879529030456,402.58611158300806]]},{"text":"有些工厂，","box":[[65.26645685278453,408.214143903008],[114.94840703628857,408.214143903008],[114.94840