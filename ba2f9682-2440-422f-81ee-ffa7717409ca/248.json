[[237.69709220502406,392.882578472496],[288.931603028528,392.882578472496],[288.931603028528,402.586082472496],[237.69709220502406,402.586082472496]]},{"text":"又送林彪审定。","box":[[284.8561507555361,392.882578472496],[357.4383897860481,392.882578472496],[357.4383897860481,402.586082472496],[284.8561507555361,402.586082472496]]},{"text":"３","box":[[357.438292751008,392.882578472496],[367.14179675100803,392.882578472496],[367.14179675100803,402.586082472496],[357.438292751008,402.586082472496]]},{"text":"月","box":[[65.265767904,408.214114792496],[74.969271904,408.214114792496],[74.969271904,417.917618792496],[65.265767904,417.917618792496]]},{"text":"","box":[[74.969271904,408.214114792496],[75.18927290399999,408.214114792496],[75.18927290399999,408.214114792496],[74.969271904,408.214114792496]]},{"text":"２","box":[[77.104052487504,408.214114792496],[86.807556487504,408.214114792496],[86.807556487504,417.917618792496],[77.104052487504,417.917618792496]]},{"text":"２","box":[[81.955794784,408.214114792496],[91.659298784,408.214114792496],[91.659298784,417.917618792496],[81.955794784,417.917618792496]]},{"text":"日晚，","box":[[89.81563302400001,408.214114792496],[119.50837467100801,408.214114792496],[119.50837467100801,417.917618792496],[89.81563302400001,417.917618792496]]},{"text":"林彪改毕定稿，","box":[[114.753648007504,408.214114792496],[184.42482613451193,408.214114792496],[184.42482613451193,417.917618792496],[114.753648007504,417.917618792496]]},{"text":"全文如下：","box":[[179.670109174512,408.214114792496],[229.35206906151998,408.214114792496],[229.35206906151998,417.917618792496],[179.670109174512,417.917618792496]]},{"text":"贺龙、","box":[[65.265767904,423.448606368992],[94.95849014400002,423.448606368992],[94.95849014400002,433.152110368992],[65.265767904,433.152110368992]]},{"text":"荣臻、","box":[[90.20378288750402,423.448606368992],[119.89650512750404,423.448606368992],[119.89650512750404,433.152110368992],[90.20378288750402,433.152110368992]]},{"text":"陈毅、","box":[[115.14178816750403,423.448606368992],[144.83452011100806,423.448606368992],[144.83452011100806,433.152110368992],[115.14178816750403,433.152110368992]]},{"text":"伯承、","box":[[140.07979344750404,423.448606368992],[169.772515687504,423.448606368992],[169.772515687504,433.152110368992],[140.07979344750404,433.152110368992]]},{"text":"向前、","box":[[165.01779872750404,423.448606368992],[194.71054037451202,423.448606368992],[194.71054037451202,433.152110368992],[165.01779872750404,433.152110368992]]},{"text":"剑英常委诸同志：","box":[[189.95582341451205,423.448606368992],[269.62163977203204,423.448606368992],[269.62163977203204,433.152110368992],[189.95582341451205,433.152110368992]]},{"text":"送去江青同志召开的部队文艺工作座谈会纪要，","box":[[85.25501525451207,438.68310764899195],[304.457131800496,438.68310764899195],[304.457131800496,448.38661164899196],[85.25501525451207,448.38661164899196]]},{"text":"请阅。这个纪","box":[[300.47873397451207,438.68310764899195],[362.58114016750403,438.68310764899195],[362.58114016750403,448.38661164899196],[300.47873397451207,448.38661164899196]]},{"text":"要，","box":[[65.26576790400003,453.917628336],[84.96388102400005,453.917628336],[84.96388102400005,463.621132336],[65.26576790400003,463.621132336]]},{"text":"经过参加座谈会的同志们反复研究，","box":[[80.40323414400004,453.917628336],[244.10130780998398,453.917628336],[244.10130780998398,463.621132336],[80.40323414400004,463.621132336]]},{"text":"又经过主席三次亲自审阅修","box":[[239.7347601204961,453.917628336],[362.5811789815201,453.917628336],[362.5811789815201,463.621132336],[239.7347601204961,463.621132336]]},{"text":"改，","box":[[65.26569997947217,469.15212961599997],[84.96381309947219,469.15212961599997],[84.96381309947219,478.855633616],[65.26569997947217,478.855633616]]},{"text":"是一个很好的文件，","box":[[80.40316621947218,469.15212961599997],[172.1012499089602,469.15212961599997],[172.1012499089602,478.855633616],[80.40316621947218,478.855633616]]},{"text":"用毛泽东思想回答了社会主义时期文化革命","box":[[167.7347022194722,469.15212961599997],[362.5810722429762,469.15212961599997],[362.5810722429762,478.855633616],[167.7347022194722,478.855633616]]},{"text":"的许多重大问题，","box":[[65.26563205494432,484.483646528992],[144.93142900545638,484.483646528992],[144.93142900545638,494.187150528992],[65.26563205494432,494.187150528992]]},{"text":"不仅有极大的现实意义，","box":[[140.17669263844834,484.483646528992],[249.82633635596824,484.483646528992],[249.82633635596824,494.187150528992],[140.17669263844834,494.187150528992]]},{"text":"而且有深远的历史意义。","box":[[245.07159998896034,484.483646528992],[354.72120489246424,484.483646528992],[354.72120489246424,494.187150528992],[245.07159998896034,494.187150528992]]},{"text":"十六年来，","box":[[85.25489881246426,499.718167216],[134.93684899596832,499.718167216],[134.93684899596832,509.421671216],[85.25489881246426,509.421671216]]},{"text":"文艺战线上存在着尖锐的阶级斗争，","box":[[130.1821320359683,499.718167216],[294.36550704750425,499.718167216],[294.36550704750425,509.421671216],[130.1821320359683,509.421671216]]},{"text":"谁战胜谁的问题","box":[[289.99890113699234,499.718167216],[362.5811498710083,499.718167216],[362.5811498710083,509.421671216],[289.99890113699234,509.421671216]]},{"text":"还没有解决。文艺这个阵地，","box":[[65.26573879348831,514.952668496],[196.16601745699234,514.952668496],[196.16601745699234,524.656172496],[65.26573879348831,524.656172496]]},{"text":"无产阶级不去占领，","box":[[191.79945036049637,514.952668496],[284.17685695801634,514.952668496],[284.17685695801634,524.656172496],[191.79945036049637,524.656172496]]},{"text":"资产阶级就必然去","box":[[279.8102510475044,514.952668496],[362.5811983885283,514.952668496],[362.5811983885283,524.656172496],[279.8102510475044,524.656172496]]},{"text":"占领，","box":[[65.26578731100835,530.1871794795039],[94.95850955100838,530.1871794795039],[94.95850955100838,539.8906834795039],[65.26578731100835,539.8906834795039]]},{"text":"斗争是不可避免的。这是在意识形态领域里极为广泛、","box":[[90.20380229451237,530.1871794795039],[336.18767721203227,530.1871794795039],[336.18767721203227,539.8906834795039],[90.20380229451237,539.8906834795039]]},