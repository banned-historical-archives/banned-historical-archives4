417.91761879249606],[177.05031835057622,417.91761879249606]]},{"text":"政治局会议决定，","box":[[85.25515110356822,423.448616072496],[164.92094805408027,423.448616072496],[164.92094805408027,433.152120072496],[85.25515110356822,433.152120072496]]},{"text":"在公安部内进行“","box":[[160.16622139057625,423.448616072496],[239.8320183410882,423.448616072496],[239.8320183410882,433.152120072496],[160.16622139057625,433.152120072496]]},{"text":"动员整顿工作”","box":[[236.04766148459228,423.448616072496],[305.71883961160023,423.448616072496],[305.71883961160023,433.152120072496],[236.04766148459228,433.152120072496]]},{"text":"。","box":[[301.9344633480963,423.448616072496],[311.6379673480963,423.448616072496],[311.6379673480963,433.152120072496],[301.9344633480963,433.152120072496]]},{"text":"２","box":[[84.67277590400002,438.683107648992],[94.37627990400001,438.683107648992],[94.37627990400001,448.386611648992],[84.67277590400002,448.386611648992]]},{"text":"４","box":[[89.52453760750402,438.683107648992],[99.22804160750401,438.683107648992],[99.22804160750401,448.386611648992],[89.52453760750402,448.386611648992]]},{"text":"日下午","box":[[97.384366144,438.683107648992],[127.07708838400002,438.683107648992],[127.07708838400002,448.386611648992],[97.384366144,448.386611648992]]},{"text":"","box":[[127.077088384,438.683107648992],[127.347088384,438.683107648992],[127.347088384,438.683107648992],[127.077088384,438.683107648992]]},{"text":"１","box":[[129.697034464,438.683107648992],[139.40053846400002,438.683107648992],[139.40053846400002,448.386611648992],[129.697034464,448.386611648992]]},{"text":"８","box":[[134.54878646400002,438.683107648992],[144.25229046400003,438.683107648992],[144.25229046400003,448.386611648992],[134.54878646400002,448.386611648992]]},{"text":"时，","box":[[143.184914727504,438.683107648992],[162.883037551008,438.683107648992],[162.883037551008,448.386611648992],[143.184914727504,448.386611648992]]},{"text":"毛泽东召集周恩来等政治局成员开会。晚","box":[[158.904600911008,438.683107648992],[349.57842540049586,438.683107648992],[349.57842540049586,448.386611648992],[158.904600911008,448.386611648992]]},{"text":"","box":[[349.57842540049603,438.683107648992],[349.888428400496,438.683107648992],[349.888428400496,438.683107648992],[349.57842540049603,438.683107648992]]},{"text":"２","box":[[352.58654075100804,438.683107648992],[362.29004475100805,438.683107648992],[362.29004475100805,448.386611648992],[352.58654075100804,448.386611648992]]},{"text":"１","box":[[357.43825393699206,438.683107648992],[367.14175793699206,438.683107648992],[367.14175793699206,448.386611648992],[357.43825393699206,448.386611648992]]},{"text":"时，","box":[[65.265767904,453.917628336],[84.96388102400002,453.917628336],[84.96388102400002,463.621132336],[65.265767904,463.621132336]]},{"text":"周恩来主持政治局会议继续研究关于李震之死的问题。","box":[[80.209164064,453.917628336],[319.78879426604783,453.917628336],[319.78879426604783,463.621132336],[80.209164064,463.621132336]]},{"text":"于桑组织技术人员，","box":[[85.25505406852804,469.15212961599997],[175.98283587553607,469.15212961599997],[175.98283587553607,478.855633616],[85.25505406852804,478.855633616]]},{"text":"经过","box":[[171.6162493720321,469.15212961599997],[191.60545790852808,469.15212961599997],[191.60545790852808,478.855633616],[171.6162493720321,478.855633616]]},{"text":"","box":[[191.6054579085281,469.15212961599997],[191.8654529085281,469.15212961599997],[191.8654529085281,469.15212961599997],[191.6054579085281,469.15212961599997]]},{"text":"２","box":[[194.128320431008,469.152119912496],[203.83182443100802,469.152119912496],[203.83182443100802,478.855623912496],[194.128320431008,478.855623912496]]},{"text":"２","box":[[198.980053024,469.152119912496],[208.683557024,469.152119912496],[208.683557024,478.855623912496],[198.980053024,478.855623912496]]},{"text":"日、","box":[[207.228041127504,469.152119912496],[226.926163951008,469.152119912496],[226.926163951008,478.855623912496],[207.228041127504,478.855623912496]]},{"text":"２","box":[[221.977367207504,469.152119912496],[231.680871207504,469.152119912496],[231.680871207504,478.855623912496],[221.977367207504,478.855623912496]]},{"text":"３","box":[[226.829119207504,469.152119912496],[236.532623207504,469.152119912496],[236.532623207504,478.855623912496],[226.829119207504,478.855623912496]]},{"text":"日、","box":[[235.077087904,469.152119912496],[254.77521072750397,469.152119912496],[254.77521072750397,478.855623912496],[235.077087904,478.855623912496]]},{"text":"２","box":[[249.826413984,469.152119912496],[259.529917984,469.152119912496],[259.529917984,478.855623912496],[249.826413984,478.855623912496]]},{"text":"４","box":[[254.678165984,469.152119912496],[264.381669984,469.152119912496],[264.381669984,478.855623912496],[254.678165984,478.855623912496]]},{"text":"日，","box":[[262.926154087504,469.152119912496],[282.62429631801604,469.152119912496],[282.62429631801604,478.855623912496],[262.926154087504,478.855623912496]]},{"text":"３","box":[[277.675480167504,469.152119912496],[287.37898416750403,469.152119912496],[287.37898416750403,478.855623912496],[277.675480167504,478.855623912496]]},{"text":"天的调查，","box":[[285.923468271008,469.152119912496],[336.4787241110079,469.152119912496],[336.4787241110079,478.855623912496],[285.923468271008,478.855623912496]]},{"text":"做出结","box":[[332.112137607504,469.152119912496],[362.58117898152,469.152119912496],[362.58117898152,478.855623912496],[332.112137607504,478.855623912496]]},{"text":"论：","box":[[65.26576790399997,484.483636825488],[84.96388102399999,484.483636825488],[84.96388102399999,494.18714082548803],[65.26576790399997,494.18714082548803]]},{"text":"李震是自杀。","box":[[80.20916406399998,484.483636825488],[139.88573307100805,484.483636825488],[139.88573307100805,494.18714082548803],[80.20916406399998,494.18714082548803]]},{"text":"谢富治的爱人刘湘屏不同意于桑的结论。抗日战争时期，","box":[[85.25500555100801,499.718157512496],[336.76985834151986,499.718157512496],[336.76985834151986,509.42166151249603],[85.25500555100801,509.42166151249603]]},{"text":"刘湘屏","box":[[332.306256205024,499.718157512496],[362.58120809203206,499.718157512496],[362.58120809203206,509.42166151249603],[332.306256205024,509.42166151249603]]},{"text":"和李震同在太行地区工作，","box":[[65.26579701451203,514.952658792496],[184.91002074152004,514.952658792496],[184.91002074152004,524.6561627924959],[65.26579701451203,524.6561627924959]]},{"text":"很熟悉。李震死的前几天还到过她家。在","box":[[180.15530378152008,514.952658792496],[362.58121779553596,514.952658792496],[362.58121779553596,524.6561627924959],[180.15530378152008,524.6561627924959]]},{"text":"周恩来召开的会议上，","box":[[65.26580671801611,530.187169776],[167.05554427100816,530.187169776],[167.05554427100816,539.890673776],[65.26580671801611,539.890673776]]},{"text":"刘湘屏极力说于桑处理得不对，","box":[[162.68897717451216,530.187169776],[305.8156320640002,530.187169776],[305.8156320640002,539.890673776],[162.68897717451216,539.890673776]]},{"text":"说于桑是老公","box":[[301.44904556049636,530.187169776],[362.5811789815204,530.187169776],[362.5811789815204,539.890673776],[301.44904556049636,539.890673776]]},{"text":"安，","box":[[65.26573879348842,545.518706096],[84.96385191348844,545.518706096],[84.96385191348844,555.2222100959999],[65.26573879348842,555.2222100959999]]},{"text":"却明知故犯，","box":[[80.20913495348843,545.518706096],[139.8857039604965,545.518706096],[139.8857039604965,555.2222100959999],[80.20913495348843,555.2222100959999]]},{"text":"破坏了现场。刘复之也看过现场。刘湘屏认为李震","box":[[135.22803174400048,545.518706096],[362.5811983885285,545.518706096],[362.5811983885285,555.2222100959999],[135.22803174400048,555.2222100959999]]}]}