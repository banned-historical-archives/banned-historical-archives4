92],[84.67277590400002,433.152110368992]]},{"text":"月","box":[[92.53261414400001,423.448606368992],[102.236118144,423.448606368992],[102.236118144,433.152110368992],[92.53261414400001,433.152110368992]]},{"text":"","box":[[102.236118144,423.448606368992],[102.456119144,423.448606368992],[102.456119144,423.448606368992],[102.236118144,423.448606368992]]},{"text":"１","box":[[104.370898727504,423.448606368992],[114.074402727504,423.448606368992],[114.074402727504,433.152110368992],[104.370898727504,433.152110368992]]},{"text":"８","box":[[109.222650727504,423.448606368992],[118.926154727504,423.448606368992],[118.926154727504,433.152110368992],[109.222650727504,433.152110368992]]},{"text":"日，","box":[[117.082488967504,423.448606368992],[136.780602087504,423.448606368992],[136.780602087504,433.152110368992],[117.082488967504,433.152110368992]]},{"text":"江青写信给毛泽东：","box":[[132.025885127504,423.448606368992],[223.62698229451198,423.448606368992],[223.62698229451198,433.152110368992],[132.025885127504,433.152110368992]]},{"text":"“","box":[[219.45449498152004,423.448606368992],[229.15799898152005,423.448606368992],[229.15799898152005,433.152110368992],[219.45449498152004,433.152110368992]]},{"text":"我做蠢事，","box":[[225.76177258152003,423.448606368992],[276.899209551008,423.448606368992],[276.899209551008,433.152110368992],[225.76177258152003,433.152110368992]]},{"text":"对不起主席！","box":[[272.726751348528,423.448606368992],[334.34397263801594,423.448606368992],[334.34397263801594,433.152110368992],[272.726751348528,433.152110368992]]},{"text":"”","box":[[329.589275085024,423.448606368992],[339.292779085024,423.448606368992],[339.292779085024,433.152110368992],[329.589275085024,433.152110368992]]},{"text":"“","box":[[336.090642172032,423.448606368992],[345.794146172032,423.448606368992],[345.794146172032,433.152110368992],[336.090642172032,433.152110368992]]},{"text":"今后","box":[[342.397900365024,423.448606368992],[362.581208092032,423.448606368992],[362.581208092032,433.152110368992],[342.397900365024,433.152110368992]]},{"text":"当努力学习，","box":[[65.26579701451197,438.68310764899195],[124.94235631801602,438.68310764899195],[124.94235631801602,448.38661164899196],[65.26579701451197,448.38661164899196]]},{"text":"克服形而上学、","box":[[120.187639358016,438.68310764899195],[189.85880778151994,438.68310764899195],[189.85880778151994,448.38661164899196],[120.187639358016,448.38661164899196]]},{"text":"片面性。","box":[[185.104110228528,438.68310764899195],[224.79143188502397,438.68310764899195],[224.79143188502397,448.38661164899196],[185.104110228528,448.38661164899196]]},{"text":"”","box":[[220.03673433203198,438.68310764899195],[229.740238332032,438.68310764899195],[229.740238332032,448.38661164899196],[220.03673433203198,448.38661164899196]]},{"text":"２","box":[[84.67277590400002,453.917628336],[94.37627990400001,453.917628336],[94.37627990400001,463.621132336],[84.67277590400002,463.621132336]]},{"text":"月","box":[[92.53261414400001,453.917628336],[102.236118144,453.917628336],[102.236118144,463.621132336],[92.53261414400001,463.621132336]]},{"text":"","box":[[102.236118144,453.917628336],[102.456119144,453.917628336],[102.456119144,453.917628336],[102.236118144,453.917628336]]},{"text":"２","box":[[104.370898727504,453.917628336],[114.074402727504,453.917628336],[114.074402727504,463.621132336],[104.370898727504,463.621132336]]},{"text":"０","box":[[109.222650727504,453.917628336],[118.926154727504,453.917628336],[118.926154727504,463.621132336],[109.222650727504,463.621132336]]},{"text":"日，","box":[[117.082488967504,453.917628336],[136.780602087504,453.917628336],[136.780602087504,463.621132336],[117.082488967504,463.621132336]]},{"text":"中共中央下发关于“","box":[[132.025885127504,453.917628336],[221.7833165345119,453.917628336],[221.7833165345119,463.621132336],[132.025885127504,463.621132336]]},{"text":"走后门”","box":[[218.09599471801602,453.917628336],[258.26851098151997,453.917628336],[258.26851098151997,463.621132336],[218.09599471801602,463.621132336]]},{"text":"问题的通知，","box":[[254.67820479801603,453.917628336],[315.42214954152,453.917628336],[315.42214954152,463.621132336],[254.67820479801603,463.621132336]]},{"text":"指出：","box":[[310.958508591008,453.917628336],[340.94233595100803,453.917628336],[340.94233595100803,463.621132336],[310.958508591008,463.621132336]]},{"text":"“","box":[[336.478733814512,453.917628336],[346.182237814512,453.917628336],[346.182237814512,463.621132336],[336.478733814512,463.621132336]]},{"text":"在批","box":[[342.591951038016,453.917628336],[362.58117898152005,453.917628336],[362.58117898152005,463.621132336],[342.591951038016,463.621132336]]},{"text":"林批孔运动中，","box":[[65.26576790400003,469.15212961599997],[134.9369363275041,469.15212961599997],[134.9369363275041,478.855633616],[65.26576790400003,478.855633616]]},{"text":"不少单位提出了领导干部‘","box":[[130.18221936750408,469.15212961599997],[252.25229968750398,469.15212961599997],[252.25229968750398,478.855633616],[130.18221936750408,478.855633616]]},{"text":"走后门’","box":[[248.6620129110081,469.15212961599997],[289.12561488750407,469.15212961599997],[289.12561488750407,478.855633616],[248.6620129110081,478.855633616]]},{"text":"送子女参军、","box":[[285.63234374400014,469.15212961599997],[346.86146368750406,469.15212961599997],[346.86146368750406,478.855633616],[285.63234374400014,478.855633616]]},{"text":"入学","box":[[342.4949159980161,469.15212961599997],[362.5811692780161,469.15212961599997],[362.5811692780161,478.855633616],[342.4949159980161,478.855633616]]},{"text":"等问题。中央根据毛主席的指示，","box":[[65.26575820049607,484.483646528992],[217.0285898710081,484.483646528992],[217.0285898710081,494.187150528992],[65.26575820049607,494.187150528992]]},{"text":"认真讨论了这个问题。中央认为，","box":[[212.75905781451215,484.483646528992],[367.62699135801614,484.483646528992],[367.62699135801614,494.187150528992],[212.75905781451215,494.187150528992]]},{"text":"对来自群众的批评，","box":[[65.26579701451215,499.718167216],[155.3143432450242,499.718167216],[155.3143432450242,509.421671216],[65.26579701451215,509.421671216]]},{"text":"领导干部首先应当表示欢迎。但是，","box":[[150.94775674152018,499.718167216],[315.32517272254427,499.718167216],[315.32517272254427,509.421671216],[150.94775674152018,509.421671216]]},{"text":"这个问题牵","box":[[310.95858621904034,499.718167216],[362.5812663130563,499.718167216],[362.5812663130563,509.421671216],[310.95858621904034,509.421671216]]},{"text":"涉到几百万人，","box":[[65.26585523553632,514.952668496],[134.9370236590404,514.952668496],[134.9370236590404,524.656172496],[65.26585523553632,524.656172496]]},{"text":"开后门来的也有好人，","box":[[130.18230669904037,514.952668496],[229.93437633656026,514.952668496],[229.93437633656026,524.656172496],[130.18230669904037,524.656172496]]},{"text":"从前门来的也有坏人，","box":[[225.37370034604837,514.952668496],[326.5812567695522,514.952668496],[326.5812567695522,524.656172496],[225.37370034604837,524.656172496]]},{"text":"需要具体","box":[[322.02063900006425,514.952668496],[362.58132453408024,514.952668496],[362.58132453408024,524.656172496],[322.02063900006425,524.656172496]]},{"text":"分析，","box":[[65.26588434604827,530.1871794795039],[94.95860658604829,530.1871794795039],[94.95860658604829,539.8906834795039],[65.26588434604827,539.8906834795039]]},{"text":"慎重对待。当前，","box":[[90.20389932955229,530.1871794795039],[170.06373724955228,530.1871794795039],[170.06373724955228,539.8906834795039],[90.20389932955229,539.8906834795039]]},{"text":"批林批孔刚刚展开，","box":[[165.69715074604832,530.1871794795039],[258.0745379365603,530.1871794795039],[258.0745379365603,539.8906834795039],[165.69715074604832,539.8906834795039]]},{"text":"又夹着走后门，","box":[[253.70795143305637,530.1871794795039],[325.70798022356837,530.1871794795039],[325.70798022356837,539.8906834795039],[253.70795143305637,539.8906834795039]]},{"text":"有可能冲","box":[[321.43844816707235,530.1871794795039],[362.5813245340803,530.1871794795039],[362.5813245340803,539.8906834795039],[321.43844816707235,539.8906834795039]]},{"text":"淡批林批孔。因此，","box":[[65.26591345656033,545.518715799504],[154.8292747835684,545.518715799504],[154.8292747835684,555.2222197995039],[65.26591345656033,555.2222197995039]]},{"text":"中央认为，","box":[[150.36565324006435,545.518715799504],[201.2120433105763,545.518715799504],[201.2120433105763,555.2222197995039],[150.36565324006435,555.2222197995039]]},{"text":"这个问题应进行调查研究，","box":[[196.84545680707234,545.518715799504],[320.7592514045923,545.518715799504],[320.7592514045923,555.2222197995039],[196.84545680707234,555.2222197995039]]},{"text":"确定政策，","box":[[316.48969023758434,545.518715799504],[367.62716602108827,545.518715799504],[367.62716602108827,555.2222197995039],[316.48969023758434,555.2222197995039]]}]}