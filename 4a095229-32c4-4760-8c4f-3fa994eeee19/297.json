673,499.718186623008],[264.67303709860875,499.718186623008],[264.67303709860875,509.42169062300803],[162.68922946561673,509.42169062300803]]},{"text":"明明黄浦江边矗立着能","box":[[260.3064505951049,499.718186623008],[362.5814312726249,499.718186623008],[362.5814312726249,509.42169062300803],[260.3064505951049,509.42169062300803]]},{"text":"制造万吨轮的造船厂，","box":[[65.26599108459294,514.952687903008],[167.055728637585,514.952687903008],[167.055728637585,524.656191903008],[65.26599108459294,524.656191903008]]},{"text":"江上停泊着我国的远洋轮，","box":[[162.689161541089,514.952687903008],[285.244387950577,514.952687903008],[285.244387950577,524.656191903008],[162.689161541089,524.656191903008]]},{"text":"而在安东尼奥尼的","box":[[280.8778208540811,514.952687903008],[362.5813730516011,514.952687903008],[362.5813730516011,524.656191903008],[280.8778208540811,524.656191903008]]},{"text":"镜头下，","box":[[65.26593286356916,530.187198886512],[104.95326422356919,530.187198886512],[104.95326422356919,539.8907028865119],[65.26593286356916,539.8907028865119]]},{"text":"黄浦江里的大货船都是外国的，","box":[[100.19855696707319,530.187198886512],[240.1231234610891,530.187198886512],[240.1231234610891,539.8907028865119],[100.19855696707319,539.8907028865119]]},{"text":"小木船都是中国的；","box":[[235.56246687758522,530.187198886512],[326.5813343975851,530.187198886512],[326.5813343975851,539.8907028865119],[235.56246687758522,539.8907028865119]]},{"text":"安东尼奥","box":[[322.02071662809715,530.187198886512],[362.58140216211314,530.187198886512],[362.58140216211314,539.8907028865119],[322.02071662809715,539.8907028865119]]},{"text":"尼把林县作为“","box":[[65.26596197408116,545.518735206512],[134.93713039758524,545.518735206512],[134.93713039758524,555.222239206512],[65.26596197408116,555.222239206512]]},{"text":"中国的第一个社会主义山区”","box":[[131.1527638375852,545.518735206512],[260.79162579510506,545.518735206512],[260.79162579510506,555.222239206512],[131.1527638375852,555.222239206512]]},{"text":"介绍给观众，","box":[[257.0072689386092,545.518735206512],[316.6838088351051,545.518735206512],[316.6838088351051,555.222239206512],[257.0072689386092,555.222239206512]]},{"text":"其目的恰恰","box":[[312.12321047262515,545.518735206512],[362.58147008664116,545.518735206512],[362.58147008664116,555.222239206512],[312.12321047262515,555.222239206512]]}]}