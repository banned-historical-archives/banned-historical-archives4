6707222,447.41627095249606]]},{"text":"那不行。对破坏铁路运输的人要查办。","box":[[90.20395755057623,437.71276695249605],[259.72421124459214,437.71276695249605],[259.72421124459214,447.41627095249606],[90.20395755057623,447.41627095249606]]},{"text":"三、","box":[[85.25520932459224,453.044283865488],[104.95332244459226,453.044283865488],[104.95332244459226,462.747787865488],[85.25520932459224,462.747787865488]]},{"text":"要做好思想工作，","box":[[100.19861518809626,453.044283865488],[181.51398841160022,453.044283865488],[181.51398841160022,462.747787865488],[100.19861518809626,462.747787865488]]},{"text":"要用无产阶级党性去克服资产阶级派性。","box":[[177.43851673160026,453.044283865488],[367.6271466140801,453.044283865488],[367.6271466140801,462.747787865488],[177.43851673160026,462.747787865488]]},{"text":"搞资产阶级派性是可耻的。领导干部千万注意，","box":[[65.26595227057618,468.47285522548805],[275.63795780459213,468.47285522548805],[275.63795780459213,478.17635922548806],[65.26595227057618,478.17635922548806]]},{"text":"对本单位不同意见的","box":[[271.0773400351042,468.47285522548805],[362.5814506796321,468.47285522548805],[362.5814506796321,478.17635922548806],[271.0773400351042,478.17635922548806]]},{"text":"群众，","box":[[65.26597167758422,483.804410952496],[94.95869391758424,483.804410952496],[94.95869391758424,493.50791495249604],[65.26597167758422,493.50791495249604]]},{"text":"不能只站在一边，","box":[[90.20398666108824,483.804410952496],[172.10155071758425,483.804410952496],[172.10155071758425,493.50791495249604],[90.20398666108824,493.50791495249604]]},{"text":"站一边是要犯错误的。要站在两派之上，","box":[[168.02607903758425,483.804410952496],[356.85625717408016,483.804410952496],[356.85625717408016,493.50791495249604],[168.02607903758425,493.50791495249604]]},{"text":"要","box":[[352.87785934809625,483.804410952496],[362.58136334809626,483.804410952496],[362.58136334809626,493.50791495249604],[352.87785934809625,493.50791495249604]]},{"text":"超脱。","box":[[65.26595227057624,499.23297260899204],[94.95867451057626,499.23297260899204],[94.95867451057626,508.93647660899205],[65.26595227057624,508.93647660899205]]},{"text":"四、","box":[[85.25517051057625,514.564508928992],[104.95328363057627,514.564508928992],[104.95328363057627,524.268012928992],[85.25517051057625,524.268012928992]]},{"text":"凡是制造分裂，","box":[[100.19857637408026,514.564508928992],[169.86974479758425,514.564508928992],[169.86974479758425,524.268012928992],[100.19857637408026,524.268012928992]]},{"text":"破坏团结，","box":[[165.2120628775843,514.564508928992],[216.15548798809624,514.564508928992],[216.15548798809624,524.268012928992],[165.2120628775843,524.268012928992]]},{"text":"杀人放火，","box":[[211.98296186108828,514.564508928992],[263.12043764459224,514.564508928992],[263.12043764459224,524.268012928992],[211.98296186108828,524.268012928992]]},{"text":"强奸抢劫，","box":[[258.9479115175843,514.564508928992],[310.08542611510427,514.564508928992],[310.08542611510427,524.268012928992],[258.9479115175843,524.268012928992]]},{"text":"投机倒把，","box":[[305.9129193951043,514.564508928992],[357.0503757716003,514.564508928992],[357.0503757716003,524.268012928992],[305.9129193951043,524.268012928992]]},{"text":"贪","box":[[352.8779078656163,514.564508928992],[362.5814118656163,514.564508928992],[362.5814118656163,524.268012928992],[352.8779078656163,524.268012928992]]},{"text":"污盗窃，","box":[[65.26600078809628,529.993080288992],[104.95333214809631,529.993080288992],[104.95333214809631,539.696584288992],[65.26600078809628,539.696584288992]]},{"text":"破坏铁路运输等，","box":[[100.1986248916003,529.993080288992],[181.12585795510432,529.993080288992],[181.12585795510432,539.696584288992],[100.1986248916003,539.696584288992]]},{"text":"过了杠的，","box":[[176.75929085860838,529.993080288992],[227.60566152211237,529.993080288992],[227.60566152211237,539.696584288992],[176.75929085860838,539.696584288992]]},{"text":"都要查办，","box":[[223.23909442561643,529.993080288992],[274.0854650891205,529.993080288992],[274.0854650891205,539.696584288992],[223.23909442561643,539.696584288992]]},{"text":"一律专政。不管哪一","box":[[269.8159136256165,529.993080288992],[362.58145067963244,529.993080288992],[362.58145067963244,539.696584288992],[269.8159136256165,539.696584288992]]},{"text":"派，","box":[[65.26603960211247,545.3246166089921],[84.96415272211249,545.3246166089921],[84.96415272211249,555.028120608992],[65.26603960211247,555.028120608992]]},{"text":"都不能包庇。","box":[[80.20943576211248,545.3246166089921],[139.88600476912055,545.3246166089921],[139.88600476912055,555.028120608992],[80.20943576211248,555.028120608992]]}]}