[94.37628960750402,438.683107648992],[104.07979360750402,438.683107648992],[104.07979360750402,448.386611648992],[94.37628960750402,448.386611648992]]},{"text":"２","box":[[99.22804160750403,438.683107648992],[108.93154560750402,438.683107648992],[108.93154560750402,448.386611648992],[99.22804160750403,448.386611648992]]},{"text":"年的秋天，","box":[[107.08787984750401,438.683107648992],[156.76982032750402,438.683107648992],[156.76982032750402,448.386611648992],[107.08787984750401,448.386611648992]]},{"text":"张春桥从北京回到上海，","box":[[152.01511307100805,438.683107648992],[263.702482925024,438.683107648992],[263.702482925024,448.386611648992],[152.01511307100805,448.386611648992]]},{"text":"依然住在兴国路招待所","box":[[259.43293146152007,438.683107648992],[362.58127601655997,438.683107648992],[362.58127601655997,448.386611648992],[259.43293146152007,448.386611648992]]},{"text":"五号楼。也就是在这一次，","box":[[65.265835828528,453.917628336],[185.78338461904002,453.917628336],[185.78338461904002,463.621132336],[65.265835828528,463.621132336]]},{"text":"他和文静作了最后的告别，","box":[[181.51383315553602,453.917628336],[305.33057330604805,453.917628336],[305.33057330604805,463.621132336],[181.51383315553602,463.621132336]]},{"text":"然后分手。张","box":[[301.1580665860481,453.917628336],[362.58128572006405,453.917628336],[362.58128572006405,463.621132336],[301.1580665860481,463.621132336]]},{"text":"春桥此后长期住在北京钓鱼台九号楼，","box":[[65.26587464254408,469.152129616],[235.07722375305602,469.152129616],[235.07722375305602,478.85563361600003],[65.26587464254408,478.85563361600003]]},{"text":"文静也从此搬出了上海兴国路","box":[[230.5166059835681,469.152129616],[362.581411865616,469.152129616],[362.581411865616,478.85563361600003],[230.5166059835681,478.85563361600003]]},{"text":"招待所，","box":[[65.26589404955212,484.483646528992],[104.95322540955215,484.483646528992],[104.95322540955215,494.187150528992],[65.26589404955212,494.187150528992]]},{"text":"搬回了康平路大院内的家里。","box":[[100.19851815305614,484.483646528992],[229.83736070356807,484.483646528992],[229.83736070356807,494.187150528992],[100.19851815305614,494.187150528992]]},{"text":"邓颖超来到康平路上海市委大院的消息，","box":[[85.25514140006416,499.718167216],[268.6514058140802,499.718167216],[268.6514058140802,509.421671216],[85.25514140006416,509.421671216]]},{"text":"不胫而走，","box":[[264.2848775316002,499.718167216],[315.22830264211217,499.718167216],[315.22830264211217,509.421671216],[264.2848775316002,509.421671216]]},{"text":"大院里的家","box":[[310.9587220680962,499.718167216],[362.58140216211217,499.718167216],[362.58140216211217,509.421671216],[310.9587220680962,509.421671216]]},{"text":"属们和工作人员们都奔走相告，","box":[[65.2659910845922,514.952668496],[204.89945245860818,514.952668496],[204.89945245860818,524.656172496],[65.2659910845922,524.656172496]]},{"text":"可是为什么偏偏不去张春桥的家？大","box":[[200.14473549860824,514.952668496],[362.5814021621121,514.952668496],[362.5814021621121,524.656172496],[200.14473549860824,524.656172496]]},{"text":"家都在背后窃窃私议：","box":[[65.26599108459214,530.187179479504],[164.92100627510416,530.187179479504],[164.92100627510416,539.890683479504],[65.26599108459214,539.890683479504]]},{"text":"“","box":[[160.16627961160015,530.187179479504],[169.86978361160016,530.187179479504],[169.86978361160016,539.890683479504],[160.16627961160015,539.890683479504]]},{"text":"邓大姐肯定不愿意见到文静。","box":[[166.08542675510415,530.187179479504],[295.724298416128,530.187179479504],[295.724298416128,539.890683479504],[166.08542675510415,539.890683479504]]},{"text":"”","box":[[290.96958145612814,530.187179479504],[300.67308545612815,530.187179479504],[300.67308545612815,539.890683479504],[290.96958145612814,539.890683479504]]},{"text":"大人们不便公开议论，","box":[[85.25525784211212,545.518715799504],[184.91026332912008,545.518715799504],[184.91026332912008,555.2222197995039],[85.25525784211212,555.2222197995039]]},{"text":"只有不懂事的小孩子叽叽喳喳，","box":[[180.15554636912015,545.518715799504],[321.5356093526241,545.518715799504],[321.5356093526241,555.2222197995039],[180.15554636912015,555.2222197995039]]},{"text":"问个不休。","box":[[317.0720072161281,545.518715799504],[367.62724364912015,545.518715799504],[367.62724364912015,555.2222197995039],[317.0720072161281,555.2222197995039]]}]}