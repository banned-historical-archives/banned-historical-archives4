5],[342.73519351014437,377.64808689600005],[342.73519351014437,377.64808689600005],[342.4952265101444,377.64808689600005]]},{"text":"７","box":[[344.82374725451206,377.648077192496],[354.52725125451207,377.648077192496],[354.52725125451207,387.351581192496],[344.82374725451206,387.351581192496]]},{"text":"月","box":[[352.87767498152004,377.648077192496],[362.58117898152005,377.648077192496],[362.58117898152005,387.351581192496],[352.87767498152004,387.351581192496]]},{"text":"１","box":[[64.683557664,392.882578472496],[74.387061664,392.882578472496],[74.387061664,402.586082472496],[64.683557664,402.586082472496]]},{"text":"８","box":[[69.53530966400001,392.882578472496],[79.238813664,392.882578472496],[79.238813664,402.586082472496],[69.53530966400001,402.586082472496]]},{"text":"日，","box":[[77.395147904,392.882578472496],[97.09326102400001,392.882578472496],[97.09326102400001,402.586082472496],[77.395147904,402.586082472496]]},{"text":"冒着盛暑，","box":[[92.338544064,392.882578472496],[142.02049424750408,392.882578472496],[142.02049424750408,402.586082472496],[92.338544064,402.586082472496]]},{"text":"赶写了两封长信，","box":[[137.26576758400003,392.882578472496],[216.93156453451195,392.882578472496],[216.93156453451195,402.586082472496],[137.26576758400003,402.586082472496]]},{"text":"一封致邓小平，","box":[[212.17684757451204,392.882578472496],[281.84802570152,392.882578472496],[281.84802570152,402.586082472496],[212.17684757451204,402.586082472496]]},{"text":"一封致毛泽东。","box":[[277.09332814852803,392.882578472496],[346.764496572032,392.882578472496],[346.764496572032,402.586082472496],[277.09332814852803,402.586082472496]]},{"text":"这是因为他的一位作家朋友—","box":[[85.25502495801601,408.214114792496],[214.79684217203197,408.214114792496],[214.79684217203197,417.917618792496],[85.25502495801601,417.917618792496]]},{"text":"—","box":[[210.04212521203203,408.214114792496],[219.74562921203204,408.214114792496],[219.74562921203204,417.917618792496],[210.04212521203203,417.917618792496]]},{"text":"—白桦，","box":[[215.08793758852804,408.214114792496],[254.775278652032,408.214114792496],[254.775278652032,417.917618792496],[215.08793758852804,417.917618792496]]},{"text":"拍着胸脯对他说：","box":[[250.02056169203203,408.214114792496],[330.850740308528,408.214114792496],[330.850740308528,417.917618792496],[250.02056169203203,417.917618792496]]},{"text":"“","box":[[326.387138172032,408.214114792496],[336.090642172032,408.214114792496],[336.090642172032,417.917618792496],[326.387138172032,417.917618792496]]},{"text":"你只管","box":[[332.500355395536,408.214114792496],[362.58123720254406,408.214114792496],[362.58123720254406,417.917618792496],[332.500355395536,417.917618792496]]},{"text":"写，","box":[[65.26582612502403,423.44861607249595],[84.96393924502405,423.44861607249595],[84.96393924502405,433.15212007249596],[65.26582612502403,433.15212007249596]]},{"text":"有人给你送。","box":[[80.20922228502404,423.44861607249595],[139.8857912920321,423.44861607249595],[139.8857912920321,433.15212007249596],[80.20922228502404,433.15212007249596]]},{"text":"”","box":[[135.13107433203209,423.44861607249595],[144.8345783320321,423.44861607249595],[144.8345783320321,433.15212007249596],[135.13107433203209,433.15212007249596]]},{"text":"白桦说的这个送信人，","box":[[85.25506377203209,438.68311735249597],[184.91006925904009,438.68311735249597],[184.91006925904009,448.386621352496],[85.25506377203209,448.386621352496]]},{"text":"就是贺捷生。","box":[[180.15535229904012,438.68311735249597],[239.83193100955208,438.68311735249597],[239.83193100955208,448.386621352496],[180.15535229904012,448.386621352496]]},{"text":"胡乔木当天就看到了张天民的信，","box":[[85.25510258604811,453.91763803950397],[237.5031385670721,453.91763803950397],[237.5031385670721,463.621142039504],[85.25510258604811,463.621142039504]]},{"text":"他还是提出了修改意见，","box":[[233.13655206356816,453.91763803950397],[346.66753916006417,453.91763803950397],[346.66753916006417,463.621142039504],[233.13655206356816,463.621142039504]]},{"text":"希望","box":[[342.3980459175842,453.91763803950397],[362.58135364459224,453.91763803950397],[362.58135364459224,463.621142039504],[342.3980459175842,463.621142039504]]},{"text":"信的笔调不要太冷，","box":[[65.26594256707222,469.15213931950393],[155.12040901408025,469.15213931950393],[155.12040901408025,478.85564331950394],[65.26594256707222,478.85564331950394]]},{"text":"一点就透的地方不宜说得过满；","box":[[150.75382251057624,469.15213931950393],[295.0449852116002,469.15213931950393],[295.0449852116002,478.85564331950394],[150.75382251057624,478.85564331950394]]},{"text":"要冷静，","box":[[290.6784181151043,469.15213931950393],[331.23909394561633,469.15213931950393],[331.23909394561633,478.85564331950394],[290.6784181151043,478.85564331950394]]},{"text":"客观，","box":[[326.96949396459235,469.15213931950393],[357.14740110809635,469.15213931950393],[357.14740110809635,478.85564331950394],[326.96949396459235,478.85564331950394]]},{"text":"字","box":[[352.87790786561635,469.15213931950393],[362.58141186561636,469.15213931950393],[362.58141186561636,478.85564331950394],[352.87790786561635,478.85564331950394]]},{"text":"字有据，","box":[[65.26600078809633,484.48365623249595],[104.95333214809637,484.48365623249595],[104.95333214809637,494.18716023249596],[65.26600078809633,494.18716023249596]]},{"text":"不让人感到偏激，","box":[[100.19862489160036,484.48365623249595],[179.86441213860834,484.48365623249595],[179.86441213860834,494.18716023249596],[100.19862489160036,494.18716023249596]]},{"text":"一定要建议让影片重新公演。","box":[[175.1096951786084,484.48365623249595],[304.74856683963225,484.48365623249595],[304.74856683963225,494.18716023249596],[175.1096951786084,494.18716023249596]]},{"text":"当天，","box":[[85.25526754561633,499.71817691950395],[114.94799948912035,499.71817691950395],[114.94799948912035,509.42168091950396],[85.25526754561633,509.42168091950396]]},{"text":"张天民对信稿又作了一番修改。第三天，","box":[[110.19328252912034,499.71817691950395],[290.67848603963216,499.71817691950395],[290.67848603963216,509.42168091950396],[110.19328252912034,509.42168091950396]]},{"text":"按照预先的约定，","box":[[286.2148839031363,499.71817691950395],[367.6272727596323,499.71817691950395],[367.6272727596323,509.42168091950396],[286.2148839031363,509.42168091950396]]},{"text":"将写好的两封内容相同的信，","box":[[65.26607841612832,514.9526781995039],[194.90492096664033,514.9526781995039],[194.90492096664033,524.6561821995039],[65.26607841612832,524.6561821995039]]},{"text":"由他的妻子赵亮送给贺捷生。","box":[[190.15020400664037,514.9526781995039],[319.7890853711683,514.9526781995039],[319.7890853711683,524.6561821995039],[190.15020400664037,524.6561821995039]]},{"text":"就在张天民写信的","box":[[85.25534517364835,530.187189183008],[164.92115182766435,530.187189183008],[164.92115182766435,539.8906931830079],[85.25534517364835,539.8906931830079]]},{"text":"","box":[[164.92115182766437,530.187189183008],[165.17111282766433,530.187189183008],[165.17111282766433,530.187189183008],[164.92115182766437,530.187189183008]]},{"text":"７","box":[[167.34664939100801,530.187160072496],[177.05015339100802,530.187160072496],[177.05015339100802,539.890664072496],[167.34664939100801,539.890664072496]]},{"text":"月","box":[[175.691653127504,530.187160072496],[185.395157127504,530.187160072496],[185.395157127504,539.890664072496],[175.691653127504,539.890664072496]]},{"text":"","box":[[185.395157127504,530.187160072496],[185.66515712750402,530.187160072496],[185.66515712750402,530.187160072496],[185.395157127504,530.187160072496]]},{"text":"１","box":[[188.015103207504,530.187160072496],[197.71860720750402,530.187160072496],[197.71860720750402,539.890664072496],[188.015103207504,539.890664072496]]},{"text":"８","box":[[192.86685520750402,530.187160072496],[202.57035920750403,530.187160072496],[202.57035920750403,539.890664072496],[192.86685520750402,539.890664072496]]},{"text":"日下午，","box":[[201.211878351008,530.187160072496],[241.772534774512,530.187160072496],[241.772534774512,539.890664072496],[201.211878351008,539.890664072496]]},{"text":"胡乔木根据贺捷生提供的材","box":[[237.60002805451202,530.187160072496],[362.581198388528,530.187160072496],[362.581198388528,539.890664072496],[237.60002805451202,539.890664072496]]},{"text":"料向邓小平汇报了他所了解到的关于电影《","box":[[65.26578731100801,545.5186963924959],[257.49221125451203,545.5186963924959],[257.49221125451203,555.2222003924959],[65.26578731100801,555.2222003924959]]},{"text":"创业》","box":[[253.90190507100814,545.5186963924959],[284.07978310400017,545.5186963924959],[284.07978310400017,555.2222003924959],[253.90190507100814,555.2222003924959]]},{"text":"受压制的情况和人","box":[[280.5865119604962,545.5186963924959],[362.58120809203217,545.5186963924959],[362.58120809203217,555.2222003924959],[280.5865119604962,555.2222003924959]]}]}