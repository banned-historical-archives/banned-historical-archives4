4976],[219.84277099057607,270.812449634976],[219.84277099057607,280.51595363497603],[110.1931369765601,280.51595363497603]]},{"text":"”","box":[[215.08803462356812,270.812449634976],[224.79153862356813,270.812449634976],[224.79153862356813,280.51595363497603],[215.08803462356812,280.51595363497603]]},{"text":"“","box":[[85.2551511035681,286.144044176],[94.95865510356809,286.144044176],[94.95865510356809,295.84754817600003],[85.2551511035681,295.84754817600003]]},{"text":"要教育专业和业余文艺工作者，","box":[[91.1742982470721,286.144044176],[230.80775962108802,286.144044176],[230.80775962108802,295.84754817600003],[91.1742982470721,295.84754817600003]]},{"text":"要重视自己的文化艺术，","box":[[226.05304266108814,286.144044176],[337.06114782809595,286.144044176],[337.06114782809595,295.84754817600003],[226.05304266108814,295.84754817600003]]},{"text":"不要轻","box":[[332.50049124459207,286.144044176],[362.5813730516001,286.144044176],[362.5813730516001,295.84754817600003],[332.50049124459207,295.84754817600003]]},{"text":"视，","box":[[65.26596197408008,301.378506641984],[84.9640750940801,301.378506641984],[84.9640750940801,311.082010641984],[65.26596197408008,311.082010641984]]},{"text":"不要盲目崇拜。毛主席对刘诗昆说：","box":[[80.20935813408009,301.378506641984],[239.73502211510407,301.378506641984],[239.73502211510407,311.082010641984],[80.20935813408009,311.082010641984]]},{"text":"‘","box":[[234.98030515510413,301.378506641984],[244.68380915510414,301.378506641984],[244.68380915510414,311.082010641984],[234.98030515510413,311.082010641984]]},{"text":"要写自己民族的东西。外国","box":[[241.09350297160015,301.378506641984],[362.5814021621121,301.378506641984],[362.5814021621121,311.082010641984],[241.09350297160015,311.082010641984]]},{"text":"的东西，","box":[[65.26599108459214,316.61300792198404],[104.95332244459217,316.61300792198404],[104.95332244459217,326.31651192198404],[65.26599108459214,326.31651192198404]]},{"text":"有的是吹出来的，","box":[[100.19861518809617,316.61300792198404],[179.86440243510415,316.61300792198404],[179.86440243510415,326.31651192198404],[100.19861518809617,326.31651192198404]]},{"text":"其实思想上、","box":[[175.10968547510421,316.61300792198404],[235.07733049160015,316.61300792198404],[235.07733049160015,326.31651192198404],[175.10968547510421,326.31651192198404]]},{"text":"艺术上都不行。有人却把它们","box":[[230.5167127221122,316.61300792198404],[362.5814506796322,316.61300792198404],[362.5814506796322,326.31651192198404],[230.5167127221122,326.31651192198404]]},{"text":"当作楷模，","box":[[65.26601049160024,331.84752860899204],[114.94796067510428,331.84752860899204],[114.94796067510428,341.55103260899205],[65.26601049160024,341.55103260899205]]},{"text":"认为不可变。费城交响乐团来中国，","box":[[110.19324371510426,331.84752860899204],[270.49514920211226,331.84752860899204],[270.49514920211226,341.55103260899205],[110.19324371510426,341.55103260899205]]},{"text":"听了我们的音乐，","box":[[265.9345120256163,331.84752860899204],[347.0558054656163,331.84752860899204],[347.0558054656163,341.55103260899205],[265.9345120256163,341.55103260899205]]},{"text":"认为","box":[[342.5922033291203,331.84752860899204],[362.58143127262434,331.84752860899204],[362.58143127262434,341.55103260899205],[342.5922033291203,341.55103260899205]]},{"text":"很好，","box":[[65.26602019510432,347.17904552198405],[94.95874243510434,347.17904552198405],[94.95874243510434,356.88254952198406],[65.26602019510432,356.88254952198406]]},{"text":"而我们自己却不以为然。外贸部反映，","box":[[90.20403517860834,347.17904552198405],[261.9560462751043,347.17904552198405],[261.9560462751043,356.88254952198406],[90.20403517860834,356.88254952198406]]},{"text":"外国人对《","box":[[257.49244413860845,347.17904552198405],[308.62993932912036,347.17904552198405],[308.62993932912036,356.88254952198406],[257.49244413860845,356.88254952198406]]},{"text":"杜泉山》","box":[[305.13666818561643,347.17904552198405],[345.8913655786084,347.17904552198405],[345.8913655786084,356.88254952198406],[305.13666818561643,356.88254952198406]]},{"text":"的反","box":[[342.4951876961284,347.17904552198405],[362.5