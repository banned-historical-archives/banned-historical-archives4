2678199504],[362.58169326723277,524.656182199504],[85.2554422086888,524.656182199504]]},{"text":"我，","box":[[65.26628218971285,530.187189183008],[84.96439530971287,530.187189183008],[84.96439530971287,539.8906931830079],[65.26628218971285,539.8906931830079]]},{"text":"我看看你，","box":[[80.20967834971286,530.187189183008],[129.8916382367209,530.187189183008],[129.8916382367209,539.8906931830079],[80.20967834971286,539.8906931830079]]},{"text":"谁也不说话。","box":[[125.13692127672087,530.187189183008],[184.8134902837288,530.187189183008],[184.8134902837288,539.8906931830079],[125.13692127672087,539.8906931830079]]},{"text":"散会后，","box":[[85.25552954022484,545.518725503008],[124.94287060372886,545.518725503008],[124.94287060372886,555.222229503008],[85.25552954022484,555.222229503008]]},{"text":"陈永贵大步往外走，","box":[[120.18815364372885,545.518725503008],[213.1477316672328,545.518725503008],[213.1477316672328,555.222229503008],[120.18815364372885,555.222229503008]]},{"text":"纪登奎快步跟上去，","box":[[209.07225998723285,545.518725503008],[303.4873345002248,545.518725503008],[303.4873345002248,555.222229503008],[209.07225998723285,555.222229503008]]},{"text":"伸出大拇指赞","box":[[299.50889786022486,545.518725503008],[362.58161563920083,545.518725503008],[362.58161563920083,555.222229503008],[299.50889786022486,555.222229503008]]}]}