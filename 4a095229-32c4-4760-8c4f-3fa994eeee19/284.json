ox":[[213.05044433612846,377.64808689600005],[362.58147008664025,377.64808689600005],[362.58147008664025,387.35159089600006],[213.05044433612846,387.35159089600006]]},{"text":"个领导人，","box":[[65.2660978231363,392.882588176],[114.94804800664035,392.882588176],[114.94804800664035,402.586092176],[65.2660978231363,402.586092176]]},{"text":"为了让自己的子女当兵上大学，","box":[[110.19333104664032,392.882588176],[253.5141239406563,392.882588176],[253.5141239406563,402.586092176],[110.19333104664032,402.586092176]]},{"text":"什么一个批示，","box":[[249.14753743715238,392.882588176],[320.7594454746723,392.882588176],[320.7594454746723,402.586092176],[249.14753743715238,402.586092176]]},{"text":"一点头啊，","box":[[316.48988430766434,392.882588176],[367.62736009116827,392.882588176],[367.62736009116827,402.586092176],[316.48988430766434,402.586092176]]},{"text":"一个电话，","box":[[65.26616574766427,408.21412449600007],[114.8510711876643,408.21412449600007],[114.8510711876643,417.9176284960001],[65.26616574766427,417.9176284960001]]},{"text":"比主席的批示都重要哎。这是为什么？几千年的封资修的","box":[[110.09635422766428,408.21412449600007],[362.58158652868815,408.21412449600007],[362.58158652868815,417.9176284960001],[110.09635422766428,417.9176284960001]]},{"text":"流毒，","box":[[65.2661463406564,423.44862577600003],[94.95886858065643,423.44862577600003],[94.95886858065643,433.15212977600004],[65.2661463406564,433.15212977600004]]},{"text":"孔老二的一些流毒，","box":[[90.20416132416042,423.44862577600003],[179.86455769116841,423.44862577600003],[179.86455769116841,433.15212977600004],[90.20416132416042,433.15212977600004]]},{"text":"旧的习惯势力，","box":[[175.10984073116848,423.44862577600003],[246.04246467467243,423.44862577600003],[246.04246467467243,433.15212977600004],[175.10984073116848,433.15212977600004]]},{"text":"还有什么山头主义，宗派主","box":[[241.67587817116848,423.44862577600003],[362.5815283076645,423.44862577600003],[362.5815283076645,433.15212977600004],[241.67587817116848,433.15212977600004]]},{"text":"义，","box":[[65.26611723014457,438.683127056],[84.96423035014459,438.683127056],[84.96423035014459,448.386631056],[65.26611723014457,448.386631056]]},{"text":"裙带关系，","box":[[80.20951339014458,438.683127056],[129.8914732771526,438.683127056],[129.8914732771526,448.386631056],[80.20951339014458,448.386631056]]},{"text":"把一些人就给俘虏了，","box":[[125.13675631715259,438.683127056],[227.31469225116854,438.683127056],[227.31469225116854,448.386631056],[125.13675631715259,448.386631056]]},{"text":"这些关系超过了阶级关系，","box":[[222.9481057476646,438.683127056],[346.66777204416064,438.683127056],[346.66777204416064,448.386631056],[222.9481057476646,448.386631056]]},{"text":"超过","box":[[342.3982788016807,438.683127056],[362.5815865286887,438.683127056],[362.5815865286887,448.386631056],[342.3982788016807,448.386631056]]},{"text":"了政治关系，","box":[[65.26617545116869,453.91764774300805],[124.94273475467274,453.91764774300805],[124.94273475467274,463.62115174300806],[65.26617545116869,463.62115174300806]]},{"text":"超过了党的关系。我们的干部要坚持原则，","box":[[120.18801779467272,453.91764774300805],[311.2500115546726,453.91764774300805],[311.2500115546726,463.62115174300806],[120.18801779467272,463.62115174300806]]},{"text":"要不受捧，","box":[[306.7864094181767,453.91764774300805],[357.3416846651847,453.91764774300805],[357.3416846651847,463.62115174300806],[306.7864094181767,463.62115174300806]]},{"text":"不","box":[[352.8780825286887,453.91764774300805],[362.5815865286887,453.91764774300805],[362.5815865286887,463.62115174300806],[352.8780825286887,463.62115174300806]]},{"text":"受压，","box":[[65.26617545116869,469.152149023008],[94.8618529476647,469.152149023008],[94.8618529476647,478.855653023008],[65.26617545116869,478.855653023008]]},{"text":"不受软硬兼施这一套东西。什么请客吃饭啦，","box":[[90.1071456911687,469.152149023008],[295.5303447781765,469.152149023008],[295.5303447781765,478.855653023008],[90.1071456911687,478.855653023008]]},{"text":"他请客你可以不","box":[[291.1637582746727,469.152149023008],[362.5816156392008,469.152149023008],[362.5816156392008,478.855653023008],[291.1637582746727,478.855653023008]]},{"text":"去嘛。所以有的群众就给我们说嘛，","box":[[65.2661754511688,484.483665936],[226.53844104168078,484.483665936],[226.53844104168078,494.18716993600003],[65.2661754511688,494.18716993600003]]},{"text":"说‘","box":[[222.26888957817684,484.483665936],[242.45219730518482,484.483665936],[242.45219730518482,494.18716993600003],[222.26888957817684,494.18716993600003]]},{"text":"酒杯一端，","box":[[238.95892616168084,484.483665936],[289.80533563920085,484.483665936],[289.80533563920085,494.18716993600003],[238.95892616168084,494.18716993600003]]},{"text":"政策放宽；","box":[[285.53575506518484,484.483665936],[336.3821645427048,484.483665936],[336.3821645427048,494.18716993600003],[285.53575506518484,494.18716993600003]]},{"text":"筷子一","box":[[332.11260337569684,484.483665936],[362.58164474971284,484.483665936],[362.58164474971284,494.18716993600003],[332.11260337569684,494.18716993600003]]},{"text":"提，","box":[[65.26623367219281,499.718186623008],[84.96434679219283,499.718186623008],[84.96434679219283,509.42169062300803],[65.26623367219281,509.42169062300803]]},{"text":"可以可以。","box":[[80.11259479219282,499.718186623008],[129.79454497569685,499.718186623008],[129.79454497569685,509.42169062300803],[80.11259479219282,509.42169062300803]]},{"text":"’","box":[[125.03983771920083,499.718186623008],[134.74334171920083,499.718186623008],[134.74334171920083,509.42169062300803],[125.03983771920083,509.42169062300803]]},{"text":"可是也就有那么些人有人愿意请他的客，","box":[[130.95895575219282,499.718186623008],[315.6167242037287,499.718186623008],[315.6167242037287,509.42169062300803],[130.95895575219282,509.42169062300803]]},{"text":"他就愿意去","box":[[311.3471436297129,499.718186623008],[362.5817223777448,499.718186623008],[362.5817223777448,509.42169062300803],[311.3471436297129,509.42169062300803]]},{"text":"吃饭，","box":[[65.26631130022486,514.952687903008],[94.95903354022488,514.952687903008],[94.95903354022488,524.656191903008],[65.26631130022486,524.656191903008]]},{"text":"吃饭了你不干还好嘛，","box":[[90.20432628372888,514.952687903008],[189.85932206723288,514.952687903008],[189.85932206723288,524.656191903008],[90.20432628372888,524.656191903008]]},{"text":"吃饭吃了就给人家干，","box":[[185.10462451424092,514.952687903008],[285.82700573774486,514.952687903008],[285.82700573774486,524.656191903008],[185.10462451424092,524.656191903008]]},{"text":"几十年的原则他都","box":[[281.2663491542409,514.952687903008],[362.58177089526487,514.952687903008],[362.58177089526487,524.656191903008],[281.2663491542409,524.656191903008]]},{"text":"能坚持，","box":[[65.26633070723295,530.187198886512],[104.95366206723298,530.187198886512],[104.95366206723298,539.8907028865119],[65.26633070723295,539.8907028865119]]},{"text":"但是一两小时就被人家给拉过去了，","box":[[100.19895481073698,530.187198886512],[259.82165383176084,530.187198886512],[259.82165383176084,539.8907028865119],[100.19895481073698,539.8907028865119]]},{"text":"俘虏了。","box":[[255.066917464753,530.187198886512],[294.754258528257,530.187198886512],[294.754258528257,539.8907028865119],[255.066917464753,539.8907028865119]]},{"text":"”","box":[[289.99954156825703,530.187198886512],[299.70304556825704,530.187198886512],[299.70304556825704,539.8907028865119],[289.99954156825703,539.8907028865119]]},{"text":"“","box":[[85.25559746475304,545.518735206512],[94.95910146475303,545.518735206512],[94.95910146475303,555.222239206512],[85.25559746475304,555.222239206512]]},{"text":"有的群众给我们说，","box":[[91.17474460825704,545.518735206512],[180.932185718769,545.518735206512],[180.932185718769,555.222239206512],[91.17474460825704,555.222239206512]]},{"text":"他说有些领导，","box":[[176.66262455176104,545.518735206512],[248.27453258928102,545.518735206512],[248.27453258928102,555.222239206512],[176.66262455176104,555.222239206512]]},{"text":"就是他那省的了，","box":[[244.00498112577708,545.518735206512],[325.99963844329704,545.518735206512],[325.99963844329704,555.222239206512],[244.00498112577708,555.222239206512]]},{"text":"去给他作","box":[[321.7300772762891,545.518735206512],[362.58186793030507,545.518735206512],[362.58186793030507,555.222239206512],[321.7300772762891,555.222239206512]]}]}