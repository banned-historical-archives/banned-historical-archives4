[[247.98276763100802,316.613007921984],[257.686271631008,316.613007921984],[257.686271631008,326.316511921984],[247.98276763100802,326.316511921984]]},{"text":"","box":[[257.686271631008,316.613007921984],[257.966269631008,316.613007921984],[257.966269631008,316.613007921984],[257.686271631008,316.613007921984]]},{"text":"４","box":[[260.403233344,316.613007921984],[270.106737344,316.613007921984],[270.106737344,326.316511921984],[260.403233344,326.316511921984]]},{"text":"人丧生。她说：","box":[[268.845310934512,316.613007921984],[340.2630906710079,316.613007921984],[340.2630906710079,326.316511921984],[268.845310934512,326.316511921984]]},{"text":"“","box":[[336.09058395100794,316.613007921984],[345.79408795100795,316.613007921984],[345.79408795100795,326.316511921984],[336.09058395100794,326.316511921984]]},{"text":"见大","box":[[342.39788095801595,316.613007921984],[362.58118868502396,316.613007921984],[362.58118868502396,326.316511921984],[342.39788095801595,326.316511921984]]},{"text":"雨下得这个样，","box":[[65.26577760750394,331.847528608992],[134.936946031008,331.847528608992],[134.936946031008,341.551032608992],[65.26577760750394,341.551032608992]]},{"text":"我就在屋里蒸馍，","box":[[130.18222907100798,331.847528608992],[209.84802602151996,331.847528608992],[209.84802602151996,341.551032608992],[130.18222907100798,341.551032608992]]},{"text":"蒸了一屉子又一屉子，","box":[[205.09330906152002,331.847528608992],[306.20381103801594,331.847528608992],[306.20381103801594,341.551032608992],[205.09330906152002,341.551032608992]]},{"text":"眼见着雨越来","box":[[301.643154454512,331.847528608992],[362.581237202544,331.847528608992],[362.581237202544,341.551032608992],[301.643154454512,341.551032608992]]},{"text":"越大，","box":[[65.26579701451203,347.179045521984],[94.95851925451205,347.179045521984],[94.95851925451205,356.882549521984],[65.26579701451203,356.882549521984]]},{"text":"我把衣服都翻出来，","box":[[90.20381199801605,347.179045521984],[179.86420836502404,347.179045521984],[179.86420836502404,356.882549521984],[90.20381199801605,356.882549521984]]},{"text":"穿上了两条裤子，","box":[[175.1094914050241,347.179045521984],[255.357479188528,347.179045521984],[255.357479188528,356.882549521984],[175.1094914050241,356.882549521984]]},{"text":"三件上衣，","box":[[250.79684201203207,347.179045521984],[301.060992732032,347.179045521984],[301.060992732032,356.882549521984],[250.79684201203207,356.882549521984]]},{"text":"连袄都穿上了，","box":[[296.50033614852805,347.179045521984],[367.6270010615201,347.179045521984],[367.6270010615201,356.882549521984],[296.50033614852805,356.882549521984]]},{"text":"就听儿子喊，","box":[[65.26580671801611,362.413546801984],[124.94236602152016,362.413546801984],[124.94236602152016,372.117050801984],[65.26580671801611,372.117050801984]]},{"text":"快走快走，","box":[[120.18764906152013,362.413546801984],[169.86959924502412,362.413546801984],[169.86959924502412,372.117050801984],[120.18764906152013,372.117050801984]]},{"text":"快上大队院！俺娘俩还没有走到大队部，","box":[[165.11490169203213,362.413546801984],[347.05560169203204,362.413546801984],[347.05560169203204,372.117050801984],[165.11490169203213,372.117050801984]]},{"text":"就听","box":[[342.5919995555361,362.413546801984],[362.58122749904015,362.413546801984],[362.58122749904015,372.117050801984],[342.5919995555361,372.117050801984]]},{"text":"见轰隆隆一片房倒的声音，","box":[[65.26581642152013,377.648086896],[184.91004014852814,377.648086896],[184.91004014852814,387.351590896],[65.26581642152013,387.351590896]]},{"text":"还不及挪步，","box":[[180.15532318852817,377.648086896],[239.83190189904013,377.648086896],[239.83190189904013,387.351590896],[180.15532318852817,387.351590896]]},{"text":"人就漂在水里了。","box":[[235.07716553203218,377.648086896],[314.74298188955214,377.648086896],[314.74298188955214,387.351590896],[235.07716553203218,387.351590896]]},{"text":"”","box":[[309.9882649295522,377.648086896],[319.6917689295522,377.648086896],[319.6917689295522,387.351590896],[309.9882649295522,387.351590896]]},{"text":"魏长河回忆：","box":[[85.25508317904018,392.882588176]