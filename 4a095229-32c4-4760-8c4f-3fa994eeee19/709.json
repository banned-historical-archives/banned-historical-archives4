258604825,347.17903581848003],[336.47881144254427,347.17903581848003],[336.47881144254427,356.88253981848004],[255.06642258604825,356.88253981848004]]},{"text":"争论是","box":[[332.11222493904035,347.17903581848003],[362.58126631305635,347.17903581848003],[362.58126631305635,356.88253981848004],[332.11222493904035,356.88253981848004]]},{"text":"争论嘛，","box":[[65.26585523553632,362.41353709848],[104.95318659553635,362.41353709848],[104.95318659553635,372.11704109848],[65.26585523553632,372.11704109848]]},{"text":"为什么撤职？杜林这个人活了八十多岁，","box":[[100.19847933904035,362.41353709848],[284.37103377656035,362.41353709848],[284.37103377656035,372.11704109848],[100.19847933904035,372.11704109848]]},{"text":"名誉不好。处分人","box":[[280.19852705656047,362.41353709848],[362.5813148305764,362.41353709848],[362.5813148305764,372.11704109848],[280.19852705656047,372.11704109848]]},{"text":"要注意，","box":[[65.26590375305642,377.64807719249603],[104.95323511305645,377.64807719249603],[104.95323511305645,387.35158119249604],[65.26590375305642,387.35158119249604]]},{"text":"动不动就要撤职，","box":[[100.19852785656045,377.64807719249603],[179.86431510356843,377.64807719249603],[179.86431510356843,387.35158119249604],[100.19852785656045,387.35158119249604]]},{"text":"动不动就要关起来，","box":[[175.1095981435685,377.64807719249603],[265.6432904635684,377.64807719249603],[265.6432904635684,387.35158119249604],[175.1095981435685,387.35158119249604]]},{"text":"表现是神经衰弱症。林","box":[[261.08265328707245,377.64807719249603],[362.58131483057633,377.64807719249603],[362.58131483057633,387.35158119249604],[261.08265328707245,387.35158119249604]]},{"text":"彪不跑，","box":[[65.26590375305642,392.882578472496],[104.95323511305645,392.882578472496],[104.95323511305645,402.586082472496],[65.26590375305642,402.586082472496]]},{"text":"我们也不会杀他，","box":[[100.19852785656045,392.882578472496],[179.86431510356843,392.882578472496],[179.86431510356843,402.586082472496],[100.19852785656045,402.586082472496]]},{"text":"批是要批的。","box":[[175.1095981435685,392.882578472496],[234.7861574470724,392.882578472496],[234.7861574470724,402.586082472496],[175.1095981435685,402.586082472496]]},{"text":"”","box":[[230.03144048707247,392.882578472496],[239.73494448707248,392.882578472496],[239.73494448707248,402.586082472496],[230.03144048707247,402.586082472496]]},{"text":"“","box":[[85.25515110356847,408.21411479249605],[94.95865510356846,408.21411479249605],[94.95865510356846,417.91761879249606],[85.25515110356847,417.91761879249606]]},{"text":"蒋介石的时候，","box":[[91.17429824707247,408.21411479249605],[162.10691248707246,408.21411479249605],[162.10691248707246,417.91761879249606],[91.17429824707247,417.91761879249606]]},{"text":"报纸、","box":[[157.7403453905765,408.21411479249605],[187.72416304707247,408.21411479249605],[187.72416304707247,417.91761879249606],[157.7403453905765,417.91761879249606]]},{"text":"广播、","box":[[183.35757654356848,408.21411479249605],[213.34142331057646,408.21411479249605],[213.34142331057646,417.91761879249606],[183.35757654356848,417.91761879249606]]},{"text":"学校、","box":[[208.97483680707248,408.21411479249605],[238.95868357408045,408.21411479249605],[238.95868357408045,417.91761879249606],[208.97483680707248,417.91761879249606]]},{"text":"电影都是他们的，","box":[[234.59207766356846,408.21411479249605],[316.00449563057646,408.21411479249605],[316.00449563057646,417.91761879249606],[234.59207766356846,417.91761879249606]]},{"text":"他们蒙蔽人","box":[[311.6379285340805,408.21411479249605],[362.58136334809654,408.21411479249605],[362.58136334809654,417.91761879249606],[311.6379285340805,417.91761879249606]]},{"text":"民。我们都是从那儿来的。我学孔夫子、","box":[[65.26592316006457,423.448616072496],[247.49776709408056,423.448616072496],[247.49776709408056,433.152120072496],[65.26592316006457,433.152120072496]]},{"text":"资本家十三年，","box":[[243.22823503758462,423.448616072496],[315.1311899740806,423.448616072496],[315.1311899740806,433.152120072496],[243.22823503758462,433.152120072496]]},{"text":"就是不知道","box":[[310.95870266108864,423.448616072496],[362.5813827551046,423.448616072496],[362.5813827551046,433.152120072496],[310.95870266108864,433.152120072496]]},{"text":"马列，","box":[[65.26597167758462,438.68311735249597],[94.95869391758464,438.68311735249597],[94.95869391758464,448.386621352496],[65.26597167758462,448.386621352496]]},{"text":"十月革命后才学马列，","box":[[90.20398666108863,438.68311735249597],[189.85898244459264,438.68311735249597],[189.85898244459264,448.386621352496],[90.20398666108863,448.386621352496]]},{"text":"过去不知道。反动派没有多少威力，","box":[[185.10428489160068,438.68311735249597],[347.0557666516006,438.68311735249597],[347.0557666516006,448.386621352496],[185.10428489160068,448.386621352496]]},{"text":"靠剥","box":[[342.5921645151047,438.68311735249597],[362.5813924586087,438.68311735249597],[362.5813924586087,448.386621352496],[342.5921645151047,448.386621352496]]},{"text":"削过生活，","box":[[65.2659813810887,453.917638039504],[114.94793156459274,453.917638039504],[114.94793156459274,463.62114203950404],[65.2659813810887,463.62114203950404]]},{"text":"他的兵都是靠抓壮丁，","box":[[110.19321460459271,453.917638039504],[209.8482297951047,453.917638039504],[209.8482297951047,463.62114203950404],[110.19321460459271,463.62114203950404]]},{"text":"所以我们不怕他们。怕死的是林彪，","box":[[205.19053817160074,453.917638039504],[367.6271854280967,453.917638039504],[367.6271854280967,463.62114203950404],[205.19053817160074,463.62114203950404]]},{"text":"叫他打锦州，","box":[[65.26599108459277,469.152139319504],[124.94255038809682,469.152139319504],[124.94255038809682,478.855643319504],[65.26599108459277,478.855643319504]]},{"text":"他不打，","box":[[120.67299892459279,469.152139319504],[161.52476046809676,469.152139319504],[161.52476046809676,478.855643319504],[120.67299892459279,478.855643319504]]},{"text":"最后两天他去了，","box":[[157.44929849160079,469.152139319504],[240.6083180680967,469.152139319504],[240.6083180680967,478.855643319504],[157.44929849160079,478.855643319504]]},{"text":"俘虏十万人。又消灭了廖耀","box":[[236.53284638809677,469.152139319504],[362.5812954235687,469.152139319504],[362.5812954235687,478.855643319504],[236.53284638809677,478.855643319504]]},{"text":"湘。长春沈阳解放。","box":[[65.26596197408077,484.483656232496],[154.8293330045928,484.483656232496],[154.8293330045928,494.187160232496],[65.26596197408077,494.187160232496]]},{"text":"释放俘虏放得好，","box":[[85.25519962108879,499.718176919504],[164.9209965716008,499.718176919504],[164.9209965716008,509.421680919504],[85.25519962108879,509.421680919504]]},{"text":"国民党怕得很。","box":[[160.1662699080968,499.718176919504],[229.83744803510479,499.718176919504],[229.83744803510479,509.421680919504],[160.1662699080968,509.421680919504]]},{"text":"”","box":[[225.08273107510485,499.718176919504],[234.78623507510486,499.718176919504],[234.78623507510486,509.421680919504],[225.08273107510485,509.421680919504]]},{"text":"这份谈话内容是由江青亲自作的记录。到","box":[[85.25522873160085,514.952678199504],[266.22561714561675,514.952678199504],[266.22561714561675,524.656182199504],[85.25522873160085,524.656182199504]]},{"text":"","box":[[266.2256171456169,514.952678199504],[266.4755891456168,514.952678199504],[266.4755891456168,514.952678199504],[266.2256171456169,514.952678199504]]},{"text":"１","box":[[268.65122144750404,514.952658792496],[278.35472544750405,514.952658792496],[278.35472544750405,524.6561627924959],[268.65122144750404,524.6561627924959]]},{"text":"１","box":[[273.502973447504,514.952658792496],[283.206477447504,514.952658792496],[283.206477447504,524.6561627924959],[273.502973447504,524.6561627924959]]},{"text":"月","box":[[281.653936214512,514.952658792496],[291.357440214512,514.952658792496],[291.357440214512,524.6561627924959],[281.653936214512,524.6561627924959]]},{"text":"","box":[[291.357440214512,514.952658792496],[291.607438214512,514.952658792496],[291.607438214512,514.952658792496],[291.357440214512,514.952658792496]]},{"text":"１","box":[[293.78329680750403,514.952658792496],[303.48680080750404,514.952658792496],[303.48680080750404,524.6561627924959],[293.78329680750403,524.6561627924959]]},{"text":"５","box":[[298.635048807504,514.952658792496],[308.338552807504,514.952658792496],[308.338552807504,524.6561627924959],[298.635048807504,524.6561627924959]]},{"text":"日，","box":[[306.78599216750405,514.952658792496],[326.48413439801607,514.952658792496],[326.48413439801607,524.6561627924959],[306.78599216750405,524.6561627924959]]},{"text":"毛泽东又","box":[[322.02053226152003,514.952658792496],[362.581217795536,514.952658792496],[362.581217795536,524.6561627924959],[322.02053226152003,524.6561627924959]]},{"text":"对记录作了几处修改。","box":[[65.26577760750405,530.187169776],[164.92079279801607,530.187169776],[164.92079279801607,539.890673776],[65.26577760750405,539.890673776]]},{"text":"７","box":[[84.67277590400002,545.518696392496],[94.37627990400001,545.518696392496],[94.37627990400001,555.222200392496],[84.67277590400002,555.222200392496]]},{"text":"月","box":[[92.53261414400001,545.518696392496],[102.236118144,545.518696392496],[102.236118144,555.222200392496],[92.53261414400001,555.222200392496]]},{"text":"","box":[[102.236118144,545.518696392496],[102.456119144,545.518696392496],[102.456119144,545.518696392496],[102.236118144,545.518696392496]]},{"text":"１","box":[[104.370898727504,545.518696392496],[114.074402727504,545.518696392496],[114.074402727504,555.222200392496],[104.370898727504,555.222200392496]]},{"text":"４","box":[[109.222650727504,545.518696392496],[118.926154727504,545.518696392496],[118.926154727504,555.222200392496],[109.222650727504,555.222200392496]]},{"text":"日当天，","box":[[117.082488967504,545.518696392496],[156.76982032750402,545.518696392496],[156.76982032750402,555.222200392496],[117.082488967504,555.222200392496]]},{"text":"周扬就被放出来了。","box":[[152.01511307100802,545.518696392496],[241.6754997345119,545.518696392496],[241.6754997345119,555.222200392496],[152.01511307100802,555.222200392496]]}]}