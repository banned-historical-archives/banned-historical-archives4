":[[311.7348083180166,453.91764774300793],[362.5812177955366,453.91764774300793],[362.5812177955366,463.62115174300794],[311.7348083180166,463.62115174300794]]},{"text":"谈，","box":[[65.26577760750462,469.15214902300795],[84.96389072750463,469.15214902300795],[84.96389072750463,478.85565302300796],[65.26577760750462,478.85565302300796]]},{"text":"议论周恩来伟大的人格，","box":[[80.20917376750462,469.15214902300795],[189.85879807801663,469.15214902300795],[189.85879807801663,478.85565302300796],[80.20917376750462,478.85565302300796]]},{"text":"久久不能平静。","box":[[185.10410052502465,469.15214902300795],[254.77525924502459,469.15214902300795],[254.77525924502459,478.85565302300796],[185.10410052502465,478.85565302300796]]},{"text":"８","box":[[84.67277590400002,484.483636825488],[94.37627990400001,484.483636825488],[94.37627990400001,494.18714082548803],[84.67277590400002,494.18714082548803]]},{"text":"月","box":[[92.53261414400001,484.483636825488],[102.236118144,484.483636825488],[102.236118144,494.18714082548803],[92.53261414400001,494.18714082548803]]},{"text":"","box":[[102.236118144,484.483636825488],[102.456119144,484.483636825488],[102.456119144,484.483636825488],[102.236118144,484.483636825488]]},{"text":"２","box":[[104.370898727504,484.483636825488],[114.074402727504,484.483636825488],[114.074402727504,494.18714082548803],[104.370898727504,494.18714082548803]]},{"text":"８","box":[[109.222650727504,484.483636825488],[118.926154727504,484.483636825488],[118.926154727504,494.18714082548803],[109.222650727504,494.18714082548803]]},{"text":"日，","box":[[117.082488967504,484.483636825488],[136.780602087504,484.483636825488],[136.780602087504,494.18714082548803],[117.082488967504,494.18714082548803]]},{"text":"“","box":[[132.025885127504,484.483636825488],[141.72938912750402,484.483636825488],[141.72938912750402,494.18714082548803],[132.025885127504,494.18714082548803]]},{"text":"十大”","box":[[137.94503227100802,484.483636825488],[168.025875264,484.483636825488],[168.025875264,494.187