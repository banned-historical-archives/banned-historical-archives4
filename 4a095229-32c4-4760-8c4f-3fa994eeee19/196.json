[[65.26648596329653,453.917667150016],[164.92150115380855,453.917667150016],[164.92150115380855,463.621171150016],[65.26648596329653,463.621171150016]]},{"text":"此时已是","box":[[85.25572361030453,469.152168430016],[124.94305497030456,469.152168430016],[124.94305497030456,478.855672430016],[85.25572361030453,478.855672430016]]},{"text":"","box":[[124.94305497030454,469.152168430016],[125.16297997030449,469.152168430016],[125.16297997030449,469.152168430016],[124.94305497030454,469.152168430016]]},{"text":"１","box":[[127.077098087504,469.152119912496],[136.780602087504,469.152119912496],[136.780602087504,478.855623912496],[127.077098087504,478.855623912496]]},{"text":"２","box":[[131.928859791008,469.152119912496],[141.632363791008,469.152119912496],[141.632363791008,478.855623912496],[131.928859791008,478.855623912496]]},{"text":"日凌晨","box":[[139.78868832750402,469.152119912496],[170.25770059100802,469.152119912496],[170.25770059100802,478.855623912496],[139.78868832750402,478.855623912496]]},{"text":"","box":[[170.25770059100805,469.152119912496],[170.51769859100804,469.152119912496],[170.51769859100804,469.152119912496],[170.25770059100805,469.152119912496]]},{"text":"１","box":[[172.78059222400003,469.152119912496],[182.48409622400004,469.152119912496],[182.48409622400004,478.855623912496],[172.78059222400003,478.855623912496]]},{"text":"时。周恩来说：","box":[[181.028590031008,469.152119912496],[251.96121397451196,469.152119912496],[251.96121397451196,478.855623912496],[181.028590031008,478.855623912496]]},{"text":"“","box":[[247.594627471008,469.152119912496],[257.298131471008,469.152119912496],[257.298131471008,478.855623912496],[247.594627471008,478.855623912496]]},{"text":"于桑回部后，","box":[[253.70782528750402,469.152119912496],[315.228108572032,469.152119912496],[315.228108572032,478.855623912496],[253.70782528750402,478.855623912496]]},{"text":"你们要抓紧","box":[[310.95852799801605,469.152119912496],[362.581208092032,469.152119912496],[362.581208092032,478.855623912496],[310.95852799801605,478.855623912496]]},{"text":"对他的错误进行批判。这个人什么都不在乎，","box":[[65.26579701451203,484.483636825488],[265.5461486850239,484.483636825488],[265.5461486850239,494.18714082548803],[65.26579701451203,494.18714082548803]]},{"text":"批判他，","box":[[260.98549210152,484.483636825488],[301.06099273203193,484.483636825488],[301.06099273203193,494.18714082548803],[260.98549210152,494.18714082548803]]},{"text":"一是挽救本人，","box":[[296.500336148528,484.483636825488],[367.62700106152005,484.483636825488],[367.62700106152005,494.18714082548803],[296.500336148528,494.18714082548803]]},{"text":"二是教育大家，","box":[[65.26580671801605,499.718157512496],[134.93697514152012,499.718157512496],[134.93697514152012,509.42166151249603],[65.26580671801605,509.42166151249603]]},{"text":"提高思想。","box":[[130.1822581815201,499.718157512496],[179.8642180685281,499.718157512496],[179.8642180685281,509.42166151249603],[130.1822581815201,509.42166151249603]]},{"text":"”","box":[[175.10950110852812,499.718157512496],[184.81300510852813,499.718157512496],[184.81300510852813,509.42166151249603],[175.10950110852812,509.42166151249603]]},{"text":"第二天，","box":[[85.2550443650241,514.952658792496],[124.94238542852813,514.952658792496],[124.94238542852813,524.6561627924959],[85.2550443650241,524.6561627924959]]},{"text":"施义之同吴忠拿着周恩来批准的报告给于桑看了，","box":[[120.18766846852812,514.952658792496],[346.66748093904005,514.952658792496],[346.66748093904005,524.6561627924959],[120.18766846852812,524.6561627924959]]},{"text":"把于","box":[[342.3979876965602,514.952658792496],[362.58129542356824,514.952658792496],[362.58129542356824,524.6561627924959],[342.3979876965602,524.6561627924959]]},{"text":"桑接到中组部招待所。后来于桑回部作了检查，","box":[[65.26588434604821,530.187169776],[275.6378898800641,530.187169776],[275.6378898800641,539.890673776],[65.26588434604821,539.890673776]]},{"text":"经大小会数次批判后","box":[[271.0772721105762,530.187169776],[362.5813827551041,530.187169776],[362.5813827551041,539.890673776],[271.0772721105762,539.890673776]]},{"text":"恢复工作。根据周恩来的指示，","box":[[65.2659037530562,545.518706096],[204.8023203835682,545.518706096],[204.8023203835682,555.2222100959999],[65.2659037530562,555.2222100959999]]},{"text":"仍为核心小组成员。","box":[[200.04762283057622,545.518706096],[289.7079997905761,545.518706096],[289.7079997905761,555.2222100959999],[200.04762283057622,555.2222100959999]]}]}