":[[342.4952362136485,484.48366593599997],[362.5814894936485,484.48366593599997],[362.5814894936485,494.187169936],[342.4952362136485,494.187169936]]},{"text":"水又不可太清。一点错误没有，","box":[[65.2660784161285,499.71818662300797],[206.54912576664051,499.71818662300797],[206.54912576664051,509.421690623008],[65.2660784161285,509.421690623008]]},{"text":"没有这样的人。哪有那么清的，","box":[[202.27957430313654,499.71818662300797],[346.57072730065653,499.71818662300797],[346.57072730065653,509.421690623008],[202.27957430313654,509.421690623008]]},{"text":"水至","box":[[342.3982205806566,499.71818662300797],[362.5815283076646,499.71818662300797],[362.5815283076646,509.421690623008],[342.3982205806566,509.421690623008]]},{"text":"清则无鱼。","box":[[65.26611723014457,514.9526879030079],[114.94806741364862,514.9526879030079],[114.94806741364862,524.6561919030079],[65.26611723014457,524.6561919030079]]},{"text":"”","box":[[110.19335045364859,514.9526879030079],[119.89685445364859,514.9526879030079],[119.89685445364859,524.6561919030079],[110.19335045364859,524.6561919030079]]},{"text":"他说：","box":[[85.25533547014459,530.187198886512],[114.8510226701446,530.187198886512],[114.8510226701446,539.8907028865119],[85.25533547014459,539.8907028865119]]},{"text":"“","box":[[110.09629600664059,530.187198886512],[119.79980000664058,530.187198886512],[119.79980000664058,539.8907028865119],[110.09629600664059,539.8907028865119]]},{"text":"政治局委员要懂得一点历史，","box":[[116.01545285364858,530.187198886512],[245.6543051076645,530.187198886512],[245.6543051076645,539.8907028865119],[116.01545285364858,539.8907028865119]]},{"text":"不仅中国史，","box":[[241.09364852416059,530.187198886512],[301.54648814766455,530.187198886512],[301.54648814766455,539.8907028865119],[241.09364852416059,539.8907028865119]]},{"text":"世界史，","box":[[296.9858315641606,530.187198886512],[337.06133219467256,530.187198886512],[337.06133219467256,539.8907028865119],[296.9858315641606,539.8907028865119]]},{"text":"分门别","box":[[332.5006756111686,530.187198886512],[362.58155741817666,530.187198886512],[362.58155741817666,539.8907028865119],[332.5006756111686,539.8907028865119]]},{"text":"类的政治史、","box":[[65.26614634065663,545.5187352065119],[124.84567060416066,545.5187352065119],[124.84567060416066,555.2222392065119],[65.26614634065663,555.2222392065119]]},{"text":"经济史、","box":[[120.09094394065663,545.5187352065119],[159.7782753006566,545.5187352065119],[159.7782753006566,555.2222392065119],[120.09094394065663,555.2222392065119]]},{"text":"小说史也要懂一点。从乌龟壳（","box":[[155.0235777476646,545.5187352065119],[295.33627469817657,545.5187352065119],[295.33627469817657,555.2222392065119],[155.0235777476646,555.2222392065119]]},{"text":"指使用甲骨文的","box":[[291.7459491076646,545.5187352065119],[362.58160593569664,545.5187352065119],[362.58160593569664,555.2222392065119],[291.7459491076646,555.2222392065119]]}]}