0641984],[342.5920383695521,311.082010641984]]},{"text":"结人，","box":[[65.2658552355361,316.61300792198404],[94.95857747553612,316.61300792198404],[94.95857747553612,326.31651192198404],[65.2658552355361,326.31651192198404]]},{"text":"因此受人民群众的爱戴，","box":[[90.20387021904011,316.61300792198404],[199.8534848260481,316.61300792198404],[199.8534848260481,326.31651192198404],[90.20387021904011,326.31651192198404]]},{"text":"人们称他作“","box":[[195.09876786604815,316.61300792198404],[254.7753271695521,316.61300792198404],[254.7753271695521,326.31651192198404],[195.09876786604815,326.31651192198404]]},{"text":"及时雨”","box":[[250.99097031305615,316.61300792198404],[290.67831137656015,316.61300792198404],[290.67831137656015,326.31651192198404],[250.99097031305615,326.31651192198404]]},{"text":"。在封建社会中，","box":[[286.8939739270722,316.61300792198404],[367.6270980965602,316.61300792198404],[367.6270980965602,326.31651192198404],[286.8939739270722,326.31651192198404]]},{"text":"官逼民反，","box":[[65.2659037530562,331.84752860899204],[114.94785393656024,331.84752860899204],[114.94785393656024,341.55103260899205],[65.2659037530562,341.55103260899205]]},{"text":"宋江被逼上梁山后，","box":[[110.19313697656021,331.84752860899204],[201.60016406356817,331.84752860899204],[201.60016406356817,341.55103260899205],[110.19313697656021,341.55103260899205]]},{"text":"领导起义，","box":[[197.23359696707223,331.84752860899204],[248.07996763057622,331.84752860899204],[248.07996763057622,341.55103260899205],[197.23359696707223,341.55103260899205]]},{"text":"同封建统治阶级坚决斗争，","box":[[243.71338112707227,331.84752860899204],[367.6271563175843,331.84752860899204],[367.6271563175843,341.55103260899205],[243.71338112707227,341.55103260899205]]},{"text":"起了很大作用。这些积极方面，","box":[[65.26596197408031,347.17904552198405],[206.54900932459233,347.17904552198405],[206.54900932459233,356.88254952198406],[65.26596197408031,356.88254952198406]]},{"text":"应充分予以肯定。另一方面，","box":[[202.27945786108836,347.17904552198405],[336.09080713160034,347.17904552198405],[336.09080713160034,356.88254952198406],[202.27945786108836,356.88254952198406]]},{"text":"应看到","box":[[331.91833922561636,347.17904552198405],[362.58141186561636,347.17904552198405],[362.58141186561636,356.88254952198406],[331.91833922561636,356.88254952198406]]},{"text":"宋江由于出身于地主阶级，","box":[[65.26600078809633,362.413546801984],[184.91022451510435,362.413546801984],[184.91022451510435,372.117050801984],[65.26600078809633,372.117050801984]]},{"text":"有阶级的局限性和动摇性。他有一种正统","box":[[180.15550755510438,362.413546801984],[362.58146038313635,362.413546801984],[362.58146038313635,372.117050801984],[180.15550755510438,372.117050801984]]},{"text":"观念，","box":[[65.26602019510443,377.64808689600005],[94.95874243510445,377.64808689600005],[94.95874243510445,387.35159089600006],[65.26602019510443,387.35159089600006]]},{"text":"认为不能反皇帝，","box":[[90.20403517860845,377.64808689600005],[169.86981272211244,377.64808689600005],[169.86981272211244,387.35159089600006],[90.20403517860845,387.35159089600006]]},{"text":"以至对皇帝的招安抱有幻想，","box":[[165.1151151691205,377.64808689600005],[296.0154035361284,377.64808689600005],[296.0154035361284,387.35159089600006],[165.1151151691205,387.35159089600006]]},{"text":"认为可以戴罪立","box":[[291.45474695262453,377.64808689600005],[362.58147979014456,377.64808689600005],[362.58147979014456,387.35159089600006],[291.45474695262453,387.35159089600006]]},{"text":"功，","box":[[65.26603960211258,392.882588176],[84.9641527221126,392.882588176],[84.9641527221126,402.586092176],[65.26603960211258,402.586092176]]},{"text":"可以不犯法。宋江上梁山，","box":[[80.1124007221126,392.882588176],[199.65960881612858,392.882588176],[199.65960881612858,402.586092176],[80.1124007221126,402.586092176]]},{"text":"不是轻易上去的。在杀阎婆惜（","box":[[194.90487244912063,392.882588176],[336.09087505612854,392.882588176],[336.09087505612854,402.586092176],[194.90487244912063,402.586092176]]},{"text":"他杀惜","box":[[332.5005882796326,392.882588176],[362.58147008664065,392.882588176],[362.58147008664065,402.586092176],[332.5005882796326,402.586092176]]},{"text":"是因为在政治上受到威胁）","box":[[65.26605900912062,408.21412449600007],[184.91028273612864,408.21412449600007],[184.91028273612864,417.9176284960001],[65.26605900912062,417.9176284960001]]},{"text":"后，","box":[[181.12592587963266,408.21412449600007],[200.82404870313664,408.21412449600007],[200.82404870313664,417.9176284960001],[181.12592587963266,417.9176284960001]]},{"text":"他起初宁愿被刺配到江州，","box":[[196.06933174313667,408.21412449600007],[316.68387675963254,408.21412449600007],[316.68387675963254,417.9176284960001],[196.06933174313667,417.9176284960001]]},{"text":"也不愿上梁","box":[[312.12327839715266,408.21412449600007],[362.5815380111687,408.21412449600007],[362.5815380111687,417.9176284960001],[312.12327839715266,417.9176284960001]]},{"text":"山。后来统治阶级要判处他死刑，","box":[[65.2660978231367,423.44862577600003],[217.02893919715274,423.44862577600003],[217.02893919715274,433.15212977600004],[65.2660978231367,433.15212977600004]]},{"text":"他才下决心上山。林冲也是如此，","box":[[212.75940714065678,423.44862577600003],[367.6273406841608,423.44862577600003],[367.6273406841608,433.15212977600004],[212.75940714065678,433.15212977600004]]},{"text":"他被迫害充军后，","box":[[65.2661463406568,438.683127056],[144.93194329116886,438.683127056],[144.93194329116886,448.386631056],[65.2661463406568,448.386631056]]},{"text":"由于高俅派陆谦火烧草料场，","box":[[140.17720692416083,438.683127056],[270.68934542766476,438.683127056],[270.68934542766476,448.386631056],[140.17720692416083,448.386631056]]},{"text":"谋害他的性命，","box":[[266.1287082511688,438.683127056],[336.7702173711687,438.683127056],[336.7702173711687,448.386631056],[266.1287082511688,448.386631056]]},{"text":"他才被","box":[[332.30661523467273,438.683127056],[362.5815671216808,438.683127056],[362.5815671216808,448.386631056],[332.30661523467273,448.386631056]]},{"text":"逼上梁山。在这一点上，","box":[[65.26615604416077,453.91764774300805],[174.81875501817677,453.91764774300805],[174.81875501817677,463.62115174300806],[65.26615604416077,463.62115174300806]]},{"text":"李逵、","box":[[170.0640186511688,453.91764774300805],[199.75676029817677,453.91764774300805],[199.75676029817677,463.62115174300806],[170.0640186511688,463.62115174300806]]},{"text":"鲁智深和武松等人不一样，","box":[[195.0020239311688,453.91764774300805],[316.19881800168076,453.91764774300805],[316.19881800168076,463.62115174300806],[195.0020239311688,463.62115174300806]]},{"text":"他们是反对","box":[[311.7351964581768,453.91764774300805],[362.5816059356968,453.91764774300805],[362.5816059356968,463.62115174300806],[311.7351964581768,463.62115174300806]]},{"text":"招安的。李逵的性格不象宋江那样复杂，","box":[[65.26616574766484,469.152149023008],[245.16917842518484,469.152149023008],[245.16917842518484,478.855653023008],[65.26616574766484,478.855653023008]]},{"text":"他心直口快，","box":[[240.6085218416809,469.152149023008],[301.0613614651848,469.152149023008],[301.0613614651848,478.855653023008],[240.6085218416809,478.855653023008]]},{"text":"坚决反对招安。","box":[[296.50070488168086,469.152149023008],[367.6273697946729,469.152149023008],[367.6273697946729,478.855653023008],[296.50070488168086,478.855653023008]]},{"text":"《","box":[[65.26617545116892,484.483665936],[74.96967945116891,484.483665936],[74.96967945116891,494.18716993600003],[65.26617545116892,494.18716993600003]]},{"text":"水浒》","box":[[71.08827785116893,484.483665936],[100.78100009116895,484.483665936],[100.78100009116895,494.18716993600003],[71.08827785116893,494.18716993600003]]},{"text":"的一百二十回本中谈到李逵反招安，","box":[[96.99664323467294,484.483665936],[256.61932284868885,484.483665936],[256.61932284868885,494.18716993600003],[96.99664323467294,494.18716993600003]]},{"text":"要造反。但由于他对宋江","box":[[251.86462529569695,484.483665936],[362.581664156721,484.483665936],[362.581664156721,494.18716993600003],[251.86462529569695,494.18716993600003]]},{"text":"死也要跟着，","box":[[65.26622396868902,499.718186623008],[124.94278327219307,499.718186623008],[124.94278327219307,509.42169062300803],[65.26622396868902,509.42169062300803]]},{"text":"所以宋江接受招安，","box":[[120.18806631219304,499.718186623008],[209.848472382705,499.718186623008],[209.848472382705,509.42169062300803],[120.18806631219304,509.42169062300803]]},{"text":"他只好接受。","box":[[205.09375542270507,499.718186623008],[264.77031472620905,499.718186623008],[264.77031472620905,509.42169062300803],[205.09375542270507,509.42169062300803]]},{"text":"对宋江接受招安，","box":[[85.25548102270508,514.952687903008],[164.9212779732171,514.952687903008],[164.9212779732171,524.656191903008],[85.25548102270508,524.656191903008]]},{"text":"还要从当时的历史背景来分析。当时，","box":[[160.55468176620911,514.952687903008],[336.0911176437291,514.952687903008],[336.0911176437291,524.656191903008],[160.55468176620911,524.656191903008]]},{"text":"宋朝受","box":[[331.91864973774517,514.952687903008],[362.5817223777451,514.952687903008],[362.5817223777451,524.656191903008],[331.91864973774517,524.656191903008]]},{"text":"辽、","box":[[65.26631130022514,530.187198886512],[84.96442442022516,530.187198886512],[84.96442442022516,539.8907028865119],[65.26631130022514,539.8907028865119]]},{"text":"金等外寇入侵，","box":[[80.20970746022515,530.187198886512],[149.8808855872332,530.187198886512],[149.8808855872332,539.8907028865119],[80.20970746022515,539.8907028865119]]},{"text":"需要抵御外敌。宋江接受招安，","box":[[145.1261783307372,530.187198886512],[285.6329356577452,530.187198886512],[285.6329356577452,539.8907028865119],[145.1261783307372,539.8907028865119]]},{"text":"虽然反映了他的动","box":[[281.0722984812492,530.187198886512],[362.5817805987692,530.187198886512],[362.5817805987692,539.8907028865119],[281.0722984812492,539.8907028865119]]},{"text":"摇性，","box":[[65.26634041073726,545.518735206512],[94.95906265073728,545.518735206512],[94.95906265073728,555.222239206512],[65.26634041073726,555.222239206512]]},{"text":"但不能因此说他是两面派人物。金圣叹对宋江采取了憎恨仇视","box":[[90.20435539424128,545.518735206512],[362.5818388197931,545.518735206512],[362.5818388197931,555.222239206512],[90.20435539424128,555.222239206512]]}]}