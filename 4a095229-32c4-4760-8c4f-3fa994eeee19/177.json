,[357.0505892486884,530.187198886512],[357.0505892486884,539.8907028865119],[263.99397618518447,539.8907028865119]]},{"text":"很","box":[[352.87812134270445,530.187198886512],[362.58162534270446,530.187198886512],[362.58162534270446,539.8907028865119],[352.87812134270445,539.8907028865119]]},{"text":"香的白兰花，","box":[[65.26621426518443,545.5187352065119],[124.94277356868848,545.5187352065119],[124.94277356868848,555.2222392065119],[65.26621426518443,555.2222392065119]]},{"text":"交给一个穿军衣的护士，","box":[[120.18805660868846,545.5187352065119],[229.9347353662084,545.5187352065119],[229.9347353662084,555.2222392065119],[120.18805660868846,555.2222392065119]]},{"text":"让她先放在冰箱里，","box":[[225.37405937569648,545.5187352065119],[316.3929463027044,545.5187352065119],[316.3929463027044,555.2222392065119],[225.37405937569648,555.2222392065119]]},{"text":"让刘诗昆走","box":[[311.83228971920045,545.5187352065119],[362.58168356372846,545.5187352065119],[362.58168356372846,555.2222392065119],[311.83228971920045,555.2222392065119]]}]}