316.61300792198404]]},{"text":"９","box":[[303.583835847504,316.613007921984],[313.28733984750403,316.613007921984],[313.28733984750403,326.316511921984],[303.583835847504,326.316511921984]]},{"text":"时准时赶到","box":[[311.734798614512,316.613007921984],[362.581208092032,316.613007921984],[362.581208092032,326.316511921984],[311.734798614512,326.316511921984]]},{"text":"那里，","box":[[65.26576790400003,331.847528608992],[94.86144540049604,331.847528608992],[94.86144540049604,341.551032608992],[65.26576790400003,341.551032608992]]},{"text":"还提醒他“","box":[[90.10673814400003,331.847528608992],[139.78867862400006,331.847528608992],[139.78867862400006,341.551032608992],[90.10673814400003,341.551032608992]]},{"text":"要穿军装，","box":[[136.00432176750405,331.847528608992],[185.68628165451204,331.847528608992],[185.68628165451204,341.551032608992],[136.00432176750405,341.551032608992]]},{"text":"军容风纪要严整，","box":[[180.93156469451208,331.847528608992],[260.791412318016,331.847528608992],[260.791412318016,341.551032608992],[180.93156469451208,341.551032608992]]},{"text":"要告诉警卫人员，","box":[[256.2307751415201,331.847528608992],[337.06097316502394,331.847528608992],[337.06097316502394,341.551032608992],[256.2307751415201,341.551032608992]]},{"text":"你是师","box":[[332.50031658152005,331.847528608992],[362.5811983885281,331.847528608992],[362.5811983885281,341.551032608992],[332.50031658152005,341.551032608992]]},{"text":"部的，","box":[[65.26578731100807,347.179045521984],[94.95850955100809,347.179045521984],[94.95850955100809,356.882549521984],[65.26578731100807,356.882549521984]]},{"text":"在清华支左，","box":[[90.20380229451209,347.179045521984],[149.8803518945121,347.179045521984],[149.8803518945121,356.882549521984],[90.20380229451209,356.882549521984]]},{"text":"来这里送信，","box":[[145.12564463801613,347.179045521984],[204.80220394152008,347.179045521984],[204.80220394152008,356.882549521984],[145.12564463801613,356.882549521984]]},{"text":"他们会接待你的。","box":[[200.04750638852812,347.179045521984],[279.713264525024,347.179045521984],[279.713264525024,356.882549521984],[200.04750638852812,356.882549521984]]},{"text":"”","box":[[274.9585669720321,347.179045521984],[284.6620709720321,347.179045521984],[284.6620709720321,356.882549521984],[274.9585669720321,356.882549521984]]},{"text":"第二天，","box":[[85.25504436502408,362.413546801984],[124.9423854285281,362.413546801984],[124.9423854285281,372.117050801984],[85.25504436502408,372.117050801984]]},{"text":"惠宪钧衣帽整齐，","box":[[120.18766846852809,362.413546801984],[201.01788589904004,362.413546801984],[201.01788589904004,372.117050801984],[120.18766846852809,372.117050801984]]},{"text":"驱车准时赶到邓小平住宅门前。下车","box":[[196.74835384254408,362.413546801984],[362.58127601656,362.413546801984],[362.58127601656,372.117050801984],[196.74835384254408,372.117050801984]]},{"text":"后，","box":[[65.26586493904006,377.648086896],[84.96397805904007,377.648086896],[84.96397805904007,387.351590896],[65.26586493904006,387.351590896]]},{"text":"警卫人员礼貌地问了他姓名，","box":[[80.20926109904006,377.648086896],[212.17696401656008,377.648086896],[212.17696401656008,387.351590896],[80.20926109904006,387.351590896]]},{"text":"引他到接待室。一位排长热情地接","box":[[207.81037751305612,377.648086896],[362.5813439410881,377.648086896],[362.5813439410881,387.351590896],[207.81037751305612,387.351590896]]},{"text":"待了他，","box":[[65.26593286356814,392.882588176],[104.95326422356817,392.882588176],[104.95326422356817,402.586092176],[65.26593286356814,402.586092176]]},{"text":"并说副参谋长昨晚打过电话。惠宪钧表示感谢，","box":[[100.19855696707216,392.882588176],[315.3253085716001,392.882588176],[315.3253085716001,402.586092176],[100.19855696707216,402.586092176]]},{"text":"把信交给了","box":[[310.9587220680963,392.882588176],[362.5814021621122,392.882588176],[362.5814021621122,402.586092176],[310.9587220680963,402.586092176]]},{"text":"他。那位排长旋即送进院里去了。过了不久，","box":[[65.26599108459226,408.21412449599995],[265.25525704211213,408.21412449599995],[265.25525704211213,417.91762849599996],[65.26599108459226,417.91762849599996]]},{"text":"他满面笑容地回到接待","box":[[260.6946198656163,408.21412449599995],[362.5814894936482,408.21412449599995],[362.5814894936482,417.91762849599996],[260.6946198656163,417.91762849599996]]},{"text":"室，","box":[[65.2660104916003,423.448625776],[84.96412361160031,423.448625776],[84.96412361160031,433.152129776],[65.2660104916003,433.152129776]]},{"text":"对惠宪钧说：","box":[[80.11237161160031,423.448625776],[139.78893091510434,423.448625776],[139.78893091510434,433.152129776],[80.11237161160031,433.152129776]]},{"text":"“","box":[[135.03422365860834,423.448625776],[144.73772765860835,423.448625776],[144.73772765860835,433.152129776],[135.03422365860834,433.152129776]]},{"text":"王秘书正在忙，","box":[[140.95335139510433,423.448625776],[210.62453922561627,423.448625776],[210.62453922561627,433.152129776],[140.95335139510433,433.152129776]]},{"text":"让我转告清华的同志，","box":[[205.86980285860832,423.448625776],[306.49516844912023,423.448625776],[306.49516844912023,433.152129776],[205.86980285860832,433.152129776]]},{"text":"现在邓副主席","box":[[301.9345118656163,423.448625776],[362.58147979014433,423.448625776],[362.58147979014433,433.152129776],[301.9345118656163,433.152129776]]},{"text":"在中央开会，","box":[[65.26603960211236,438.683127056],[124.9425989056164,438.683127056],[124.9425989056164,448.386631056],[65.26603960211236,448.386631056]]},{"text":"中午回来就把信交给他。","box":[[120.18788194561638,438.683127056],[229.8375062561283,438.683127056],[229.8375062561283,448.386631056],[120.18788194561638,448.386631056]]},{"text":"”","box":[[225.0827892961284,438.683127056],[234.7862932961284,438.683127056],[234.7862932961284,448.386631056],[225.0827892961284,448.386631056]]},{"text":"惠宪钧这时感到是一块石头落了地，","box":[[85.2552869526244,453.91764774300793],[245.0720560536483,453.91764774300793],[245.0720560536483,463.62115174300794],[85.2552869526244,463.62115174300794]]},{"text":"想到邓小平今天就能看到他","box":[[240.51139947014443,453.91764774300793],[362.5816059356964,453.91764774300793],[362.5816059356964,463.62115174300794],[240.51139947014443,463.62115174300794]]},{"text":"们的信，","box":[[65.26608811963246,469.15214902300795],[104.95341947963249,469.15214902300795],[104.95341947963249,478.85565302300796],[65.26608811963246,478.85565302300796]]},{"text":"高兴得没法说了。","box":[[100.19871222313648,469.15214902300795],[179.86449947014447,469.15214902300795],[179.86449947014447,478.85565302300796],[100.19871222313648,478.85565302300796]]},{"text":"给毛泽东的信送出之后的第九天，","box":[[85.25532576664051,484.48366593599997],[234.8834059641604,484.48366593599997],[234.8834059641604,494.187169936],[85.25532576664051,494.187169936]]},{"text":"刘冰和惠宪钧按照原定计划到","box":[[230.32274938065652,484.48366593599997],[362.58161563920044,484.48366593599997],[362.58161563920044,494.187169936],[230.32274938065652,494.187169936]]},{"text":"北京市委，","box":[[65.2661366371525,499.71818662300797],[114.94808682065654,499.71818662300797],[114.94808682065654,509.421690623008],[65.2661366371525,509.421690623008]]},{"text":"向市委科教组组长肖英和军代表作了汇报。汇报结束后，","box":[[110.19336986065652,499.71818662300797],[367.6273989051844,499.71818662300797],[367.6273989051844,509.421690623008],[110.19336986065652,509.421690623008]]},{"text":"刘冰诚恳地说：","box":[[65.26620456168052,514.9526879030079],[134.9373729851846,514.9526879030079],[134.9373729851846,524.6561919030079],[65.26620456168052,524.6561919030079]]},{"text":"“","box":[[130.18265602518457,514.9526879030079],[139.88616002518458,514.9526879030079],[139.88616002518458,524.6561919030079],[130.18265602518457,524.6561919030079]]},{"text":"这件事对清华大学关系重大，","box":[[136.10180316868858,514.9526879030079],[269.0398273756965,514.9526879030079],[269.0398273756965,524.6561919030079],[136.10180316868858,524.6561919030079]]},{"text":"请你们向市委汇报，","box":[[264.7702953192006,514.9526879030079],[357.14766310270454,514.9526879030079],[357.14766310270454,524.6561919030079],[264.7702953192006,524.6561919030079]]},{"text":"我","box":[[352.8781698602246,514.9526879030079],[362.5816738602246,514.9526879030079],[362.5816738602246,524.6561919030079],[352.8781698602246,524.6561919030079]]},{"text":"们希望市委第一书记吴德能接见我们。","box":[[65.26626278270459,530.187198886512],[234.88356122022452,530.187198886512],[234.88356122022452,539.8907028865119],[65.26626278270459,539.8907028865119]]},{"text":"”","box":[[230.1288442602246,530.187198886512],[239.83234826022462,530.187198886512],[239.83234826022462,539.8907028865119],[230.1288442602246,539.8907028865119]]},{"text":"肖英也不无诚恳地说：","box":[[85.2555198367206,545.5187352065119],[184.91052532372856,545.5187352065119],[184.91052532372856,555.2222392065119],[85.2555198367206,555.2222392065119]]},{"text":"“","box":[[180.15580836372862,545.5187352065119],[189.85931236372863,545.5187352065119],[189.85931236372863,555.2222392065119],[180.15580836372862,555.2222392065119]]},{"text":"我们要把记录整理一下，","box":[[186.07495550723263,545.5187352065119],[296.3067803542406,545.5187352065119],[296.3067803542406,555.2222392065119],[186.07495550723263,555.2222392065119]]},{"text":"会马上向市委领","box":[[291.74612377073663,545.5187352065119],[362.58178059876866,545.5187352065119],[362.58178059876866,555.2222392065119],[291.74612377073663,555.2222392065119]]}]}