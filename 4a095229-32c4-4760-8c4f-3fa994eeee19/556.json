0827,384.925695488992],[191.21751181860824,384.925695488992],[191.21751181860824,394.629199488992],[161.13664941860827,394.629199488992]]},{"text":"的湖边，","box":[[187.72426008211227,384.925695488992],[227.99378227510425,384.925695488992],[227.99378227510425,394.629199488992],[187.72426008211227,394.629199488992]]},{"text":"大家下了吉普车，","box":[[223.53018013860827,384.925695488992],[305.33072856211226,384.925695488992],[305.33072856211226,394.629199488992],[223.53018013860827,394.629199488992]]},{"text":"县委朱书记和","box":[[300.9641226516003,384.925695488992],[362.5814506796322,384.925695488992],[362.5814506796322,394.629199488992],[300.9641226516003,394.629199488992]]},{"text":"社员们就像见了老朋友一样打着招呼。看得出来，","box":[[65.26603960211224,400.936496496],[285.82674374313615,400.936496496],[285.82674374313615,410.640000496],[65.26603960211224,410.640000496]]},{"text":"他和社员们都很熟","box":[[281.26608715963226,400.936496496],[362.5815089006562,400.936496496],[362.5815089006562,410.640000496],[281.26608715963226,410.640000496]]},{"text":"识。随后，","box":[[65.2660687126243,416.94725868899206],[114.85098385612834,416.94725868899206],[114.85098385612834,426.6507626889921],[65.2660687126243,426.6507626889921]]},{"text":"朱书记也随手拿起一个铁钩子，","box":[[110.09626689612831,416.94725868899206],[258.8510220301442,416.94725868899206],[258.8510220301442,426.6507626889921],[110.09626689612831,426.6507626889921]]},{"text":"非常熟练地和社员一起","box":[[255.06666517364832,416.94725868899206],[362.58138275510424,416.94725868899206],[362.58138275510424,426.6507626889921],[255.06666517364832,426.6507626889921]]},{"text":"拉网。","box":[[65.26612693364825,433.055094736],[94.95884917364828,433.055094736],[94.95884917364828,442.758598736],[65.26612693364825,442.758598736]]},{"text":"干了一个时辰后，","box":[[85.25534517364827,449.06587633600003],[164.92114212416024,449.06587633600003],[164.92114212416024,458.76938033600004],[85.25534517364827,458.76938033600004]]},{"text":"大家回到公社招待所休息。过了一会儿，","box":[[160.55454591715227,449.06587633600003],[346.5707855216802,449.06587633600003],[346.5707855216802,458.76938033600004],[160.55454591715227,458.76938033600004]]},{"text":"公社","box":[[342.3982788016803,449.06587633600003],[362.5815865286883,449.06587633600003],[362.5815865286883,458.76938033600004],[342.3982788016803,458.76938033600004]]},{"text":"书记来叫他们吃饭。到了食堂，","box":[[65.2661754511683,465.07667734300804],[204.8026017851843,465.07667734300804],[204.8026017851843,474.78018134300805],[65.2661754511683,474.78018134300805]]},{"text":"只见餐桌中间摆着一盘硕大肥美的鲤","box":[[200.0479042321923,465.07667734300804],[362.5817029707363,465.07667734300804],[362.5817029707363,474.78018134300805],[200.0479042321923,474.78018134300805]]},{"text":"鱼，","box":[[65.26618515467237,481.18449398300805],[84.96429827467239,481.18449398300805],[84.96429827467239,490.88799798300806],[65.26618515467237,490.88799798300806]]},{"text":"桌上还放着四个酒盅。县委朱书记走到桌前，","box":[[80.20958131467238,481.18449398300805],[280.6840127686883,481.18449398300805],[280.6840127686883,490.88799798300806],[80.20958131467238,490.88799798300806]]},{"text":"把酒盅摞起来，","box":[[276.1233949992004,481.18449398300805],[347.05600923920036,481.18449398300805],[347.05600923920036,490.88799798300806],[276.1233949992004,490.88799798300806]]},{"text":"放在","box":[[342.5924071027044,481.18449398300805],[362.5816350462084,481.18449398300805],[362.5816350462084,490.88799798300806],[342.5924071027044,490.88799798300806]]},{"text":"一边。公社邢书记说：","box":[[65.2662239686884,497.19526587950406],[164.8241944156964,497.19526587950406],[164.8241944156964,506.8987698795041],[65.2662239686884,506.8987698795041]]},{"text":"“","box":[[85.2554519121924,513.303101926512],[94.9589559121924,513.303101926512],[94.9589559121924,523.006605926512],[85.2554519121924,523.006605926512]]},{"text":"不是特意准备的，","box":[[91.1745990556964,513.303101926512],[170.84038630270442,513.303101926512],[170.84038630270442,523.006605926512],[91.1745990556964,523.006605926512]]},{"text":"赶上了，","box":[[166.08566934270445,513.303101926512],[205.7730104062084,513.303101926512],[205.7730104062084,523.006605926512],[166.08566934270445,523.006605926512]]},{"text":"喝两口吧。","box":[[201.01829344620845,513.303101926512],[250.7002339262084,513.303101926512],[250.7002339262084,523.006605926512],[201.01829344620845,523.006605926512]]},{"text":"”","box":[[245.94553637321644,513.303101926512],[255.64904037321645,513.303101926512],[255.64904037321645,523.006605926512],[245.94553637321644,523.006605926512]]},{"text":"县委书记不说话，","box":[[85.25549072620842,529.3138835265121],[164.92128767672042,529.3138835265121],[164.92128767672042,539.017387526512],[85.25549072620842,539.017387526512]]},{"text":"摇摇头。","box":[[160.16656101321644,529.3138835265121],[199.85390207672043,529.3138835265121],[199.85390207672043,539.017387526512],[160.16656101321644,539.017387526512]]},{"text":"公社邢书记又笑着对郭冰说：","box":[[85.25551983672044,545.3246651265121],[214.89436238723238,545.3246651265121],[214.89436238723238,555.028169126512],[85.25551983672044,555.028169126512]]},{"text":"“","box":[[210.13964542723247,545.3246651265121],[219.84314942723248,545.3246651265121],[219.84314942723248,555.028169126512],[210.13964542723247,555.028169126512]]},{"text":"大冷天，","box":[[216.05879257073647,545.3246651265121],[255.74613363424046,545.3246651265121],[255.74613363424046,555.028169126512],[216.05879257073647,555.028169126512]]},{"text":"喝点，","box":[[250.9914166742405,545.3246651265121],[280.6841389142405,545.3246651265121],[280.6841389142405,555.028169126512],[250.9914166742405,555.028169126512]]},{"text":"暖暖身子。","box":[[275.9294413612485,545.3246651265121],[325.61140124825647,545.3246651265121],[325.61140124825647,555.028169126512],[275.9294413612485,555.028169126512]]},{"text":"”","box":[[320.8566648812485,545.3246651265121],[330.5601688812485,545.3246651265121],[330.5601688812485,555.028169126512],[320.8566648812485,555.028169126512]]}]}