528992],[65.265806718016,494.187150528992]]},{"text":"若按正常程序，","box":[[160.166095245024,484.483646528992],[229.93432781904,484.483646528992],[229.93432781904,494.187150528992],[160.166095245024,494.187150528992]]},{"text":"这些信是要经汪东兴往上转的，","box":[[225.37365182852804,484.483646528992],[367.6270204685279,484.483646528992],[367.6270204685279,494.187150528992],[225.37365182852804,494.187150528992]]},{"text":"但汪东兴并没有转，","box":[[65.26582612502398,499.718167216],[154.92624189904,499.718167216],[154.92624189904,509.421671216],[65.26582612502398,509.421671216]]},{"text":"不知怎么“","box":[[150.171505532032,499.718167216],[199.853455715536,499.718167216],[199.853455715536,509.421671216],[150.171505532032,509.421671216]]},{"text":"弄到林贼那里去了”","box":[[196.06909885904003,499.718167216],[285.72949522604796,499.718167216],[285.72949522604796,509.421671216],[196.06909885904003,509.421671216]]},{"text":"。","box":[[281.94511896254403,499.718167216],[291.64862296254404,499.718167216],[291.64862296254404,509.421671216],[281.94511896254403,509.421671216]]},{"text":"这一天有关人员揭发出来的问题主要有：","box":[[85.25509288254403,514.952668496],[264.8670101435679,514.952668496],[264.8670101435679,524.656172496],[85.25509288254403,524.656172496]]},{"text":"将自己摆在不适当位置，","box":[[85.254986144,530.187160072496],[195.972005598016,530.187160072496],[195.972005598016,539.890664072496],[85.254986144,539.890664072496]]},{"text":"有事不请示汪东兴，","box":[[191.60541909451203,530.187160072496],[283.982825692032,530.187160072496],[283.982825692032,539.890664072496],[191.60541909451203,539.890664072496]]},{"text":"实质上是想将办公","box":[[279.6162391885281,530.187160072496],[362.581237202544,530.187160072496],[362.581237202544,539.890664072496],[279.6162391885281,539.890664072496]]},{"text":"厅的权抓到自己手里。没有批由王办的事，","box":[[65.26582612502403,545.5186963924959],[258.4626683930561,545.5186963924959],[258.4626683930561,555.2222003924959],[65.26582612502403,555.2222003924959]]},{"text":"王为什么经手，","box":[[254.09606248254414,545.5186963924959],[325.70795111305614,545.5186963924959],[325.70795111305614,555.2222003924959],[254.09606248254414,555.2222003924959]]},{"text":"应视为抓","box":[[321.4384190565602,545.5186963924959],[362.5812954235681,545.5186963924959],[362.5812954235681,555.2222003924959],[321.4384190565602,555.2222003924959]]}]}