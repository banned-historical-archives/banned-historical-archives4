6999999999995],[117.98,304.36999999999995]]},{"text":"这","box":[[132.02,290.3299999999999],[146.06,290.3299999999999],[146.06,304.36999999999995],[132.02,304.36999999999995]]},{"text":"......","box":[[146.06,290.3299999999999],[174.14,290.3299999999999],[174.14,304.36999999999995],[146.06,304.36999999999995]]},{"text":"好吗","box":[[174.02,290.3299999999999],[202.10000000000002,290.3299999999999],[202.10000000000002,304.36999999999995],[174.02,304.36999999999995]]},{"text":"?","box":[[202.01,290.3299999999999],[209.03,290.3299999999999],[209.03,304.36999999999995],[202.01,304.36999999999995]]},{"text":"为这种事","box":[[209.09,290.3299999999999],[265.12364,290.3299999999999],[265.12364,304.36999999999995],[209.09,304.36999999999995]]},{"text":"?”","box":[[265.13,290.3299999999999],[286.13,290.3299999999999],[286.13,304.36999999999995],[265.13,304.36999999999995]]},{"text":"周希汉","box":[[286.13,290.3299999999999],[328.12364,290.3299999999999],[328.12364,304.36999999999995],[286.13,304.36999999999995]]},{"text":"在","box":[[328.15,290.3299999999999],[342.19,290.3299999999999],[342.19,304.36999999999995],[328.15,304.36999999999995]]},{"text":"犹豫。","box":[[342.19,290.3299999999999],[384.18364,290.3299999999999],[384.18364,304.36999999999995],[342.19,304.36999999999995]]},{"text":"他","box":[[379.51,290.3299999999999],[393.55,290.3299999999999],[393.55,304.36999999999995],[379.51,304.36999999999995]]},{"text":"和周恩来是建国后","box":[[393.55,290.3299999999999],[505.4909200000001,290.3299999999999],[505.4909200000001,304.36999999999995],[393.55,304.36999999999995]]},{"text":"才熟悉起来的。","box":[[90.024,313.01],[188.17763999999997,313.01],[188.17763999999997,327.05],[90.024,327.05]]},{"text":"在","box":[[183.37595999999994,313.01],[197.41595999999993,313.01],[197.41595999999993,327.05],[183.37595999999994,327.05]]},{"text":"","box":[[197.41595999999993,313.01],[200.93,313.01],[200.93,313.01],[197.41595999999993,313.01]]},{"text":"1954","box":[[200.93,313.01],[228.9398,313.01],[228.9398,327.05],[200.93,327.05]]},{"text":"","box":[[228.93980000000005,313.01],[232.49,313.01],[232.49,313.01],[228.93980000000005,313.01]]},{"text":"年的一个国务院专题会议上","box":[[232.49,313.01],[400.46456,313.01],[400.46456,327.05],[232.49,327.05]]},{"text":",","box":[[400.51,313.01],[407.53,313.01],[407.53,327.05],[400.51,327.05]]},{"text":"周恩来对周希汉","box":[[407.47,313.01],[505.62364,313.01],[505.62364,327.05],[407.47,327.05]]},{"text":"能在很短时间里熟悉海军业务特别欣赏。","box":[[90.024,335.68999999999994],[341.98583999999994,335.68999999999994],[341.98583999999994,349.72999999999996],[90.024,349.72999999999996]]},{"text":"有一次在首都机场迎接外宾","box":[[337.3105200000001,335.68999999999994],[505.2850800000001,335.68999999999994],[505.2850800000001,349.72999999999996],[337.3105200000001,349.72999999999996]]},{"text":",","box":[[505.42,335.68999999999994],[512.44,335.68999999999994],[512.44,349.72999999999996],[505.42,349.72999999999996]]},{"text":"在向外宾介绍到周希汉时","box":[[90.024,358.48999999999995],[247.3421999999999,358.48999999999995],[247.3421999999999,372.53],[90.024,372.53]]},{"text":",","box":[[247.73,358.48999999999995],[254.75,358.48999999999995],[254.75,372.53],[247.73,372.53]]},{"text":"周恩来充满自豪地说","box":[[255.17,358.48999999999995],[383.83256000000006,358.48999999999995],[383.83256000000006,372.53],[255.17,372.53]]},{"text":":“","box":[[384.19,358.48999999999995],[405.31,358.48999999999995],[405.31,372.53],[384.19,372.53]]},{"text":"这是我们自己培","box":[[405.55,358.48999999999995],[505.55692000000005,358.48999999999995],[505.55692000000005,372.53],[405.55,372.53]]},{"text":"养出来的海军专家周希汉。","box":[[90.024,381.16999999999996],[258.1249199999999,381.16999999999996],[258.1249199999999,395.21],[90.024,395.21]]},{"text":"他的名字在我们中文里的意思是少有的英","box":[[253.3232399999999,381.16999999999996],[505.28507999999994,381.16999999999996],[505.28507999999994,395.21],[253.3232399999999,395.21]]},{"text":"雄。","box":[[90.024,403.86999999999995],[118.104,403.86999999999995],[118.104,417.90999999999997],[90.024,417.90999999999997]]},{"text":"”","box":[[118.1,403.86999999999995],[132.14,403.86999999999995],[132.14,417.90999999999997],[118.1,417.90999999999997]]},{"text":"由于一时想不到更好的办法","box":[[117.98,426.54999999999995],[289.6891999999999,426.54999999999995],[289.6891999999999,440.59],[117.98,440.59]]},{"text":",","box":[[290.09,426.54999999999995],[297.10999999999996,426.54999999999995],[297.10999999999996,440.59],[290.09,440.59]]},{"text":"周希汉决定听从妻子的建议","box":[[297.53,426.54999999999995],[469.2392,426.54999999999995],[469.2392,440.59],[297.53,440.59]]},{"text":",","box":[[469.78,426.54999999999995],[476.79999999999995,426.54999999999995],[476.79999999999995,440.59],[469.78,440.59]]},{"text":"打电","box":[[477.1,426.54999999999995],[505.54,426.54999999999995],[505.54,440.59],[477.1,440.59]]},{"text":"话给周恩来。","box":[[90.024,449.22999999999996],[174.13763999999998,449.22999999999996],[174.13763999999998,463.27],[90.024,463.27]]},{"text":"电话是通了","box":[[117.98,471.90999999999997],[188.05363999999997,471.90999999999997],[188.05363999999997,485.95],[117.98,485.95]]},{"text":",","box":[[188.09,471.90999999999997],[195.11,471.90999999999997],[195.11,485.95],[188.09,485.95]]},{"text":"可惜周恩来不在。","box":[[195.05,471.90999999999997],[307.11728,471.90999999999997],[307.11728,485.95],[195.05,485.95]]},{"text":"工作人员问有什么事需要转达的","box":[[302.44196000000005,471.90999999999997],[498.37016000000006,471.90999999999997],[498.37016000000006,485.95],[302.44196000000005,485.95]]},{"text":",","box":[[498.46,471.90999999999997],[505.47999999999996,471.90999999999997],[505.47999999999996,485.95],[498.46,485.95]]},{"text":"周希汉犹豫了一下说不用了","box":[[90.024,494.59],[258.1249199999999,494.59],[258.1249199999999,508.63],[90.024,508.63]]},{"text":",","box":[[258.05,494.59],[265.07,494.59],[265.07,508.63],[258.05,508.63]]},{"text":"就挂断了电话。","box":[[265.13,494.59],[363.15728,494.59],[363.15728,508.63],[265.13,508.63]]},{"text":"找不到周恩来","box":[[117.98,517.27],[202.09364,517.27],[202.09364,531.31],[117.98,531.31]]},{"text":",","box":[[202.01,517.27],[209.03,517.27],[209.03,531.31],[202.01,531.31]]},{"text":"周希汉又想起了徐向前","box":[[209.09,517.27],[349.11091999999996,517.27],[349.11091999999996,531.31],[209.09,531.31]]},{"text":"。","box":[[349.15,517.27],[363.19,517.27],[363.19,531.31],[349.15,531.31]]},{"text":"红军时期","box":[[358.38832,517.27],[414.54832,517.27],[414.54832,531.31],[358.38832,531.31]]},{"text":",","box":[[414.43,517.27],[421.45,517.27],[421.45,531.31],[414.43,531.31]]},{"text":"周希汉曾在徐","box":[[421.51,517.27],[505.49728000000005,517.27],[505.49728000000005,531.31],[421.51,531.31]]},{"text":"向前身边工作多年","box":[[90.024,540.0699999999999],[202.68096,540.0699999999999],[202.68096,554.1099999999999],[90.024,554.1099999999999]]},{"text":",","box":[[202.73,540.0699999999999],[209.75,540.0699999999999],[209.75,554.1099999999999],[202.73,554.1099999999999]]},{"text":"建立了深厚的友谊。建国后","box":[[209.69,540.0699999999999],[378.61928,540.0699999999999],[378.61928,554.1099999999999],[209.69,554.1099999999999]]},{"text":",","box":[[378.67,540.0699999999999],[385.69,540.0699999999999],[385.69,554.1099999999999],[378.67,554.1099999999999]]},{"text":"他还常去看徐向前","box":[[385.87,540.0699999999999],[498.30232,540.0699999999999],[498.30232,554.1099999999999],[385.87,554.1099999999999]]},{"text":",","box":[[498.46,540.0699999999999],[505.47999999999996,540.0699999999999],[505.47999999999996,554.1099999999999],[498.46,554.1099999999999]]},{"text":"却从未为个人的事向徐帅开过口。倒是徐向前似乎早有所料","box":[[90.024,562.75],[456.18720000000013,562.75],[456.18720000000013,576.79],[90.024,576.79]]},{"text":",","box":[[456.34,562.75],[463.35999999999996,562.75],[463.35999999999996,576.79],[456.34,576.79]]},{"text":"当电话","box":[[463.42,562.75],[505.65232000000003,562.75],[505.65232000000003,576.79],[463.42,576.79]]},{"text":"接通后","box":[[90.024,585.46],[132.71964,585.46],[132.71964,599.5],[90.024,599.5]]},{"text":",","box":[[132.98,585.46],[140,585.46],[140,599.5],[132.98,599.5]]},{"text":"徐","box":[[140.3,585.46],[154.34,585.46],[154.34,599.5],[140.3,599.5]]},{"text":"向前","box":[[154.7,585.46],[183.01999999999998,585.46],[183.01999999999998,599.5],[154.7,599.5]]},{"text":"镇定而关切地问","box":[[183.26,585.46],[283.2669199999999,585.46],[283.2669199999999,599.5],[183.26,599.5]]},{"text":":“","box":[[283.61,585.46],[304.61,585.46],[304.61,599.5],[283.61,599.5]]},{"text":"周希汉","box":[[304.97,585.46],[347.57000000000005,585.46],[347.57000000000005,599.5],[304.97,599.5]]},{"text":"?","box":[[347.83,585.46],[354.84999999999997,585.46],[354.84999999999997,599.5],[347.83,599.5]]},{"text":"怎么不讲话","box":[[355.27,585.46],[426.49492000000004,585.46],[426.49492000000004,599.5],[355.27,599.5]]},{"text":"?","box":[[426.79,585.46],[433.81,585.46],[433.81,599.5],[426.79,599.5]]},{"text":"遇到什么问","box":[[434.11,585.46],[505.46128000000004,585.46],[505.46128000000004,599.5],[434.11,599.5]]},{"text":"题了","box":[[90.024,608.14],[118.104,608.14],[118.104,622.18],[90.024,622.18]]},{"text":"?”","box":[[118.1,608.14],[139.1,608.14],[139.1,622.18],[118.1,622.18]]},{"text":"“","box":[[114.02,630.8199999999999],[128.06,630.8199999999999],[128.06,644.8599999999999],[114.02,644.8599999999999]]},{"text":"是。他们要开我的批斗会。人都拥到我的院子里了。我","box":[[128.06,630.8199999999999],[463.1667200000001,630.8199999999999],[463.1667200000001,644.8599999999999],[128.06,644.8599999999999]]},{"text":"......”","box":[[463.3,630.8199999999999],[505.42,630.8199999999999],[505.42,644.8599999999999],[463.3,644.8599999999999]]},{"text":"周希汉欲言又止。","box":[[90.024,653.5],[202.09127999999995,653.5],[202.09127999999995,667.54],[90.024,667.54]]},{"text":"徐向前没等周希汉说完","box":[[114.02,676.18],[256.10479999999995,676.18],[256.10479999999995,690.2199999999999],[114.02,690.2199999999999]]},{"text":",","box":[[256.49,676.18],[263.51,676.18],[263.51,690.2199999999999],[256.49,690.2199999999999]]},{"text":"就直接打断他说","box":[[263.69,676.18],[362.98088000000007,676.18],[362.98088000000007,690.2199999999999],[263.69,690.2199999999999]]},{"text":":“","box":[[363.31,676.18],[384.43,676.18],[384.43,690.2199999