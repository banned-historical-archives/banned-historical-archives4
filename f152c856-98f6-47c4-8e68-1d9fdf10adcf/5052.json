.9000000000001]]},{"text":"汝其","box":[[430.7,612.7],[461.09999999999997,612.7],[461.09999999999997,627.9000000000001],[430.7,627.9000000000001]]},{"text":"直接向","box":[[460.72,612.7],[505.9248,612.7],[505.9248,627.9000000000001],[460.72,627.9000000000001]]},{"text":"程","box":[[90.05,641.9],[105.25,641.9],[105.25,657.1],[90.05,657.1]]},{"text":"世清","box":[[105.27,641.9],[135.67,641.9],[135.67,657.1],[105.27,657.1]]},{"text":"禀报，但提醒说：","box":[[135.7,641.9],[256.50960000000003,641.9],[256.50960000000003,657.1],[135.7,657.1]]},{"text":"“程政委听完汇报后，会询问你对问题","box":[[248.90959999999993,641.9],[505.72879999999986,641.9],[505.72879999999986,657.1],[248.90959999999993,657.1]]},{"text":"的看法以及解决的办法。","box":[[90.05,671.12],[255.66920000000005,671.12],[255.66920000000005,686.32],[90.05,686.32]]},{"text":"”","box":[[247.67399999999995,671.12],[262.87399999999997,671.12],[262.87399999999997,686.32],[247.67399999999995,686.32]]},{"text":"程","box":[[120.07,700.33],[135.26999999999998,700.33],[135.26999999999998,715.5300000000001],[120.07,715.5300000000001]]},{"text":"世清","box":[[135.3,700.33],[165.70000000000002,700.33],[165.70000000000002,715.5300000000001],[135.3,715.5300000000001]]},{"text":"听完","box":[[165.73,700.33],[196.13,700.33],[196.13,715.5300000000001],[165.73,715.5300000000001]]},{"text":"汝其的汇","box":[[196.13,700.33],[256.5348,700.33],[256.5348,715.5300000000001],[196.13,715.5300000000001]]},{"text":"报后，果然问他：","box":[[256.58,700.33],[377.38960000000003,700.33],[377.38960000000003,715.5300000000001],[256.58,715.5300000000001]]},{"text":"“你对这个问题怎么","box":[[369.7895999999999,700.33],[505.7991999999999,700.33],[505.7991999999999,715.5300000000001],[369.7895999999999,715.5300000000001]]},{"text":"看","box":[[90.05,729.55],[105.25,729.55],[105.25,744.75],[90.05,744.75]]},{"text":"?","box":[[105.27,729.55],[112.86999999999999,729.55],[112.86999999999999,744.75],[105.27,744.75]]},{"text":"”","box":[[112.87,729.55],[128.07,729.55],[128.07,744.75],[112.87,744.75]]},{"text":"“随意枪毙人，这是一件严重的违法事件，是绝对不能容","box":[[120.07,758.775],[490.91959999999995,758.775],[490.91959999999995,773.975],[120.07,773.975]]},{"text":"忍","box":[[490.73,758.775],[505.93,758.775],[505.93,773.975],[490.73,773.975]]}]}