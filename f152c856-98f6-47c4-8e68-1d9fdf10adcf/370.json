5.45999999999995]]},{"text":"，对“红三司”更是迎头一闷棍，安文江心里","box":[[229.97,131.41999999999996],[505.49096,131.41999999999996],[505.49096,145.45999999999995],[229.97,145.45999999999995]]},{"text":"咯噔一下，他","box":[[90.024,154.0999999999999],[172.94423999999998,154.0999999999999],[172.94423999999998,168.1399999999999],[90.024,168.1399999999999]]},{"text":"想到了完蛋","box":[[172.94,154.0999999999999],[243.01363999999998,154.0999999999999],[243.01363999999998,168.1399999999999],[172.94,168.1399999999999]]},{"text":"、","box":[[243.05,154.0999999999999],[257.09000000000003,154.0999999999999],[257.09000000000003,168.1399999999999],[243.05,168.1399999999999]]},{"text":"垮台","box":[[255.89,154.0999999999999],[283.84999999999997,154.0999999999999],[283.84999999999997,168.1399999999999],[255.89,168.1399999999999]]},{"text":"、","box":[[283.85,154.0999999999999],[297.89000000000004,154.0999999999999],[297.89000000000004,168.1399999999999],[283.85,168.1399999999999]]},{"text":"坐牢","box":[[296.69,154.0999999999999],[324.65,154.0999999999999],[324.65,168.1399999999999],[296.69,168.1399999999999]]},{"text":"......","box":[[324.67,154.0999999999999],[352.75,154.0999999999999],[352.75,168.1399999999999],[324.67,168.1399999999999]]},{"text":"甚至想到了潜逃。","box":[[352.75,154.0999999999999],[464.69092,154.0999999999999],[464.69092,168.1399999999999],[352.75,168.1399999999999]]},{"text":"但面子","box":[[463.54,154.0999999999999],[505.53364000000005,154.0999999999999],[505.53364000000005,168.1399999999999],[463.54,168.1399999999999]]},{"text":"上的工作还是要做的。","box":[[90.024,176.89999999999998],[230.04491999999993,176.89999999999998],[230.04491999999993,190.93999999999997],[90.024,190.93999999999997]]},{"text":"他","box":[[227.1667199999999,176.89999999999998],[241.2067199999999,176.89999999999998],[241.2067199999999,190.93999999999997],[227.1667199999999,190.93999999999997]]},{"text":"装得镇定自若，","box":[[241.13,176.89999999999998],[339.15728,176.89999999999998],[339.15728,190.93999999999997],[241.13,190.93999999999997]]},{"text":"一面电令复","box":[[336.31,176.89999999999998],[406.25728000000004,176.89999999999998],[406.25728000000004,190.93999999999997],[336.31,190.93999999999997]]},{"text":"旦、","box":[[406.27,176.89999999999998],[434.22364,176.89999999999998],[434.22364,190.93999999999997],[406.27,190.93999999999997]]},{"text":"交大、","box":[[431.21908,176.89999999999998],[473.33908,176.89999999999998],[473.33908,190.93999999999997],[431.21908,190.93999999999997]]},{"text":"师院、","box":[[470.33452000000005,176.89999999999998],[512.32816,176.89999999999998],[512.32816,190.93999999999997],[470.33452000000005,190.93999999999997]]},{"text":"政法学院等派人增援，","box":[[90.024,199.57999999999993],[230.04491999999993,199.57999999999993],[230.04491999999993,213.61999999999992],[90.024,213.61999999999992]]},{"text":"一边请已在公安局内的其他组织的头头和代表","box":[[225.3695999999999,199.57999999999993],[505.28508,199.57999999999993],[505.28508,213.61999999999992],[225.3695999999999,213.61999999999992]]},{"text":"近","box":[[90.024,222.26],[104.064,222.26],[104.064,236.29999999999998],[90.024,236.29999999999998]]},{"text":"","box":[[104.064,222.26],[107.54,222.26],[107.54,222.26],[104.064,222.26]]},{"text":"50","box":[[107.54,222.26],[121.64000000000001,222.26],[121.64000000000001,236.29999999999998],[107.54,236.29999999999998]]},{"text":"","box":[[121.64,222.26],[125.06,222.26],[125.06,222.26],[121.64,222.26]]},{"text":"人到会议室，向他们介绍情况。","box":[[125.06,222.26],[321.11456,222.26],[321.11456,236.29999999999998],[125.06,236.29999999999998]]},{"text":"介绍会很快变成辩论会，大家唇枪舌剑争得十分激烈。政法“红","box":[[117.98,244.9699999999999],[505.2874400000002,244.9699999999999],[505.2874400000002,259.00999999999993],[117.98,259.00999999999993]]},{"text":"三司”和公安局内的造反派拿出关于上海市公安局局长黄赤波、杜蔚","box":[[90.024,267.65],[505.2850800000002,267.65],[505.2850800000002,281.69],[90.024,281.69]]},{"text":"然从政治言论到生活作风的一些材料及他们按照上海市委的指示执","box":[[90.024,290.3299999999999],[505.24296000000027,290.3299999999999],[505.24296000000027,304.36999999999995],[90.024,304.36999999999995]]},{"text":"行资产阶级反动路线的证据，","box":[[90.024,313.01],[272.16491999999994,313.01],[272.16491999999994,327.05],[90.024,327.05]]},{"text":"证明接管上海市公安局是正确的。","box":[[266.16983999999997,313.01],[476.13804,313.01],[476.13804,327.05],[266.16983999999997,327.05]]},{"text":"于是，","box":[[470.25528000000037,313.01],[512.2489200000003,313.01],[512.2489200000003,327.05],[470.25528000000037,327.05]]},{"text":"“红三司”在","box":[[90.024,335.68999999999994],[174.13763999999998,335.68999999999994],[174.13763999999998,349.72999999999996],[90.024,349.72999999999996]]},{"text":"","box":[[174.13763999999995,335.68999999999994],[178.7,335.68999999999994],[178.7,335.68999999999994],[174.13763999999995,335.68999999999994]]},{"text":"4","box":[[178.7,335.68999999999994],[185.72,335.68999999999994],[185.72,349.72999999999996],[178.7,349.72999999999996]]},{"text":"","box":[[185.72,335.68999999999994],[190.49,335.68999999999994],[190.49,335.68999999999994],[185.72,335.68999999999994]]},{"text":"小时的辩论中始终占着上风。","box":[[190.49,335.68999999999994],[372.3782,335.68999999999994],[372.3782,349.72999999999996],[190.49,349.72999999999996]]},{"text":"“工总司”的一位代表","box":[[365.4143600000002,335.68999999999994],[505.43528000000026,335.68999999999994],[505.43528000000026,349.72999999999996],[365.4143600000002,349.72999999999996]]},{"text":"当场撕了“工总司”的证件，表态说：","box":[[90.024,358.48999999999995],[328.07219999999995,358.48999999999995],[328.07219999999995,372.53],[90.024,372.53]]},{"text":"“我支持你们！","box":[[320.99604000000005,358.48999999999995],[419.02332000000007,358.48999999999995],[419.02332000000007,372.53],[320.99604000000005,372.53]]},{"text":"”","box":[[412.05948000000024,358.48999999999995],[426.09948000000026,358.48999999999995],[426.09948000000026,372.53],[412.05948000000024,372.53]]},{"text":"财政厅“财革会”头头对“红三司”头头安文江说：","box":[[117.98,381.16999999999996],[441.9108800000001,381.16999999999996],[441.9108800000001,395.21],[117.98,395.21]]},{"text":"“我们","box":[[434.9470400000003,381.16999999999996],[477.0670400000003,381.16999999999996],[477.0670400000003,395.21],[434.9470400000003,395.21]]},{"text":"和你","box":[[477.34,381.16999999999996],[505.41999999999996,381.16999999999996],[505.41999999999996,395.21],[477.34,395.21]]},{"text":"们站在一起。","box":[[90.024,403.86999999999995],[174.13763999999998,403.86999999999995],[174.13763999999998,417.90999999999997],[90.024,417.90999999999997]]},{"text":"”","box":[[167.06147999999996,403.86999999999995],[181.10147999999995,403.86999999999995],[181.10147999999995,417.90999999999997],[167.06147999999996,417.90999999999997]]},{"text":"首都红卫兵驻沪代表表示：","box":[[117.98,426.54999999999995],[285.95455999999996,426.54999999999995],[285.95455999999996,440.59],[117.98,440.59]]},{"text":"“北京红卫兵愿做你们后盾！","box":[[278.99072,426.54999999999995],[461.00528,426.54999999999995],[461.00528,440.59],[278.99072,440.59]]},{"text":"”","box":[[454.04144000000036,426.54999999999995],[468.0814400000004,426.54999999999995],[468.0814400000004,440.59],[454.04144000000036,440.59]]},{"text":"公安局大院里挤满了“红三司”队员。隆冬子夜，北风凄厉，红","box":[[117.98,449.22999999999996],[505.44188000000014,449.22999999999996],[505.44188000000014,463.27],[117.98,463.27]]},{"text":"卫兵唱着如泣如诉的歌：","box":[[90.024,471.90999999999997],[244.08491999999993,471.90999999999997],[244.08491999999993,485.95],[90.024,485.95]]},{"text":"“抬头望见北斗星，心中想念毛泽东......”","box":[[237.0087599999999,471.90999999999997],[503.01059999999995,471.90999999999997],[503.01059999999995,485.95],[237.0087599999999,485.95]]},{"text":"当","box":[[117.98,494.59],[132.02,494.59],[132.02,508.63],[117.98,508.63]]},{"text":"晚，","box":[[132.02,494.59],[160.10000000000002,494.59],[160.10000000000002,508.63],[132.02,508.63]]},{"text":"《人民日报》","box":[[152.18,494.59],[236.16727999999998,494.59],[236.16727999999998,508.63],[152.18,508.63]]},{"text":"、","box":[[229.09111999999996,494.59],[243.13111999999995,494.59],[243.13111999999995,508.63],[229.09111999999996,508.63]]},{"text":"《红旗》杂志","box":[[235.08619999999996,494.59],[318.24511999999993,494.59],[318.24511