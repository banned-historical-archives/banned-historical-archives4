9.00999999999993],[117.98,259.00999999999993]]},{"text":"张春桥和姚文元","box":[[157.7,244.9699999999999],[255.72727999999995,244.9699999999999],[255.72727999999995,259.00999999999993],[157.7,259.00999999999993]]},{"text":"已于当天上午飞往","box":[[255.77,244.9699999999999],[367.71092000000004,244.9699999999999],[367.71092000000004,259.00999999999993],[255.77,259.00999999999993]]},{"text":"上海，他们降落在上海","box":[[367.75,244.9699999999999],[505.5104799999999,244.9699999999999],[505.5104799999999,259.00999999999993],[367.75,259.00999999999993]]},{"text":"机场","box":[[90.024,267.65],[118.104,267.65],[118.104,281.69],[90.024,281.69]]},{"text":"后","box":[[118.1,267.65],[132.14,267.65],[132.14,281.69],[118.1,281.69]]},{"text":"，就直奔兴国路上的兴国招待所。","box":[[132.14,267.65],[341.9818399999999,267.65],[341.9818399999999,281.69],[132.14,281.69]]},{"text":"放下","box":[[117.98,290.3299999999999],[146.06,290.3299999999999],[146.06,304.36999999999995],[117.98,304.36999999999995]]},{"text":"电话不久，","box":[[146.06,290.3299999999999],[216.13363999999999,290.3299999999999],[216.13363999999999,304.36999999999995],[146.06,304.36999999999995]]},{"text":"张春桥留在上海的秘书何秀文就带","box":[[211.33195999999995,290.3299999999999],[421.30015999999995,290.3299999999999],[421.30015999999995,304.36999999999995],[211.33195999999995,304.36999999999995]]},{"text":"着","box":[[421.39,290.3299999999999],[435.43,290.3299999999999],[435.43,304.36999999999995],[421.39,304.36999999999995]]},{"text":"市委警卫处","box":[[435.43,290.3299999999999],[505.37728000000004,290.3299999999999],[505.37728000000004,304.36999999999995],[435.43,304.36999999999995]]},{"text":"的人员来检查","box":[[90.024,313.01],[174.13763999999998,313.01],[174.13763999999998,327.05],[90.024,327.05]]},{"text":"武康路","box":[[174.14,313.01],[216.01999999999998,313.01],[216.01999999999998,327.05],[174.14,327.05]]},{"text":"","box":[[216.01999999999995,313.01],[219.53,313.01],[219.53,313.01],[216.01999999999995,313.01]]},{"text":"2","box":[[219.53,313.01],[226.55,313.01],[226.55,327.05],[219.53,327.05]]},{"text":"","box":[[226.55,313.01],[230.09,313.01],[230.09,313.01],[226.55,313.01]]},{"text":"号的","box":[[230.09,313.01],[258.17,313.01],[258.17,327.05],[230.09,327.05]]},{"text":"会场。","box":[[258.05,313.01],[300.17,313.01],[300.17,327.05],[258.05,327.05]]},{"text":"1","box":[[117.98,335.68999999999994],[125,335.68999999999994],[125,349.72999999999996],[117.98,349.72999999999996]]},{"text":"","box":[[125,335.68999999999994],[128.54,335.68999999999994],[128.54,335.68999999999994],[125,335.68999999999994]]},{"text":"月","box":[[128.54,335.68999999999994],[142.57999999999998,335.68999999999994],[142.57999999999998,349.72999999999996],[128.54,349.72999999999996]]},{"text":"","box":[[142.57999999999998,335.68999999999994],[146.06,335.68999999999994],[146.06,335.68999999999994],[142.57999999999998,335.68999999999994]]},{"text":"4","box":[[146.06,335.68999999999994],[153.08,335.68999999999994],[153.08,349.72999999999996],[146.06,349.72999999999996]]},{"text":"","box":[[153.08,335.68999999999994],[156.62,335.68999999999994],[156.62,335.68999999999994],[153.08,335.68999999999994]]},{"text":"日","box":[[156.62,335.68999999999994],[170.66,335.68999999999994],[170.66,349.72999999999996],[156.62,349.72999999999996]]},{"text":"下午两点多钟，穿着军装、披着军大衣的张春桥和姚文","box":[[170.54,335.68999999999994],[505.2957200000001,335.68999999999994],[505.2957200000001,349.72999999999996],[170.54,349.72999999999996]]},{"text":"元","box":[[90.024,358.48999999999995],[104.064,358.48999999999995],[104.064,372.53],[90.024,372.53]]},{"text":"来到武康路","box":[[104.06,358.48999999999995],[174.13363999999999,358.48999999999995],[174.13363999999999,372.53],[104.06,372.53]]},{"text":"","box":[[174.13363999999996,358.48999999999995],[177.62,358.48999999999995],[177.62,358.48999999999995],[174.13363999999996,358.48999999999995]]},{"text":"2","box":[[177.62,358.48999999999995],[184.64000000000001,358.48999999999995],[184.64000000000001,372.53],[177.62,372.53]]},{"text":"","box":[[184.64000000000001,358.48999999999995],[188.09,358.48999999999995],[188.09,358.48999999999995],[184.64000000000001,358.48999999999995]]},{"text":"号的小楼，徐景贤、朱永嘉、郭仁杰、王承龙、","box":[[188.09,358.48999999999995],[478.47932000000003,358.48999999999995],[478.47932000000003,372.53],[188.09,372.53]]},{"text":"程绮","box":[[477.58,358.48999999999995],[505.53999999999996,358.48999999999995],[505.53999999999996,372.53],[477.58,372.53]]},{"text":"华、吴瑞武、王知常","box":[[90.024,381.16999999999996],[213.74447999999995,381.16999999999996],[213.74447999999995,395.21],[90.024,395.21]]},{"text":"等一班秀才早已等候在那里。","box":[[213.77,381.16999999999996],[395.78456,381.16999999999996],[395.78456,395.21],[213.77,395.21]]},{"text":"张春桥、姚文元热","box":[[394.63,381.16999999999996],[505.50388,381.16999999999996],[505.50388,395.21],[394.63,395.21]]},{"text":"情地和","box":[[90.024,403.86999999999995],[132.144,403.86999999999995],[132.144,417.90999999999997],[90.024,417.90999999999997]]},{"text":"他们","box":[[132.14,403.86999999999995],[160.09999999999997,403.86999999999995],[160.09999999999997,417.90999999999997],[132.14,417.90999999999997]]},{"text":"一一握手。大家围着长桌子坐","box":[[160.1,403.86999999999995],[340.42976,403.86999999999995],[340.42976,417.90999999999997],[160.1,417.90999999999997]]},{"text":"下，张春桥首先开了腔","box":[[340.51,403.86999999999995],[478.97248,403.86999999999995],[478.97248,417.90999999999997],[340.51,417.90999999999997]]},{"text":"，他","box":[[478.9,403.86999999999995],[505.41999999999996,403.86999999999995],[505.41999999999996,417.90999999999997],[478.9,417.90999999999997]]},{"text":"说","box":[[90.024,426.54999999999995],[104.064,426.54999999999995],[104.064,440.59],[90.024,440.59]]},{"text":"：","box":[[104.06,426.54999999999995],[118.1,426.54999999999995],[118.1,440.59],[104.06,440.59]]},{"text":"“","box":[[117.98,449.22999999999996],[132.02,449.22999999999996],[132.02,463.27],[117.98,463.27]]},{"text":"写作班的造反在上海，","box":[[132.02,449.22999999999996],[272.0409199999999,449.22999999999996],[272.0409199999999,463.27],[132.02,463.27]]},{"text":"在全国的影响都比较大，","box":[[266.15815999999995,449.22999999999996],[420.21907999999996,449.22999999999996],[420.21907999999996,463.27],[266.15815999999995,463.27]]},{"text":"推动了全国省、","box":[[414.2240000000003,449.22999999999996],[512.2512800000003,449.22999999999996],[512.2512800000003,463.27],[414.2240000000003,463.27]]},{"text":"市一级的党政机关干部起来造反。最近，中央文革正在起草党政机关","box":[[90.024,471.90999999999997],[505.4114400000002,471.90999999999997],[505.4114400000002,485.95],[90.024,485.95]]},{"text":"文化大革命的若干条，里面也吸收了你们的经验。毛主席在接见中央","box":[[90.024,494.59],[505.4114400000002,494.59],[505.4114400000002,508.63],[90.024,508.63]]},{"text":"文革的成员时，肯定了你们的造反，文元已经给你们传达过了，所以","box":[[90.024,517.27],[505.2850800000002,517.27],[505.2850800000002,531.31],[90.024,531.31]]},{"text":"今天我们想先见见你们","box":[[90.024,540.0699999999999],[230.04491999999993,540.0699999999999],[230.04491999999993,554.1099999999999],[90.024,554.1099999999999]]},{"text":"......","box":[[230.09,540.0699999999999],[258.17,540.0699999999999],[258.17,554.1099999999999],[230.09,554.1099999999999]]},{"text":"”","box":[[258.05,540.0699999999999],[272.09000000000003,540.0699999999999],[272.09000000000003,554.1099999999999],[258.05,554.1099999999999]]},{"text":"张春桥说到这里，略为停顿一下，脱下军帽放在桌上，用手抚了","box":[[117.98,562.75],[505.2874400000002,562.75],[505.2874400000002,576.79],[117.98,576.79]]},{"text":"一下他那稍带卷曲的头发，慢悠悠地点了一支烟，接着往下说：","box":[[90.024,585.46],[484.3795200000002,585.46],[484.3795200000002,599.5],[90.024,599.5]]},{"text":"“","box":[[477.46,585.46],[491.5,585.46],[491.5,599.5],[477.46,599.5]]},{"text":"这","box":[[491.62,585.46],[505.66,585.46],[505.66,599.5],[491.62,599.5]]},{"text":"次我和文元到上海来，是中央文革派我们来的。我们两个来了当然是","box":[[90.024,608.14],[505.42548000000016,608.14],[505.42548000000016,622.18],[90.024,622.18]]},{"text":"和你们站在一起的，","box":[[90.024,630.8199999999999],[216.83328,630.8199999999999],[216.83328,644.8599999999999],[90.024,644.8599999999999]]},{"text":"‘","box":[[209.81,630.8199999999999],[223.85,630.8199999999999],[223.85,644.8599999999999],[209.81,644.8599999999999]]},{"text":"联络站","box":[[223.97,630.8199999999999],[266.09,630.8199999999999],[266.09,644.8599999999999],[223.97,644.8599999999999]]},{"text":"’","box":[[266.21,630.8199999999999],[280.25,630.8199999999999],[280.25,644.8599999999999],[266.21,644.8599999999999]]},{"text":"就作为我们的","box":[[280.25,630.8199999999999],[364.71464000000003,630.8199999999999],[364.71464000000003,644.8599999999999],[280.25,644.8599999999999]]},{"text":"工作机构。我们到上海","box":[[364.75,630.8199999999999],[505.59928,630.8199999999999],[505.59928,644.8599999999999],[364.75,644.8599999999999]]},{"text":"来的任务，是做调查研究，然后向中央报告","box":[[90.024,653.5],[352.9089599999999,653.5],[352.9089599999999,667.54],[90.024,667.54]]},{"text":"。","box":[[352.99,653.5],[367.03000000000003,653.5],[367.03000000000003,667.54],[352.99,667.54]]},{"text":"所以我们不想介入上海","box":[[365.35,653.5],[505.37092000000007,653.5],[505.37092000000007,667.54],[365.35,667.54]]},{"text":"的具体活动，同时，我们到上海来的消息，暂时也不想公开。你们的","box":[[90.024,676.18],[505.5658800000002,676.18],[505.5658800000002,690.2199999999999],[90.024,690.2199999999999]]},{"text":"工作还是按照原来的计划进行，不必等我们。","box":[[90.024,698.8599999999999],[369.93948,698.8599999999999],[369.93948,712.8999999999999],[90.024,712.8999999999999]]},{"text":"”","box":[[363.07,698.8599999999999],[377.11,698.8599999999999],[377.11,712.8999999999999],[363.07,712.8999999999999]]},{"text":"这时，姚文元说：","box":[[117.98,721.66],[228.48883999999998,721.66],[228.48883999999998,735.6999999999999],[117.98,735.6999999999999]]},{"text":"“","box":[[226.85,721.66],[240.89,721.66],[240.89,735.6999999999999],[226.85,735.6999999999999]]},{"text":"你们讲一讲最近有些什么动向，那个大会打","box":[[240.89,721.66],[505.3334,721.66],[505.3334,735.6999999999999],[240.89,735.6999999999999]]},{"text":"算什么时候开？","box":[[90.024,744.3599999999999],[188.17763999999997,744.3599999999999],[188.17763999999997,758.3999999999999],[90.024,758.3999999999999]]},{"text":"”","box":[[188.09,744.3599999999999],[202.13,744.3599999999999],[202.13,758.3999999999999],[188.09,758.3999999999999]]}]}