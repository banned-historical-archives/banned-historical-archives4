7.78]]},{"text":"","box":[[186.5,123.74000000000001],[190.61,123.74000000000001],[190.61,123.74000000000001],[186.5,123.74000000000001]]},{"text":"16","box":[[190.61,123.74000000000001],[204.59,123.74000000000001],[204.59,137.78],[190.61,137.78]]},{"text":"","box":[[204.59000000000003,123.74000000000001],[208.73,123.74000000000001],[208.73,123.74000000000001],[204.59000000000003,123.74000000000001]]},{"text":"时","box":[[208.73,123.74000000000001],[222.76999999999998,123.74000000000001],[222.76999999999998,137.78],[208.73,137.78]]},{"text":"","box":[[222.76999999999998,123.74000000000001],[226.97,123.74000000000001],[226.97,123.74000000000001],[222.76999999999998,123.74000000000001]]},{"text":"25","box":[[226.97,123.74000000000001],[240.95,123.74000000000001],[240.95,137.78],[226.97,137.78]]},{"text":"","box":[[240.95000000000002,123.74000000000001],[245.21,123.74000000000001],[245.21,123.74000000000001],[240.95000000000002,123.74000000000001]]},{"text":"分，","box":[[245.21,123.74000000000001],[273.17,123.74000000000001],[273.17,137.78],[245.21,137.78]]},{"text":"彭德怀乘坐的","box":[[273.17,123.74000000000001],[357.15728,123.74000000000001],[357.15728,137.78],[273.17,137.78]]},{"text":"","box":[[357.1572800000002,123.74000000000001],[361.39,123.74000000000001],[361.39,123.74000000000001],[357.1572800000002,123.74000000000001]]},{"text":"34","box":[[361.39,123.74000000000001],[375.37,123.74000000000001],[375.37,137.78],[361.39,137.78]]},{"text":"","box":[[375.36999999999995,123.74000000000001],[379.51,123.74000000000001],[379.51,123.74000000000001],[375.36999999999995,123.74000000000001]]},{"text":"次列车到达北京火车","box":[[379.51,123.74000000000001],[505.49092,123.74000000000001],[505.49092,137.78],[379.51,137.78]]},{"text":"站。车刚停稳，就看到北京卫戍区李钟奇副司令员带着一队军人","box":[[90.024,146.41999999999996],[477.3314400000002,146.41999999999996],[477.3314400000002,160.45999999999995],[90.024,160.45999999999995]]},{"text":"已经","box":[[477.46,146.41999999999996],[505.41999999999996,146.41999999999996],[505.41999999999996,160.45999999999995],[477.46,160.45999999999995]]},{"text":"站在车站的月台上","box":[[90.024,169.0999999999999],[202.09127999999995,169.0999999999999],[202.09127999999995,183.1399999999999],[90.024,183.1399999999999]]},{"text":"。车门一开，","box":[[202.01,169.0999999999999],[286.12364,169.0999999999999],[286.12364,183.1399999999999],[202.01,183.1399999999999]]},{"text":"王大宾","box":[[286.13,169.0999999999999],[328.01,169.0999999999999],[328.01,183.1399999999999],[286.13,183.1399999999999]]},{"text":"走出车厢，李钟奇迎了上去，","box":[[328.03,169.0999999999999],[510.04456,169.0999999999999],[510.04456,183.1399999999999],[328.03,183.1399999999999]]},{"text":"交给他一封北京卫戍区的大信封，","box":[[90.024,191.89999999999998],[301.18559999999997,191.89999999999998],[301.18559999999997,205.93999999999997],[90.024,205.93999999999997]]},{"text":"上面写着：","box":[[301.37,191.89999999999998],[371.79464,191.89999999999998],[371.79464,205.93999999999997],[301.37,205.93999999999997]]},{"text":"“","box":[[364.75,191.89999999999998],[378.79,191.89999999999998],[378.79,205.93999999999997],[364.75,205.93999999999997]]},{"text":"谷万才、王大宾同志","box":[[378.91,191.89999999999998],[505.60696,191.89999999999998],[505.60696,205.93999999999997],[378.91,205.93999999999997]]},{"text":"收","box":[[90.024,214.57999999999993],[104.064,214.57999999999993],[104.064,228.61999999999992],[90.024,228.61999999999992]]},{"text":"”","box":[[104.06,214.57999999999993],[118.1,214.57999999999993],[118.1,228.61999999999992],[104.06,228.61999999999992]]},{"text":"。","box":[[111.14,214.57999999999993],[125.18,214.57999999999993],[125.18,228.61999999999992],[111.14,228.61999999999992]]},{"text":"王大宾抽出信笺一看，里面写的是：","box":[[123.98,214.57999999999993],[346.79479999999995,214.57999999999993],[346.79479999999995,228.61999999999992],[123.98,228.61999999999992]]},{"text":"“奉总理指示，把反党分子","box":[[338.6375600000001,214.57999999999993],[505.4187200000001,214.57999999999993],[505.4187200000001,228.61999999999992],[338.6375600000001,228.61999999999992]]},{"text":"彭德怀交卫戍区副司令员李中奇同志。","box":[[90.024,237.26],[328.07219999999995,237.26],[328.07219999999995,251.29999999999998],[90.024,251.29999999999998]]},{"text":"”","box":[[320.99604000000005,237.26],[335.03604000000007,237.26],[335.03604000000007,251.29999999999998],[320.99604000000005,251.29999999999998]]},{"text":"王大宾立即将信交谷参谋长看，并向他介绍：","box":[[117.98,259.9699999999999],[399.67856,259.9699999999999],[399.67856,274.00999999999993],[117.98,274.00999999999993]]},{"text":"“这是北京卫戍区","box":[[392.7147200000002,259.9699999999999],[505.25936000000024,259.9699999999999],[505.25936000000024,274.00999999999993],[392.7147200000002,274.00999999999993]]},{"text":"李副司令员”","box":[[90.024,282.65],[174.13763999999998,282.65],[174.13763999999998,296.69],[90.024,296.69]]},{"text":"。","box":[[167.06147999999996,282.65],[181.10147999999995,282.65],[181.10147999999995,296.69],[167.06147999999996,296.69]]},{"text":"谷万才说","box":[[117.98,305.3299999999999],[174.14,305.3299999999999],[174.14,319.36999999999995],[117.98,319.36999999999995]]},{"text":"：","box":[[174.02,305.3299999999999],[188.06,305.3299999999999],[188.06,319.36999999999995],[174.02,319.36999999999995]]},{"text":"“不用介绍了，我们是老战友。","box":[[181.09616,305.3299999999999],[377.02436,305.3299999999999],[377.02436,319.36999999999995],[181.09616,319.36999999999995]]},{"text":"”","box":[[370.06052000000017,305.3299999999999],[384.1005200000002,305.3299999999999],[384.1005200000002,319.36999999999995],[370.06052000000017,319.36999999999995]]},{"text":"这时，彭德怀正准备下车，北京地质学院“东方红”一把手朱成","box":[[117.98,328.01],[505.5682400000002,328.01],[505.5682400000002,342.05],[117.98,342.05]]},{"text":"昭也带人赶到了，说要带彭德怀回地院。","box":[[90.024,350.68999999999994],[341.98583999999994,350.68999999999994],[341.98583999999994,364.72999999999996],[90.024,364.72999999999996]]},{"text":"王大宾把卫戍区的信递给朱成昭，说：","box":[[117.98,373.48999999999995],[356.0281999999999,373.48999999999995],[356.0281999999999,387.53],[117.98,387.53]]},{"text":"“周总理有指示。","box":[[348.9520400000001,373.48999999999995],[461.0193200000001,373.48999999999995],[461.0193200000001,387.53],[348.9520400000001,387.53]]},{"text":"”","box":[[454.05548000000033,373.48999999999995],[468.09548000000035,373.48999999999995],[468.09548000000035,387.53],[454.05548000000033,387.53]]},{"text":"谷万才也说：","box":[[117.98,396.16999999999996],[202.09364,396.16999999999996],[202.09364,410.21],[117.98,410.21]]},{"text":"“要坚决执行周总理的指示。","box":[[195.01747999999998,396.16999999999996],[377.03203999999994,396.16999999999996],[377.03203999999994,410.21],[195.01747999999998,410.21]]},{"text":"”","box":[[370.06820000000016,396.16999999999996],[384.1082000000002,396.16999999999996],[384.1082000000002,410.21],[370.06820000000016,410.21]]},{"text":"朱成昭仔细看了北京卫戍区的那封信函后","box":[[117.98,418.86999999999995],[371.48623999999995,418.86999999999995],[371.48623999999995,432.90999999999997],[117.98,432.90999999999997]]},{"text":"说：","box":[[371.59,418.86999999999995],[399.78999999999996,418.86999999999995],[399.78999999999996,432.90999999999997],[371.59,432.90999999999997]]},{"text":"“这封信有误，你","box":[[392.83,418.86999999999995],[505.37464,418.86999999999995],[505.37464,432.90999999999997],[392.83,432.90999999999997]]},{"text":"们看。信里写的是交给‘李中奇’","box":[[90.024,441.54999999999995],[298.3354799999999,441.54999999999995],[298.3354799999999,455.59],[90.024,455.59]]},{"text":"，不是‘","box":[[291.25931999999995,441.54999999999995],[345.38352,441.54999999999995],[345.38352,455.59],[291.25931999999995,455.59]]},{"text":"李钟奇","box":[[345.43,441.54999999999995],[387.55,441.54999999999995],[387.55,455.59],[345.43,455.59]]},{"text":"’","box":[[387.43,441.54999999999995],[401.47,441.54999999999995],[401.47,455.59],[387.43,455.59]]},{"text":"，","box":[[394.51,441.54999999999995],[408.55,441.54999999999995],[408.55,455.59],[394.51,455.59]]},{"text":"没搞清楚前人不","box":[[407.47,441.54999999999995],[505.49728000000005,441.54999999999995],[505.49728000000005,455.59],[407.47,455.59]]},{"text":"能交，先回地院，我们不能再出错。我们要回去问清楚。","box":[[90.024,464.22999999999996],[440.01312000000013,464.22999999999996],[440.01312000000013,478.27],[90.024,478.27]]},{"text":"”","box":[[432.9369600000003,464.22999999999996],[446.9769600000003,464.22999999999996],[446.9769600000003,478.27],[432.9369600000003,478.27]]},{"text":"其实朱成昭认识李钟奇。","box":[[117.98,486.90999999999997],[272.0409199999999,486.90999999999997],[272.0409199999999,500.95],[117.98,500.95]]},{"text":"李钟奇与其争辩，","box":[[114.02,509.59],[226.08727999999996,509.59],[226.08727999999996,523.63],[114.02,523.63]]},{"text":"双方","box":[[225.65,509.59],[253.73000000000002,509.59],[253.73000000000002,523.63],[225.65,523.63]]},{"text":"一时","box":[[253.73,509.59],[281.81,509.59],[281.81,523.63],[253.73,523.63]]},{"text":"争执不下","box":[[281.69,509.59],[337.72364000000005,509.59],[337.72364000000005,523.63],[281.69,523.63]]},{"text":"，于是李钟奇给卫戍区打电","box":[[337.63,509.59],[505.36587999999995,509.59],[505.36587999999995,523.63],[337.63,523.63]]},{"text":"话请示汇报。朱成昭也给周恩来和中央","box":[[90.024,532.27],[327.94584,532.27],[327.94584,546.31],[90.024,546.31]]},{"text":"文革打电话汇报。","box":[[328.03,532.27],[439.97092,532.27],[439.97092,546.31],[328.03,546.31]]},{"text":"此时，中央文革小组和政治局正在人民大会堂开联席会批评陶","box":[[114.02,555.0699999999999],[505.45364000000023,555.0699999999999],[505.45364000000023,569.1099999999999],[114.02,569.1099999999999]]},{"text":"铸。戚本禹起身去接的电话。他回来后马上向与会人员通报说：","box":[[90.024,577.78],[478.8898800000002,577.78],[478.8898800000002,591.8199999999999],[90.024,591.8199999999999]]},{"text":"“","box":[[477.34,577.78],[491.38,577.78],[491.38,591.8199999999999],[477.34,591.8199999999999]]},{"text":"彭","box":[[491.38,577.78],[505.42,577.78],[505.42,591.8199999999999],[491.38,591.8199999999999]]},{"text":"德怀已由红卫兵押到北京站。","box":[[90.024,600.4599999999999],[272.03855999999996,600.4599999999999],[272.03855999999996,614.4999999999999],[90.024,614.4999999999999]]},{"text":"”","box":[[265.07471999999996,600.4599999999999],[279.11472,600.4599999999999],[279.11472,614.4999999999999],[265.07471999999996,614.4999999999999]]},{"text":"很快，戚本禹赶到北京站，他动员朱成昭执行周恩来的指示，但","box":[[117.98,623.14],[505.2874400000002,623.14],[505.2874400000002,637.18],[117.98,637.18]]},{"text":"朱成昭坚持己见，一定要先带彭德怀回地院。","box":[[90.024,645.8199999999999],[370.06584,645.8199999999999],[370.06584,659.8599999999999],[90.024,659.8599999999999]]},{"text":"因为彭德怀没有下车，","box":[[117.98,668.5],[258.82928,668.5],[258.82928,682.54],[117.98,682.54]]},{"text":"34","box":[[258.89,668.5],[272.99,668.5],[272.99,682.54],[258.89,682.54]]},{"text":"","box":[[272.98999999999995,668.5],[280.01,668.5],[280.01,668.5],[272.98999999999995,668.5]]},{"text":"次空车也不能开走，在北京站已经停","box":[[280.01,668.5],[505.43624,668.5],[505.43624,682.54],[280.01,682.54]]},{"text":"了很长时间。","box":[[90.024,691.18],[174.13763999999998,691.18],[174.13763999999998,705.2199999999999],[90.024,705.2199999999999]]},{"text":"为了不影响北京站的正常运行，","box":[[172.58,691.18],[368.5082,691.18],[368.5082,705.2199999999999],[172.58,705.2199999999999]]},{"text":"戚本禹对双方说，","box":[[366.91,691.18],[478.97728000000006,691.18],[478.97728000000006,705.2199999999999],[366.91,705.2199999999999]]},{"text":"火车","box":[[477.34,691.18],[505.41999999999996,691.18],[505.41999999999996,705.2199999999999],[477.34,705.2199999999999]]},{"text":"先离开北京站，","box":[[90.024,713.8599999999999],[188.17763999999997,713.8599999999999],[188.17763999999997,727.8999999999999],[90.024,727.8999999999999]]},{"text":"开到西直门火车站","box":[[185.78,713.8599999999999],[297.84727999999996,713.8599999999999],[297.84727999999996,727.8999999999999],[185.78,727.8999999999999]]},{"text":"停放","box":[[297.77,713.8599999999999],[325.72999999999996,713.8599999999999],[325.72999999999996,727.8999999999999],[297.77,727.8999999999999]]},{"text":"。","box":[[325.75,713.8599999999999],[339.79,713.8599999999999],[339.79,727.8999999999999],[325.75,727.8999999999999]]},{"text":"可从那里先将彭德怀接到地","box":[[337.39,713.8599999999999],[505.49091999999996,713.8599999999999],[505.49091999999996,727.8999999999999],[337.39,727.8999999999999]]},{"text":"院。","box":[[90.024,736.68],[118.104,736.68],[118.104,750.7199999999999],[90.024,750.7199999999999]]},{"text":"随后，戚本禹回到钓鱼台。当晚，陈伯达召集他和王力、关锋，","box":[[117.98,759.3599999999999],[509.96276000000023,759.3599999999999],[509.96276000000023,773.3999999999999],[117.98,773.3999999999999]]}]}