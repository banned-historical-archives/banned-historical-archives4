,{"text":"5","box":[[165.74,349.72999999999996],[173.24,349.72999999999996],[173.24,364.72999999999996],[165.74,364.72999999999996]]},{"text":"","box":[[173.24,349.72999999999996],[177.02,349.72999999999996],[177.02,349.72999999999996],[173.24,349.72999999999996]]},{"text":"月","box":[[177.02,349.72999999999996],[192.02,349.72999999999996],[192.02,364.72999999999996],[177.02,364.72999999999996]]},{"text":"","box":[[192.02,349.72999999999996],[195.77,349.72999999999996],[195.77,349.72999999999996],[192.02,349.72999999999996]]},{"text":"2","box":[[195.77,349.72999999999996],[203.27,349.72999999999996],[203.27,364.72999999999996],[195.77,364.72999999999996]]},{"text":"","box":[[203.27,349.72999999999996],[207.05,349.72999999999996],[207.05,349.72999999999996],[203.27,349.72999999999996]]},{"text":"日那天对“革联会”采取行动，冲击办公大楼。","box":[[207.05,349.72999999999996],[512.9300000000001,349.72999999999996],[512.9300000000001,364.72999999999996],[207.05,364.72999999999996]]},{"text":"5","box":[[90.024,379.00999999999993],[97.524,379.00999999999993],[97.524,394.00999999999993],[90.024,394.00999999999993]]},{"text":"","box":[[97.524,379.00999999999993],[101.3,379.00999999999993],[101.3,379.00999999999993],[97.524,379.00999999999993]]},{"text":"月","box":[[101.3,379.00999999999993],[116.3,379.00999999999993],[116.3,394.00999999999993],[101.3,394.00999999999993]]},{"text":"","box":[[116.3,379.00999999999993],[120.02,379.00999999999993],[120.02,379.00999999999993],[116.3,379.00999999999993]]},{"text":"2","box":[[120.02,379.00999999999993],[127.52,379.00999999999993],[127.52,394.00999999999993],[120.02,394.00999999999993]]},{"text":"","box":[[127.52,379.00999999999993],[131.3,379.00999999999993],[131.3,379.00999999999993],[127.52,379.00999999999993]]},{"text":"日凌晨","box":[[131.3,379.00999999999993],[176.3,379.00999999999993],[176.3,394.00999999999993],[131.3,394.00999999999993]]},{"text":"","box":[[176.3,379.00999999999993],[180.02,379.00999999999993],[180.02,379.00999999999993],[176.3,379.0