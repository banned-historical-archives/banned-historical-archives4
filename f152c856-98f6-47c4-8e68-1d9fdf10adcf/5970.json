7999999999,202.09999999999997]]},{"text":"28","box":[[442.3,186.89999999999998],[457.5,186.89999999999998],[457.5,202.09999999999997],[442.3,202.09999999999997]]},{"text":"","box":[[457.50000000000006,186.89999999999998],[461.13,186.89999999999998],[461.13,186.89999999999998],[457.50000000000006,186.89999999999998]]},{"text":"日，柳","box":[[461.13,186.89999999999998],[505.9244,186.89999999999998],[505.9244,202.09999999999997],[461.13,202.09999999999997]]},{"text":"州两派又联合发出《告全市人民书》","box":[[90.05,218.10000000000002],[330.4836,218.10000000000002],[330.4836,233.3],[90.05,233.3]]},{"text":"。","box":[[322.88359999999994,218.10000000000002],[338.08359999999993,218.10000000000002],[338.08359999999993,233.3],[322.88359999999994,233.3]]},{"text":"10","box":[[114.08,249.33000000000004],[129.28,249.33000000000004],[129.28,264.53000000000003],[114.08,264.53000000000003]]},{"text":"","box":[[129.28,249.33000000000004],[134.5,249.33000000000004],[134.5,249.33000000000004],[129.28,249.33000000000004]]},{"text":"月","box":[[134.5,249.33000000000004],[149.7,249.33000000000004],[149.7,264.53000000000003],[134.5,264.53000000000003]]},{"text":"","box":[[149.7,249.33000000000004],[155.3,249.33000000000004],[155.3,249.33000000000004],[149.7,249.33000000000004]]},{"text":"28","box":[[155.3,249.33000000000004],[170.1,249.33000000000004],[170.1,264.53000000000003],[155.3,264.53000000000003]]},{"text":"","box":[[170.1,249.33000000000004],[175.72,249.33000000000004],[175.72,249.33000000000004],[170.1,249.33000000000004]]},{"text":"日，北京给柳州来电，传达周恩来指示：参加护路","box":[[175.72,249.33000000000004],[505.77279999999996,249.33000000000004],[505.77279999999996,264.53000000000003],[175.72,264.53000000000003]]},{"text":"的桂林“老多”要回校复课闹革命，不再参加护路。","box":[[90.05,280.53],[435.698,280.53],[435.698,295.72999999999996],[90.05,295.72999999999996]]},{"text":"11","box":[[114.08,311.75],[129.28,311.75],[129.28,326.95],[114.08,326.95]]},{"text":"","box":[[129.28,311.75],[132.9,311.75],[132.9,311.75],[129.28,311.75]]},{"text":"月","box":[[132.9,311.75],[148.1,311.75],[148.1,326.95],[132.9,326.95]]},{"text":"","box":[[148.1,311.75],[151.7,311.75],[151.7,311.75],[148.1,311.75]]},{"text":"6","box":[[151.7,311.75],[159.29999999999998,311.75],[159.29999999999998,326.95],[151.7,326.95]]},{"text":"","box":[[159.29999999999998,311.75],[162.9,311.75],[162.9,311.75],[159.29999999999998,311.75]]},{"text":"日，韦国清做出《我在广西文化大革命中所犯方向路","box":[[162.9,311.75],[505.7511999999999,311.75],[505.7511999999999,326.95],[162.9,326.95]]},{"text":"线错误的检讨》","box":[[90.05,342.98],[195.6596,342.98],[195.6596,358.18],[90.05,358.18]]},{"text":"。","box":[[187.66439999999997,342.98],[202.86439999999996,342.98],[202.86439999999996,358.18],[187.66439999999997,358.18]]},{"text":"11","box":[[202.55,342.98],[217.75,342.98],[217.75,358.18],[202.55,358.18]]},{"text":"","box":[[217.75,342.98],[221.35,342.98],[221.35,342.98],[217.75,342.98]]},{"text":"月","box":[[221.35,342.98],[236.54999999999998,342.98],[236.54999999999998,358.18],[221.35,358.18]]},{"text":"","box":[[236.54999999999998,342.98],[240.18,342.98],[240.18,342.98],[236.54999999999998,342.98]]},{"text":"8","box":[[240.18,342.98],[247.78,342.98],[247.78,358.18],[240.18,358.18]]},{"text":"","box":[[247.78,342.98],[251.38,342.98],[251.38,342.98],[247.78,342.98]]},{"text":"日，中共广西军区委员会上交《广西军","box":[[251.38,342.98],[505.8128,342.98],[505.8128,358.18],[251.38,358.18]]},{"text":"区在支左中所犯方向路线错误的检查报告》","box":[[90.05,374.18],[375.6884,374.18],[375.6884,389.38],[90.05,389.38]]},{"text":"。","box":[[367.69319999999993,374.18],[382.8931999999999,374.18],[382.8931999999999,389.38],[367.69319999999993,389.38]]},{"text":"11","box":[[120.07,405.4],[135.26999999999998,405.4],[135.26999999999998,420.59999999999997],[120.07,420.59999999999997]]},{"text":"月","box":[[135.3,405.4],[150.5,405.4],[150.5,420.59999999999997],[135.3,420.59999999999997]]},{"text":"8","box":[[150.5,405.4],[158.1,405.4],[158.1,420.59999999999997],[150.5,420.59999999999997]]},{"text":"日，广西两派在北京达成关于《广西两派促进革命大","box":[[157.7,405.4],[502.95279999999997,405.4],[502.95279999999997,420.59999999999997],[157.7,420.59999999999997]]},{"text":"联合的十条协议》。这《十条协议》","box":[[90.05,436.6],[327.6716,436.6],[327.6716,451.8],[90.05,451.8]]},{"text":"传到南宁，当天两派","box":[[326.63,436.6],[461.0436,436.6],[461.0436,451.8],[326.63,451.8]]},{"text":"12","box":[[461.13,436.6],[475.93,436.6],[475.93,451.8],[461.13,451.8]]},{"text":"万人","box":[[475.92,436.6],[505.92,436.6],[505.92,451.8],[475.92,451.8]]},{"text":"在南宁隆重集会表示拥护。","box":[[90.05,467.83],[270.47400000000005,467.83],[270.47400000000005,483.03],[90.05,483.03]]},{"text":"当晚","box":[[120.07,499.05],[150.47,499.05],[150.47,514.25],[120.07,514.25]]},{"text":"22","box":[[150.5,499.05],[165.7,499.05],[165.7,514.25],[150.5,514.25]]},{"text":"时，","box":[[165.33,499.05],[195.33480000000003,499.05],[195.33480000000003,514.25],[165.33,514.25]]},{"text":"周恩来在北京","box":[[189.3308,499.05],[279.34520000000003,499.05],[279.34520000000003,514.25],[189.3308,514.25]]},{"text":"接见广州两派代表。","box":[[279.4,499.05],[414.61920000000003,499.05],[414.61920000000003,514.25],[279.4,514.25]]},{"text":"陪同接见的有：","box":[[408.2199999999999,499.05],[513.4344,499.05],[513.4344,514.25],[408.2199999999999,514.25]]},{"text":"戚本禹、吴法宪、李作鹏、黄永胜、陈郁、王首道。","box":[[90.05,530.25],[435.698,530.25],[435.698,545.45],[90.05,545.45]]},{"text":"周恩来十分高兴地说：","box":[[120.07,561.48],[270.48920000000004,561.48],[270.48920000000004,576.6800000000001],[120.07,576.6800000000001]]},{"text":"“广","box":[[267.2819999999999,561.48],[297.2867999999999,561.48],[297.2867999999999,576.6800000000001],[267.2819999999999,576.6800000000001]]},{"text":"州三个工人组织联合起来了，十","box":[[297.4,561.48],[505.8224,561.48],[505.8224,576.6800000000001],[297.4,576.6800000000001]]},{"text":"万人的大会不要延期了吧？”","box":[[90.05,592.67],[285.67400000000004,592.67],[285.67400000000004,607.87],[90.05,607.87]]},{"text":"“工革联”代表答：“定下来了，明天上午","box":[[120.07,623.9],[405.7084,623.9],[405.7084,639.1],[120.07,639.1]]},{"text":"9","box":[[405.47,623.9],[413.07000000000005,623.9],[413.07000000000005,639.1],[405.47,639.1]]},{"text":"点。”","box":[[413.07,623.9],[458.2748,623.9],[458.2748,639.1],[413.07,639.1]]},{"text":"周恩来：“好？中央发个贺电去！”","box":[[120.07,655.12],[360.5036,655.12],[360.5036,670.32],[120.07,670.32]]},{"text":"他接着说：“海员联合了，铁路联合了，现在就是革筹小组","box":[[120.07,686.3199999999999],[505.72439999999995,686.3199999999999],[505.72439999999995,701.52],[120.07,701.52]]},{"text":"问题了。你们赞成搞个革筹小组回去推动呢，还是回去搞军管","box":[[90.05,717.55],[495.70759999999996,717.55],[495.70759999999996,732.75],[90.05,732.75]]},{"text":"呢？”","box":[[90.05,748.75],[135.64999999999998,748.75],[135.64999999999998,763.95],[90.05,763.95]]}]}