1.29999999999995]]},{"text":"，南京军区政委","box":[[345.43,116.29999999999995],[450.43,116.29999999999995],[450.43,131.29999999999995],[345.43,131.29999999999995]]},{"text":"杜平召集","box":[[450.46,116.29999999999995],[510.46,116.29999999999995],[510.46,131.29999999999995],[450.46,131.29999999999995]]},{"text":"驻浙江空五军政委陈励耘、浙江省军区政委南萍、浙江省军区副","box":[[90.024,145.45999999999992],[510.33900000000006,145.45999999999992],[510.33900000000006,160.45999999999992],[90.024,160.45999999999992]]},{"text":"司令员阮贤榜等驻杭三军负责人和群众组织代表张永生、罗晴涛","box":[[90.024,174.62],[510.33900000000006,174.62],[510.33900000000006,189.62],[90.024,189.62]]},{"text":"等","box":[[90.024,203.89999999999998],[105.024,203.89999999999998],[105.024,218.89999999999998],[90.024,218.89999999999998]]},{"text":"人参加的","box":[[105.02,203.89999999999998],[165.01999999999998,203.89999999999998],[165.01999999999998,218.89999999999998],[105.02,218.89999999999998]]},{"text":"紧急","box":[[165.02,203.89999999999998],[195.02,203.89999999999998],[195.02,218.89999999999998],[165.02,218.89999999999998]]},{"text":"联席","box":[[195.05,203.89999999999998],[225.17000000000002,203.89999999999998],[225.17000000000002,218.89999999999998],[195.05,218.89999999999998]]},{"text":"会议，传达落实周恩来指示","box":[[225.17,203.89999999999998],[405.275,203.89999999999998],[405.275,218.89999999999998],[225.17,218.89999999999998]]},{"text":"，","box":[[405.31,203.89999999999998],[420.31,203.89999999999998],[420.31,218.89999999999998],[405.31,218.89999999999998]]},{"text":"研究如何保证","box":[[420.31,203.89999999999998],[510.415,203.89999999999998],[510.415,218.89999999999998],[420.31,218.89999999999998]]},{"text":"安全地把江华送到嘉兴机场。此前，中央","box":[[90.024,233.05999999999995],[360.23400000000004,233.05999999999995],[360.23400000000004,248.05999999999995],[90.024,248.05999999999995]]},{"text":"已","box":[[360.31,233.05999999999995],[375.31,233.05999999999995],[375.31,248.05999999999995],[360.31,248.05999999999995]]},{"text":"委派杜平以中央驻浙","box":[[375.31,233.05999999999995],[510.415,233.05999999999995],[510.415,248.05999999999995],[375.31,248.05999999999995]]},{"text":"江代表的身份负责处理浙江问题。","box":[[90.024,262.25],[315.024,262.25],[315.024,277.25],[90.024,277.25]]},{"text":"为确保江华安全到达嘉兴机场，会议决定实行分段负责制：","box":[[114.14,291.40999999999997],[510.45500000000027,291.40999999999997],[510.45500000000027,306.40999999999997],[114.14,306.40999999999997]]},{"text":"第一段，","box":[[90.024,320.56999999999994],[150.024,320.56999999999994],[150.024,335.56999999999994],[90.024,335.56999999999994]]},{"text":"江华从浙江美院出来到上车，","box":[[146.424,320.56999999999994],[341.424,320.56999999999994],[341.424,335.56999999999994],[146.424,335.56999999999994]]},{"text":"由造反派代表张永生负责；","box":[[337.82399999999996,320.56999999999994],[517.824,320.56999999999994],[517.824,335.56999999999994],[337.82399999999996,335.56999999999994]]},{"text":"第二段，上车后到海宁盐","box":[[90.024,349.72999999999996],[255.129,349.72999999999996],[255.129,364.72999999999996],[90.024,364.72999999999996]]},{"text":"官，由空五军负责；第三段，从盐官到","box":[[255.17,349.72999999999996],[510.38,349.72999999999996],[510.38,364.72999999999996],[255.17,364.72999999999996]]},{"text":"嘉兴机场，由浙江省军区负责。","box":[[90.024,379.00999999999993],[300.024,379.00999999999993],[300.024,394.00999999999993],[90.024,394.00999999999993]]},{"text":"当天","box":[[114.14,408.18999999999994],[144.38,408.18999999999994],[144.38,423.18999999999994],[114.14,423.18999999999994]]},{"text":"，周恩来办公室联络员宋杲","box":[[144.62,408.18999999999994],[327.21500000000003,408.18999999999994],[327.21500000000003,423.18999999999994],[144.62,423.18999999999994]]},{"text":"来","box":[[327.67,408.18999999999994],[342.67,408.18999999999994],[342.67,423.18999999999994],[327.67,423.18999999999994]]},{"text":"到江华","box":[[342.91,408.18999999999994],[388.4800000000001,408.18999999999994],[388.4800000000001,423.18999999999994],[342.91,423.18999999999994]]},{"text":"所在的","box":[[388.75,408.18999999999994],[434.23,408.18999999999994],[434.23,423.18999999999994],[388.75,423.18999999999994]]},{"text":"浙江美术学","box":[[434.47,408.18999999999994],[510.4900000000001,408.18999999999994],[510.4900000000001,423.18999999999994],[434.47,423.18999999999994]]},{"text":"院，握住他的手说：","box":[[90.024,437.34999999999997],[225.024,437.34999999999997],[225.024,452.34999999999997],[90.024,452.34999999999997]]},{"text":"“","box":[[225.05,437.34999999999997],[240.05,437.34999999999997],[240.05,452.34999999999997],[225.05,452.34999999999997]]},{"text":"首长，周总理派我来接你到北京。","box":[[240.05,437.34999999999997],[465.05,437.34999999999997],[465.05,452.34999999999997],[240.05,452.34999999999997]]},{"text":"”","box":[[465.1,437.34999999999997],[480.1,437.34999999999997],[480.1,452.34999999999997],[465.1,452.34999999999997]]},{"text":"此时，江华坐在被关押的房间里","box":[[114.14,466.50999999999993],[323.66,466.50999999999993],[323.66,481.50999999999993],[114.14,481.50999999999993]]},{"text":"问道","box":[[323.71,466.50999999999993],[353.71,466.50999999999993],[353.71,481.50999999999993],[323.71,481.50999999999993]]},{"text":"：","box":[[353.71,466.50999999999993],[368.71,466.50999999999993],[368.71,481.50999999999993],[353.71,481.50999999999993]]},{"text":"“","box":[[360.67,466.50999999999993],[375.67,466.50999999999993],[375.67,481.50999999999993],[360.67,481.50999999999993]]},{"text":"我去了，吴仲廉怎么","box":[[375.67,466.50999999999993],[510.295,466.50999999999993],[510.295,481.50999999999993],[375.67,481.50999999999993]]},{"text":"办？","box":[[90.024,495.66999999999996],[120.024,495.66999999999996],[120.024,510.66999999999996],[90.024,510.66999999999996]]},{"text":"”","box":[[120.02,495.66999999999996],[135.01999999999998,495.66999999999996],[135.01999999999998,510.66999999999996],[120.02,510.66999999999996]]},{"text":"吴仲廉","box":[[114.14,524.8299999999999],[159.62,524.8299999999999],[159.62,539.8299999999999],[114.14,539.8299999999999]]},{"text":"是江华的爱人，","box":[[159.86,524.8299999999999],[266.33,524.8299999999999],[266.33,539.8299999999999],[159.86,539.8299999999999]]},{"text":"时任","box":[[266.69,524.8299999999999],[296.93,524.82