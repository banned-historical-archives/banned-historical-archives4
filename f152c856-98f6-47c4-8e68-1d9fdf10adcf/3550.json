9999998,423.18999999999994],[120.02,423.18999999999994]]},{"text":"","box":[[225.01999999999998,408.18999999999994],[228.77,408.18999999999994],[228.77,408.18999999999994],[225.01999999999998,408.18999999999994]]},{"text":"7","box":[[228.77,408.18999999999994],[236.27,408.18999999999994],[236.27,423.18999999999994],[228.77,423.18999999999994]]},{"text":"","box":[[236.27,408.18999999999994],[240.05,408.18999999999994],[240.05,408.18999999999994],[236.27,408.18999999999994]]},{"text":"月","box":[[240.05,408.18999999999994],[255.05,408.18999999999994],[255.05,423.18999999999994],[240.05,423.18999999999994]]},{"text":"","box":[[255.05,408.18999999999994],[258.77,408.18999999999994],[258.77,408.18999999999994],[255.05,408.18999999999994]]},{"text":"4","box":[[258.77,408.18999999999994],[266.27,408.18999999999994],[266.27,423.18999999999994],[258.77,423.18999999999994]]},{"text":"","box":[[266.27,408.18999999999994],[270.05,408.18999999999994],[270.05,408.18999999999994],[266.27,408.18999999999994]]},{"text":"日。","box":[[270.05,408.18999999999994],[300.05,408.18999999999994],[300.05,423.18999999999994],[270.05,423.18999999999994]]},{"text":"7","box":[[120.02,437.34999999999997],[127.52,437.34999999999997],[127.52,452.34999999999997],[120.02,452.34999999999997]]},{"text":"","box":[[127.52,437.34999999999997],[131.3,437.34999999999997],[131.3,437.34999999999997],[127.52,437.34999999999997]]},{"text":"月","box":[[131.3,437.34999999999997],[146.3,437.34999999999997],[146.3,452.34999999999997],[131.3,452.34999999999997]]},{"text":"","box":[[146.3,437.34999999999997],[150.02,437.34999999999997],[150.02,437.34999999999997],[146.3,437.34999999999997]]},{"text":"1","box":[[150.02,437.34999999999997],[157.52,437.34999999999997],[157.52,452.34999999999997],[150.02,452.34999999999997]]},{"text":"","box":[[157.52,437.34999999999997],[161.3,437.34999999999997],[161.3,437.34999999999997],[157.52,437.34999999999997]]},{"text":"日","box":[[161.3,437.34999999999997],[176.3,437.34999999999997]