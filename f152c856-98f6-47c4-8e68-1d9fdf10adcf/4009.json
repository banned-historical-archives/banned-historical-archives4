[135.07999999999998,203.89999999999998],[135.07999999999998,218.89999999999998],[120.02,218.89999999999998]]},{"text":"","box":[[135.07999999999998,203.89999999999998],[139.82,203.89999999999998],[139.82,203.89999999999998],[135.07999999999998,203.89999999999998]]},{"text":"日","box":[[139.82,203.89999999999998],[154.82,203.89999999999998],[154.82,218.89999999999998],[139.82,218.89999999999998]]},{"text":"","box":[[154.82,203.89999999999998],[159.5,203.89999999999998],[159.5,203.89999999999998],[154.82,203.89999999999998]]},{"text":"15","box":[[159.5,203.89999999999998],[174.44,203.89999999999998],[174.44,218.89999999999998],[159.5,218.89999999999998]]},{"text":"","box":[[174.44,203.89999999999998],[179.18,203.89999999999998],[179.18,203.89999999999998],[174.44,203.89999999999998]]},{"text":"时","box":[[179.18,203.89999999999998],[194.18,203.89999999999998],[194.18,218.89999999999998],[179.18,218.89999999999998]]},{"text":"","box":[[194.18,203.89999999999998],[198.89,203.89999999999998],[198.89,203.89999999999998],[194.18,203.89999999999998]]},{"text":"40","box":[[198.89,203.89999999999998],[213.82999999999998,203.89999999999998],[213.82999999999998,218.89999999999998],[198.89,218.89999999999998]]},{"text":"","box":[[213.82999999999998,203.89999999999998],[218.57,203.89999999999998],[218.57,203.89999999999998],[213.82999999999998,203.89999999999998]]},{"text":"分，","box":[[218.57,203.89999999999998],[248.57,203.89999999999998],[248.57,218.89999999999998],[218.57,218.89999999999998]]},{"text":"“","box":[[241.01,203.89999999999998],[256.01,203.89999999999998],[256.01,218.89999999999998],[241.01,218.89999999999998]]},{"text":"百万雄师","box":[[256.01,203.89999999999998],[316.01,203.89999999999998],[316.01,218.89999999999998],[256.01,218.89999999999998]]},{"text":"”","box":[[316.03,203.89999999999998],[331.03,203.89999999999998],[331.03,218.89999999999998],[316.03,218.89999999999998]]},{"text":"汉阳分站近千人，乘","box":[[331.03,203.89999999999998],[466.03,203.89999999999998],[466.03,2