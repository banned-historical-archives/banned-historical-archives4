","box":[[449.38,721.66],[505.41364,721.66],[505.41364,735.6999999999999],[449.38,735.6999999999999]]},{"text":"队","box":[[90.024,744.3599999999999],[104.064,744.3599999999999],[104.064,758.3999999999999],[90.024,758.3999999999999]]},{"text":"’","box":[[104.06,744.3599999999999],[118.1,744.3599999999999],[118.1,758.3999999999999],[104.06,758.3999999999999]]},{"text":"负责向","box":[[116.9,744.3599999999999],[159.02,744.3599999999999],[159.02,758.3999999999999],[116.9,758.3999999999999]]},{"text":"‘","box":[[157.7,744.3599999999999],[171.73999999999998,744.3599999999999],[171.73999999999998,758.3999999999999],[157.7,758.3999999999999]]},{"text":"赤卫队","box":[[171.74,744.3599999999999],[213.73364,744.3599999999999],[213.73364,758.3999999999999],[171.74,758.3999999999999]]},{"text":"’","box":[[213.77,744.3599999999999],[227.81,744.3599999999999],[227.81,758.3999999999999],[213.77,758.3999999999999]]},{"text":"斗争","box":[[226.61,744.3599999999999],[254.69,744.3599999999999],[254.69,758.3999999999999],[226.61,758.3999999999999]]},{"text":"......","box":[[254.69,744.3599999999999],[282.65,744.3599999999999],[282.65,758.3999999999999],[254.69,758.3999999999999]]},{"text":"各高校造反派做好积极配合。","box":[[282.65,744.3599999999999],[464.66456,744.3599999999999],[464.66456,758.3999999999999],[282.65,758.3999999999999]]},{"text":"”","box":[[463.54,744.3599999999999],[477.58000000000004,744.3599999999999],[477.58000000000004,758.3999999999999],[463.54,758.3999999999999]]},{"text":"同时","box":[[477.58,744.3599999999999],[505.53999999999996,744.3599999999999],[505.53999999999996,758.3999999999999],[477.58,758.3999999999999]]}]}