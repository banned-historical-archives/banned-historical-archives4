,[150.01999999999998,763.3199999999999],[120.02,763.3199999999999]]},{"text":",","box":[[150.02,748.3199999999999],[157.52,748.3199999999999],[157.52,763.3199999999999],[150.02,763.3199999999999]]},{"text":"按周恩来的要求","box":[[157.58,748.3199999999999],[262.58000000000004,748.3199999999999],[262.58000000000004,763.3199999999999],[157.58,763.3199999999999]]},{"text":",","box":[[262.61,748.3199999999999],[270.11,748.3199999999999],[270.11,763.3199999999999],[262.61,763.3199999999999]]},{"text":"","box":[[270.11,748.3199999999999],[277.73,748.3199999999999],[277.73,748.3199999999999],[270.11,748.3199999999999]]},{"text":"邢文举详细介绍了“","box":[[277.73,748.3199999999999],[410.21000000000004,748.3199999999999],[410.21000000000004,763.3199999999999],[277.73,763.3199999999999]]},{"text":"827","box":[[410.23,748.3199999999999],[432.85,748.3199999999999],[432.85,763.3199999999999],[410.23,763.3199999999999]]},{"text":"”派头头的","box":[[432.91,748.3199999999999],[505.39000000000004,748.3199999999999],[505.39000000000004,763.3199999999999],[432.91,763.3199999999999]]}]}