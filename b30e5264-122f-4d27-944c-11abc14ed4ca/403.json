:[[167.44463537440026,545.518744910016],[167.70453437440023,545.518744910016],[167.70453437440023,545.518744910016],[167.44463537440026,545.518744910016]]},{"text":"９","box":[[169.96656636049602,545.518696392496],[179.67007036049603,545.518696392496],[179.67007036049603,555.222200392496],[169.96656636049602,555.222200392496]]},{"text":"月下旬到北京去时，","box":[[178.21456416750402,545.518696392496],[269.912667264,545.518696392496],[269.912667264,555.222200392496],[178.21456416750402,555.222200392496]]},{"text":"曾问过春桥同志，","box":[[265.546061353488,545.518696392496],[346.95849872750404,545.518696392496],[346.95849872750404,555.222200392496],[265.546061353488,555.222200392496]]},{"text":"中央","box":[[342.5919122240001,545.518696392496],[362.58114016750415,545.518696392496],[362.58114016750415,555.222200392496],[342.5919122240001,555.222200392496]]}]}