,331.84752860899204],[179.86438302809634,341.55103260899205],[100.19859578108836,341.55103260899205]]},{"text":"在工厂家田头视察......","box":[[175.1096660680964,331.84752860899204],[274.76466185160035,331.84752860899204],[274.76466185160035,341.55103260899205],[175.1096660680964,341.55103260899205]]},{"text":"直到","box":[[85.25522873160037,347.17904552198405],[104.95334185160038,347.17904552198405],[104.95334185160038,356.88254952198406],[85.25522873160037,356.88254952198406]]},{"text":"","box":[[104.95334185160037,347.17904552198405],[105.14331785160033,347.17904552198405],[105.14331785160033,347.17904552198405],[104.95334185160037,347.17904552198405]]},{"text":"１","box":[[106.796774727504,347.179045521984],[116.500278727504,347.179045521984],[116.500278727504,356.882549521984],[106.796774727504,356.882549521984]]},{"text":"９","box":[[111.648526727504,347.179045521984],[121.352030727504,347.179045521984],[121.352030727504,356.882549521984],[111.648526727504,356.882549521984]]},{"text":"７","box":[[116.50027872750401,347.179045521984],[126.203782727504,347.179045521984],[126.203782727504,356.882549521984],[116.50027872750401,356.882549521984]]},{"text":"６","box":[[121.352040431008,347.179045521984],[131.055544431008,347.179045521984],[131.055544431008,356.882549521984],[121.352040431008,356.882549521984]]},{"text":"年","box":[[129.21186896750402,347.179045521984],[138.91537296750403,347.179045521984],[138.91537296750403,356.882549521984],[129.21186896750402,356.882549521984]]},{"text":"","box":[[138.91537296750403,347.179045521984],[139.10537296750402,347.179045521984],[139.10537296750402,347.179045521984],[138.91537296750403,347.179045521984]]},{"text":"９","box":[[140.759038727504,347.179045521984],[150.462542727504,347.179045521984],[150.462542727504,356.882549521984],[140.759038727504,356.882549521984]]},{"text":"月","box":[[148.618886671008,347.179045521984],[158.322390671008,347.179045521984],[158.322390671008,356.882549521984],[148.618886671008,356.882549521984]]},{"text":"","box":[[158.322390671008,347.179045521984],[158.512389671008,347.179045521984],[158.512389671008,347.179045521984],[158.322390671008,347.179045521984]]},{"text":"９","box":[[160.166046727504,347.179045521984],[169.86955072750402,347.179045521984],[169.86955072750402,356.882549521984],[160.166046727504,356.882549521984]]},{"text":"日，","box":[[168.025875264,347.179045521984],[187.723998087504,347.179045521984],[187.723998087504,356.882549521984],[168.025875264,356.882549521984]]},{"text":"人们才在不知所措的茫然中结束了这种想","box":[[182.96928112750402,347.179045521984],[362.58111105699174,347.179045521984],[362.58111105699174,356.882549521984],[182.96928112750402,356.882549521984]]},{"text":"法。","box":[[65.2658164215199,362.413546801984],[84.96392954151992,362.413546801984],[84.96392954151992,372.117050801984],[65.2658164215199,372.117050801984]]},{"text":"在中国这样一块特定的土地上，","box":[[85.25503466151991,377.648086896],[224.79145129203184,377.648086896],[224.79145129203184,387.351590896],[85.25503466151991,387.351590896]]},{"text":"在","box":[[220.32784915553592,377.648086896],[230.03135315553592,377.648086896],[230.03135315553592,387.351590896],[220.32784915553592,387.351590896]]},{"text":"","box":[[230.03135315553592,377.648086896],[230.28134515553594,377.648086896],[230.28134515553594,377.648086896],[230.03135315553592,377.648086896]]},{"text":"２","box":[[232.457151527504,377.648077192496],[242.16065552750402,377.648077192496],[242.16065552750402,387.351581192496],[232.457151527504,387.351581192496]]},{"text":"０","box":[[237.30890352750401,377.648077192496],[247.01240752750402,377.648077192496],[247.01240752750402,387.351581192496],[237.30890352750401,387.351581192496]]},{"text":"世纪","box":[[245.459866294512,377.648077192496],[265.449065127504,377.648077192496],[265.449065127504,387.351581192496],[245.459866294512,387.351581192496]]},{"text":"","box":[[265.449065127504,377.648077192496],[265.699065127504,377.648077192496],[265.699065127504,377.648077192496],[265.449065127504,377.648077