922230081],[164.92112271715226,513.3030922230081],[164.92112271715226,523.006596223008],[85.25532576664028,523.006596223008]]},{"text":"张春桥更是念念不忘，","box":[[160.45749147014428,513.3030922230081],[263.2176473476643,513.3030922230081],[263.2176473476643,523.006596223008],[160.45749147014428,523.006596223008]]},{"text":"语气中表现出对邓小平","box":[[258.8510608441604,513.3030922230081],[362.5815768251843,513.3030922230081],[362.5815768251843,523.006596223008],[258.8510608441604,523.006596223008]]},{"text":"的仇恨和担心，","box":[[65.26616574766433,529.3138738230081],[134.9373341711684,529.3138738230081],[134.9373341711684,539.017377823008],[65.26616574766433,539.017377823008]]},{"text":"并一再地强调，","box":[[130.18261721116838,529.3138738230081],[199.85379533817633,529.3138738230081],[199.85379533817633,539.017377823008],[130.18261721116838,539.017377823008]]},{"text":"现在关键是由谁来担任接班人，","box":[[195.0990783781764,529.3138738230081],[336.77022707467233,529.3138738230081],[336.77022707467233,539.017377823008],[195.0990783781764,539.017377823008]]},{"text":"如果这","box":[[332.30662493817636,529.3138738230081],[362.5815768251844,529.3138738230081],[362.5815768251844,539.017377823008],[332.30662493817636,539.017377823008]]},{"text":"个班接不好，","box":[[65.26616574766439,545.3246554230082],[124.94272505116844,545.3246554230082],[124.94272505116844,555.0281594230081],[65.26616574766439,555.0281594230081]]},{"text":"中国就会出现资本主义复辟，","box":[[120.18800809116841,545.3246554230082],[249.8268700486883,545.3246554230082],[249.8268700486883,555.0281594230081],[120.18800809116841,555.0281594230081]]},{"text":"邓小平就会重新上台......","box":[[245.0721336816804,545.3246554230082],[354.7217385851843,545.3246554230082],[354.7217385851843,555.0281594230081],[245.0721336816804,555.0281594230081]]}]}