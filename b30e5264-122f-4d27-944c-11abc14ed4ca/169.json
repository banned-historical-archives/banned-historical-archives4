86896],[362.5812663130561,387.351590896],[352.8777623130561,387.351590896]]},{"text":"冲出包围，","box":[[65.2658552355361,392.88258817599996],[114.94780541904014,392.88258817599996],[114.94780541904014,402.58609217599997],[65.2658552355361,402.58609217599997]]},{"text":"但已来不及了，","box":[[110.19308845904011,392.88258817599996],[181.90198301904007,392.88258817599996],[181.90198301904007,402.58609217599997],[110.19308845904011,402.58609217599997]]},{"text":"他们一边狂喊“","box":[[177.7294957060481,392.88258817599996],[250.99093149904002,392.88258817599996],[250.99093149904002,402.58609217599997],[177.7294957060481,402.58609217599997]]},{"text":"回去！回去！","box":[[247.59470509904008,392.88258817599996],[308.7267900025441,392.88258817599996],[308.7267900025441,402.58609217599997],[247.59470509904008,402.58609217599997]]},{"text":"”","box":[[303.97207304254414,392.88258817599996],[313.67557704254415,392.88258817599996],[313.67557704254415,402.58609217599997],[303.97207304254414,402.58609217599997]]},{"text":"一边舞着棍","box":[[310.5704363555361,392.88258817599996],[362.5812177955361,392.88258817599996],[362.5812177955361,402.58609217599997],[310.5704363555361,402.58609217599997]]},{"text":"棒，","box":[[65.26584553203207,408.21412449599995],[84.96395865203209,408.21412449599995],[84.96395865203209,417.91762849599996],[65.26584553203207,417.91762849599996]]},{"text":"劈头盖脑打来。猛然一脚，","box":[[80.20924169203208,408.21412449599995],[201.21198508955206,408.21412449599995],[201.21198508955206,417.91762849599996],[80.20924169203208,417.91762849599996]]},{"text":"我被踢倒在地，","box":[[196.8453985860481,408.21412449599995],[268.4572678095521,408.21412449599995],[268.4572678095521,417.91762849599996],[196.8453985860481,417.91762849599996]]},{"text":"十多人围着我，","box":[[264.09070071305615,408.21412449599995],[336.1877257295521,408.21412449599995],[336.1877257295521,417.91762849599996],[264.09070071305615,417.91762849599996]]},{"text":"连打带","box":[[331.91823248707215,408.21412449599995],[362.58130512707214,408.21412449599995],[362.58130512707214,417.91762849599996],[331.91823248707215,417.91762849599996]]},{"text":"踢，","box":[[65.26589404955212,423.448625776],[84.96400716955213,423.448625776],[84.96400716955213,433.152129776],[65.26589404955212,433.152129776]]},{"text":"直打得我头晕目眩，","box":[[80.20929020955212,423.448625776],[169.86969628006415,423.448625776],[169.86969628006415,433.152129776],[80.20929020955212,433.152129776]]},{"text":"不能动弹。等到我慢慢清醒了一些，","box":[[165.11499872707216,423.448625776],[326.48427024707206,423.448625776],[326.48427024707206,433.152129776],[165.11499872707216,433.152129776]]},{"text":"才感觉还","box":[[322.02066811057614,423.448625776],[362.5813536445921,423.448625776],[362.5813536445921,433.152129776],[322.02066811057614,433.152129776]]},{"text":"有一个人压着我的腿，","box":[[65.26591345656016,438.68312705599993],[167.0556510095522,438.68312705599993],[167.0556510095522,448.38663105599994],[65.26591345656016,448.38663105599994]]},{"text":"胳膊旁边躺着另一个人，","box":[[162.6890839130562,438.68312705599993],[274.9586057860482,438.68312705599993],[274.9586057860482,448.38663105599994],[162.6890839130562,448.38663105599994]]},{"text":"不远处传来人们挨打","box":[[270.59201928254436,438.68312705599993],[362.58129542356835,438.68312705599993],[362.58129542356835,448.38663105599994],[270.59201928254436,448.38663105599994]]},{"text":"的惨叫声。他们发现我有抄诗的本子，","box":[[65.26585523553643,453.91764774300793],[234.7861089295524,453.91764774300793],[234.7861089295524,463.62115174300794],[65.26585523553643,463.62115174300794]]},{"text":"便把我连夜押到了监狱。","box":[[230.03139196955246,453.91764774300793],[339.6809968730564,453.91764774300793],[339.6809968730564,463.62115174300794],[230.03139196955246,463.62115174300794]]},{"text":"姚文元在","box":[[85.254986144,469.152119912496],[124.94231750400004,469.152119912496],[124.94231750400004,478.855623912496],[85.254986144,478.855623912496]]},{"text":"","box":[[124.94231750400002,469.152119912496],[125.16231850400001,469.152119912496],[125.16231850400001,469.152119912496],[124.94231750400002,469.152119912496]]},{"text":"４","box":[[127.077098087504,469.152119912496],[136.780602087504,469.152119912496],[136.780602087504,478.855623912496],[127.077098087504,478.855623912496]]},{"text":"月","box":[[134.93693632750401,469.152119912496],[144.64044032750402,469.152119912496],[144.64044032750402,478.855623912496],[134.93693632750401,478.855623912496]]},{"text":"","box":[[144.64044032750402,469.152119912496],[144.86044032750402,469.152119912496],[144.86044032750402,469.152119912496],[144.64044032750402,469.152119912496]]},{"text":"５","box":[[146.77521120750401,469.152119912496],[156.47871520750402,469.152119912496],[156.47871520750402,478.855623912496],[146.77521120750401,478.855623912496]]},{"text":"日的日记中写道：","box":[[154.63505915100802,469.152119912496],[234.30084639801595,469.152119912496],[234.30084639801595,478.855623912496],[154.63505915100802,478.855623912496]]},{"text":"今天经历了惊心动魄的阶级斗争，","box":[[85.254986144,484.483636825488],[234.8830663415199,484.483636825488],[234.8830663415199,494.18714082548803],[85.254986144,494.18714082548803]]},{"text":"我目睹了反革命暴徒在天安门","box":[[230.32240975801602,484.483636825488],[362.5812760165599,484.483636825488],[362.5812760165599,494.18714082548803],[230.32240975801602,494.18714082548803]]},{"text":"广场的表演，","box":[[65.26579701451203,499.718157512496],[124.94235631801608,499.718157512496],[124.94235631801608,509.42166151249603],[65.26579701451203,509.42166151249603]]},{"text":"并自始至终参加了镇压反革命分子的斗争。","box":[[120.18763935801606,499.718157512496],[309.79417544254386,499.718157512496],[309.79417544254386,509.42166151249603],[120.18763935801606,509.42166151249603]]},{"text":"下午二时，","box":[[85.25507347553602,514.952658792496],[134.93702365904005,514.952658792496],[134.93702365904005,524.6561627924959],[85.25507347553602,524.6561627924959]]},{"text":"政治局紧急开会，","box":[[130.18230669904005,514.952658792496],[210.430313889552,514.952658792496],[210.430313889552,524.6561627924959],[130.18230669904005,524.6561627924959]]},{"text":"人民（","box":[[205.96669234604806,514.952658792496],[236.24160541904007,514.952658792496],[236.24160541904007,524.6561627924959],[205.96669234604806,524.6561627924959]]},{"text":"天安门）","box":[[232.65131864254408,514.952658792496],[273.21195565904003,514.952658792496],[273.21195565904003,524.6561627924959],[232.65131864254408,524.6561627924959]]},{"text":"广场上发生了赤裸裸","box":[[269.8157292590401,514.952658792496],[362.58132453408,514.952658792496],[362.58132453408,524.6561627924959],[269.8157292590401,524.6561627924959]]},{"text":"的反革命煽动，","box":[[65.26588434604804,530.187169776],[134.9370527695521,530.187169776],[134.9370527695521,539.890673776],[65.26588434604804,539.890673776]]},{"text":"反革命暴动事件，","box":[[130.1823358095521,530.187169776],[209.84813276006406,530.187169776],[209.84813276006406,539.890673776],[130.1823358095521,539.890673776]]},{"text":"打了解放军，","box":[[205.09341580006412,530.187169776],[265.54623601656004,530.187169776],[265.54623601656004,539.890673776],[205.09341580006412,539.890673776]]},{"text":"烧了汽车，","box":[[260.9855794330561,530.187169776],[311.249730153056,530.187169776],[311.249730153056,539.890673776],[260.9855794330561,539.890673776]]},{"text":"打了民警，","box":[[306.78612801656004,530.187169776],[357.34140326356805,530.187169776],[357.34140326356805,539.890673776],[306.78612801656004,539.890673776]]},{"text":"打","box":[[352.8778011270721,530.187169776],[362.5813051270721,530.187169776],[362.5813051270721,539.890673776],[352.8778011270721,539.890673776]]},{"text":"了革命群众，","box":[[65.26589404955206,545.518706096],[124.84541831305609,545.518706096],[124.84541831305609,555.2222100959999],[65.26589404955206,555.2222100959999]]},{"text":"并且冲到人民大会堂东门。一批人在记（","box":[[120.09069164955206,545.518706096],[299.60558357407984,545.518706096],[299.60558357407984,555.2222100959999],[120.09069164955206,555.2222100959999]]},{"text":"纪）","box":[[295.821226717584,545.518706096],[315.71338080707204,545.518706096],[315.71338080707204,555.2222100959999],[295.821226717584,555.2222100959999]]},{"text":"念碑进行反","box":[[312.12311343758404,545.518706096],[362.58137305160005,545.518706096],[362.58137305160005,555.2222100959999],[312.12311343758404,555.2222100959999]]}]}