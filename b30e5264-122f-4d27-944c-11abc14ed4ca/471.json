511.362372016],[79.23881366400002,511.362372016]]},{"text":"年春节的时候，","box":[[87.09866160750401,501.658868016],[156.76983003100804,501.658868016],[156.76983003100804,511.362372016],[87.09866160750401,511.362372016]]},{"text":"放","box":[[152.01512277451204,501.658868016],[161.71862677451205,501.658868016],[161.71862677451205,511.362372016],[152.01512277451204,511.362372016]]},{"text":"","box":[[161.71862677451205,501.658868016],[161.93862377451205,501.658868016],[161.93862377451205,501.658868016],[161.71862677451205,501.658868016]]},{"text":"３","box":[[163.853368544,501.658868016],[173.55687254400002,501.658868016],[173.55687254400002,511.362372016],[163.853368544,511.362372016]]},{"text":"天假让谢保安回家。以后就是在铁路上到处","box":[[171.713216487504,501.658868016],[362.5812372025439,501.658868016],[362.5812372025439,511.362372016],[171.713216487504,511.362372016]]},{"text":"批斗，","box":[[65.26579701451203,516.990394632496],[94.95851925451205,516.990394632496],[94.95851925451205,526.6938986324959],[65.26579701451203,526.6938986324959]]},{"text":"东起黄石，","box":[[90.20381199801605,516.990394632496],[139.8857524780161,516.990394632496],[139.8857524780161,526.6938986324959],[90.20381199801605,526.6938986324959]]},{"text":"西迄十堰，","box":[[135.13104522152008,516.990394632496],[184.81299540502403,516.990394632496],[184.81299540502403,526.6938986324959],[135.13104522152008,526.6938986324959]]},{"text":"北至广水，","box":[[180.05825903801608,516.990394632496],[229.74023833203208,516.990394632496],[229.74023833203208,526.6938986324959],[180.05825903801608,526.6938986324959]]},{"text":"南到蒲蕲。","box":[[224.9855019650241,516.990394632496],[274.66744244502405,516.990394632496],[274.66744244502405,526.6938986324959],[224.9855019650241,526.6938986324959]]}]}