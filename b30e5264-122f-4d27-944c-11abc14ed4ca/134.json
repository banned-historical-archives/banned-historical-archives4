[[100.19863459510438,331.84752860899204],[159.8751938986084,331.84752860899204],[159.8751938986084,341.55103260899205],[100.19863459510438,341.55103260899205]]},{"text":"要批邓。","box":[[155.12047693860842,331.84752860899204],[194.8078277056164,331.84752860899204],[194.8078277056164,341.55103260899205],[155.12047693860842,341.55103260899205]]},{"text":"”","box":[[190.05311074561644,331.84752860899204],[199.75661474561645,331.84752860899204],[199.75661474561645,341.55103260899205],[190.05311074561644,341.55103260899205]]},{"text":"华国锋还说中央准备派杨贵到河南帮助解决问题。","box":[[85.25524813860844,347.17904552198405],[304.84562128664027,347.17904552198405],[304.84562128664027,356.88254952198406],[85.25524813860844,356.88254952198406]]},{"text":"刘建勋向中央报告河南的情况，","box":[[85.25530635963241,362.413546801984],[224.88876773364834,362.413546801984],[224.88876773364834,372.117050801984],[85.25530635963241,372.117050801984]]},{"text":"原本是请求中央给个指示、","box":[[220.23110522065642,362.413546801984],[342.1070766466404,362.413546801984],[342.1070766466404,372.117050801984],[220.23110522065642,372.117050801984]]},{"text":"办法，","box":[[337.6434745101444,362.413546801984],[367.62730187014444,362.413546801984],[367.62730187014444,372.117050801984],[337.6434745101444,372.117050801984]]},{"text":"或者派人来河南帮助工作，","box":[[65.26610752664038,377.64808689600005],[187.62729296664043,377.64808689600005],[187.62729296664043,387.35159089600006],[65.26610752664038,387.35159089600006]]},{"text":"没想到被接到北京，","box":[[183.26068705612843,377.64808689600005],[274.9588095596324,377.64808689600005],[274.9588095596324,387.35159089600006],[183.26068705612843,387.35159089600006]]},{"text":"而且还得在政治局会","box":[[270.5922230561285,377.64808689600005],[362.58149919715254,377.64808689600005],[362.58149919715254,387.35159089600006],[270.5922230561285,387.35159089600006]]},{"text":"议上面对面地批判邓小平。这使他陷于两难境地，","box":[[65.26605900912057,392.882588176],[289.6111491171526,392.882588176],[289.6111491171526,402.586092176],[65.26605900912057,402.586092176]]},{"text":"不批邓过不了关。","box":[[285.2445432066407,392.882588176],[367.62732127715265,392.882588176],[367.62732127715265,402.586092176],[285.2445432066407,402.586092176]]},{"text":"批邓，","box":[[65.26612693364865,408.21412449600007],[94.95884917364867,408.21412449600007],[94.95884917364867,417.9176284960001],[65.26612693364865,417.9176284960001]]},{"text":"意味着什么，","box":[[90.20414191715267,408.21412449600007],[149.88069151715268,408.21412449600007],[149.88069151715268,417.9176284960001],[90.20414191715267,417.9176284960001]]},{"text":"他是非常清楚的。抗日战争时期，","box":[[145.1259842606567,408.21412449600007],[295.8214401946726,408.21412449600007],[295.8214401946726,417.9176284960001],[145.1259842606567,417.9176284960001]]},{"text":"在太行山抗日根","box":[[291.26076420416075,408.21412449600007],[362.5815865286888,408.21412449600007],[362.5815865286888,417.9176284960001],[291.26076420416075,417.9176284960001]]},{"text":"据地，","box":[[65.2661463406568,423.44862577600003],[94.95886858065683,423.44862577600003],[94.95886858065683,433.15212977600004],[65.2661463406568,433.15212977600004]]},{"text":"他就是邓小平的老部下，","box":[[90.20416132416082,423.44862577600003],[201.79446702766484,423.44862577600003],[201.79446702766484,433.15212977600004],[90.20416132416082,433.15212977600004]]},{"text":"他对邓小平太了解了。而今老部下要","box":[[197.42791933817688,423.44862577600003],[362.5816156392008,423.44862577600003],[362.5816156392008,433.15212977600004],[197.42791933817688,433.15212977600004]]},{"text":"批老上级，","box":[[65.26620456168087,438.683127056],[114.94815474518491,438.683127056],[114.94815474518491,448.386631056],[65.26620456168087,448.386631056]]},{"text":"用他自己的话说，","box":[[110.19343778518488,438.683127056],[189.85921532868883,438.683127056],[189.85921532868883,448.386631056],[110.19343778518488,448.386631056]]},{"text":"“","box":[[185.10451777569688,438.683127056],[194.8080217756969,438.683127056],[194.8080217756969,448.386631056],[185.10451777569688,448.386631056]]},{"text":"这回批邓，","box":[[191.02366491920088,438.683127056],[240.70560539920086,438.683127056],[240.70560539920086,448.386631056],[191.02366491920088,448.386631056]]},{"text":"可是作了难啊！","box":[[235.95088843920092,438.683127056],[305.6220859732169,438.683127056],[305.6220859732169,448.386631056],[235.95088843920092,448.386631056]]},{"text":"”","box":[[300.86736901321694,438.683127056],[310.57087301321695,438.683127056],[310.57087301321695,448.386631056],[300.86736901321694,448.386631056]]},{"text":"当晚","box":[[85.25547131920095,453.91764774300805],[104.95358443920097,453.91764774300805],[104.95358443920097,463.62115174300806],[85.25547131920095,463.62115174300806]]},{"text":"","box":[[104.95358443920095,453.91764774300805],[105.17353543920086,453.91764774300805],[105.17353543920086,453.91764774300805],[104.95358443920095,453.91764774300805]]},{"text":"２","box":[[107.08787984750401,453.917628336],[116.79138384750401,453.917628336],[116.79138384750401,463.621132336],[107.08787984750401,463.621132336]]},{"text":"２","box":[[111.93963184750402,453.917628336],[121.64313584750401,453.917628336],[121.64313584750401,463.621132336],[111.93963184750402,463.621132336]]},{"text":"时多，","box":[[119.799470087504,453.917628336],[149.492202031008,453.917628336],[149.492202031008,463.621132336],[119.799470087504,463.621132336]]},{"text":"刘建勋一行被接到人民大会堂，","box":[[144.73748507100802,453.917628336],[285.3412968450239,453.917628336],[285.3412968450239,463.621132336],[144.73748507100802,463.621132336]]},{"text":"由工作人员领进一","box":[[280.87767530152,453.917628336],[362.58122749904,453.917628336],[362.58122749904,463.621132336],[280.87767530152,463.621132336]]},{"text":"个不大的会议室，","box":[[65.26578731100807,469.15212961599997],[146.58113142400012,469.15212961599997],[146.58113142400012,478.855633616],[65.26578731100807,478.855633616]]},{"text":"在京的政治局委员们都已神情严肃地在里面就座。","box":[[142.21455462400007,469.15212961599997],[367.626884619472,469.15212961599997],[367.626884619472,478.855633616],[142.21455462400007,478.855633616]]},{"text":"杨贵列席会议。刘建勋等人一入座，","box":[[65.26569027596827,484.483646528992],[224.7913251464802,484.483646528992],[224.7913251464802,494.187150528992],[65.26569027596827,494.187150528992]]},{"text":"华国锋就宣布开会：","box":[[220.03662759348828,484.483646528992],[309.6970045534882,484.483646528992],[309.6970045534882,494.187150528992],[220.03662759348828,494.187150528992]]},{"text":"“","box":[[85.25495703348827,499.718167216],[94.95846103348826,499.718167216],[94.95846103348826,509.421671216],[85.25495703348827,509.421671216]]},{"text":"今天开政治局会议，","box":[[91.17410417699227,499.718167216],[180.83451024750428,499.718167216],[180.83451024750428,509.421671216],[91.17410417699227,509.421671216]]},{"text":"研究一下河南问题。今天把刘建勋、","box":[[176.07979328750434,499.718167216],[336.9638993110082,499.718167216],[336.9638993110082,509.421671216],[176.07979328750434,509.421671216]]},{"text":"王维群","box":[[332.5002971745123,499.718167216],[362.58117898152034,499.718167216],[362.58117898152034,509.421671216],[332.5002971745123,509.421671216]]},{"text":"叫来汇报一下河南的情况。","box":[[65.26576790400031,514.952668496],[184.90999163100832,514.952668496],[184.90999163100832,524.656172496],[65.26576790400031,524.656172496]]},{"text":"”","box":[[180.15527467100836,514.952668496],[189.85877867100837,514.952668496],[189.85877867100837,524.656172496],[180.15527467100836,524.656172496]]},{"text":"接着，","box":[[85.25500555100835,530.1871794795039],[114.94773749451237,530.1871794795039],[114.94773749451237,539.8906834795039],[85.25500555100835,539.8906834795039]]},{"text":"刘建勋开始汇报。他先汇报了河南各地的混乱情况，","box":[[110.19302053451236,530.1871794795039],[346.57044589904024,530.1871794795039],[346.57044589904024,539.8906834795039],[110.19302053451236,539.8906834795039]]},{"text":"检讨","box":[[342.3979391790404,530.1871794795039],[362.5812469060484,530.1871794795039],[362.5812469060484,539.8906834795039],[342.3979391790404,539.8906834795039]]},{"text":"自己工作没有做好，","box":[[65.2658358285284,545.518715799504],[155.02327693904044,545.518715799504],[155.02327693904044,555.2222197995039],[65.2658358285284,555.2222197995039]]},{"text":"边哭边说，","box":[[150.85076051553642,545.518715799504],[201.9882265955364,545.518715799504],[201.9882265955364,555.2222197995039],[150.85076051553642,555.2222197995039]]},{"text":"很动感情。他检讨了","box":[[197.81573928254443,545.518715799504],[290.67827256254435,545.518715799504],[290.67827256254435,555.2222197995039],[197.81573928254443,555.2222197995039]]},{"text":"","box":[[290.6782725625444,545.518715799504],[290.95826156254435,545.518715799504],[290.95826156254435,545.518715799504],[290.6782725625444,545.518715799504]]},{"text":"１","box":[[293.39514694400003,545.518696392496],[303.09865094400004,545.518696392496],[303.09865094400004,555.222200392496],[293.39514694400003,555.222200392496]]},{"text":"９","box":[[298.246898944,545.518696392496],[307.950402944,545.518696392496],[307.950402944,555.222200392496],[298.246898944,555.222200392496]]},{"text":"７","box":[[303.098631536992,545.518696392496],[312.802135536992,545.518696392496],[312.802135536992,555.222200392496],[303.098631536992,555.222200392496]]},{"text":"５","box":[[307.950383536992,545.518696392496],[317.653887536992,545.518696392496],[317.653887536992,555.222200392496],[307.950383536992,555.222200392496]]},{"text":"年批判、","box":[[316.392480534512,545.518696392496],[357.050152591008,545.518696392496],[357.050152591008,555.222200392496],[316.392480534512,555.222200392496]]},{"text":"下","box":[[352.87768468502395,545.518696392496],[362.58118868502396,545.518696392496],[362.58118868502396,555.222200392496],[352.87768468502395,555.222200392496]]}]}