[[306.7860309815201,308.753140571472],[357.34130622852814,308.753140571472],[357.34130622852814,318.456644571472],[306.7860309815201,318.456644571472]]},{"text":"七","box":[[352.87770409203216,308.753140571472],[362.58120809203217,308.753140571472],[362.58120809203217,318.456644571472],[352.87770409203216,318.456644571472]]},{"text":"零八落的混凝土梁柱东倒西歪，","box":[[65.26579701451215,324.47284616198397],[208.19839152750419,324.47284616198397],[208.19839152750419,334.176350161984],[65.26579701451215,334.176350161984]]},{"text":"落而未落的楼板悬挂在空中，","box":[[203.83182443100824,324.47284616198397],[336.67274567348824,324.47284616198397],[336.67274567348824,334.176350161984],[203.83182443100824,334.176350161984]]},{"text":"到处是","box":[[332.30619798400033,324.47284616198397],[362.5811498710084,324.47284616198397],[362.5811498710084,334.176350161984],[332.30619798400033,334.176350161984]]},{"text":"断墙残壁......","box":[[65.26573879348837,340.28956738548794],[124.94229809699242,340.28956738548794],[124.94229809699242,349.99307138548795],[65.26573879348837,349.99307138548795]]},{"text":"唐山火车站，","box":[[85.2549667369924,356.10626920198393],[144.93154544750442,356.10626920198393],[144.93154544750442,365.80977320198394],[85.2549667369924,365.80977320198394]]},{"text":"东部铁轨成蛇形弯曲，","box":[[140.1768090804964,356.10626920198393],[239.92886901451232,356.10626920198393],[239.92886901451232,365.80977320198394],[140.1768090804964,365.80977320198394]]},{"text":"其轮廓像一只扁平的铁葫芦。","box":[[235.3682318380164,356.10626920198393],[367.62696224750437,356.10626920198393],[367.62696224750437,365.80977320198394],[235.3682318380164,365.80977320198394]]},{"text":"水泥马路，","box":[[65.26576790400043,371.82593597847995],[114.94771808750447,371.82593597847995],[114.94771808750447,381.52943997847996],[65.26576790400043,381.52943997847996]]},{"text":"被拦腰震断，","box":[[110.19300112750444,371.82593597847995],[169.86956043100844,371.82593597847995],[169.86956043100844,381.52943997847996],[110.19300112750444,381.52943997847996]]},{"text":"一截向左，","box":[[165.11486287801645,371.82593597847995],[214.7968033580164,371.82593597847995],[214.7968033580164,381.52943997847996],[165.11486287801645,381.52943997847996]]},{"text":"一截向右，","box":[[210.04208639801647,371.82593597847995],[259.72402687801645,371.82593597847995],[259.72402687801645,381.52943997847996],[210.04208639801647,381.52943997847996]]},{"text":"错位达一米之多。","box":[[254.9693293250245,371.82593597847995],[334.6351165720324,371.82593597847995],[334.6351165720324,381.52943997847996],[254.9693293250245,381.52943997847996]]},{"text":"路边越堆越高的尸体山！头颅被挤碎的，","box":[[85.25502495801646,387.642657201984],[268.1661529860484,387.642657201984],[268.1661529860484,397.346161201984],[85.25502495801646,397.346161201984]]},{"text":"双脚被砸烂的，","box":[[263.99362685904055,387.642657201984],[336.0906712825445,387.642657201984],[336.0906712825445,397.346161201984],[263.99362685904055,397.346161201984]]},{"text":"身体被","box":[[331.91820337656054,387.642657201984],[362.58127601656054,387.642657201984],[362.58127601656054,397.346161201984],[331.91820337656054,397.346161201984]]},{"text":"压扁的，","box":[[65.26586493904051,403.45935901848],[104.95319629904054,403.45935901848],[104.95319629904054,413.16286301848],[65.26586493904051,413.16286301848]]},{"text":"胸腔被戳穿的......最令人心颤的，","box":[[100.19848904254454,403.45935901848],[253.1257411930565,403.45935901848],[253.1257411930565,413.16286301848],[100.19848904254454,413.16286301848]]},{"text":"是那一具具挂在危楼上的","box":[[248.7591935035686,403.45935901848],[362.58137305160056,403.45935901848],[362.58137305160056,413.16286301848],[248.7591935035686,413.16286301848]]},{"text":"尸体。有的仅仅一只手被楼板压住，","box":[[65.2659328635686,419.17904520198397],[227.31449818108862,419.17904520198397],[227.31449818108862,428.882549201984],[65.2659328635686,428.882549201984]]},{"text":"砸裂的头耷拉着；","box":[[222.94791167758464,419.17904520198397],[304.9425495880966,419.17904520198397],[304.9425495880966,428.882549201984],[222.94791167758464,428.882549201984]]},{"text":"有的跳楼时被","box":[[300.5759630845927,419.17904520198397],[362.5813924586086,419.17904520198397],[362.5813924586086,428.882549201984],[300.5759630845927,428.882549201984]]},{"text":"砸住双脚，","box":[[65.26598138108864,434.99574701847996],[114.94793156459268,434.99574701847996],[114.94793156459268,444.69925101847997],[65.26598138108864,444.69925101847997]]},{"text":"整个人倒悬在空中。这是遇难者中最敏感的一群，","box":[[110.19321460459265,434.99574701847996],[336.18787128211267,434.99574701847996],[336.18787128211267,444.69925101847997],[110.19321460459265,444.69925101847997]]},{"text":"已经从","box":[[331.9183780396328,434.99574701847996],[362.5814506796328,434.99574701847996],[362.5814506796328,444.69925101847997],[331.9183780396328,444.69925101847997]]},{"text":"酣梦中惊醒逃生，","box":[[65.26603960211276,450.8124876489919],[145.9992122891208,450.8124876489919],[145.9992122891208,460.5159916489919],[65.26603960211276,460.5159916489919]]},{"text":"然而他们的逃路却被死神截断。有一位年轻的母","box":[[141.92374060912078,450.8124876489919],[362.5813924586087,450.8124876489919],[362.5813924586087,460.5159916489919],[141.92374060912078,460.5159916489919]]},{"text":"亲，","box":[[65.26602019510483,466.53215442548793],[84.96413331510485,466.53215442548793],[84.96413331510485,476.23565842548794],[65.26602019510483,476.23565842548794]]},{"text":"在三层楼的窗口已经探出半个身子，","box":[[80.40348643510484,466.53215442548793],[244.10156010108878,466.53215442548793],[244.10156010108878,476.23565842548794],[80.40348643510484,476.23565842548794]]},{"text":"沉重的楼板便落下来把她压","box":[[239.7350124116009,466.53215442548793],[362.5814312726249,466.53215442548793],[362.5814312726249,476.23565842548794],[239.7350124116009,476.23565842548794]]},{"text":"在窗台上。她死在半空，","box":[[65.26595227057697,482.3488562419839],[174.81855124459298,482.3488562419839],[174.81855124459298,492.05236024198393],[65.26595227057697,492.05236024198393]]},{"text":"怀里抱着孩子，","box":[[170.063814877585,482.3488562419839],[239.92906308459294,482.3488562419839],[239.92906308459294,492.05236024198393],[170.063814877585,492.05236024198393]]},{"text":"在死的一瞬间，","box":[[235.368425908097,482.3488562419839],[306.00993502809706,482.3488562419839],[306.00993502809706,492.05236024198393],[235.368425908097,492.05236024198393]]},{"text":"还本能地保护","box":[[301.44927844459306,482.3488562419839],[362.5814118656171,482.3488562419839],[362.5814118656171,492.05236024198393],[301.44927844459306,492.05236024198393]]},{"text":"着小生命。随着危楼在余震中颤抖，","box":[[65.26597167758513,498.1655774654879],[224.79160654809706,498.1655774654879],[224.79160654809706,507.8690814654879],[65.26597167758513,507.8690814654879]]},{"text":"母亲垂落的头发在雾气中拂动。","box":[[220.03690899510514,498.1655774654879],[359.67035096211293,498.1655774654879],[359.67035096211293,507.8690814654879],[220.03690899510514,507.8690814654879]]},{"text":"当时除了双手几乎再没有其他可以用来营救的工具。人们似乎","box":[[85.25523843510501,513.8852539454879],[362.5814894936489,513.8852539454879],[362.5814894936489,523.5887579454878],[85.25523843510501,523.5887579454878]]},{"text":"感觉不到指甲剥落、","box":[[65.26607841612906,529.7019654654879],[154.9264941901451,529.7019654654879],[154.9264941901451,539.4054694654878],[65.26607841612906,539.4054694654878]]},{"text":"血肉模糊的手的巨疼，","box":[[150.17175782313709,529.7019654654879],[250.311938510145,529.7019654654879],[250.311938510145,539.4054694654878],[150.17175782313709,539.4054694654878]]},{"text":"而往往是闻声便拼命扒，","box":[[245.75130133364908,529.7019654654879],[357.3416070371531,529.7019654654879],[357.3416070371531,539.4054694654878],[245.75130133364908,539.4054694654878]]},{"text":"等","box":[[352.8780049006571,529.7019654654879],[362.5815089006571,529.7019654654879],[362.5815089006571,539.4054694654878],[352.8780049006571,539.4054694654878]]},{"text":"扒出时却没了任何气息。","box":[[65.2660978231371,545.5186769854879],[174.91572213364913,545.5186769854879],[174.91572213364913,555.2221809854879],[65.2660978231371,555.2221809854879]]}]}