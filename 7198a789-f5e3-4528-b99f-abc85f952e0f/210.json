5.359],[201.959,265.359]]},{"text":"抗战胜利后","box":[[217.68,249.409],[297.58790000000005,249.409],[297.58790000000005,265.359],[217.68,265.359]]},{"text":"，","box":[[297.6,249.409],[313.55,249.409],[313.55,265.359],[297.6,265.359]]},{"text":"昔阳人对给日本人干过事的","box":[[313.32,249.409],[505.3046000000002,249.409],[505.3046000000002,265.359],[313.32,265.359]]},{"text":"人特别痛恨","box":[[90,280.60900000000004],[169.90789999999998,280.60900000000004],[169.90789999999998,296.559],[90,296.559]]},{"text":"，","box":[[170.04,280.60900000000004],[185.98999999999998,280.60900000000004],[185.98999999999998,296.559],[170.04,296.559]]},{"text":"用石头砸死不少这些被叫做","box":[[185.64,280.60900000000004],[377.6246000000001,280.60900000000004],[377.6246000000001,296.559],[185.64,296.559]]},{"text":"“","box":[[377.64,280.60900000000004],[393.59,280.60900000000004],[393.59,296.559],[377.64,296.559]]},{"text":"汉奸","box":[[393.6,280.60900000000004],[425.5094,280.60900000000004],[425.5094,296.559],[393.6,296.559]]},{"text":"”","box":[[425.64,280.60900000000004],[441.59,280.60900000000004],[441.59,296.559],[425.64,296.559]]},{"text":"的人","box":[[441.6,280.60900000000004],[473.5094,280.60900000000004],[473.5094,296.559],[441.6,296.559]]},{"text":"。","box":[[473.64,280.60900000000004],[489.59,280.60900000000004],[489.59,296.559],[473.64,296.559]]},{"text":"陈","box":[[489.24,280.60900000000004],[505.19,280.60900000000004],[505.19,296.559],[489.24,296.559]]},{"text":"永贵非常恐慌","box":[[90,311.8090000000001],[185.98760000000001,311.8090000000001],[185.98760000000001,327.75900000000007],[90,327.75900000000007]]},{"text":"，","box":[[186,311.8090000000001],[201.95,311.8090000000001],[201.95,327.75900000000007],[186,327.75900000000007]]},{"text":"也担心自己被砸死","box":[[201.72,311.8090000000001],[329.62640000000005,311.8090000000001],[329.62640000000005,327.75900000000007],[201.72,327.75900000000007]]},{"text":"。","box":[[329.759,311.8090000000001],[345.709,311.8090000000001],[345.709,327.75900000000007],[329.759,327.75900000000007]]},{"text":"于是","box":[[345.48,311.8090000000001],[377.3894,311.8090000000001],[377.3894,327.75900000000007],[345.48,327.75900000000007]]},{"text":"，","box":[[377.52,311.8090000000001],[393.46999999999997,311.8090000000001],[393.46999999999997,327.75900000000007],[377.52,327.75900000000007]]},{"text":"他向大寨第一任","box":[[393.24,311.8090000000001],[505.1870000000001,311.8090000000001],[505.1870000000001,327.75900000000007],[393.24,327.75900000000007]]},{"text":"共产党村长赵怀恩托孤","box":[[90,343.009],[249.94550000000004,343.009],[249.94550000000004,358.959],[90,358.959]]},{"text":"，","box":[[249.959,343.009],[265.909,343.009],[265.909,358.959],[249.959,358.959]]},{"text":"请求村长保护他的子女长大","box":[[265.68,343.009],[457.66460000000023,343.009],[457.66460000000023,358.959],[265.68,358.959]]},{"text":"。","box":[[457.68,343.009],[473.63,343.009],[473.63,358.959],[457.68,358.959]]},{"text":"赵怀","box":[[473.28,343.009],[505.1894,343.009],[505.1894,358.959],[473.28,358.959]]},{"text":"恩比较实事求是，说：","box":[[90,374.209],[249.94550000000004,374.209],[249.94550000000004,390.159],[90,390.159]]},{"text":"“","box":[[122.04,405.409],[137.99,405.409],[137.99,421.359],[122.04,421.359]]},{"text":"当时日本人来了","box":[[138,405.409],[249.94700000000003,405.409],[249.94700000000003,421.359],[138,421.359]]},{"text":"，","box":[[249.959,405.409],[265.909,405.409],[265.909,421.359],[249.959,421.359]]},{"text":"总要有人出面","box":[[263.04,405.409],[359.0276000000001,405.409],[359.0276000000001,421.359],[263.04,421.359]]},{"text":"，","box":[[359.04,405.409],[374.99,405.409],[374.99,421.359],[359.04,421.359]]},{"text":"别人不敢","box":[[372.24,405.409],[436.1885000000001,405.409],[436.1885000000001,421.359],[372.24,421.359]]},{"text":"，","box":[[436.2,405.409],[452.15,405.409],[452.15,421.359],[436.2,421.359]]},{"text":"他胆大","box":[[449.28,405.409],[497.26910000000004,405.409],[497.26910000000004,421.359],[449.28,421.359]]},{"text":"，","box":[[497.28,405.409],[513.23,405.409],[513.23,421.359],[497.28,421.359]]},{"text":"他当了伪代表","box":[[90,436.609],[185.98760000000001,436.609],[185.98760000000001,452.55899999999997],[90,452.55899999999997]]},{"text":"，","box":[[186,436.609],[201.95,436.609],[201.95,452.55899999999997],[186,452.55899999999997]]},{"text":"参加了","box":[[201.72,436.609],[249.7091,436.609],[249.7091,452.55899999999997],[201.72,452.55899999999997]]},{"text":"‘","box":[[249.72,436.609],[265.67,436.609],[265.67,452.55899999999997],[249.72,452.55899999999997]]},{"text":"兴亚会","box":[[265.8,436.609],[313.7891000000001,436.609],[313.7891000000001,452.55899999999997],[265.8,452.55899999999997]]},{"text":"’","box":[[313.8,436.609],[329.75,436.609],[329.75,452.55899999999997],[313.8,452.55899999999997]]},{"text":"，","box":[[329.52,436.609],[345.46999999999997,436.609],[345.46999999999997,452.55899999999997],[329.52,452.55899999999997]]},{"text":"可能为日本人提供的真","box":[[345.24,436.609],[505.1855000000002,436.609],[505.1855000000002,452.55899999999997],[345.24,452.55899999999997]]},{"text":"假情报都有，这是非常时期的非常事，我看可以原谅。","box":[[90,467.809],[473.97860000000026,467.809],[473.97860000000026,483.759],[90,483.759]]},{"text":"”","box":[[474,467.809],[489.95,467.809],[489.95,483.759],[474,483.759]]},{"text":"调查组","box":[[122.04,499.009],[170.02910000000003,499.009],[170.02910000000003,514.9590000000001],[122.04,514.9590000000001]]},{"text":"将","box":[[170.04,499.009],[185.98999999999998,499.009],[185.98999999999998,514.9590000000001],[170.04,514.9590000000001]]},{"text":"调查的结果立即向谢振华","box":[[186,499.009],[361.9049000000001,499.009],[361.9049000000001,514.9590000000001],[186,514.9590000000001]]},{"text":"作了","box":[[362.04,499.009],[393.9494,499.009],[393.9494,514.9590000000001],[362.04,514.9590000000001]]},{"text":"汇报","box":[[393.959,499.009],[425.8684,499.009],[425.8684,514.9590000000001],[393.959,514.9590000000001]]},{"text":"。","box":[[426,499.009],[441.95,499.009],[441.95,514.9590000000001],[426,514.9590000000001]]},{"text":"谢振华当","box":[[441.24,499.009],[505.1885000000001,499.009],[505.1885000000001,514.9590000000001],[441.24,514.9590000000001]]},{"text":"即召开支左领导小组会议。","box":[[90,530.209],[281.98460000000006,530.209],[281.98460000000006,546.159],[90,546.159]]},{"text":"9","box":[[122.04,561.409],[130.01500000000001,561.409],[130.01500000000001,577.359],[122.04,577.359]]},{"text":"月，谢振华派赴北京出席全国军工会议的副军长李金","box":[[129.959,561.409],[497.85790000000026,561.409],[497.85790000000026,577.359],[129.959,577.359]]},{"text":"时专门将陈永贵的问题呈报周恩来。周恩来当即指示说：","box":[[90,592.609],[489.93800000000033,592.609],[489.93800000000033,608.5590000000001],[90,608.5590000000001]]},{"text":"“","box":[[122.04,623.809],[137.99,623.809],[137.99,639.759],[122.04,639.759]]},{"text":"69","box":[[138,623.809],[154.01485,623.809],[154.01485,639.759],[138,639.759]]},{"text":"军的同志要顾全大局，不要扩散，复印件可报中","box":[[153.959,623.809],[489.9391000000003,623.809],[489.9391000000003,639.759],[153.959,639.759]]},{"text":"央。","box":[[90,655.009],[121.9094,655.009],[121.9094,670.9590000000001],[90,670.9590000000001]]},{"text":"”","box":[[122.04,655.009],[137.99,655.009],[137.99,670.9590000000001],[122.04,670.9590000000001]]},{"text":"李金时从北京开完会回到太原","box":[[122.04,686.209],[329.9840000000001,686.209],[329.9840000000001,702.159],[122.04,702.159]]},{"text":"，","box":[[330,686.209],[345.95,686.209],[345.95,702.159],[330,702.159]]},{"text":"立即将周恩来的指示向","box":[[345.24,686.209],[505.1855000000002,686.209],[505.1855000000002,702.159],[345.24,702.159]]},{"text":"军党委作了传达，并把有关事宜向谢振华作了汇报。","box":[[90,717.409],[457.89890000000025,717.409],[457.89890000000025,733.359],[90,733.359]]},{"text":"12","box":[[122.04,748.609],[138.05485,748.609],[138.05485,764.5590000000001],[122.04,764.5590000000001]]},{"text":"月","box":[[138,748.609],[153.95,748.609],[153.95,764.5590000000001],[138,764.5590000000001]]},{"text":"，","box":[[153.959,748.609],[169.909,748.609],[169.909,764.5590000000001],[153.959,764.5590000000001]]},{"text":"69","box":[[169.68,748.609],[185.69485,748.609],[185.69485,764.5590000000001],[169.68,764.5590000000001]]},{"text":"军正式以军党委的名义","box":[[185.64,748.609],[345.5855,748.609],[345.5855,764.5590000000001],[185.64,764.5590000000001]]},{"text":"，","box":[[345.6,748.609],[361.55,748.609],[361.55,764.5590000000001],[345.6,764.5590000000001]]},{"text":"将陈永贵的历史问题","box":[[361.32,748.609],[505.30610000000013,748.609],[505.30610000000013,764.5590000000001],[361.32,764.5590000000001]]}]}