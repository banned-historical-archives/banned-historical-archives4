,"box":[[297.6,592.609],[425.5064000000002,592.609],[425.5064000000002,608.5590000000001],[297.6,608.5590000000001]]},{"text":"，","box":[[425.64,592.609],[441.59,592.609],[441.59,608.5590000000001],[425.64,608.5590000000001]]},{"text":"和毛家湾","box":[[441.24,592.609],[505.1885000000001,592.609],[505.1885000000001,608.5590000000001],[441.24,608.5590000000001]]},{"text":"断绝了联系","box":[[90,623.809],[169.90789999999998,623.809],[169.90789999999998,639.759],[90,639.759]]},{"text":"。","box":[[170.04,623.809],[185.98999999999998,623.809],[185.98999999999998,639.759],[170.04,639.759]]},{"text":"这可把整天习惯于忙忙碌碌应付叶群的郭连凯","box":[[185.28,623.809],[505.1804000000004,623.809],[505.1804000000004,639.759],[185.28,639.759]]},{"text":"急坏了","box":[[90,655.009],[137.9891,655.009],[137.9891,670.9590000000001],[90,670.9590000000001]]},{"text":"，","box":[[138,655.009],[153.95,655.009],[153.95,670.9590000000001],[138,670.9590000000001]]},{"text":"他经常在办公室","box":[[153.6,655.009],[265.547,655.009],[265.547,670.9590000000001],[153.6,670.9590000000001]]},{"text":"里","box":[[265.68,655.009],[281.63,655.009],[281.63,670.9590000000001],[265.68,670.9590000000001]]},{"text":"念叨","box":[[281.64,655.009],[313.5494,655.009],[313.5494,670.9590000000001],[281.64,670.9590000000001]]},{"text":"：","box":[[313.68,655.009],[329.63,655.009],[329.63,670.9590000000001],[313.68,670.9590000000001]]},{"text":"“","box":[[329.28,655.009],[345.22999999999996,655.009],[345.22999999999996,670.9590000000001],[329.28,670.9590000000001]]},{"text":"怎么搞的？庐山怎么一","box":[[345.24,655.009],[505.1855000000002,655.009],[505.1855000000002,670.9590000000001],[345.24,670.9590000000001]]},{"text":"次电话也不来了呢？","box":[[90,686.209],[233.98610000000005,686.209],[233.98610000000005,702.159],[90,702.159]]},{"text":"......","box":[[234,686.209],[265.9094,686.209],[265.9094,702.159],[234,702.159]]},{"text":"出了什么事了吗？","box":[[266.04,686.209],[393.9464000000002,686.209],[393.9464000000002,702.159],[266.04,702.159]]},{"text":"”","box":[[393.959,686.209],[409.909,686.209],[409.909,702.159],[393.959,702.159]]},{"text":"其他秘书","box":[[122.04,717.409],[185.98850000000002,717.409],[185.98850000000002,733.359],[122.04,733.359]]},{"text":"也觉得情况有些反常","box":[[186,717.409],[329.9861000000001,717.409],[329.9861000000001,733.359],[186,733.359]]},{"text":"。","box":[[330,717.409],[345.95,717.409],[345.95,733.359],[330,733.359]]},{"text":"就在他","box":[[345.24,717.409],[393.2291000000001,717.409],[393.2291000000001,733.359],[345.24,733.359]]},{"text":"们正在疑惑的时","box":[[393.24,717.409],[505.1870000000001,717.409],[505.1870000000001,733.359],[393.24,733.359]]},{"text":"候","box":[[90,748.609],[105.95,748.609],[105.95,764.5590000000001],[90,764.5590000000001]]},{"text":"，","box":[[105.959,748.609],[121.909,748.609],[121.909,764.5590000000001],[105.959,764.5590000000001]]},{"text":"李春生的电话过来了","box":[[121.68,748.609],[265.66610000000003,748.609],[265.66610000000003,764.5590000000001],[121.68,764.5590000000001]]},{"text":"。","box":[[265.68,748.609],[281.63,748.609],[281.63,764.5590000000001],[265.68,764.5590000000001]]},{"text":"接电话的留守秘书张云生赶紧","box":[[281.28,748.609],[489.2240000000002,748.609],[489.2240000000002,764.5590000000001],[281.28,764.5590000000001]]},{"text":"问","box":[[489.24,748.609],[505.19,748.609],[505.19,764.5590000000001],[489.24,764.5590000000001]]}]}