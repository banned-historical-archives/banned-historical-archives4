441.5855000000002,436.609],[441.5855000000002,452.55899999999997],[281.64,452.55899999999997]]},{"text":"。","box":[[441.6,436.609],[457.55,436.609],[457.55,452.55899999999997],[441.6,452.55899999999997]]},{"text":"讲话的","box":[[457.32,436.609],[505.30910000000006,436.609],[505.30910000000006,452.55899999999997],[457.32,452.55899999999997]]},{"text":"中心意思","box":[[90,467.809],[153.9485,467.809],[153.9485,483.759],[90,483.759]]},{"text":"，","box":[[153.959,467.809],[169.909,467.809],[169.909,483.759],[153.959,483.759]]},{"text":"就是坚持毛主席是天才","box":[[165.6,467.809],[325.5455000000002,467.809],[325.5455000000002,483.759],[165.6,483.759]]},{"text":"。","box":[[325.68,467.809],[341.63,467.809],[341.63,483.759],[325.68,483.759]]},{"text":"他讲天才是有针对性的","box":[[337.32,467.809],[497.2655000000002,467.809],[497.2655000000002,483.759],[337.32,483.759]]},{"text":"，","box":[[497.28,467.809],[513.23,467.809],[513.23,483.759],[497.28,483.759]]},{"text":"讲话后反映很强烈","box":[[90,499.009],[217.90640000000002,499.009],[217.90640000000002,514.9590000000001],[90,514.9590000000001]]},{"text":"。","box":[[218.04,499.009],[233.98999999999998,499.009],[233.98999999999998,514.9590000000001],[218.04,514.9590000000001]]},{"text":"他讲了","box":[[233.759,499.009],[281.7481,499.009],[281.7481,514.9590000000001],[233.759,514.9590000000001]]},{"text":"，","box":[[281.759,499.009],[297.709,499.009],[297.709,514.9590000000001],[281.759,514.9590000000001]]},{"text":"我也准备讲","box":[[297.6,499.009],[377.5079000000001,499.009],[377.5079000000001,514.9590000000001],[297.6,514.9590000000001]]},{"text":"，","box":[[377.52,499.009],[393.46999999999997,499.009],[393.46999999999997,514.9590000000001],[377.52,514.9590000000001]]},{"text":"在明天的小组会","box":[[393.24,499.009],[505.1870000000001,499.009],[505.1870000000001,514.9590000000001],[393.24,514.9590000000001]]},{"text":"上","box":[[90,530.209],[105.95,530.209],[105.95,546.159],[90,546.159]]},{"text":"，","box":[[105.959,530.209],[121.909,530.209],[121.909,546.159],[105.959,546.159]]},{"text":"我也准备开上一炮","box":[[121.8,530.209],[249.70640000000003,530.209],[249.70640000000003,546.159],[121.8,546.159]]},{"text":"。","box":[[249.72,530.209],[265.67,530.209],[265.67,546.159],[249.72,546.159]]},{"text":"我不想讲别的","box":[[265.559,530.209],[361.5466000000001,530.209],[361.5466000000001,546.159],[265.559,546.159]]},{"text":"，","box":[[361.559,530.209],[377.509,530.209],[377.509,546.159],[361.559,546.159]]},{"text":"就是讲讲毛主席的","box":[[377.28,530.209],[505.18640000000016,530.209],[505.18640000000016,546.159],[377.28,546.159]]},{"text":"领袖地位问题","box":[[90,561.409],[185.98760000000001,561.409],[185.98760000000001,577.359],[90,577.359]]},{"text":"。","box":[[186,561.409],[201.95,561.409],[201.95,577.359],[186,577.359]]},{"text":"一个党","box":[[196.44,561.409],[244.4291,561.409],[244.4291,577.359],[196.44,577.359]]},{"text":"，","box":[[244.44,561.409],[260.39,561.409],[260.39,577.359],[244.44,577.359]]},{"text":"一个国家","box":[[254.88,561.409],[318.8285000000001,561.409],[318.8285000000001,577.359],[254.88,577.359]]},{"text":"，","box":[[318.84,561.409],[334.78999999999996,561.409],[334.78999999999996,577.359],[318.84,577.359]]},{"text":"没有一个好的领袖行吗？","box":[[329.4,561.409],[505.42520000000025,561.409],[505.42520000000025,577.359],[329.4,577.359]]},{"text":"那肯定不行","box":[[90,592.609],[169.90789999999998,592.609],[169.90789999999998,608.5590000000001],[90,608.5590000000001]]},{"text":"！","box":[[170.04,592.609],[185.98999999999998,592.609],[185.98999999999998,608.5590000000001],[170.04,608.5590000000001]]},{"text":"苏联没有一个好领袖","box":[[185.759,592.609],[329.7451000000001,592.609],[329.7451000000001,608.5590000000001],[185.759,608.5590000000001]]},{"text":"，","box":[[329.759,592.609],[345.709,592.609],[345.709,608.5590000000001],[329.759,608.5590000000001]]},{"text":"结果变修了","box":[[345.6,592.609],[425.5079000000001,592.609],[425.5079000000001,608.5590000000001],[345.6,608.5590000000001]]},{"text":"。","box":[[425.52,592.609],[441.46999999999997,592.609],[441.4699999