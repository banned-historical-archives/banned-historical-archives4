","box":[[338.04,343.009],[353.99,343.009],[353.99,358.959],[338.04,358.959]]},{"text":"2","box":[[122.04,374.209],[130.01500000000001,374.209],[130.01500000000001,390.159],[122.04,390.159]]},{"text":"、","box":[[129.959,374.209],[145.909,374.209],[145.909,390.159],[129.959,390.159]]},{"text":"“","box":[[137.64,374.209],[153.58999999999997,374.209],[153.58999999999997,390.159],[137.64,390.159]]},{"text":"进一步巩固和发展按系统","box":[[153.6,374.209],[329.5049,374.209],[329.5049,390.159],[153.6,390.159]]},{"text":"、","box":[[329.64,374.209],[345.59,374.209],[345.59,390.159],[329.64,390.159]]},{"text":"按单位的革命的大联合","box":[[345.24,374.209],[505.1855000000002,374.209],[505.1855000000002,390.159],[345.24,390.159]]},{"text":"和革命的三结合","box":[[90,405.409],[201.947,405.409],[201.947,421.359],[90,421.359]]},{"text":"”","box":[[201.959,405.409],[217.909,405.409],[217.909,421.359],[201.959,421.359]]},{"text":"；","box":[[210,405.409],[225.95,405.409],[225.95,421.359],[210,421.359]]},{"text":"3","box":[[122.04,436.609],[130.01500000000001,436.609],[130.01500000000001,452.55899999999997],[122.04,452.55899999999997]]},{"text":"、","box":[[129.959,436.609],[145.909,436.609],[145.909,452.55899999999997],[129.959,452.55899999999997]]},{"text":"“","box":[[138,436.609],[153.95,436.609],[153.95,452.55899999999997],[138,452.55899999999997]]},{"text":"深入开展革命的大批判","box":[[153.959,436.609],[313.9045000000001,436.609],[313.9045000000001,452.55899999999997],[153.959,452.55899999999997]]},{"text":"”","box":[[314.04,436.609],[329.99,436.609],[329.99,452.55899999999997],[314.04,452.55899999999997]]},{"text":"；","box":[[321.959,436.609],[337.909,436.609],[337.909,452.55899999999997],[321.959,452.55899999999997]]},{"text":"4","box":[[122.04,467.809],[130.01500000000001,467.809],[130.01500000000001,483.759],[122.04,483.759]]},{"text":"、","box":[[129.959,467.809],[145.909,467.809],[145.909,483.759],[129.959,483.759]]},{"text":"“","box":[[138,467.809],[153.95,467.809],[153.95,483.759],[138,483.759]]},{"text":"继续做好