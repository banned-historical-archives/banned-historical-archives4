18.04,639.759]]},{"text":"，","box":[[489.959,623.809],[505.909,623.809],[505.909,639.759],[489.959,639.759]]},{"text":"在牛皮纸口袋上","box":[[90,655.009],[201.947,655.009],[201.947,670.9590000000001],[90,670.9590000000001]]},{"text":"，","box":[[201.959,655.009],[217.909,655.009],[217.909,670.9590000000001],[201.959,670.9590000000001]]},{"text":"工整地写上","box":[[217.68,655.009],[297.58790000000005,655.009],[297.58790000000005,670.9590000000001],[217.68,670.9590000000001]]},{"text":"“","box":[[297.6,655.009],[313.55,655.009],[313.55,670.9590000000001],[297.6,670.9590000000001]]},{"text":"即呈","box":[[313.68,655.009],[345.5894,655.009],[345.5894,670.9590000000001],[313.68,670.9590000000001]]},{"text":"","box":[[345.5894,655.009],[667.4014,655.009],[667.4014,655.009],[345.5894,655.009]]},{"text":"毛主席","box":[[361.68,655.009],[409.66910000000007,655.009],[409.66910000000007,670.9590000000001],[361.68,670.9590000000001]]},{"text":"”","box":[[409.68,655.009],[425.63,655.009],[425.63,670.9590000000001],[409.68,670.9590000000001]]},{"text":"五个字","box":[[425.64,655.009],[473.62910000000005,655.009],[473.62910000000005,670.9590000000001],[425.64,670.9590000000001]]},{"text":"。","box":[[473.64,655.009],[489.59,655.009],[489.59,670.9590000000001],[473.64,670.9590000000001]]},{"text":"派","box":[[489.24,655.009],[505.19,655.009],[505.19,670.9590000000001],[489.24,670.9590000000001]]},{"text":"人送出报告稿后，陈伯达心里仍然七上八下。","box":[[90,686.209],[409.9004000000002,686.209],[409.9004000000002,702.159],[90,702.159]]},{"text":"送出的稿子一直没有回音","box":[[122.04,717.409],[297.9449,717.409],[297.9449,733.359],[122.04,733.359]]},{"text":"。","box":[[297.959,717.409],[313.909,717.409],[313.909,733.359],[297.959,733.359]]},{"text":"陈伯达的起草工作","box":[[313.32,717.409],[441.2264000000002,717.409],[441.2264000000002,733.359],[313.32,733.359]]},{"text":"更加","box":[[441.24,717.409],[473.1494,717.409],[473.1494,733.359],[441.24,733.359]]},{"text":"缓慢","box":[[473.28,717.409],[505.1894,717.409],[505.1894,733.359],[473.28,733.