],[505.1885000000001,249.409],[505.1885000000001,265.359],[441.24,265.359]]},{"text":"隔离的感觉","box":[[90,280.60900000000004],[169.90789999999998,280.60900000000004],[169.90789999999998,296.559],[90,296.559]]},{"text":"。","box":[[170.04,280.60900000000004],[185.98999999999998,280.60900000000004],[185.98999999999998,296.559],[170.04,296.559]]},{"text":"当时在京西宾馆食堂吃饭","box":[[185.64,280.60900000000004],[361.5449000000001,280.60900000000004],[361.5449000000001,296.559],[185.64,296.559]]},{"text":"，","box":[[361.68,280.60900000000004],[377.63,280.60900000000004],[377.63,296.559],[361.68,296.559]]},{"text":"在餐桌上经常碰到","box":[[377.28,280.60900000000004],[505.18640000000016,280.60900000000004],[505.18640000000016,296.559],[377.28,296.559]]},{"text":"其他省","box":[[90,311.8090000000001],[137.9891,311.8090000000001],[137.9891,327.75900000000007],[90,327.75900000000007]]},{"text":"、","box":[[138,311.8090000000001],[153.95,311.8090000000001],[153.95,327.75900000000007],[138,327.75900000000007]]},{"text":"市的一些首长","box":[[153.72,311.8090000000001],[249.70760000000004,311.8090000000001],[249.70760000000004,327.75900000000007],[153.72,327.75900000000007]]},{"text":"。","box":[[249.72,311.8090000000001],[265.67,311.8090000000001],[265.67,327.75900000000007],[249.72,327.75900000000007]]},{"text":"有","box":[[265.44,311.8090000000001],[281.39,311.8090000000001],[281.39,327.75900000000007],[265.44,327.75900000000007]]},{"text":"一","box":[[281.52,311.8090000000001],[297.46999999999997,311.8090000000001],[297.46999999999997,327.75900000000007],[281.52,327.75900000000007]]},{"text":"天","box":[[297.48,311.8090000000001],[313.43,311.8090000000001],[313.43,327.75900000000007],[297.48,327.75900000000007]]},{"text":"，","box":[[313.44,311.8090000000001],[329.39,311.8090000000001],[329.39,327.75900000000007],[313.44,327.75900000000007]]},{"text":"吴焱金问同桌的一位老干","box":[[329.28,311.8090000000001],[505.1849000000002,311.8090000000001],[505.1849000000002,327.75900000000007],[329.28,327.75900000000007]]},{"text":"部贵姓？","box":[[90,343.009],[153.9485,343.009],[153.9485,358.959],[90,358.959]]},{"text":"那人","box":[[153.959,343.009],[185.8684,343.009],[185.8684,358.959],[153.959,358.959]]},{"text":"说：","box":[[186,343.009],[217.9094,343.009],[217.9094,358.959],[186,358.959]]},{"text":"“","box":[[122.04,374.209],[137.99,374.209],[137.99,390.159],[122.04,390.159]]},{"text":"我叫谭启龙。","box":[[138,374.209],[233.98760000000004,374.209],[233.98760000000004,390.159],[138,390.159]]},{"text":"”","box":[[234,374.209],[249.95,374.209],[249.95,390.159],[234,390.159]]},{"text":"吴焱金说","box":[[122.04,405.409],[185.98850000000002,405.409],[185.98850000000002,421.359],[122.04,421.359]]},{"text":"：","box":[[186,405.409],[201.95,405.409],[201.95,421.359],[186,421.359]]},{"text":"“","box":[[197.64,405.409],[213.58999999999997,405.409],[213.58999999999997,421.359],[197.64,421.359]]},{"text":"久闻大名","box":[[213.6,405.409],[277.54850000000005,405.409],[277.54850000000005,421.359],[213.6,421.359]]},{"text":"，","box":[[277.68,405.409],[293.63,405.409],[293.63,421.359],[277.68,421.359]]},{"text":"毛主席说过","box":[[291.48,405.409],[371.3879000000001,405.409],[371.3879000000001,421.359],[291.48,421.359]]},{"text":"，","box":[[371.4,405.409],[387.34999999999997,405.409],[387.34999999999997,421.359],[371.4,421.359]]},{"text":"山东有个谭启龙","box":[[385.32,405.409],[497.26700000000017,405.409],[497.26700000000017,421.359],[385.32,421.359]]},{"text":"，","box":[[497.28,405.409],[513.23,405.409],[513.23,421.359],[497.28,421.359]]},{"text":"从小当红军","box":[[90,436.609],[169.90789999999998,436.609],[169.90789999999998,452.55899999999997],[90,452.55899999999997]]},{"text":"......","box":[[170.04,436.609],[201.9494,436.609],[201.9494,452.55899999999997],[170.04,452.55899999999997]]},{"text":"”","box":[[201.959,436.609],[217.909,436.609],[217.909,452.55899999999997],[201.959,452.55899999999997]]},{"text":"然后","box":[[218.04,436.609],[249.9494,436.609],[249.9494,452.55899999999997],[218.04,452.55899999999997]]},{"text":"问他：","box":[[249.959,436.609],[297.94810000000007,436.609],[297.94810000000007,452.55899999999997],[249.959,452.55899999999997]]},{"text":"“","box":[[297.959,436.609],[313.909,436.609],[313.909,452.55899999999997],[297.959,452.55899999999997]]},{"text":"你和毛主席很熟吗？","box":[[314.04,436.609],[458.02610000000016,436.609],[458.02610000000016,452.55899999999997],[314.04,452.55899999999997]]},{"text":"”","box":[[458.04,436.609],[473.99,436.609],[473.99,452.55899999999997],[458.04,452.55899999999997]]},{"text":"谭启龙回答：","box":[[122.04,467.809],[218.02760000000006,467.809],[218.02760000000006,483.759],[122.04,483.759]]},{"text":"“","box":[[218.04,467.809],[233.98999999999998,467.809],[233.98999999999998,483.759],[218.04,483.759]]},{"text":"见面不多，还是","box":[[234,467.809],[345.94700000000006,467.809],[345.94700000000006,483.759],[234,483.759]]},{"text":"1931","box":[[345.959,467.809],[377.93324999999993,467.809],[377.93324999999993,483.759],[345.959,483.759]]},{"text":"年见过面。","box":[[378,467.809],[457.9079000000001,467.809],[457.9079000000001,483.759],[378,483.759]]},{"text":"”","box":[[458.04,467.809],[473.99,467.809],[473.99,483.759],[458.04,483.759]]},{"text":"“","box":[[122.04,499.009],[137.99,499.009],[137.99,514.9590000000001],[122.04,514.9590000000001]]},{"text":"那","box":[[138,499.009],[153.95,499.009],[153.95,514.9590000000001],[138,514.9590000000001]]},{"text":"毛主席怎么记得你的名字呢？","box":[[153.959,499.009],[361.90300000000013,499.009],[361.90300000000013,514.9590000000001],[153.959,514.9590000000001]]},{"text":"”","box":[[362.04,499.009],[377.99,499.009],[377.99,514.9590000000001],[362.04,514.9590000000001]]},{"text":"谭启龙说","box":[[122.04,530.209],[185.98850000000002,530.209],[185.98850000000002,546.159],[122.04,546.159]]},{"text":"：","box":[[186,530.209],[201.95,530.209],[201.95,546.159],[186,546.159]]},{"text":"“","box":[[201.6,530.209],[217.54999999999998,530.209],[217.54999999999998,546.159],[201.6,546.159]]},{"text":"这我就不知道了","box":[[217.68,530.209],[329.62700000000007,530.209],[329.62700000000007,546.159],[217.68,546.159]]},{"text":"，","box":[[329.64,530.209],[345.59,530.209],[345.59,546.159],[329.64,546.159]]},{"text":"伟大领袖嘛","box":[[345.48,530.209],[425.3879000000001,530.209],[425.3879000000001,546.159],[345.48,546.159]]},{"text":"，","box":[[425.4,530.209],[441.34999999999997,530.209],[441.34999999999997,546.159],[425.4,546.159]]},{"text":"记忆力当","box":[[441.24,530.209],[505.1885000000001,530.209],[505.1885000000001,546.159],[441.24,546.159]]},{"text":"然不同于一般人。","box":[[90,561.409],[217.90640000000002,561.409],[217.90640000000002,577.359],[90,577.359]]},{"text":"”","box":[[218.04,561.409],[233.98999999999998,561.409],[233.98999999999998,577.359],[218.04,577.359]]},{"text":"吴焱金又问他到这里来干什么，谭启龙","box":[[122.04,592.609],[393.94190000000015,592.609],[393.94190000000015,608.5590000000001],[122.04,608.5590000000001]]},{"text":"没有回","box":[[393.959,592.609],[441.94810000000007,592.609],[441.94810000000007,608.5590000000001],[393.959,608.5590000000001]]},{"text":"答。","box":[[441.959,592.609],[473.8684,592.609],[473.8684,608.5590000000001],[441.959,608.5590000000001]]},{"text":"过几天，吴焱金又同一个脸上有麻子的军人同桌吃饭","box":[[122.04,623.809],[489.9389000000003,623.809],[489.9389000000003,639.759],[122.04,639.759]]},{"text":"，","box":[[489.959,623.809],[505.909,623.809],[505.909,639.759],[489.959,639.759]]},{"text":"有人告诉他","box":[[90,655.009],[169.90789999999998,655.009],[169.90789999999998,670.9590000000001],[90,670.9590000000001]]},{"text":"，","box":[[170.04,655.009],[185.98999999999998,655.009],[185.98999999999998,670.9590000000001],[170.04,670.9590000000001]]},{"text":"这个麻子就是王效禹","box":[[185.64,655.009],[329.62610000000006,655.009],[329.62610000000006,670.9590000000001],[185.64,670.9590000000001]]},{"text":"。","box":[[329.64,655.009],[345.59,655.009],[345.59,670.9590000000001],[329.64,670.9590000000001]]},{"text":"吴焱金赶紧问王效禹住","box":[[345.24,655.009],[505.1855000000002,655.009],[505.1855000000002,670.9590000000001],[345.24,670.9590000000001]]},{"text":"在哪里？王效禹告诉他住在","box":[[90,686.209],[281.98460000000006,686.209],[281.98460000000006,702.159],[90,702.159]]},{"text":"405","box":[[282,686.209],[306.05469999999997,686.209],[306.05469999999997,702.159],[282,702.159]]},{"text":"号房间。吴焱金把情况告诉","box":[[306,686.209],[497.9846000000002,686.209],[497.9846000000002,702.159],[306,702.159]]},{"text":"胡厚民","box":[[90,717.409],[137.9891,717.409],[137.9891,733.359],[90,733.359]]},{"text":"，","box":[[138,717.409],[153.95,717.409],[153.95,733.359],[138,733.359]]},{"text":"胡厚民立即来了兴趣","box":[[153.6,717.409],[297.5861,717.409],[297.5861,733.359],[153.6,733.359]]},{"text":"，","box":[[297.6,717.409],[313.55,717.409],[313.55,733.359],[297.6,733.359]]},{"text":"要同吴焱金一块去拜访王效","box":[[313.32,717.409],[505.3046000000002,717.409],[505.3046000000002,733.359],[313.32,733.359]]},{"text":"禹。","box":[[90,748.609],[121.9094,748.609],[121.9094,764.5590000000001],[90,764.5590000000001]]}]}