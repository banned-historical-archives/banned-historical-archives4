":[[441.24,655.009],[505.1885000000001,655.009],[505.1885000000001,670.9590000000001],[441.24,670.9590000000001]]},{"text":"的代表也都参加了接见。","box":[[90,686.209],[265.90490000000005,686.209],[265.90490000000005,702.159],[90,702.159]]},{"text":"坐下后，","box":[[122.04,717.409],[185.98850000000002,717.409],[185.98850000000002,733.359],[122.04,733.359]]},{"text":"康","box":[[186,717.409],[201.95,717.409],[201.95,733.359],[186,733.359]]},{"text":"生问","box":[[201.959,717.409],[233.8684,717.409],[233.8684,733.359],[201.959,733.359]]},{"text":"：","box":[[234,717.409],[249.95,717.409],[249.95,733.359],[234,733.359]]},{"text":"“","box":[[242.04,717.409],[257.99,717.409],[257.99,733.359],[242.04,733.359]]},{"text":"今天接见都去了吧","box":[[258,717.409],[385.9064000000002,717.409],[385.9064000000002,733.359],[258,733.359]]},{"text":"！","box":[[386.04,717.409],[401.99,717.409],[401.99,733.359],[386.04,733.359]]},{"text":"”","box":[[393.959,717.409],[409.909,717.409],[409.909,733.359],[393.959,733.359]]},{"text":"众","box":[[122.04,748.609],[137.99,748.609],[137.99,764.5590000000001],[122.04,764.5590000000001]]},{"text":"人答","box":[[138,748.609],[169.9094,748.609],[169.9094,764.5590000000001],[138,764.5590000000001]]},{"text":"：","box":[[170.04,748.609],[185.98999999999998,748.609],[185.98999999999998,764.5590000000001],[170.04,764.5590000000001]]},{"text":"“","box":[[177.959,748.609],[193.909,748.609],[193.909,764.5590000000001],[177.959,764.5590000000001]]},{"text":"都去了，我们很幸福","box":[[194.04,748.609],[338.02610000000004,748.609],[338.02610000000004,764.5590000000001],[194.04,764.5590000000001]]},{"text":"。","box":[[338.04,748.609],[353.99,748.609],[353.99,764.5590000000001],[338.04,764.5590000000001]]},{"text":"”","box":[[345.959,748.609],[361.909,748.609],[361.909,764.5590000000001],[345.959,764.5590000000001]]}]}