559]]},{"text":"”","box":[[114,280.60900000000004],[129.95,280.60900000000004],[129.95,296.559],[114,296.559]]},{"text":"并让秘书打电话告诉了宋城。","box":[[129.959,280.60900000000004],[337.9030000000001,280.60900000000004],[337.9030000000001,296.559],[129.959,296.559]]},{"text":"中央办公厅秘书处根据中央指示","box":[[122.04,311.8090000000001],[345.94340000000005,311.8090000000001],[345.94340000000005,327.75900000000007],[122.04,327.75900000000007]]},{"text":"，","box":[[345.959,311.8090000000001],[361.909,311.8090000000001],[361.909,327.75900000000007],[345.959,327.75900000000007]]},{"text":"通知收回华北组","box":[[361.68,311.8090000000001],[473.6270000000002,311.8090000000001],[473.6270000000002,327.75900000000007],[361.68,327.75900000000007]]},{"text":"《","box":[[473.28,311.8090000000001],[489.22999999999996,311.8090000000001],[489.22999999999996,327.75900000000007],[473.28,327.75900000000007]]},{"text":"六","box":[[489.24,311.8090000000001],[505.19,311.8090000000001],[505.19,327.75900000000007],[489.24,327.75900000000007]]},{"text":"号简报","box":[[90,343.009],[138.5891,343.009],[138.5891,358.959],[90,358.959]]},{"text":"》","box":[[138.84,343.009],[154.79,343.009],[154.79,358.959],[138.84,358.959]]},{"text":"。林彪、叶群一面让警卫参谋抄一份留存，一面给","box":[[147.12,343.009],[505.18279999999993,343.009],[505.18279999999993,358.959],[147.12,358.959]]},{"text":"中办秘书处打电话说","box":[[90,374.209],[233.98610000000005,374.209],[233.98610000000005,390.159],[90,390.159]]},{"text":"：","box":[[234,374.209],[249.95,374.209],[249.95,390.159],[234,390.159]]},{"text":"“","box":[[241.8,374.209],[257.75,374.209],[257.75,390.159],[241.8,390.159]]},{"text":"毛主席决定收回","box":[[257.759,374.209],[369.70600000000013,374.209],[369.70600000000013,390.159],[257.759,390.159]]},{"text":"《","box":[[369.6,374.209],[385.55,374.209],[385.55,390.159],[369.6,390.159]]},{"text":"简报","box":[[385.559,374.209],[417.46840000000003,374.209],[417.46840000000003,390.159],[385.559,390.159]]},{"text":"》","box":[[417.6,374.209],[433.55,374.209],[433.55,390.159],[417.6,390.159]]},{"text":"，","box":[[425.52,374.209],[441.46999999999997,374.209],[441.46999999999997,390.159],[425.52,390.159]]},{"text":"我们带头","box":[[441.24,374.209],[505.1885000000001,374.209],[505.1885000000001,390.159],[441.24,390.159]]},{"text":"交","box":[[90,405.409],[105.95,405.409],[105.95,421.359],[90,421.359]]},{"text":"。","box":[[105.959,405.409],[121.909,405.409],[121.909,421.359],[105.959,421.359]]},{"text":"”","box":[[114,405.409],[129.95,405.409],[129.95,421.359],[114,421.359]]},{"text":"汪东兴给吴法宪打电话说","box":[[122.04,436.609],[297.9449,436.609],[297.9449,452.55899999999997],[122.04,452.55899999999997]]},{"text":"：","box":[[297.959,436.609],[313.909,436.609],[313.909,452.55899999999997],[297.959,452.55899999999997]]},{"text":"“","box":[[306,436.609],[321.95,436.609],[321.95,452.55899999999997],[306,452.55899999999997]]},{"text":"以后不要通电话了","box":[[321.959,436.609],[449.8654000000002,436.609],[449.8654000000002,452.55899999999997],[321.959,452.55899999999997]]},{"text":"。","box":[[450,436.609],[465.95,436.609],[465.95,452.55899999999997],[450,452.55899999999997]]},{"text":"”","box":[[458.04,436.609],[473.99,436.609],[473.99,452.55899999999997],[458.04,452.55899999999997]]},{"text":"林立果打电话转告王维国不要发言了。","box":[[122.04,467.809],[393.94190000000015,467.809],[393.94190000000015,483.759],[122.04,483.759]]},{"text":"晚饭后","box":[[122.04,499.009],[170.02910000000003,499.009],[170.02910000000003,514.9590000000001],[122.04,514.9590000000001]]},{"text":"，","box":[[170.04,499.009],[185.98999999999998,499.009],[185.98999999999998,514.9590000000001],[170.04,514.9590000000001]]},{"text":"陈励耘到福州空军政委韦祖珍房里去玩","box":[[185.64,499.009],[457.5419000000002,499.009],[457.5419000000002,514.9590000000001],[185.64,514.9590000000001]]},{"text":"，","box":[[457.68,499.009],[473.63,499.009],[473.63,514.9590000000001],[457.68,514.9590000000001]]},{"text":"韩先","box":[[473.28,499.009],[505.1894,499.009],[505.1894,514.9590000000001],[473.28,514.9590000000001]]},{"text":"楚也跟了进来，问他","box":[[90,530.209],[233.98610000000005,530.209],[233.98610000000005,546.159],[90,546.159]]},{"text":"：","box":[[234,530.209],[249.95,530.209],[249.95,546.159],[234,546.159]]},{"text":"“","box":[[242.04,530.209],[257.99,530.209],[257.99,546.159],[242.04,546.159]]},{"text":"老陈，你到底在讲谁","box":[[258,530.209],[401.9861000000002,530.209],[401.9861000000002,546.159],[258,546.159]]},{"text":"?","box":[[402,530.209],[409.975,530.209],[409.975,546.159],[402,546.159]]},{"text":"”","box":[[410.04,530.209],[425.99,530.209],[425.99,546.159],[410.04,546.159]]},{"text":"陈励耘反问","box":[[122.04,561.409],[201.94790000000003,561.409],[201.94790000000003,577.359],[122.04,577.359]]},{"text":"：","box":[[201.959,561.409],[217.909,561.409],[217.909,577.359],[201.959,577.359]]},{"text":"“","box":[[209.64,561.409],[225.58999999999997,561.409],[225.58999999999997,577.359],[209.64,577.359]]},{"text":"你看我是讲谁","box":[[225.6,561.409],[321.58760000000007,561.409],[321.58760000000007,577.359],[225.6,577.359]]},{"text":"?","box":[[321.6,561.409],[329.57500000000005,561.409],[329.57500000000005,577.359],[321.6,577.359]]},{"text":"我不是说过嘛","box":[[329.64,561.409],[425.6276000000001,561.409],[425.6276000000001,577.359],[329.64,577.359]]},{"text":"，","box":[[425.64,561.409],[441.59,561.409],[441.59,577.359],[425.64,577.359]]},{"text":"谁反对毛","box":[[441.24,561.409],[505.1885000000001,561.409],[505.1885000000001,577.359],[441.24,577.359]]},{"text":"主席就讲谁","box":[[90,592.609],[169.90789999999998,592.609],[169.90789999999998,608.5590000000001],[90,608.5590000000001]]},{"text":"。","box":[[170.04,592.609],[185.98999999999998,592.609],[185.98999999999998,608.5590000000001],[170.04,608.5590000000001]]},{"text":"”","box":[[177.959,592.609],[193.909,592.609],[193.909,608.5590000000001],[177.959,608.5590000000001]]},{"text":"韩先楚不说话，走了。","box":[[122.04,623.809],[281.9855,623.809],[281.9855,639.759],[122.04,639.759]]},{"text":"韩先楚一走","box":[[122.04,655.009],[201.94790000000003,655.009],[201.94790000000003,670.9590000000001],[122.04,670.9590000000001]]},{"text":"，","box":[[201.959,655.009],[217.909,655.009],[217.909,670.9590000000001],[201.959,670.9590000000001]]},{"text":"韦祖珍拉陈励耘出去散步","box":[[215.04,655.009],[390.9449000000002,655.009],[390.9449000000002,670.9590000000001],[215.04,670.9590000000001]]},{"text":"，","box":[[391.08,655.009],[407.03,655.009],[407.03,670.9590000000001],[391.08,670.9590000000001]]},{"text":"边走边说","box":[[404.159,655.009],[468.1075000000001,655.009],[468.1075000000001,670.9590000000001],[404.159,670.9590000000001]]},{"text":"：","box":[[468.24,655.009],[484.19,655.009],[484.19,670.9590000000001],[468.24,670.9590000000001]]},{"text":"“","box":[[473.28,655.009],[489.22999999999996,655.009],[489.22999999999996,670.9590000000001],[473.28,670.9590000000001]]},{"text":"老","box":[[489.24,655.009],[505.19,655.009],[505.19,670.9590000000001],[489.24,670.9590000000001]]},{"text":"陈","box":[[90,686.209],[105.95,686.209],[105.95,702.159],[90,702.159]]},{"text":"，","box":[[105.959,686.209],[121.909,686.209],[121.909,702.159],[105.959,702.159]]},{"text":"你说得对","box":[[121.8,686.209],[185.7485,686.209],[185.7485,702.159],[121.8,702.159]]},{"text":"，","box":[[185.759,686.209],[201.70899999999997,686.209],[201.70899999999997,702.159],[185.759,702.159]]},{"text":"张春桥算老几","box":[[201.48,686.209],[297.4676,686.209],[297.4676,702.159],[201.48,702.159]]},{"text":"，","box":[[297.6,686.209],[313.55,686.209],[313.55,702.159],[297.6,702.159]]},{"text":"他有什么资格？是他反对毛","box":[[313.32,686.209],[505.3046000000002,686.209],[505.3046000000002,702.159],[313.32,702.159]]},{"text":"主席","box":[[90,717.409],[121.9094,717.409],[121.9094,733.359],[90,733.359]]},{"text":"！","box":[[122.04,717.409],[137.99,717.409],[137.99,733.359],[122.04,733.359]]},{"text":"”","box":[[129.959,717.409],[145.909,717.409],[145.909,733.359],[129.959,733.359]]},{"text":"25","box":[[122.04,748.609],[138.05485,748.609],[138.05485,764.5590000000001],[122.04,764.5590000000001]]},{"text":"","box":[[138.05485,748.609],[216.1378500000003,748.609],[216.1378500000003,748.609],[138.05485,748.609]]},{"text":"日下午和晚上","box":[[141.959,748.609],[237.94660000000005,748.609],[237.94660000000005,764.5590000000001],[141.959,764.5590000000001]]},{"text":"，","box":[[237.959,748.609],[253.909,748.609],[253.909,764.5590000000001],[237.959,764.5590000000001]]},{"text":"周恩来同前来的陈伯达","box":[[247.68,748.609],[407.6255000000002,748.609],[407.6255000000002,764.5590000000001],[247.68,764.5590000000001]]},{"text":"、","box":[[407.64,748.609],[423.59,748.609],[423.59,764.5590000000001],[407.64,764.5590000000001]]},{"text":"汪东兴谈话","box":[[417.24,748.609],[497.1479000000001,748.609],[497.1479000000001,764.5590000000001],[417.24,764.5590000000001]]},{"text":"，","box":[[497.28,748.609],[513.23,748.609],[513.23,764.5590000000001],[497.28,764.5590000000001]]}]}