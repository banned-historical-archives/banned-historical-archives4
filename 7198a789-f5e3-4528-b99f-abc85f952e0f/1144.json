296.559]]},{"text":"给他看了","box":[[350.88,280.60900000000004],[414.8285000000001,280.60900000000004],[414.8285000000001,296.559],[350.88,296.559]]},{"text":"《","box":[[409.44,280.60900000000004],[425.39,280.60900000000004],[425.39,296.559],[409.44,296.559]]},{"text":"六号简报","box":[[425.4,280.60900000000004],[489.34850000000006,280.60900000000004],[489.34850000000006,296.559],[425.4,296.559]]},{"text":"》","box":[[489.359,280.60900000000004],[505.30899999999997,280.60900000000004],[505.30899999999997,296.559],[489.359,296.559]]},{"text":"。","box":[[497.4,280.60900000000004],[513.35,280.60900000000004],[513.35,296.559],[497.4,296.559]]},{"text":"两个人议论开了","box":[[90,311.8090000000001],[201.947,311.8090000000001],[201.947,327.75900000000007],[90,327.75900000000007]]},{"text":"，","box":[[201.959,311.8090000000001],[217.909,311.8090000000001],[217.909,327.75900000000007],[201.959,327.75900000000007]]},{"text":"越谈越觉得无所适从","box":[[217.8,311.8090000000001],[361.78610000000015,311.8090000000001],[361.78610000000015,327.75900000000007],[217.8,327.75900000000007]]},{"text":"，","box":[[361.8,311.8090000000001],[377.75,311.8090000000001],[377.75,327.75900000000007],[361.8,327.75900000000007]]},{"text":"疑窦丛生","box":[[377.52,311.8090000000001],[441.46850000000006,311.8090000000001],[441.46850000000006,327.75900000000007],[377.52,327.75900000000007]]},{"text":"。","box":[[441.6,311.8090000000001],[457.55,311.8090000000001],[457.55,327.75900000000007],[441.6,327.75900000000007]]},{"text":"上午华","box":[[457.32,311.8090000000001],[505.30910000000006,311.8090000000001],[505.30910000000006,327.75900000000007],[457.32,327.75900000000007]]},{"text":"东组又要开会","box":[[90,343.009],[185.98760000000001,343.009],[185.98760000000001,358.959],[90,358.959]]},{"text":"，","box":[[186,343.009],[201.95,343.009],[201.95,358.959],[186,358.959]]},{"text":"上海小组该怎么表态呢？他们想去找张春桥","box":[[201.24,343.009],[505.1810000000004,343.009],[505.1810000000004,358.959],[201.24,358.959]]},{"text":"请示","box":[[90,374.209],[121.