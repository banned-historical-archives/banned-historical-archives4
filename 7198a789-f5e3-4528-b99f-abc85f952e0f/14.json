ox":[[90,218.20899999999995],[105.95,218.20899999999995],[105.95,234.15899999999993],[90,234.15899999999993]]},{"text":"”","box":[[105.959,218.20899999999995],[121.909,218.20899999999995],[121.909,234.15899999999993],[105.959,234.15899999999993]]},{"text":"上","box":[[122.04,218.20899999999995],[137.99,218.20899999999995],[137.99,234.15899999999993],[122.04,234.15899999999993]]},{"text":"批写：","box":[[138,218.20899999999995],[185.9891,218.20899999999995],[185.9891,234.15899999999993],[138,234.15899999999993]]},{"text":"“","box":[[122.04,249.409],[137.99,249.409],[137.99,265.359],[122.04,265.359]]},{"text":"我对罪大恶极的刘贼","box":[[138,249.409],[281.98609999999996,249.409],[281.98609999999996,265.359],[138,265.359]]},{"text":"，","box":[[282,249.409],[297.95,249.409],[297.95,265.359],[282,265.359]]},{"text":"同样有强烈的革命义愤","box":[[297.6,249.409],[457.54550000000023,249.409],[457.54550000000023,265.359],[297.6,265.359]]},{"text":"，","box":[[457.68,249.409],[473.63,249.409],[473.63,265.359],[457.68,265.359]]},{"text":"并向","box":[[473.28,249.409],[505.1894,249.409],[505.1894,265.359],[473.28,265.359]]},{"text":"高举毛主席伟大红旗","box":[[90,280.60900000000004],[233.98610000000005,280.60900000000004],[233.98610000000005,296.559],[90,296.559]]},{"text":"，","box":[[234,280.60900000000004],[249.95,280.60900000000004],[249.95,296.559],[234,296.559]]},{"text":"对刘贼及其一伙叛徒特务进行不妥协","box":[[249.24,280.60900000000004],[505.18250000000035,280.60900000000004],[505.18250000000035,296.559],[249.24,296.559]]},{"text":"斗争的江青同志，致以衷心的敬意。","box":[[90,311.8090000000001],[345.9425000000001,311.8090000000001],[345.9425000000001,327.75900000000007],[90,327.75900000000007]]},{"text":"”","box":[[345.959,311.8090000000001],[361.909,311.8090000000001],[361.909,327.75900000000007],[345.959,327.75900000000007]]},{"text":"当天","box":[[122.04,343.009],[153.9494,343.009],[153.9494,358.959],[122.04,358.959]]},{"text":"，周恩来","box":[[153.959,343.009],[217.90750000000003,343.009],[217.90750000000003,358.959],[153.959,358.959]]},{"text":"也","box":[[218.04,343.009],[233.98999999999998,343.009],[233.98999999999998,358.959],[218.04,358.959]]},{"text":"在","box":[[234,343.009],[249.95,343.009],[249.95,358.959],[234,358.959]]},{"text":"这份","box":[[249.959,343.009],[281.8684,343.009],[281.8684,358.959],[249.959,358.959]]},{"text":"“","box":[[282,343.009],[297.95,343.009],[297.95,358.959],[282,358.959]]},{"text":"罪证材料","box":[[297.959,343.009],[361.9075000000001,343.009],[361.9075000000001,358.959],[297.959,358.959]]},{"text":"”","box":[[362.04,343.009],[377.99,343.009],[377.99,358.959],[362.04,358.959]]},{"text":"上批道：","box":[[378,343.009],[441.9485000000001,343.009],[441.9485000000001,358.959],[378,358.959]]},{"text":"“","box":[[122.04,374.209],[137.99,374.209],[137.99,390.159],[122.04,390.159]]},{"text":"我完全同意你的批注和看法","box":[[138,374.209],[329.98460000000006,374.209],[329.98460000000006,390.159],[138,390.159]]},{"text":"，","box":[[330,374.209],[345.95,374.209],[345.95,390.159],[330,390.159]]},{"text":"我也是以无比愤怒的心","box":[[345.24,374.209],[505.1855000000002,374.209],[505.1855000000002,390.159],[345.24,390.159]]},{"text":"情看着","box":[[90,405.409],[137.9891,405.409],[137.9891,421.359],[90,421.359]]},{"text":"、","box":[[138,405.409],[153.95,405.409],[153.95,421.359],[138,421.359]]},{"text":"想着","box":[[153.6,405.409],[185.5094,405.409],[185.5094,421.359],[153.6,421.359]]},{"text":"、","box":[[185.64,405.409],[201.58999999999997,405.409],[201.58999999999997,421.359],[185.64,421.359]]},{"text":"批注着这三本刘贼叛卖我们党和牺牲同志们","box":[[201.24,405.409],[505.1810000000004,405.409],[505.1810000000004,421.359],[201.24,421.359]]},{"text":"的材料。","box":[[90,436.609],[153.9485,436.609],[153.9485,452.55899999999997],[90,452.55899999999997]]},{"text":"刘贼是大叛徒","box":[[122.04,467.809],[218.02760000000006,467.809],[218.02760000000006,483.759],[122.04,483.759]]},{"text":"、","box":[[218.04,467.809],[233.98999999999998,467.809],[233.98999999999998,483.759],[218.04,483.759]]},{"text":"大工贼","box":[[233.88,467.809],[281.86909999999995,467.809],[281.86909999999995,483.759],[233.88,483.759]]},{"text":"、","box":[[281.88,467.809],[297.83,467.809],[297.83,483.759],[281.88,483.759]]},{"text":"大内奸","box":[[297.72,467.809],[345.7091000000001,467.809],[345.7091000000001,483.759],[297.72,483.759]]},{"text":"、","box":[[345.72,467.809],[361.67,467.809],[361.67,483.759],[345.72,483.759]]},{"text":"大特务","box":[[361.559,467.809],[409.5481000000001,467.809],[409.5481000000001,483.759],[361.559,483.759]]},{"text":"、","box":[[409.559,467.809],[425.509,467.809],[425.509,483.759],[409.559,483.759]]},{"text":"大汉奸","box":[[425.4,467.809],[473.38910000000004,467.809],[473.38910000000004,483.759],[425.4,483.759]]},{"text":"，","box":[[473.4,467.809],[489.34999999999997,467.809],[489.34999999999997,483.759],[473.4,483.759]]},{"text":"真","box":[[489.359,467.809],[505.30899999999997,467.809],[505.30899999999997,483.759],[489.359,483.759]]},{"text":"是五毒俱全、十恶不赦的反革命分子","box":[[90,499.009],[345.9425000000001,499.009],[345.9425000000001,514.9590000000001],[90,514.9590000000001]]},{"text":"!","box":[[345.959,499.009],[353.934,499.009],[353.934,514.9590000000001],[345.959,514.9590000000001]]},{"text":"我们要首先欢呼我们伟大领袖毛主席亲自发动和领导","box":[[122.04,530.209],[505.3043000000006,530.209],[505.3043000000006,546.159],[122.04,546.159]]},{"text":"的这场无产阶级文化大革命","box":[[90,561.409],[281.98460000000006,561.409],[281.98460000000006,577.359],[90,577.359]]},{"text":"。","box":[[282,561.409],[297.95,561.409],[297.95,577.359],[282,577.359]]},{"text":"没有这种大革命","box":[[297.6,561.409],[409.54700000000014,561.409],[409.54700000000014,577.359],[297.6,577.359]]},{"text":"，","box":[[409.68,561.409],[425.63,561.409],[425.63,577.359],[409.68,577.359]]},{"text":"怎样能够把","box":[[425.28,561.409],[505.18790000000007,561.409],[505.18790000000007,577.359],[425.28,577.359]]},{"text":"刘贼及其一伙人的叛党卖国","box":[[90,592.609],[281.98460000000006,592.609],[281.98460000000006,608.5590000000001],[90,608.5590000000001]]},{"text":"、","box":[[282,592.609],[297.95,592.609],[297.95,608.5590000000001],[282,608.5590000000001]]},{"text":"杀害同志的罪状","box":[[293.64,592.609],[405.5870000000001,592.609],[405.5870000000001,608.5590000000001],[293.64,608.5590000000001]]},{"text":"，","box":[[405.6,592.609],[421.55,592.609],[421.55,608.5590000000001],[405.6,608.5590000000001]]},{"text":"挖得这样深","box":[[417.24,592.609],[497.1479000000001,592.609],[497.1479000000001,608.5590000000001],[417.24,608.5590000000001]]},{"text":"，","box":[[497.28,592.609],[513.23,592.609],[513.23,608.5590000000001],[497.28,608.5590000000001]]},{"text":"这样广？当然我们还要继续挖下去","box":[[90,623.809],[329.98310000000004,623.809],[329.98310000000004,639.759],[90,639.759]]},{"text":"，","box":[[330,623.809],[345.95,623.809],[345.95,639.759],[330,639.759]]},{"text":"不能有丝毫松懈","box":[[345.6,623.809],[457.54700000000014,623.809],[457.54700000000014,639.759],[345.6,639.759]]},{"text":"，","box":[[457.68,623.809],[473.63,623.809],[473.63,639.759],[457.68,639.759]]},{"text":"不能","box":[[473.28,623.809],[505.1894,623.809],[505.1894,639.759],[473.28,639.759]]},{"text":"失掉警惕，如果挖不完，我们要交给后来人","box":[[90,655.009],[393.9410000000002,655.009],[393.9410000000002,670.9590000000001],[90,670.9590000000001]]},{"text":"!","box":[[393.959,655.009],[401.934,655.009],[401.934,670.9590000000001],[393.959,670.9590000000001]]},{"text":"我们要万分感谢林副主席高举毛泽东思想伟大红旗","box":[[122.04,686.209],[473.97950000000026,686.209],[473.97950000000026,702.159],[122.04,702.159]]},{"text":"，","box":[[474,686.209],[489.95,686.209],[489.95,702.159],[474,702.159]]},{"text":"把","box":[[489.24,686.209],[505.19,686.209],[505.19,702.159],[489.24,702.159]]},{"text":"毛泽东思想","box":[[90,717.409],[169.90789999999998,717.409],[169.90789999999998,733.359],[90,733.359]]},{"text":"、","box":[[170.04,717.409],[185.98999999999998,717.409],[185.98999999999998,733.359],[170.04,733.359]]},{"text":"毛主席声音","box":[[185.28,717.409],[265.1879,717.409],[265.1879,733.359],[185.28,733.359]]},{"text":"(","box":[[265.32,717.409],[273.295,717.409],[273.295,733.359],[265.32,733.359]]