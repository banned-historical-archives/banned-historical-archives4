.55899999999997],[90,452.55899999999997]]},{"text":"。","box":[[266.04,436.609],[281.99,436.609],[281.99,452.55899999999997],[266.04,452.55899999999997]]},{"text":"散会后","box":[[281.759,436.609],[329.7481000000001,436.609],[329.7481000000001,452.55899999999997],[281.759,452.55899999999997]]},{"text":"，","box":[[329.759,436.609],[345.709,436.609],[345.709,452.55899999999997],[329.759,452.55899999999997]]},{"text":"他","box":[[345.6,436.609],[361.55,436.609],[361.55,452.55899999999997],[345.6,452.55899999999997]]},{"text":"对","box":[[361.559,436.609],[377.509,436.609],[377.509,452.55899999999997],[361.559,452.55899999999997]]},{"text":"周恩来","box":[[377.52,436.609],[425.50910000000005,436.609],[425.50910000000005,452.55899999999997],[377.52,452.55899999999997]]},{"text":"说","box":[[425.52,436.609],[441.46999999999997,436.609],[441.46999999999997,452.55899999999997],[425.52,452.55899999999997]]},{"text":"：","box":[[441.6,436.609],[457.55,436.609],[457.55,452.55899999999997],[441.6,452.55899999999997]]},{"text":"“","box":[[457.32,436.609],[473.27,436.609],[473.27,452.55899999999997],[457.32,452.55899999999997]]},{"text":"我那","box":[[473.28,436.609],[505.1894,436.609],[505.1894,452.55899999999997],[473.28,452.55899999999997]]},{"text":"个稿子只剩下一点了，怎么办？","box":[[90,467.809],[313.90340000000003,467.809],[313.90340000000003,483.759],[90,483.759]]},{"text":"”","box":[[314.04,467.809],[329.99,467.809],[329.99,483.759],[314.04,483.759]]},{"text":"周恩来","box":[[122.04,499.009],[170.02910000000003,499.009],[170.02910000000003,514.9590000000001],[122.04,514.9590000000001]]},{"text":"安慰他说","box":[[170.04,499.009],[233.98850000000002,499.009],[233.98850000000002,514.9590000000001],[170.04,514.9590000000001]]},{"text":"：","box":[[234,499.009],[249.95,499.009],[249.95,514.9590000000001],[234,514.9590000000001]]},{"text":"“","box":[[249.6,499.009],[265.55,499.009],[265.55,514.9590000000001],[249.6,514.9590000000001]]},{"text":"那你就把它写完吧","box":[[265.559,499.009],[393.4654000000002,499.009],[393.4654000000002,514.9590000000001],[265.559,514.9590000000001]]},{"text":"，","box":[[393.6,499.009],[409.55,499.009],[409.55,514.9590000000001],[393.6,514.9590000000001]]},{"text":"作个纪念也好","box":[[409.32,499.009],[505.3076000000001,499.009],[505.3076000000001,514.9590000000001],[409.32,514.9590000000001]]},{"text":"嘛","box":[[90,530.209],[105.95,530.209],[105.95,546.159],[90,546.159]]},{"text":"。","box":[[105.959,530.209],[121.909,530.209],[121.909,546.159],[105.959,546.159]]},{"text":"”","box":[[121.32,530.209],[137.26999999999998,530.209],[137.26999999999998,546.159],[121.32,546.159]]},{"text":"他","box":[[137.28,530.209],[153.23,530.209],[153.23,546.159],[137.28,546.159]]},{"text":"让陈","box":[[153.24,530.209],[185.1494,530.209],[185.1494,546.159],[153.24,546.159]]},{"text":"伯达把已经","box":[[185.28,530.209],[265.1879,530.209],[265.1879,546.159],[185.28,546.159]]},{"text":"写完的报告稿","box":[[265.32,530.209],[361.3076000000001,530.209],[361.3076000000001,546.159],[265.32,546.159]]},{"text":"在","box":[[361.32,530.209],[377.27,530.209],[377.27,546.159],[361.32,546.159]]},{"text":"中央文革碰头会上","box":[[377.28,530.209],[505.18640000000016,530.209],[505.18640000000016,546.159],[377.28,546.159]]},{"text":"谈一下","box":[[90,561.409],[137.9891,561.409],[137.9891,577.359],[90,577.359]]},{"text":"。","box":[[138,561.409],[153.95,561.409],[153.95,577.359],[138,577.359]]},{"text":"于是","box":[[153.6,561.409],[185.5094,561.409],[185.5094,577.359],[153.6,577.359]]},{"text":"，","box":[[185.64,561.409],[201.58999999999997,561.409],[201.58999999999997,577.359],[185.64,577.359]]},{"text":"陈伯达","box":[[201.24,561.409],[249.22910000000002,561.409],[249.22910000000002,577.359],[201.24,577.359]]},{"text":"把写好的政治报告稿的前三部分拿了","box":[[249.24,561.409],[505.18250000000035,561.409],[505.18250000000035,577.359],[249.24,577.359]]},{"text":"出来","box":[[90,592.609],[121.9094,592.609],[121.9094,608.5590000000001],[90,608.5590000000001]]},{"text":"，","box":[[122.04,592.609],[137.99,592.609],[137.99,608.5590000000