61.409],[185.75,577.359],[169.8,577.359]]},{"text":"陈毅反动言论小集","box":[[185.759,561.409],[313.6654000000001,561.409],[313.6654000000001,577.359],[185.759,577.359]]},{"text":"》","box":[[313.8,561.409],[329.75,561.409],[329.75,577.359],[313.8,577.359]]},{"text":"当面送给陈毅同志","box":[[329.52,561.409],[457.42640000000017,561.409],[457.42640000000017,577.359],[329.52,577.359]]},{"text":"，","box":[[457.559,561.409],[473.509,561.409],[473.509,577.359],[457.559,577.359]]},{"text":"希望","box":[[473.28,561.409],[505.1894,561.409],[505.1894,577.359],[473.28,577.359]]},{"text":"你能和大家一起，彻底清算自己！","box":[[90,592.609],[329.98310000000004,592.609],[329.98310000000004,608.5590000000001],[90,608.5590000000001]]},{"text":"”","box":[[330,592.609],[345.95,592.609],[345.95,608.5590000000001],[330,608.5590000000001]]},{"text":"说罢","box":[[122.04,623.809],[153.9494,623.809],[153.9494,639.759],[122.04,639.759]]},{"text":"，","box":[[153.959,623.809],[169.909,623.809],[169.909,639.759],[153.959,639.759]]},{"text":"王洪文把小册子递给陈毅","box":[[169.8,623.809],[345.8252000000001,623.809],[345.8252000000001,639.759],[169.8,639.759]]},{"text":"。","box":[[345.72,623.809],[361.67,623.809],[361.67,639.759],[345.72,639.759]]},{"text":"陈毅一下子懵了","box":[[361.559,623.809],[473.5060000000002,623.809],[473.5060000000002,639.759],[361.559,639.759]]},{"text":"，","box":[[473.52,623.809],[489.46999999999997,623.809],[489.46999999999997,639.759],[473.52,639.759]]},{"text":"他","box":[[489.359,623.809],[505.30899999999997,623.809],[505.30899999999997,639.759],[489.359,639.759]]},{"text":"万万没有想到今天会收到这样一份见面礼","box":[[90,655.009],[377.9816000000001,655.009],[377.9816000000001,670.9590000000001],[90,670.9590000000001]]},{"text":"。","box":[[378,655.009],[393.95,655.009],[393.95,670.9590000000001],[378,670.9590000000001]]},{"text":"他欠起身子","box":[[393.6,655.009],[473.5079000000001,655.009],[473.5079000000001,670.9590000000001],[393.6,670.9590000000001]]},{"text":"，","box":[[473.64,655.009],[489.59,655.009],[489.59,670.9590000000001],[47