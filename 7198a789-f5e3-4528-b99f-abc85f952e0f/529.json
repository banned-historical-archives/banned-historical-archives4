":[[473.28,655.009],[505.1894,655.009],[505.1894,670.9590000000001],[473.28,670.9590000000001]]},{"text":"派","box":[[90,686.209],[105.95,686.209],[105.95,702.159],[90,702.159]]},{"text":"。","box":[[105.959,686.209],[121.909,686.209],[121.909,702.159],[105.959,702.159]]},{"text":"说这样的话是很光明磊落的","box":[[121.8,686.209],[313.78460000000007,686.209],[313.78460000000007,702.159],[121.8,702.159]]},{"text":"，","box":[[313.8,686.209],[329.75,686.209],[329.75,702.159],[313.8,702.159]]},{"text":"错了就错了","box":[[329.52,686.209],[409.4279000000001,686.209],[409.4279000000001,702.159],[329.52,702.159]]},{"text":"，","box":[[409.559,686.209],[425.509,686.209],[425.509,702.159],[409.559,702.159]]},{"text":"承认起来改","box":[[425.28,686.209],[505.18790000000007,686.209],[505.18790000000007,702.159],[425.28,702.159]]},{"text":"了就好","box":[[90,717.409],[137.9891,717.409],[137.9891,733.359],[90,733.359]]},{"text":"。","box":[[138,717.409],[153.95,717.409],[153.95,733.359],[138,733.359]]},{"text":"”","box":[[146.04,717.409],[161.98999999999998,717.409],[161.98999999999998,733.359],[146.04,733.359]]}]}