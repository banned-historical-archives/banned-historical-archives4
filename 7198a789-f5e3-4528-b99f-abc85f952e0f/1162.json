39.759]]},{"text":"。","box":[[170.04,623.809],[185.98999999999998,623.809],[185.98999999999998,639.759],[170.04,639.759]]},{"text":"过了","box":[[181.8,623.809],[213.70940000000002,623.809],[213.70940000000002,639.759],[181.8,639.759]]},{"text":"一会儿","box":[[213.84,623.809],[261.82910000000004,623.809],[261.82910000000004,639.759],[213.84,639.759]]},{"text":"，","box":[[261.84,623.809],[277.78999999999996,623.809],[277.78999999999996,639.759],[261.84,639.759]]},{"text":"看大家谁也没动","box":[[273.6,623.809],[385.54700000000014,623.809],[385.54700000000014,639.759],[273.6,639.759]]},{"text":"，","box":[[385.68,623.809],[401.63,623.809],[401.63,639.759],[385.68,639.759]]},{"text":"又挥挥手","box":[[397.559,623.809],[461.5075000000001,623.809],[461.5075000000001,639.759],[397.559,639.759]]},{"text":"：","box":[[461.52,623.809],[477.46999999999997,623.809],[477.46999999999997,639.759],[461.52,639.759]]},{"text":"“","box":[[473.4,623.809],[489.34999999999997,623.809],[489.34999999999997,639.759],[473.4,639.759]]},{"text":"不","box":[[489.359,623.809],[505.30899999999997,623.809],[505.30899999999997,639.759],[489.359,639.759]]},{"text":"开会了","box":[[90,655.009],[137.9891,655.009],[137.9891,670.9590000000001],[90,670.9590000000001]]},{"text":"。","box":[[138,655.009],[153.95,655.009],[153.95,670.9590000000001],[138,670.9590000000001]]},{"text":"刚才我到毛主席那里去了","box":[[153.72,655.009],[329.6249,655.009],[329.6249,670.9590000000001],[153.72,670.9590000000001]]},{"text":"，","box":[[329.759,655.009],[345.709,655.009],[345.709,670.9590000000001],[329.759,670.9590000000001]]},{"text":"这是毛主席的决定","box":[[345.6,655.009],[473.5064000000002,655.009],[473.5064000000002,670.9590000000001],[345.6,670.9590000000001]]},{"text":"，","box":[[473.52,655.009],[489.46999999999997,655.009],[489.46999999999997,670.9590000000001],[473.52,670.9590000000001]]},{"text":"休","box":[[489.359,655.009],[505.30899999999997,655.009],[505.30899999999997,670.9590000000001],[489.359,670.9590000000001]]},{"text":"会！大家回去吧。","box":[[90,686.209],[217.90640000000002,686.209],[217.90640000000002,702.159],[90,702.159]]},{"text":"”","box":[[218.04,686.209],[233.98999999999998,686.209],[233.98999999999998,702.159],[218.04,702.159]]},{"text":"人们纷纷返回住处。","box":[[122.04,717.409],[266.02610000000004,717.409],[266.02610000000004,733.359],[122.04,733.359]]},{"text":"这时，","box":[[122.04,748.609],[173.02910000000003,748.609],[173.02910000000003,764.5590000000001],[122.04,764.5590000000001]]},{"text":"韩先楚追上许世友问","box":[[174.48,748.609],[330.3473,748.609],[330.3473,764.5590000000001],[174.48,764.5590000000001]]},{"text":"他","box":[[331.8,748.609],[347.75,748.609],[347.75,764.5590000000001],[331.8,764.5590000000001]]},{"text":"：","box":[[349.32,748.609],[365.27,748.609],[365.27,764.5590000000001],[349.32,764.5590000000001]]},{"text":"“","box":[[366.84,748.609],[382.78999999999996,748.609],[382.78999999999996,764.5590000000001],[366.84,764.5590000000001]]},{"text":"你会上指的是谁","box":[[384.359,748.609],[505.30674999999997,748.609],[505.30674999999997,764.5590000000001],[384.359,764.5590000000001]]}]}