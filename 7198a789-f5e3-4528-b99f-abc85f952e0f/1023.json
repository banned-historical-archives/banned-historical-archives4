124.60900000000004],[505.909,140.55900000000003],[489.959,140.55900000000003]]},{"text":"它自己紧张起来了","box":[[90,155.80899999999997],[217.90640000000002,155.80899999999997],[217.90640000000002,171.75899999999996],[90,171.75899999999996]]},{"text":"，","box":[[218.04,155.80899999999997],[233.98999999999998,155.80899999999997],[233.98999999999998,171.75899999999996],[218.04,171.75899999999996]]},{"text":"说越南的鱼雷艇来了","box":[[233.759,155.80899999999997],[377.74510000000015,155.80899999999997],[377.74510000000015,171.75899999999996],[233.759,171.75899999999996]]},{"text":"，","box":[[377.759,155.80899999999997],[393.709,155.80899999999997],[393.709,171.75899999999996],[377.759,171.75899999999996]]},{"text":"开炮就打","box":[[393.6,155.80899999999997],[457.5485000000001,155.80899999999997],[457.5485000000001,171.75899999999996],[393.6,171.75899999999996]]},{"text":"。","box":[[457.559,155.80899999999997],[473.509,155.80899999999997],[473.509,171.75899999999996],[457.559,171.75899999999996]]},{"text":"”","box":[[473.28,155.80899999999997],[489.22999999999996,155.80899999999997],[489.22999999999996,171.75899999999996],[473.28,171.75899999999996]]},{"text":"毛","box":[[489.359,155.80899999999997],[505.30899999999997,155.80899999999997],[505.30899999999997,171.75899999999996],[489.359,171.75899999999996]]},{"text":"泽东","box":[[90,187.00900000000001],[121.9094,187.00900000000001],[121.9094,202.959],[90,202.959]]},{"text":"说着开怀大笑起来，周","box":[[122.04,187.00900000000001],[281.9855,187.00900000000001],[281.9855,202.959],[122.04,202.959]]},{"text":"恩来","box":[[282,187.00900000000001],[313.9094,187.00900000000001],[313.9094,202.959],[282,202.959]]},{"text":"和黎笋也跟着大笑起来。","box":[[314.04,187.00900000000001],[489.94490000000025,187.00900000000001],[489.94490000000025,202.959],[314.04,202.959]]},{"text":"毛","box":[[122.04,218.20899999999995],[137.99,218.20899999999995],[137.99,234.15899999999993],[122.04,234.15899999999993]]},{"text":"泽东","box":[[138.24,218.20899999999995],[170.50955000000002,218.20899999999995],[170.50955000000002,234.15899999999993],[138.24,234.15899999999993]]},{"text":"继续风趣地说","box":[[171,218.20899999999995],[268.54775,218.20899999999995],[268.54775,234.15899999999993],[171,234.15899999999993]]},{"text":"：","box":[[268.8,218.20899999999995],[284.75,218.20899999999995],[284.75,234.15899999999993],[268.8,234.15899999999993]]},{"text":"“","box":[[277.2,218.20899999999995],[293.15,218.20899999999995],[293.15,234.15899999999993],[277.2,234.15899999999993]]},{"text":"对这件事，后来美国人自己也","box":[[293.4,218.20899999999995],[505.18460000000005,218.20899999999995],[505.18460000000005,234.15899999999993],[293.4,234.15899999999993]]},{"text":"讲不出来究竟是真的还是假的","box":[[90,249.409],[297.9440000000001,249.409],[297.9440000000001,265.359],[90,265.359]]},{"text":"。","box":[[297.959,249.409],[313.909,249.409],[313.909,265.359],[297.959,265.359]]},{"text":"美国各地的新闻记者肯定那","box":[[313.32,249.409],[505.3046000000002,249.409],[505.3046000000002,265.359],[313.32,265.359]]},{"text":"是假的","box":[[90,280.60900000000004],[137.9891,280.60900000000004],[137.9891,296.559],[90,296.559]]},{"text":"，","box":[[138,280.60900000000004],[153.95,280.60900000000004],[153.95,296.559],[138,296.559]]},{"text":"那是一场虚惊","box":[[153.84,280.60900000000004],[249.82760000000005,280.60900000000004],[249.82760000000005,296.559],[153.84,296.559]]},{"text":"。","box":[[249.84,280.60900000000004],[265.79,280.60900000000004],[265.79,296.559],[249.84,296.559]]},{"text":"既然打起来了","box":[[265.68,280.60900000000004],[361.6676000000001,280.60900000000004],[361.6676000000001,296.559],[265.68,296.559]]},{"text":"，","box":[[361.68,280.60900000000004],[377.63,280.60900000000004],[377.63,296.559],[361.68,296.559]]},{"text":"那就只好打了","box":[[377.52,280.60900000000004],[473.5076000000001,280.60900000000004],[473.5076000000001,296.559],[377.52,296.559]]},{"text":"。","box":[[473.52,280.60900000000004],[489.46999999999997,280.60900000000004],[489.46999999999997,296.559],[473.52,296.559]]},{"text":"对","box":[[489.359,280.60900000000004],[505.30899999999997,280.60900000000004],[505.30899999999997,296.559],[489.359,296.559]]},{"text":"军火商是有利的","box":[[90,311.8090000000001],[201.947,311.8090000000001],[201.947,327.75900000000007],[90,327.75900000000007]]},{"text":"。","box":[[201.959,311.8090000000001],[217.909,311.8090000000001],[217.909,327.75900000000007],[201.959,327.75900000000007]]},{"text":"据说","box":[[213.6,311.8090000000001],[245.5094,311.8090000000001],[245.5094,327.75900000000007],[213.6,327.75900000000007]]},{"text":"，","box":[[245.64,311.8090000000001],[261.59,311.8090000000001],[261.59,327.75900000000007],[245.64,327.75900000000007]]},{"text":"现在美国总统尼克松每天睡觉很少","box":[[257.28,311.8090000000001],[497.26310000000024,311.8090000000001],[497.26310000000024,327.75900000000007],[257.28,327.75900000000007]]},{"text":"。","box":[[497.28,311.8090000000001],[513.23,311.8090000000001],[513.23,327.75900000000007],[497.28,327.75900000000007]]},{"text":"尼克松自己讲，他的主要精力是对付越南","box":[[90,343.009],[377.9816000000001,343.009],[377.9816000000001,358.959],[90,358.959]]},{"text":"。","box":[[378,343.009],[393.95,343.009],[393.95,358.959],[378,358.959]]},{"text":"”","box":[[386.04,343.009],[401.99,343.009],[401.99,358.959],[386.04,358.959]]},{"text":"“","box":[[122.04,374.209],[137.99,374.209],[137.99,390.159],[122.04,390.159]]},{"text":"现在还有另外一个人，西哈努克亲王，他也不好惹","box":[[138,374.209],[489.93950000000024,374.209],[489.93950000000024,390.159],[138,390.159]]},{"text":"，","box":[[489.959,374.209],[505.909,374.209],[505.909,390.159],[489.959,390.159]]},{"text":"你惹他，他就要跳出来骂娘。","box":[[90,405.409],[297.9440000000001,405.409],[297.9440000000001,421.359],[90,421.359]]},{"text":"我们有些大使馆","box":[[122.04,436.609],[233.98700000000002,436.609],[233.98700000000002,452.55899999999997],[122.04,452.55899999999997]]},{"text":"，","box":[[234,436.609],[249.95,436.609],[249.95,452.55899999999997],[234,452.55899999999997]]},{"text":"我看要整顿一下","box":[[249.6,436.609],[361.5470000000001,436.609],[361.5470000000001,452.55899999999997],[249.6,452.55899999999997]]},{"text":"。","box":[[361.68,436.609],[377.63,436.609],[377.63,452.55899999999997],[361.68,452.55899999999997]]},{"text":"我们中国一些大使","box":[[377.28,436.609],[505.18640000000016,436.609],[505.18640000000016,452.55899999999997],[377.28,452.55899999999997]]},{"text":"馆有大国沙文主义","box":[[90,467.809],[217.90640000000002,467.809],[217.90640000000002,483.759],[90,483.759]]},{"text":"，","box":[[218.04,467.809],[233.98999999999998,467.809],[233.98999999999998,483.759],[218.04,483.759]]},{"text":"尽是把人家的缺点看得多","box":[[233.64,467.809],[409.5449000000002,467.809],[409.5449000000002,483.759],[233.64,483.759]]},{"text":"，","box":[[409.68,467.809],[425.63,467.809],[425.63,483.759],[409.68,483.759]]},{"text":"不顾大局","box":[[425.28,467.809],[489.22850000000005,467.809],[489.22850000000005,483.759],[425.28,483.759]]},{"text":"。","box":[[489.24,467.809],[505.19,467.809],[505.19,483.759],[489.24,483.759]]},{"text":"”","box":[[497.28,467.809],[513.23,467.809],[513.23,483.759],[497.28,483.759]]},{"text":"他问周恩来","box":[[90,499.009],[169.90789999999998,499.009],[169.90789999999998,514.9590000000001],[90,514.9590000000001]]},{"text":"：","box":[[170.04,499.009],[185.98999999999998,499.009],[185.98999999999998,514.9590000000001],[170.04,514.9590000000001]]},{"text":"“","box":[[177.959,499.009],[193.909,499.009],[193.909,514.9590000000001],[177.959,514.9590000000001]]},{"text":"驻越南的上任大使是谁","box":[[194.04,499.009],[353.9855000000001,499.009],[353.9855000000001,514.9590000000001],[194.04,514.9590000000001]]},{"text":"?","box":[[354,499.009],[361.975,499.009],[361.975,514.9590000000001],[354,514.9590000000001]]},{"text":"”","box":[[362.04,499.009],[377.99,499.009],[377.99,514.9590000000001],[362.04,514.9590000000001]]},{"text":"周恩来","box":[[122.04,530.209],[170.02910000000003,530.209],[170.02910000000003,546.159],[122.04,546.159]]},{"text":"答","box":[[170.04,530.209],[185.98999999999998,530.209],[185.98999999999998,546.159],[170.04,546.159]]},{"text":"：","box":[[186,530.209],[201.95,530.209],[201.95,546.159],[186,546.159]]},{"text":"“","box":[[194.04,530.209],[209.98999999999998,530.209],[209.98999999999998,546.159],[194.04,546.159]]},{"text":"朱其文","box":[[210,530.209],[257.9891,530.209],[257.9891,546.159],[210,546.159]]},{"text":"。","box":[[258,530.209],[273.95,530.209],[273.95,546.159],[258,546.159]]},{"text":"”","box":[[266.04,530.209],[281.99,530.209],[281.99,546.159],[266.04,546.159]]},{"text":"毛泽东","box":[[122.04,561.409],[170.62910000000002,561.409],[170.62910000000002,577.359],[122.04,577.359]]},{"text":"说","box":[[171,561.409],[186.95,561.409],[186.95,577.359],[171,577.359]]},{"text":"：","box":[[187.2,561.409],[203.14999999999998,561.409],[203.14999999999998,577.359],[187.2,577.359]]},{"text":"“","box":[[195.6,561.409],[211.54999999999998,561.409],[211.54999999999998,577.359],[195.6,577.359]]},{"text":"朱其文跟你们搞得很僵。事实上朱其文是","box":[[211.92,561.409],[505.30234999999993,561.409],[505.30234999999993,577.359],[211.92,577.359]]},{"text":"个国民党","box":[[90,592.609],[153.9485,592.609],[153.9485,608.5590000000001],[90,608.5590000000001]]},{"text":"，","box":[[153.959,592.609],[169.909,592.609],[169.909,608.5590000000001],[153.959,608.5590000000001]]},{"text":"他要跑到外国去","box":[[169.8,592.609],[281.747,592.609],[281.747,608.5590000000001],[169.8,608.5590000000001]]},{"text":"。","box":[[281.759,592.609],[297.709,592.609],[297.709,608.5590000000001],[281.759,608.5590000000001]]},{"text":"我们不知道他是国民党","box":[[297.6,592.609],[457.54550000000023,592.609],[457.54550000000023,608.5590000000001],[297.6,608.5590000000001]]},{"text":"，","box":[[457.559,592.609],[473.509,592.609],[473.509,608.5590000000001],[457.559,608.5590000000001]]},{"text":"你们","box":[[473.28,592.609],[505.1894,592.609],[505.1894,608.5590000000001],[473.28,608.5590000000001]]},{"text":"跟国民党打交道","box":[[90,623.809],[201.947,623.809],[201.947,639.759],[90,639.759]]},{"text":"，","box":[[201.959,623.809],[217.909,623.809],[217.909,639.759],[201.959,639.759]]},{"text":"他为什么不捣你们的乱呀？那时我们也不","box":[[217.32,623.809],[505.3016000000003,623.809],[505.3016000000003,639.759],[217.32,639.759]]},{"text":"知道，但我们看到（他发回的）那些电报是不高兴的","box":[[90,655.009],[457.89890000000025,655.009],[457.89890000000025,670.9590000000001],[90,670.9590000000001]]},{"text":"。","box":[[458.04,655.009],[473.99,655.009],[473.99,670.9590000000001],[458.04,670.9590000000001]]},{"text":"”","box":[[465.959,655.009],[481.909,655.009],[481.909,670.9590000000001],[465.959,670.9590000000001]]},{"text":"黎笋","box":[[122.04,686.209],[154.42985,686.209],[154.42985,702.159],[122.04,702.159]]},{"text":"：","box":[[154.92,686.209],[170.86999999999998,686.209],[170.86999999999998,702.159],[154.92,702.159]]},{"text":"“","box":[[163.44,686.209],[179.39,686.209],[179.39,702.159],[163.44,702.159]]},{"text":"我们越南人民心中牢记着毛主席的恩情。在","box":[[180,686.209],[492.8237,686.209],[492.8237,702.159],[180,702.159]]},{"text":"","box":[[492.82369999999975,686.209],[581.9497000000042,686.209],[581.9497000000042,686.209],[492.82369999999975,686.209]]},{"text":"9","box":[[497.28,686.209],[505.255,686.209],[505.255,702.159],[497.28,702.159]]},{"text":"年的抗法战争中","box":[[90,717.409],[201.947,717.409],[201.947,733.359],[90,733.359]]},{"text":"，","box":[[201.959,717.409],[217.909,717.409],[217.909,733.359],[201.959,733.359]]},{"text":"如果没有中国共产党和毛主席的支持","box":[[217.68,717.409],[473.6225000000003,717.409],[473.6225000000003,733.359],[217.68,733.359]]},{"text":"，","box":[[473.64,717.409],[489.59,717.409],[489.59,733.359],[473.64,733.359]]},{"text":"我","box":[[489.24,717.409],[505.19,717.409],[505.19,733.359],[489.24,733.359]]},{"text":"们是不可能取得胜利的","box":[[90,748.609],[249.94550000000004,748.609],[249.94550000000004,764.5590000000001],[90,764.5590000000001]]},{"text":"。","box":[[249.959,748.609],[265.909,748.609],[265.909,764.5590000000001],[249.959,764.5590000000001]]},{"text":"我们为什么能坚持打持久战","box":[[265.68,748.609],[457.66460000000023,748.609],[457.66460000000023,764.5590000000001],[265.68,764.5590000000001]]},{"text":"，","box":[[457.68,748.609],[473.63,748.609],[473.63,764.5590000000001],[457.68,764.5590000000001]]},{"text":"尤其","box":[[473.28,748.609],[505.1894,748.609],[505.1894,764.5590000000001],[473.28,764.5590000000001]]}]}