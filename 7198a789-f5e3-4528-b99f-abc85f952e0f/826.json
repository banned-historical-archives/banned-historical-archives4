.55899999999997],[329.759,452.55899999999997]]},{"text":"是大自然的威力","box":[[345.6,436.609],[457.54700000000014,436.609],[457.54700000000014,452.55899999999997],[345.6,452.55899999999997]]},{"text":"。","box":[[457.559,436.609],[473.509,436.609],[473.509,452.55899999999997],[457.559,452.55899999999997]]},{"text":"但是","box":[[473.28,436.609],[505.1894,436.609],[505.1894,452.55899999999997],[473.28,452.55899999999997]]},{"text":"我们的队伍","box":[[90,467.809],[169.90789999999998,467.809],[169.90789999999998,483.759],[90,483.759]]},{"text":"，","box":[[170.04,467.809],[185.98999999999998,467.809],[185.98999999999998,483.759],[170.04,483.759]]},{"text":"也时时有人退隐","box":[[185.759,467.809],[297.7060000000001,467.809],[297.7060000000001,483.759],[185.759,483.759]]},{"text":"，","box":[[297.84,467.809],[313.78999999999996,467.809],[313.78999999999996,483.759],[297.84,483.759]]},{"text":"有人落荒","box":[[313.68,467.809],[377.6285000000001,467.809],[377.6285000000001,483.759],[313.68,483.759]]},{"text":"，","box":[[377.64,467.809],[393.59,467.809],[393.59,483.759],[377.64,483.759]]},{"text":"有人颓唐","box":[[393.6,467.809],[457.5485000000001,467.809],[457.5485000000001,483.759],[393.6,483.759]]},{"text":"，","box":[[457.559,467.809],[473.509,467.809],[473.509,483.759],[457.559,483.759]]},{"text":"有人","box":[[473.4,467.809],[505.3094,467.809],[505.3094,483.759],[473.4,483.759]]},{"text":"叛变","box":[[90,499.009],[121.9094,499.009],[121.9094,514.9590000000001],[90,514.9590000000001]]},{"text":"，","box":[[122.04,499.009],[137.99,499.009],[137.99,514.9590000000001],[122.04,514.9590000000001]]},{"text":"更有人结集在一起","box":[[137.759,499.009],[265.66540000000003,499.009],[265.66540000000003,514.9590000000001],[137.759,514.9590000000001]]},{"text":"，","box":[[265.8,499.009],[281.75,499.009],[281.75,514.9590000000001],[265.8,514.9590000000001]]},{"text":"奋勇地前进","box":[[281.52,499.009],[361.4279000000001,499.009],[361.4279000000001,514.9590000000001],[281.52,514.9590000000001]]},{"text":"，","box":[[361.559,499.009],[377.509,499.009],[377.509,514.9590000000001],[361.559,514.9590000000001]]},{"text":"这是一年多来运动","box":[[377.28,499.009],[505.18640000000016,499.009],[505.18640000000016,514.9590000000001],[377.28,514.9590000000001]]},{"text":"的结果，是大革命的威力，这种威力是任何人无法抗拒的","box":[[90,530.209],[489.93800000000033,530.209],[489.93800000000033,546.159],[90,546.159]]},{"text":"。","box":[[489.959,530.209],[505.909,530.209],[505.909,546.159],[489.959,546.159]]},{"text":"学者们写史","box":[[122.04,561.409],[201.94790000000003,561.409],[201.94790000000003,577.359],[122.04,577.359]]},{"text":"，","box":[[201.959,561.409],[217.909,561.409],[217.909,577.359],[201.959,577.359]]},{"text":"十有八九无血无肉","box":[[217.68,561.409],[345.58640000000014,561.409],[345.58640000000014,577.359],[217.68,577.359]]},{"text":"，","box":[[345.6,561.409],[361.55,561.409],[361.55,577.359],[345.6,577.359]]},{"text":"不是历史创造者的呼","box":[[361.32,561.409],[505.30610000000013,561.409],[505.30610000000013,577.359],[361.32,577.359]]},{"text":"声","box":[[90,592.609],[105.95,592.609],[105.95,608.5590000000001],[90,608.5590000000001]]},{"text":"，","box":[[105.959,592.609],[121.909,592.609],[121.909,608.5590000000001],[105.959,608.5590000000001]]},{"text":"为什么？就因为他们是名家","box":[[121.8,592.609],[313.78460000000007,592.609],[313.78460000000007,608.5590000000001],[121.8,608.5590000000001]]},{"text":"，","box":[[313.8,592.609],[329.75,592.609],[329.75,608.5590000000001],[313.8,608.5590000000001]]},{"text":"是黄鹤楼的秀才","box":[[329.52,592.609],[441.4670000000001,592.609],[441.4670000000001,608.5590000000001],[329.52,608.5590000000001]]},{"text":"，","box":[[441.6,592.609],[457.55,592.609],[457.55,608.5590000000001],[441.6,608.5590000000001]]},{"text":"不像长","box":[[457.32,592.609],[505.30910000000006,592.609],[505.30910000000006,608.5590000000001],[457.32,608.5590000000001]]},{"text":"江水里的弄潮儿，懂得波涛是如何汹涌，浪潮是怎样澎湃","box":[[90,623.809],[489.93800000000033,623.809],[489.93800000000033,639.759],[90,639.759]]},{"text":"，","box":[[489.959,623.809],[505.909,623.809],[505.909,639.759],[489.959,639.759]]},{"text":"回流是何等险恶","box":[[90,655.009],[201.947,655.009],[201.947,670.9590000000001],[90,670.9590000000001]]},{"text":"。","box":[[201.959,655.009],[217.909,655.009],[217.909,670.9590000000001],[201.959,670.9590000000001]]},{"text":"一个普通的工人","box":[[217.68,655.009],[329.62700000000007,655.009],[329.62700000000007,670.9590000000001],[217.68,670.9590000000001]]},{"text":"，","box":[[329.64,655.009],[345.59,655.009],[345.59,670.9590000000001],[329.64,670.9590000000001]]},{"text":"巴黎公社社员笔下的一","box":[[345.24,655.009],[505.1855000000002,655.009],[505.1855000000002,670.9590000000001],[345.24,670.9590000000001]]},{"text":"八七一年公社史就远远胜过了好多历史学家关于公社的论","box":[[90,686.209],[505.1801000000005,686.209],[505.1801000000005,702.159],[90,702.159]]},{"text":"述","box":[[90,717.409],[105.95,717.409],[105.95,733.359],[90,733.359]]},{"text":"，","box":[[105.959,717.409],[121.909,717.409],[121.909,733.359],[105.959,733.359]]},{"text":"震撼世界的我国无产阶级文化大革命如何总结它","box":[[121.68,717.409],[457.6601000000002,717.409],[457.6601000000002,733.359],[121.68,733.359]]},{"text":"，","box":[[457.68,717.409],[473.63,717.409],[473.63,733.359],[457.68,733.359]]},{"text":"如何","box":[[473.28,717.409],[505.1894,717.409],[505.1894,733.359],[473.28,733.359]]},{"text":"承受它","box":[[90,748.609],[137.9891,748.609],[137.9891,764.5590000000001],[90,764.5590000000001]]},{"text":"，","box":[[138,748.609],[153.95,748.609],[153.95,764.5590000000001],[138,764.5590000000001]]},{"text":"难道还需要等着那些黄鹤楼上看帆船的大人先生去","box":[[153.24,748.609],[505.17950000000036,748.609],[505.17950000000036,764.5590000000001],[153.24,764.5590000000001]]}]}