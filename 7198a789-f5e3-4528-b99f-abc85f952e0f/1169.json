4.60900000000004],[313.19,140.55900000000003],[297.24,140.55900000000003]]},{"text":"恩来还为这事","box":[[313.32,124.60900000000004],[409.3076000000001,124.60900000000004],[409.3076000000001,140.55900000000003],[313.32,140.55900000000003]]},{"text":"点了","box":[[409.32,124.60900000000004],[441.2294,124.60900000000004],[441.2294,140.55900000000003],[409.32,140.55900000000003]]},{"text":"聂元梓","box":[[441.24,124.60900000000004],[489.2291000000001,124.60900000000004],[489.2291000000001,140.55900000000003],[441.24,140.55900000000003]]},{"text":"的","box":[[489.24,124.60900000000004],[505.19,124.60900000000004],[505.19,140.55900000000003],[489.24,140.55900000000003]]},{"text":"名。他说","box":[[90,155.80899999999997],[154.78910000000002,155.80899999999997],[154.78910000000002,171.75899999999996],[90,171.75899999999996]]},{"text":"：","box":[[155.159,155.80899999999997],[171.10899999999998,155.80899999999997],[171.10899999999998,171.75899999999996],[155.159,171.75899999999996]]},{"text":"“","box":[[163.44,155.80899999999997],[179.39,155.80899999999997],[179.39,171.75899999999996],[163.44,171.75899999999996]]},{"text":"提议设立国家主席，是一个大阴谋。有的人不","box":[[179.759,155.80899999999997],[505.30224999999996,155.80899999999997],[505.30224999999996,171.75899999999996],[179.759,171.75899999999996]]},{"text":"了解情况","box":[[90,187.00900000000001],[153.9485,187.00900000000001],[153.9485,202.959],[90,202.959]]},{"text":"，","box":[[153.959,187.00900000000001],[169.909,187.00900000000001],[169.909,202.959],[153.959,202.959]]},{"text":"就乱表态","box":[[169.8,187.00900000000001],[233.74850000000004,187.00900000000001],[233.74850000000004,202.959],[169.8,202.959]]},{"text":"。","box":[[233.759,187.00900000000001],[249.70899999999997,187.00900000000001],[249.70899999999997,202.959],[233.759,202.959]]},{"text":"还有咱们的聂元梓","box":[[249.48,187.00900000000001],[377.3864000000001,187.00900000000001],[377.3864000000001,202.959],[249.48,202.959]]},{"text":"，","box":[[377.52,187.00900000000001],[393.46999999999997,187.00900000000001],[393.46999999999997,202.959],[377.52,202.959]]},{"text":"也在下边串连","box":[[393.24,187.00900000000001],[489.2276000000001,187.00900000000001],[489.2276000000001,202.959],[393.24,202.959]]},{"text":"。","box":[[489.24,187.00900000000001],[505.19,187.00900000000001],[505.19,202.959],[489.24,202.959]]},{"text":"”","box":[[497.28,187.00900000000001],[513.23,187.00900000000001],[513.23,202.959],[497.28,202.959]]},{"text":"这天晚上","box":[[122.04,218.20899999999995],[185.98850000000002,218.20899999999995],[185.98850000000002,234.15899999999993],[122.04,234.15899999999993]]},{"text":"，","box":[[186,218.20899999999995],[201.95,218.20899999999995],[201.95,234.15899999999993],[186,234.15899999999993]]},{"text":"徐景贤他们正在王洪文住所的客厅里交流外","box":[[201.24,218.20899999999995],[505.1810000000004,218.20899999999995],[505.1810000000004,234.15899999999993],[201.24,234.15899999999993]]},{"text":"出活动的情况，约摸","box":[[90,249.409],[236.38685000000004,249.409],[236.38685000000004,265.359],[90,265.359]]},{"text":"","box":[[236.38684999999995,249.409],[323.0498500000009,249.409],[323.0498500000009,249.409],[236.38684999999995,249.409]]},{"text":"21","box":[[240.72,249.409],[256.73485,249.409],[256.73485,265.359],[240.72,265.359]]},{"text":"","box":[[256.73485,249.409],[342.0378500000001,249.409],[342.0378500000001,249.409],[256.73485,249.409]]},{"text":"时左右，有一个穿军装的工作人员","box":[[261,249.409],[505.18460000000005,249.409],[505.18460000000005,265.359],[261,265.359]]},{"text":"走了进来，把王洪文和徐景贤叫到客厅门外，轻声说","box":[[90,280.60900000000004],[464.38234999999986,280.60900000000004],[464.38234999999986,296.559],[90,296.559]]},{"text":"：","box":[[464.64,280.60900000000004],[480.59,280.60900000000004],[480.59,296.559],[464.64,296.559]]},{"text":"“","box":[[473.04,280.60900000000004],[488.99,280.60900000000004],[488.99,296.559],[473.04,296.559]]},{"text":"首","box":[[489.24,280.60900000000004],[505.19,280.60900000000004],[505.19,296.559],[489.24,296.559]]},{"text":"长请你们去一次，车已经派来了","box":[[90,311.8090000000001],[313.90340000000003,311.8090000000001],[313.90340000000003,327.75900000000007],[90,327.75900000000007]]},{"text":"。","box":[[314.04,311.8090000000001],[329.99,311.8090000000001],[329.99,327.75900000000007],[314.04,327.75900000000007]]},{"text":"”","box":[[321.959,311.8090000000001],[337.909,311.8090000000001],[337.909,327.75900000000007],[321.959,327.75900000000007]]},{"text":"首长？哪一位首长？王洪文和徐景贤不便多问","box":[[122.04,343.009],[441.94040000000024,343.009],[441.94040000000024,358.959],[122.04,358.959]]},{"text":"，","box":[[441.959,343.009],[457.909,343.009],[457.909,358.959],[441.959,358.959]]},{"text":"只是紧","box":[[457.32,343.009],[505.30910000000006,343.009],[505.30910000000006,358.959],[457.32,358.959]]},{"text":"随着那人走下坡去，一起坐上了一辆大","box":[[90,374.209],[366.58459999999997,374.209],[366.58459999999997,390.159],[90,390.159]]},{"text":"“","box":[[366.959,374.209],[382.909,374.209],[382.909,390.159],[366.959,390.159]]},{"text":"红旗","box":[[383.28,374.209],[415.54954999999995,374.209],[415.54954999999995,390.159],[383.28,390.159]]},{"text":"”","box":[[415.8,374.209],[431.75,374.209],[431.75,390.159],[415.8,390.159]]},{"text":"轿车","box":[[432.12,374.209],[464.38955,374.209],[464.38955,390.159],[432.12,390.159]]},{"text":"。","box":[[464.64,374.209],[480.59,374.209],[480.59,390.159],[464.64,390.159]]},{"text":"“","box":[[473.04,374.209],[488.99,374.209],[488.99,390.159],[473.04,390.159]]},{"text":"红","box":[[489.24,374.209],[505.19,374.209],[505.19,390.159],[489.24,390.159]]},{"text":"旗","box":[[90,405.409],[105.95,405.409],[105.95,421.359],[90,421.359]]},{"text":"”","box":[[105.959,405.409],[121.909,405.409],[121.909,421.359],[105.959,421.359]]},{"text":"在牯岭的柏油马路上行驶片刻","box":[[121.68,405.409],[329.6240000000001,405.409],[329.6240000000001,421.359],[121.68,421.359]]},{"text":"，","box":[[329.64,405.409],[345.59,405.409],[345.59,421.359],[329.64,421.359]]},{"text":"在一个大院子的门外停","box":[[345.24,405.409],[505.1855000000002,405.409],[505.1855000000002,421.359],[345.24,421.359]]},{"text":"