215.04,577.359]]},{"text":"”","box":[[228.24,561.409],[244.19,561.409],[244.19,577.359],[228.24,577.359]]},{"text":"林彪说","box":[[244.2,561.409],[292.18910000000005,561.409],[292.18910000000005,577.359],[244.2,577.359]]},{"text":"：","box":[[292.2,561.409],[308.15,561.409],[308.15,577.359],[292.2,577.359]]},{"text":"“","box":[[305.28,561.409],[321.22999999999996,561.409],[321.22999999999996,577.359],[305.28,577.359]]},{"text":"你赶快用电话找到黄永胜","box":[[321.24,561.409],[497.14490000000023,561.409],[497.14490000000023,577.359],[321.24,577.359]]},{"text":"，","box":[[497.28,561.409],[513.23,561.409],[513.23,577.359],[497.28,577.359]]},{"text":"把我说的这几条传给他。","box":[[90,592.609],[265.90490000000005,592.609],[265.90490000000005,608.5590000000001],[90,608.5590000000001]]},{"text":"”","box":[[266.04,592.609],[281.99,592.609],[281.99,608.5590000000001],[266.04,608.5590000000001]]},{"text":"“","box":[[122.04,623.809],[137.99,623.809],[137.99,639.759],[122.04,639.759]]},{"text":"过去首长和主任都规定过","box":[[138,623.809],[313.90490000000005,623.809],[313.90490000000005,639.759],[138,639.759]]},{"text":"，","box":[[314.04,623.809],[329.99,623.809],[329.99,639.759],[314.04,639.759]]},{"text":"凡往","box":[[329.28,623.809],[361.1894,623.809],[361.1894,639.759],[329.28,639.759]]},{"text":"外","box":[[361.32,623.809],[377.27,623.809],[377.27,639.759],[361.32,639.759]]},{"text":"传的首长批示都要","box":[[377.28,623.809],[505.18640000000016,623.809],[505.18640000000016,639.759],[377.28,639.759]]},{"text":"压半天。今天这个电话是否压一压？","box":[[90,655.009],[345.9425000000001,655.009],[345.9425000000001,670.9590000000001],[90,670.9590000000001]]},{"text":"”","box":[[345.959,655.009],[361.909,655.009],[361.909,670.9590000000001],[345.959,670.9590000000001]]},{"text":"张云生","box":[[362.04,655.009],[410.0291000000001,655.009],[410.0291000000001,670.9590000000001],[362.04,670.9590000000001]]},{"text":"请示道。","box":[[410.04,655.009],[473.9885000000001,655.009],[473.9885000000001,670.9590000000001],[410.04,670.9590000000001]]},{"text":