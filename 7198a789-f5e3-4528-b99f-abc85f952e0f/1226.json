4,390.159]]},{"text":"因为他们认","box":[[425.28,374.209],[505.18790000000007,374.209],[505.18790000000007,390.159],[425.28,390.159]]},{"text":"为","box":[[90,405.409],[105.95,405.409],[105.95,421.359],[90,421.359]]},{"text":"，","box":[[105.959,405.409],[121.909,405.409],[121.909,421.359],[105.959,421.359]]},{"text":"代表人民的标志就要当国家主席","box":[[121.68,405.409],[345.5834000000001,405.409],[345.5834000000001,421.359],[121.68,421.359]]},{"text":"。","box":[[345.6,405.409],[361.55,405.409],[361.55,421.359],[345.6,421.359]]},{"text":"我在十几年以前就不","box":[[361.32,405.409],[505.30610000000013,405.409],[505.30610000000013,421.359],[361.32,421.359]]},{"text":"当了嘛","box":[[90,436.609],[137.9891,436.609],[137.9891,452.55899999999997],[90,452.55899999999997]]},{"text":"，","box":[[138,436.609],[153.95,436.609],[153.95,452.55899999999997],[138,452.55899999999997]]},{"text":"岂不是十几年以来都不代表人民了吗？我说","box":[[153.6,436.609],[457.54100000000017,436.609],[457.54100000000017,452.55899999999997],[153.6,452.55899999999997]]},{"text":"，","box":[[457.68,436.609],[473.63,436.609],[473.63,452.55899999999997],[457.68,452.55899999999997]]},{"text":"谁想","box":[[473.28,436.609],[505.1894,436.609],[505.1894,452.55899999999997],[473.28,452.55899999999997]]},{"text":"代表人民你去当嘛","box":[[90,467.809],[217.90640000000002,467.809],[217.90640000000002,483.759],[90,483.759]]},{"text":"，","box":[[218.04,467.809],[233.98999999999998,467.809],[233.98999999999998,483.759],[218.04,483.759]]},{"text":"我是不干","box":[[231.12,467.809],[295.0685,467.809],[295.0685,483.759],[231.12,483.759]]},{"text":"。","box":[[295.08,467.809],[311.03,467.809],[311.03,483.759],[295.08,483.759]]},{"text":"你把庐山炸平了","box":[[308.159,467.809],[420.1060000000001,467.809],[420.1060000000001,483.759],[308.159,483.759]]},{"text":"，","box":[[420.24,467.809],[436.19,467.809],[436.19,483.759],[420.24,483.759]]},{"text":"我也不干","box":[[433.32,467.809],[497.2685000000001,467.809],[497.2685000000001,483.759],[433.32,483.759]]},{"text":"。","box":[[497.28,467.809],[513.23,467.809],[513.23,483.759],[497.28,483.759]]},{"text":"你有啥办法呀？","box":[[90,499.009],[201.947,499.009],[201.947,514.9590000000001],[90,514.9590000000001]]},{"text":"”","box":[[201.959,499.009],[217.909,499.009],[217.909,514.9590000000001],[201.959,514.9590000000001]]},{"text":"毛泽东说","box":[[122.04,530.209],[186.94865000000001,530.209],[186.94865000000001,546.159],[122.04,546.159]]},{"text":"：","box":[[187.2,530.209],[203.14999999999998,530.209],[203.14999999999998,546.159],[187.2,546.159]]},{"text":"“","box":[[195.6,530.209],[211.54999999999998,530.209],[211.54999999999998,546.159],[195.6,546.159]]},{"text":"不讲团结不好，不讲团结得不到全党的同","box":[[211.92,530.209],[505.30234999999993,530.209],[505.30234999999993,546.159],[211.92,546.159]]},{"text":"意","box":[[90,561.409],[105.95,561.409],[105.95,577.359],[90,577.359]]},{"text":"，","box":[[105.959,561.409],[121.909,561.409],[121.909,577.359],[105.959,577.359]]},{"text":"群众也不高兴","box":[[121.68,561.409],[217.66760000000005,561.409],[217.66760000000005,577.359],[121.68,577.359]]},{"text":"。","box":[[217.68,561.409],[233.63,561.409],[233.63,577.359],[217.68,577.359]]},{"text":"......","box":[[233.28,561.409],[265.1894,561.409],[265.1894,577.359],[233.28,577.359]]},{"text":"所谓讲团结是什么呢？当然是马克","box":[[265.32,561.409],[505.30310000000026,561.409],[505.30310000000026,577.359],[265.32,577.359]]},{"text":"思列宁主义基础之上的团结","box":[[90,592.609],[281.98460000000006,592.609],[281.98460000000006,608.5590000000001],[90,608.5590000000001]]},{"text":"，","box":[[282,592.609],[297.95,592.609],[297.95,608.5590000000001],[282,608.5590000000001]]},{"text":"不是无原则的团结","box":[[297.6,592.609],[425.5064000000002,592.609],[425.5064000000002,608.5590000000001],[297.6,608.5590000000001]]},{"text":"。","box":[[425.64,592.609],[441.59,592.609],[441.59,608.5590000000001],[425.64,608.5590000000001]]},{"text":"提出团结","box":[[441.24,592.609],[505.1885000000001,592.609],[505.1885000000001,608.5590000000001],[441.24,608.5590000000001]]},{"text":"的口号","box":[[90,623.809],[13