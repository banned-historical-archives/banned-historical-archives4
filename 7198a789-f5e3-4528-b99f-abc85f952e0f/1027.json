218.20899999999995],[505.30144999999993,218.20899999999995],[505.30144999999993,234.15899999999993],[179.28,234.15899999999993]]},{"text":"们希望我们的大后方更稳固一些","box":[[90,249.409],[313.90340000000003,249.409],[313.90340000000003,265.359],[90,265.359]]},{"text":"。","box":[[314.04,249.409],[329.99,249.409],[329.99,265.359],[314.04,265.359]]},{"text":"在我们越南人民打美国人","box":[[329.28,249.409],[505.1849000000002,249.409],[505.1849000000002,265.359],[329.28,265.359]]},{"text":"的时候","box":[[90,280.60900000000004],[137.9891,280.60900000000004],[137.9891,296.559],[90,296.559]]},{"text":"，","box":[[138,280.60900000000004],[153.95,280.60900000000004],[153.95,296.559],[138,296.559]]},{"text":"中国是我们的大后方","box":[[153.72,280.60900000000004],[297.7061,280.60900000000004],[297.7061,296.559],[153.72,296.559]]},{"text":"。","box":[[297.72,280.60900000000004],[313.67,280.60900000000004],[313.67,296.559],[297.72,296.559]]},{"text":"因此","box":[[313.559,280.60900000000004],[345.46840000000003,280.60900000000004],[345.46840000000003,296.559],[313.559,296.559]]},{"text":"，","box":[[345.6,280.60900000000004],[361.55,280.60900000000004],[361.55,296.559],[345.6,296.559]]},{"text":"我们曾经发出过这样","box":[[361.32,280.60900000000004],[505.30610000000013,280.60900000000004],[505.30610000000013,296.559],[361.32,296.559]]},{"text":"的命令，即使我们的飞机受到攻击，也不要到中国机场降","box":[[90,311.8090000000001],[505.1801000000005,311.8090000000001],[505.1801000000005,327.75900000000007],[90,327.75900000000007]]},{"text":"落","box":[[90,343.009],[105.95,343.009],[105.95,358.959],[90,358.959]]},{"text":"。","box":[[105.959,343.009],[121.909,343.009],[121.909,358.959],[105.959,358.959]]},{"text":"”","box":[[114,343.009],[129.95,343.009],[129.95,358.959],[114,358.959]]},{"text":"毛泽东","box":[[122.04,374.209],[170.62910000000002,374.209],[170.62910000000002,390.159],[122.04,390.159]]},{"text":"：","box":[[171,374.209],[186.95,374.209],[186.95,390.159],[171,390.159]]},{"text":"“","box":[[179.28,374.209],[195.23,374.209],[195.23,390.159],[179.28,390.159]]},{"text":"可以嘛，我们不怕。如果美国空军要来打越","box":[[195.6,374.209],[505.30189999999993,374.209],[505.30189999999993,390.159],[195.6,390.159]]},{"text":"南空军的","box":[[90,405.409],[153.9485,405.409],[153.9485,421.359],[90,421.359]]},{"text":"‘","box":[[153.959,405.409],[169.909,405.409],[169.909,421.359],[153.959,421.359]]},{"text":"庇护所","box":[[170.04,405.409],[218.0291,405.409],[218.0291,421.359],[170.04,421.359]]},{"text":"’","box":[[218.04,405.409],[233.98999999999998,405.409],[233.98999999999998,421.359],[218.04,421.359]]},{"text":"，就让他来嘛","box":[[225.959,405.409],[321.94660000000005,405.409],[321.94660000000005,421.359],[225.959,421.359]]},{"text":"。","box":[[321.959,405.409],[337.909,405.409],[337.909,421.359],[321.959,421.359]]},{"text":"”","box":[[330,405.409],[345.95,405.409],[345.95,421.359],[330,421.359]]},{"text":"黎笋","box":[[122.04,436.609],[154.30955,436.609],[154.30955,452.55899999999997],[122.04,452.55899999999997]]},{"text":"：","box":[[154.559,436.609],[170.509,436.609],[170.509,452.55899999999997],[154.559,452.55899999999997]]},{"text":"“","box":[[162.959,436.609],[178.909,436.609],[178.909,452.55899999999997],[162.959,452.55899999999997]]},{"text":"虽然我们下了这样的命令，但我们仍然需要依","box":[[179.28,436.609],[505.30144999999993,436.609],[505.30144999999993,452.55899999999997],[179.28,452.55899999999997]]},{"text":"靠你们的支持","box":[[90,467.809],[185.98760000000001,467.809],[185.98760000000001,483.759],[90,483.759]]},{"text":"。","box":[[186,467.809],[201.95,467.809],[201.95,483.759],[186,483.759]]},{"text":"那时","box":[[201.6,467.809],[233.5094,467.809],[233.5094,483.759],[201.6,483.759]]},{"text":"，","box":[[233.64,467.809],[249.58999999999997,467.809],[249.58999999999997,483.759],[233.64,483.759]]},{"text":"你们派来的几个师在越南同样打美国","box":[[249.24,467.809],[505.18250000000035,467.809],[505.18250000000035,483.759],[249.24,483.759]]},{"text":"飞机","box":[[90,499.009],[121.9094,499.009],[121.9094,514.9590000000001],[90,514.9590000000001]]},{"text":"。","box":[[122.04,499.009],[137.99,499.009],[137.99,514.9590000000001],[122.04,514.9590000000001]]},{"text":"”","box":[[129.959,499.009],[145.909,499.009],[145.909,514.9590000000001],[129.959,514.9590000000001]]},{"text":"毛泽东","box":[[122.04,530.209],[170.62910000000002,530.209],[170.62910000000002,546.159],[122.04,546.159]]},{"text":"：","box":[[171,530.209],[186.95,530.209],[186.95,546.159],[171,546.159]]},{"text":"“","box":[[179.28,530.209],[195.23,530.209],[195.23,546.159],[179.28,546.159]]},{"text":"就是嘛。美国人就是怕打，他们没勇气。你","box":[[195.6,530.209],[505.30189999999993,530.209],[505.30189999999993,546.159],[195.6,546.159]]},{"text":"们可以谈判","box":[[90,561.409],[169.90789999999998,561.409],[169.90789999999998,577.359],[90,577.359]]},{"text":"，","box":[[170.04,561.409],[185.98999999999998,561.409],[185.98999999999998,577.359],[170.04,577.359]]},{"text":"我不是说你们不能谈判","box":[[185.64,561.409],[345.5855,561.409],[345.5855,577.359],[185.64,577.359]]},{"text":"，","box":[[345.6,561.409],[361.55,561.409],[361.55,577.359],[345.6,577.359]]},{"text":"但是你们的主要精力","box":[[361.32,561.409],[505.30610000000013,561.409],[505.30610000000013,577.359],[361.32,577.359]]},{"text":"应该放在打上","box":[[90,592.609],[185.98760000000001,592.609],[185.98760000000001,608.5590000000001],[90,608.5590000000001]]},{"text":"。","box":[[186,592.609],[201.95,592.609],[201.95,608.5590000000001],[186,608.5590000000001]]},{"text":"两次日内瓦会议是谁破坏的？你们和我们都","box":[[201.24,592.609],[505.1810000000004,592.609],[505.1810000000004,608.5590000000001],[201.24,608.5590000000001]]},{"text":"是老老实实遵守","box":[[90,623.809],[201.947,623.809],[201.947,639.759],[90,639.759]]},{"text":"(","box":[[201.959,623.809],[209.934,623.809],[209.934,639.759],[201.959,639.759]]},{"text":"会议协议","box":[[210,623.809],[273.94849999999997,623.809],[273.94849999999997,639.759],[210,639.759]]},{"text":")","box":[[273.959,623.809],[281.934,623.809],[281.934,639.759],[273.959,639.759]]},{"text":"，但是他们不干嘛！不干更好","box":[[282,623.809],[489.94400000000024,623.809],[489.94400000000024,639.759],[282,639.759]]},{"text":"。","box":[[489.959,623.809],[505.909,623.809],[505.909,639.759],[489.959,639.759]]},{"text":"因此","box":[[90,655.009],[121.9094,655.009],[121.9094,670.9590000000001],[90,670.9590000000001]]},{"text":"，","box":[[122.04,655.009],[137.99,655.009],[137.99,670.9590000000001],[122.04,670.9590000000001]]},{"text":"甚至苏联总理柯西金在他发表公开讲话时","box":[[137.64,655.009],[425.6216000000001,655.009],[425.6216000000001,670.9590000000001],[137.64,670.9590000000001]]},{"text":"，","box":[[425.64,655.009],[441.59,655.009],[441.59,670.9590000000001],[425.64,670.9590000000001]]},{"text":"也不得不","box":[[441.24,655.009],[505.1885000000001,655.009],[505.1885000000001,670.9590000000001],[441.24,670.9590000000001]]},{"text":"说只要召开国际会议，就必须同越南、老挝和柬埔寨商量","box":[[90,686.209],[489.93800000000033,686.209],[489.93800000000033,702.159],[90,702.159]]},{"text":"。","box":[[489.959,686.209],[505.909,686.209],[505.909,702.159],[489.959,702.159]]},{"text":"他们现在的领导人好多我不熟悉，不认识。柯西金我认识","box":[[90,717.409],[489.93800000000033,717.409],[489.93800000000033,733.359],[90,733.359]]},{"text":"，","box":[[489.959,717.409],[505.909,717.409],[505.909,733.359],[489.959,733.359]]},{"text":"并且同他谈过话","box":[[90,748.609],[201.947,748.609],[201.947,764.5590000000001],[90,764.5590000000001]]},{"text":"。","box":[[201.959,748.609],[217.909,748.609],[217.909,764.5590000000001],[201.959,764.5590000000001]]},{"text":"西方报纸总是造他们的","box":[[217.68,748.609],[377.62550000000016,748.609],[377.62550000000016,764.5590000000001],[217.68,764.5590000000001]]},{"text":"谣","box":[[377.64,748.609],[393.59,748.609],[393.59,764.5590000000001],[377.64,764.5590000000001]]},{"text":"，","box":[[393.6,748.609],[409.55,748.609],[409.55,764.5590000000001],[393.6,764.5590000000001]]},{"text":"说他们的领导","box":[[409.32,748.609],[505.3076000000001,748.609],[505.3076000000001,764.5590000000001],[409.32,764.5590000000001]]}]}