01],[497.2655000000002,187.00900000000001],[497.2655000000002,202.959],[337.32,202.959]]},{"text":"，","box":[[497.28,187.00900000000001],[513.23,187.00900000000001],[513.23,202.959],[497.28,202.959]]},{"text":"还要通过肺部进行呼吸","box":[[90,218.20899999999995],[249.94550000000004,218.20899999999995],[249.94550000000004,234.15899999999993],[90,234.15899999999993]]},{"text":"，","box":[[249.959,218.20899999999995],[265.909,218.20899999999995],[265.909,234.15899999999993],[249.959,234.15899999999993]]},{"text":"呼出二氧化碳","box":[[265.8,218.20899999999995],[361.7876000000001,218.20899999999995],[361.7876000000001,234.15899999999993],[265.8,234.15899999999993]]},{"text":"，","box":[[361.8,218.20899999999995],[377.75,218.20899999999995],[377.75,234.15899999999993],[361.8,234.15899999999993]]},{"text":"吸进新鲜氧气","box":[[377.52,218.20899999999995],[473.5076000000001,218.20899999999995],[473.5076000000001,234.15899999999993],[377.52,234.15899999999993]]},{"text":"，","box":[[473.52,218.20899999999995],[489.46999999999997,218.20899999999995],[489.46999999999997,234.15899999999993],[473.52,234.15899999999993]]},{"text":"这","box":[[489.24,218.20899999999995],[505.19,218.20899999999995],[505.19,234.15899999999993],[489.24,234.15899999999993]]},{"text":"就是吐故纳新","box":[[90,249.409],[185.98760000000001,249.409],[185.98760000000001,265.359],[90,265.359]]},{"text":"。","box":[[186,249.409],[201.95,249.409],[201.95,265.359],[186,265.359]]},{"text":"一个无产阶级的党也要吐故纳新","box":[[201.6,249.409],[425.50340000000017,249.409],[425.50340000000017,265.359],[201.6,265.359]]},{"text":"，","box":[[425.64,249.409],[441.59,249.409],[441.59,265.359],[425.64,265.359]]},{"text":"才能朝气","box":[[441.24,249.409],[505.1885000000001,249.409],[505.1885000000001,265.359],[441.24,265.359]]},{"text":"蓬勃。不清除废料，不吸收新鲜血液，党就没有朝气。","box":[[90,280.60900000000004],[473.97860000000026,280.60900000000004],[473.97860000000026,296.559],[90,296.559]]},{"text":"’","box":[[474,280.60900000000004],[489.95,280.60900000000004],[489.95,296.559],[474,296.559]]},{"text":"”","box":[[489.959,280.60900000000004],[505.909,280.60900000000004],[505.909,296.559],[489.959,296.559]]},{"text":"文章中提到","box":[[122.04,311.8090000000001],[201.94790000000003,311.8090000000001],[201.94790000000003,327.75900000000007],[122.04,327.75900000000007]]},{"text":"“","box":[[197.64,311.8090000000001],[213.58999999999997,311.8090000000001],[213.58999999999997,327.75900000000007],[197.64,327.75900000000007]]},{"text":"反对复旧","box":[[213.6,311.8090000000001],[277.54850000000005,311.8090000000001],[277.54850000000005,327.75900000000007],[213.6,327.75900000000007]]},{"text":"。","box":[[277.68,311.8090000000001],[293.63,311.8090000000001],[293.63,327.75900000000007],[277.68,327.75900000000007]]},{"text":"凡是领导班子统统是原班人马","box":[[289.32,311.8090000000001],[497.26400000000024,311.8090000000001],[497.26400000000024,327.75900000000007],[289.32,327.75900000000007]]},{"text":"，","box":[[497.28,311.8090000000001],[513.23,311.8090000000001],[513.23,327.75900000000007],[497.28,327.75900000000007]]},{"text":"没有吸收无产阶级的新鲜血液","box":[[90,343.009],[297.9440000000001,343.009],[297.9440000000001,358.959],[90,358.959]]},{"text":"，","box":[[297.959,343.009],[313.909,343.009],[313.909,358.959],[297.959,358.959]]},{"text":"没有革命三结合","box":[[313.68,343.009],[425.6270000000002,343.009],[425.6270000000002,358.959],[313.68,358.959]]},{"text":"，","box":[[425.64,343.009],[441.59,343.009],[441.59,358.959],[425.64,358.959]]},{"text":"或者只有","box":[[441.24,343.009],[505.1885000000001,343.009],[505.1885000000001,358.959],[441.24,358.959]]},{"text":"形式上三结合而不是革命的三结合的地方","box":[[90,374.209],[377.9816000000001,374.209],[377.9816000000001,390.159],[90,390.159]]},{"text":"，","box":[[378,374.209],[393.95,374.209],[393.95,390.159],[378,390.159]]},{"text":"不可能做好发展","box":[[393.24,374.209],[505.1870000000001,374.209],[505.1870000000001,390.159],[393.24,390.159]]},{"text":"党员的工作。","box":[[90,405.409],[185.98760000000001,405.409],[185.98760000000001,421.359],[90,421.359]]},{"text":"”","box":[[186,405.409],[201.95,405.409],[201.95,421.359],[186,421.359]]},{"text":"文章中还特别提到","box":[[122.04,436.609],[249.94640000000004,436.609],[249.94640000000004,452.55899999999997],[122.04,452.55899999999997]]},{"text":"：","box":[[249.959,436.609],[265.909,436.609],[265.909,452.55899999999997],[249.959,452.55899999999997]]},{"text":"“","box":[[254.88,436.609],[270.83,436.609],[270.83,452.55899999999997],[254.88,452.55899999999997]]},{"text":"迷信选举","box":[[270.84,436.609],[334.78850000000006,436.609],[334.78850000000006,452.55899999999997],[270.84,452.55899999999997]]},{"text":"，","box":[[334.8,436.609],[350.75,436.609],[350.75,452.55899999999997],[334.8,452.55899999999997]]},{"text":"这也是一种保守思想","box":[[345.24,436.609],[489.2261000000002,436.609],[489.2261000000002,452.55899999999997],[345.24,452.55899999999997]]},{"text":"。","box":[[489.24,436.609],[505.19,436.609],[505.19,452.55899999999997],[489.24,452.55899999999997]]},{"text":"”","box":[[497.28,436.609],[513.23,436.609],[513.23,452.55899999999997],[497.28,452.55899999999997]]},{"text":"文章特别说明","box":[[122.04,467.809],[218.02760000000006,467.809],[218.02760000000006,483.759],[122.04,483.759]]},{"text":"：","box":[[218.04,467.809],[233.98999999999998,467.809],[233.98999999999998,483.759],[218.04,483.759]]},{"text":"“","box":[[233.64,467.809],[249.58999999999997,467.809],[249.58999999999997,483.759],[233.64,483.759]]},{"text":"毛主席最近指出","box":[[249.6,467.809],[361.5470000000001,467.809],[361.5470000000001,483.759],[249.6,483.759]]},{"text":"：","box":[[361.68,467.809],[377.63,467.809],[377.63,483.759],[361.68,483.759]]},{"text":"‘","box":[[377.28,467.809],[393.22999999999996,467.809],[393.22999999999996,483.759],[377.28,483.759]]},{"text":"我们的权力是谁","box":[[393.24,467.809],[505.1870000000001,467.809],[505.1870000000001,483.759],[393.24,483.759]]},{"text":"给的？是工人阶级给的","box":[[90,499.009],[249.94550000000004,499.009],[249.94550000000004,514.9590000000001],[90,514.9590000000001]]},{"text":"，","box":[[249.959,499.009],[265.909,499.009],[265.909,514.9590000000001],[249.959,514.9590000000001]]},{"text":"是贫下中农给的","box":[[265.68,499.009],[377.6270000000002,499.009],[377.6270000000002,514.9590000000001],[265.68,514.9590000000001]]},{"text":"，","box":[[377.64,499.009],[393.59,499.009],[393.59,514.9590000000001],[377.64,514.9590000000001]]},{"text":"是占人口百分之","box":[[393.24,499.009],[505.1870000000001,499.009],[505.1870000000001,514.9590000000001],[393.24,514.9590000000001]]},{"text":"九十以上的广大劳动群众给的","box":[[90,530.209],[297.9440000000001,530.209],[297.9440000000001,546.159],[90,546.159]]},{"text":"。","box":[[297.959,530.209],[313.909,530.209],[313.909,546.159],[297.959,546.159]]},{"text":"我们代表了无产阶级","box":[[313.68,530.209],[457.66610000000014,530.209],[457.66610000000014,546.159],[313.68,546.159]]},{"text":"，","box":[[457.68,530.209],[473.63,530.209],[473.63,546.159],[457.68,546.159]]},{"text":"代表","box":[[473.28,530.209],[505.1894,530.209],[505.1894,546.159],[473.28,546.159]]},{"text":"了人民群众","box":[[90,561.409],[169.90789999999998,561.409],[169.90789999999998,577.359],[90,577.359]]},{"text":"，","box":[[170.04,561.409],[185.98999999999998,561.409],[185.98999999999998,577.359],[170.04,577.359]]},{"text":"打倒了人民的敌人","box":[[185.759,561.409],[313.6654000000001,561.409],[313.6654000000001,577.359],[185.759,577.359]]},{"text":"，","box":[[313.8,561.409],[329.75,561.409],[329.75,577.359],[313.8,577.359]]},{"text":"人民就拥护我们","box":[[329.52,561.409],[441.4670000000001,561.409],[441.4670000000001,577.359],[329.52,577.359]]},{"text":"。","box":[[441.48,561.409],[457.43,561.409],[457.43,577.359],[441.48,577.359]]},{"text":"共产党","box":[[457.2,561.409],[505.18910000000005,561.409],[505.18910000000005,577.359],[457.2,577.359]]},{"text":"基本的一条","box":[[90,592.609],[169.90789999999998,592.609],[169.90789999999998,608.5590000000001],[90,608.5590000000001]]},{"text":"，","box":[[170.04,592.609],[185.98999999999998,592.609],[185.98999999999998,608.5590000000001],[170.04,608.5590000000001]]},{"text":"就是直接依靠广大革命人民群众","box":[[185.759,592.609],[409.6624000000003,592.609],[409.6624000000003,608.5590000000001],[185.759,608.5590000000001]]},{"text":"。","box":[[409.8,592.609],[425.75,592.609],[425.75,608.5590000000001],[409.8,608.5590000000001]]},{"text":"’","box":[[425.52,592.609],[441.46999999999997,592.609],[441.46999999999997,608.5590000000001],[425.52,608.5590000000001]]},{"text":"毛主席这","box":[[441.24,592.609],[505.1885000000001,592.609],[505.1885000000001,608.5590000000001],[441.24,608.5590000000001]]},{"text":"个极端重要的指示","box":[[90,623.809],[217.90640000000002,623.809],[217.90640000000002,639.759],[90,639.759]]},{"text":"，","box":[[218.04,623.809],[233.98999999999998,623.809],[233.98999999999998,639.759],[218.04,639.759]]},{"text":"深刻地指出了无产阶级专政强大威力的","box":[[233.28,623.809],[505.18190000000027,623.809],[505.18190000000027,639.759],[233.28,639.759]]},{"text":"群众基础","box":[[90,655.009],[153.9485,655.009],[153.9485,670.9590000000001],[90,670.9590000000001]]},{"text":"，","box":[[153.959,655.009],[169.909,655.009],[169.909,670.9590000000001],[153.959,670.9590000000001]]},{"text":"深刻地批判了迷信选举的形式主义","box":[[169.68,655.009],[409.66310000000016,655.009],[409.66310000000016,670.9590000000001],[169.68,670.9590000000001]]},{"text":"，","box":[[409.68,655.009],[425.63,655.009],[425.63,670.9590000000001],[409.68,670.9590000000001]]},{"text":"指出了党的","box":[[425.28,655.009],[505.18790000000007,655.009],[505.18790000000007,670.9590000000001],[425.28,670.9590000000001]]},{"text":"建设和革命委员会建设的根本方向。","box":[[90,686.209],[345.9425000000001,686.209],[345.9425000000001,702.159],[90,702.159]]},{"text":"革命委员会是解放以来最具有革命代表性的无产阶级","box":[[122.04,717.409],[489.9389000000003,717.409],[489.9389000000003,733.359],[122.04,733.359]]},{"text":"专政的权力机构","box":[[90,748.609],[201.947,748.609],[201.947,764.5590000000001],[90,764.5590000000001]]},{"text":"。","box":[[201.959,748.609],[217.909,748.609],[217.909,764.5590000000001],[201.959,764.5590000000001]]},{"text":"但革命委员会就不是选举产生的","box":[[217.68,748.609],[441.58340000000027,748.609],[441.58340000000027,764.5590000000001],[217.68,764.5590000000001]]},{"text":"，","box":[[441.6,748.609],[457.55,748.609],[457.55,764.5590000000001],[441.6,764.5590000000001]]},{"text":"而是直","box":[[457.32,748.609],[505.30910000000006,748.609],[505.30910000000006,764.5590000000001],[457.32,764.5590000000001]]}]}