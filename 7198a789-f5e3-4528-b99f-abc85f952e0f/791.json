4,140.55900000000003],[449.28,140.55900000000003]]},{"text":"、","box":[[497.28,124.60900000000004],[513.23,124.60900000000004],[513.23,140.55900000000003],[497.28,140.55900000000003]]},{"text":"国防工办","box":[[90,155.80899999999997],[153.9485,155.80899999999997],[153.9485,171.75899999999996],[90,171.75899999999996]]},{"text":"、","box":[[153.959,155.80899999999997],[169.909,155.80899999999997],[169.909,171.75899999999996],[153.959,171.75899999999996]]},{"text":"国防科委下达了","box":[[169.68,155.80899999999997],[281.627,155.80899999999997],[281.627,171.75899999999996],[169.68,171.75899999999996]]},{"text":"“","box":[[281.64,155.80899999999997],[297.59,155.80899999999997],[297.59,171.75899999999996],[281.64,171.75899999999996]]},{"text":"第四号号令","box":[[297.6,155.80899999999997],[377.5079000000001,155.80899999999997],[377.5079000000001,171.75899999999996],[297.6,171.75899999999996]]},{"text":"”","box":[[377.64,155.80899999999997],[393.59,155.80899999999997],[393.59,171.75899999999996],[377.64,171.75899999999996]]},{"text":"：","box":[[393.6,155.80899999999997],[409.55,155.80899999999997],[409.55,171.75899999999996],[393.6,171.75899999999996]]},{"text":"要求按照国庆","box":[[409.32,155.80899999999997],[505.3076000000001,155.80899999999997],[505.3076000000001,171.75899999999996],[409.32,171.75899999999996]]},{"text":"期间战备工作要求转入战备状态。凡已组织精干指挥班子","box":[[90,187.00900000000001],[489.93800000000033,187.00900000000001],[489.93800000000033,202.959],[90,202.959]]},{"text":"、","box":[[489.959,187.00900000000001],[505.909,187.00900000000001],[505.909,202.959],[489.959,202.959]]},{"text":"有隐蔽指挥位置的单位","box":[[90,218.20899999999995],[249.94550000000004,218.20899999999995],[249.94550000000004,234.15899999999993],[90,234.15899999999993]]},{"text":"，","box":[[249.959,218.20899999999995],[265.909,218.20899999999995],[265.909,234.15899999999993],[249.959,234.15899999999993]]},{"text":"应立即进入指挥位置","box":[[265.68,218.20899999999995],[409.66610000000014,218.20899999999995],[409.66610000000014,234.15899999999993],[265.68,234.15899999999993]]},{"text":"；","box":[[409.68,218.20899999999995],[425.63,218.20899999999995],[425.63,234.15899999999993],[409.68,234.15899999999993]]},{"text":"全部人员要","box":[[425.28,218.20899999999995],[505.18790000000007,218.20899999999995],[505.18790000000007,234.15899999999993],[425.28,234.15899999999993]]},{"text":"做好就地疏散的一切准备。","box":[[90,249.409],[281.98460000000006,249.409],[281.98460000000006,265.359],[90,265.359]]},{"text":"前指值班室正在","box":[[122.04,280.60900000000004],[233.98700000000002,280.60900000000004],[233.98700000000002,296.559],[122.04,296.559]]},{"text":"向下","box":[[234,280.60900000000004],[265.9094,280.60900000000004],[265.9094,296.559],[234,296.559]]},{"text":"传达林彪","box":[[266.04,280.60900000000004],[329.9885000000001,280.60900000000004],[329.9885000000001,296.559],[266.04,296.559]]},{"text":"的","box":[[330,280.60900000000004],[345.95,280.60900000000004],[345.95,296.559],[330,296.559]]},{"text":"指示","box":[[345.959,280.60900000000004],[377.8684,280.60900000000004],[377.8684,296.559],[345.959,296.559]]},{"text":"，","box":[[378,280.60900000000004],[393.95,280.60900000000004],[393.95,296.559],[378,296.559]]},{"text":"又接到张云生","box":[[393.24,280.60900000000004],[489.2276000000001,280.60900000000004],[489.2276000000001,296.559],[393.24,296.559]]},{"text":"秘","box":[[489.24,280.60900000000004],[505.19,280.60900000000004],[505.19,296.559],[489.24,296.559]]},{"text":"书","box":[[90,311.8090000000001],[105.95,311.8090000000001],[105.95,327.75900000000007],[90,327.75900000000007]]},{"text":"打来的电话，说林彪询问一下","box":[[105.959,311.8090000000001],[313.90300000000013,311.8090000000001],[313.90300000000013,327.75900000000007],[105.959,327.75900000000007]]},{"text":"：","box":[[314.04,311.8090000000001],[329.99,311.8090000000001],[329.99,327.75900000000007],[314.04,327.75900000000007]]},{"text":"一旦密云水库遭到轰炸","box":[[330,311.8090000000001],[489.9455000000002,311.8090000000001],[489.9455000000002,327.75900000000007],[330,327.75900000000007]]},{"text":"，","box":[[489.959,311.8090000000001],[505.909,311.8090000000001],[505.909,327.75900000000007],[489.959,327.75900000000007]]},{"text":"溢洪会给北京市造成多大危害","box":[[90,343.009],[305.6252000000002,343.009],[305.6252000000002,358.959],[90,358.959]]},{"text":"？","box":[[306.24,343.009],[322.19,343.009],[322.19,358.959],[306.24,358.959]]},{"text":"是否可以考虑采取一些预","box":[[322.92,343.009],[505.18580000000026,343.009],[505.18580000000026,358.959],[322.92,358.959]]},{"text":"防措施？","box":[[90,374.209],[153.9485,374.209],[153.9485,390.159],[90,390.159]]},{"text":"阎仲川听了值班参谋的报告后","box":[[122.04,405.409],[329.9840000000001,405.409],[329.9840000000001,421.359],[122.04,421.359]]},{"text":"，","box":[[330,405.409],[345.95,405.409],[345.95,421.359],[330,421.359]]},{"text":"因不了解水库的具体情","box":[[345.24,405.409],[505.1855000000002,405.409],[505.1855000000002,421.359],[345.24,421.359]]},{"text":"况","box":[[90,436.609],[105.95,436.609],[105.95,452.55899999999997],[90,452.55899999999997]]},{"text":"，","box":[[105.959,436.609],[121.909,436.609],[121.909,452.55899999999997],[105.959,452.55899999999997]]},{"text":"便指示以总参谋部名义与北京市革命委员会副主任吴德","box":[[121.32,436.609],[505.29860000000025,436.609],[505.29860000000025,452.55899999999997],[121.32,452.55899999999997]]},{"text":"联系，请北京市提出意见。","box":[[90,467.809],[281.98460000000006,467.809],[281.98460000000006,483.759],[90,483.759]]},{"text":"吴德听了总参谋部提出的问题","box":[[122.04,499.009],[329.9840000000001,499.009],[329.9840000000001,514.9590000000001],[122.04,514.9590000000001]]},{"text":"，","box":[[330,499.009],[345.95,499.009],[345.95,514.9590000000001],[330,514.9590000000001]]},{"text":"感到事关重大","box":[[345.6,499.009],[441.5876000000001,499.009],[441.5876000000001,514.9590000000001],[345.6,514.9590000000001]]},{"text":"，","box":[[441.6,499.009],[457.55,499.009],[457.55,514.9590000000001],[441.6,514.9590000000001]]},{"text":"立即","box":[[457.32,499.009],[489.2294,499.009],[489.2294,514.9590000000001],[457.32,514.9590000000001]]},{"text":"与","box":[[489.24,499.009],[505.19,499.009],[505.19,514.9590000000001],[489.24,514.9590000000001]]},{"text":"北京","box":[[90,530.209],[121.9094,530.209],[121.9094,546.159],[90,546.159]]},{"text":"市革委会主任谢富治研究后，向周恩来作了报告。","box":[[122.04,530.209],[473.97950000000026,530.209],[473.97950000000026,546.159],[122.04,546.159]]},{"text":"周恩来要求北京市革委会派负责干部到现地勘察研究","box":[[122.04,561.409],[505.3043000000006,561.409],[505.3043000000006,577.359],[122.04,577.359]]},{"text":"后提出建议。北京市革委会随即派北京卫戍区第一副司令","box":[[90,592.609],[505.1801000000005,592.609],[505.1801000000005,608.5590000000001],[90,608.5590000000001]]},{"text":"员","box":[[90,623.809],[105.95,623.809],[105.95,639.759],[90,639.759]]},{"text":"、","box":[[105.959,623.809],[121.909,623.809],[121.909,639.759],[105.959,639.759]]},{"text":"北京市革委会副主任吴忠带领有关部门负责人和水利专","box":[[121.32,623.809],[505.29860000000025,623.809],[505.29860000000025,639.759],[121.32,639.759]]},{"text":"家赶往现场","box":[[90,655.009],[169.90789999999998,655.009],[169.90789999999998,670.9590000000001],[90,670.9590000000001]]},{"text":"。","box":[[170.04,655.009],[185.98999999999998,655.009],[185.98999999999998,670.9590000000001],[170.04,670.9590000000001]]},{"text":"大家很快便取得一致意见","box":[[185.64,655.009],[361.5449000000001,655.009],[361.5449000000001,670.9590000000001],[185.64,670.9590000000001]]},{"text":"：","box":[[361.68,655.009],[377.63,655.009],[377.63,670.9590000000001],[361.68,670.9590000000001]]},{"text":"密云水库虽然库容","box":[[377.28,655.009],[505.18640000000016,655.009],[505.18640000000016,670.9590000000001],[377.28,670.9590000000001]]},{"text":"可达","box":[[90,686.209],[122.26955000000001,686.209],[122.26955000000001,702.159],[90,702.159]]},{"text":"","box":[[122.26955000000001,686.209],[208.8785499999997,686.209],[208.8785499999997,686.209],[122.26955000000001,686.209]]},{"text":"43","box":[[126.6,686.209],[142.61485,686.209],[142.61485,702.159],[126.6,702.159]]},{"text":"","box":[[142.61485,686.209],[227.9178500000001,686.209],[227.9178500000001,686.209],[142.61485,686.209]]},{"text":"亿多立方米，但水源地区连年","box":[[146.88,686.209],[358.42550000000006,686.209],[358.42550000000006,702.159],[146.88,702.159]]},{"text":"干","box":[[358.8,686.209],[374.75,686.209],[374.75,702.159],[358.8,702.159]]},{"text":"旱少雨，实际蓄水","box":[[375.12,686.209],[505.18730000000005,686.209],[505.18730000000005,702.159],[375.12,702.159]]},{"text":"不过","box":[[90,717.409],[122.26955000000001,717.409],[122.26955000000001,733.359],[90,733.359]]},{"text":"","box":[[122.26955000000001,717.409],[208.8785499999997,717.409],[208.8785499999997,717.409],[122.26955000000001,717.409]]},{"text":"21","box":[[126.6,717.409],[142.61485,717.409],[142.61485,733.359],[126.6,733.359]]},{"text":"","box":[[142.61485,717.409],[227.9178500000001,717.409],[227.9178500000001,717.409],[142.61485,717.409]]},{"text":"亿立方米，只是","box":[[146.88,717.409],[260.62775,717.409],[260.62775,733.359],[146.88,733.359]]},{"text":"“","box":[[261,717.409],[276.95,717.409],[276.95,733.359],[261,733.359]]},{"text":"半盆水","box":[[277.32,717.409],[325.9091,717.409],[325.9091,733.359],[277.32,733.359]]},{"text":"”","box":[[326.28,717.409],[342.22999999999996,717.409],[342.22999999999996,733.359],[326.28,733.359]]},{"text":"，即使大堤被炸，对北","box":[[342.6,717.409],[505.30640000000005,717.409],[505.30640000000005,733.359],[342.6,733.359]]},{"text":"京地区也不会造成大的危害","box":[[90,748.609],[281.98460000000006,748.609],[281.98460000000006,764.5590000000001],[90,764.5590000000001]]},{"text":"；","box":[[282,748.609],[297.95,748.609],[297.95,764.5590000000001],[282,764.5590000000001]]},{"text":"密云水库是北京市区用水的主","box":[[297.24,748.609],[505.18400000000025,748.609],[505.18400000000025,764.5590000000001],[297.24,764.5590000000001]]}]}