00000000003],[377.52,140.55900000000003]]},{"text":"我看","box":[[393.6,124.60900000000004],[425.5094,124.60900000000004],[425.5094,140.55900000000003],[393.6,140.55900000000003]]},{"text":"","box":[[425.5094,124.60900000000004],[507.3213999999999,124.60900000000004],[507.3213999999999,124.60900000000004],[425.5094,124.60900000000004]]},{"text":"50","box":[[429.6,124.60900000000004],[445.61485,124.60900000000004],[445.61485,140.55900000000003],[429.6,140.55900000000003]]},{"text":"","box":[[445.61485000000005,124.60900000000004],[523.7178499999987,124.60900000000004],[523.7178499999987,124.60900000000004],[445.61485000000005,124.60900000000004]]},{"text":"年","box":[[449.52,124.60900000000004],[465.46999999999997,124.60900000000004],[465.46999999999997,140.55900000000003],[449.52,140.55900000000003]]},{"text":"、","box":[[465.6,124.60900000000004],[481.55,124.60900000000004],[481.55,140.55900000000003],[465.6,140.55900000000003]]},{"text":"10","box":[[481.44,124.60900000000004],[497.45484999999996,124.60900000000004],[497.45484999999996,140.55900000000003],[481.44,140.55900000000003]]},{"text":"0","box":[[497.4,124.60900000000004],[505.375,124.60900000000004],[505.375,140.55900000000003],[497.4,140.55900000000003]]},{"text":"年之后，可能我们这一段是历史上的一个小插曲。","box":[[90,155.80899999999997],[441.93950000000024,155.80899999999997],[441.93950000000024,171.75899999999996],[90,171.75899999999996]]},{"text":"”","box":[[441.959,155.80899999999997],[457.909,155.80899999999997],[457.909,171.75899999999996],[441.959,171.75899999999996]]},{"text":"10","box":[[122.04,187.00900000000001],[138.05485,187.00900000000001],[138.05485,202.959],[122.04,202.959]]},{"text":"","box":[[138.05485,187.00900000000001],[218.55785000000049,187.00900000000001],[218.55785000000049,187.00900000000001],[138.05485,187.00900000000001]]},{"text":"月","box":[[142.08,187.00900000000001],[158.03,187.00900000000001],[158.03,202.959],[142.08,202.959]]},{"text":"","box":[[158.03,187.00900000000001],[242.23000000000016,187.