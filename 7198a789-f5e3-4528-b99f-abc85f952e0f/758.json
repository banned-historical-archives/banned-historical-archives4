441.46840000000003,623.809],[441.46840000000003,639.759],[409.559,639.759]]},{"text":"，","box":[[441.6,623.809],[457.55,623.809],[457.55,639.759],[441.6,639.759]]},{"text":"江青","box":[[457.32,623.809],[489.2294,623.809],[489.2294,639.759],[457.32,639.759]]},{"text":"大","box":[[489.359,623.809],[505.30899999999997,623.809],[505.30899999999997,639.759],[489.359,639.759]]},{"text":"声说道","box":[[90,655.009],[137.9891,655.009],[137.9891,670.9590000000001],[90,670.9590000000001]]},{"text":"：","box":[[138,655.009],[153.95,655.009],[153.95,670.9590000000001],[138,670.9590000000001]]},{"text":"“","box":[[153.6,655.009],[169.54999999999998,655.009],[169.54999999999998,670.9590000000001],[153.6,670.9590000000001]]},{"text":"总理呀","box":[[169.68,655.009],[217.66910000000001,655.009],[217.66910000000001,670.9590000000001],[169.68,670.9590000000001]]},{"text":"，","box":[[217.68,655.009],[233.63,655.009],[233.63,670.9590000000001],[217.68,670.9590000000001]]},{"text":"你是不是也关心一下京剧革命呀？我们","box":[[233.28,655.009],[505.18190000000027,655.009],[505.18190000000027,670.9590000000001],[233.28,670.9590000000001]]},{"text":"的工作可真艰难","box":[[90,686.209],[201.947,686.209],[201.947,702.159],[90,702.159]]},{"text":"呐","box":[[201.959,686.209],[217.909,686.209],[217.909,702.159],[201.959,702.159]]},{"text":"！","box":[[218.04,686.209],[233.98999999999998,686.209],[233.98999999999998,702.159],[218.04,702.159]]},{"text":"”","box":[[234,686.209],[249.95,686.209],[249.95,702.159],[234,702.159]]},{"text":"周恩来望着江青，","box":[[122.04,717.409],[249.94640000000004,717.409],[249.94640000000004,733.359],[122.04,733.359]]},{"text":"不知是什么意思，","box":[[249.959,717.409],[377.8654000000002,717.409],[377.8654000000002,733.359],[249.959,733.359]]},{"text":"一时没有讲话","box":[[378,717.409],[473.9876000000001,717.409],[473.9876000000001,733.359],[378,733.359]]},{"text":"。","box":[[474,717.409],[489.95,717.409],[489.95,733.359],[474,733.359]]},{"text":"主持会议的","box":[[122.04,748.609],[201.94790000000003,748.609],[201.94790000