318.95,670.9590000000001],[303,670.9590000000001]]},{"text":"一朝权在手","box":[[318.84,655.009],[398.7479000000001,655.009],[398.7479000000001,670.9590000000001],[318.84,670.9590000000001]]},{"text":"，","box":[[398.88,655.009],[414.83,655.009],[414.83,670.9590000000001],[398.88,670.9590000000001]]},{"text":"便把令来行","box":[[409.32,655.009],[489.2279000000001,655.009],[489.2279000000001,670.9590000000001],[409.32,670.9590000000001]]},{"text":"’","box":[[489.359,655.009],[505.30899999999997,655.009],[505.30899999999997,670.9590000000001],[489.359,670.9590000000001]]},{"text":"。","box":[[497.4,655.009],[513.35,655.009],[513.35,670.9590000000001],[497.4,670.9590000000001]]},{"text":"曾、刘要注意，同志们一样也要注意","box":[[90,686.209],[345.9425000000001,686.209],[345.9425000000001,702.159],[90,702.159]]},{"text":"。","box":[[345.959,686.209],[361.909,686.209],[361.909,702.159],[345.959,702.159]]},{"text":"”","box":[[354,686.209],[369.95,686.209],[369.95,702.159],[354,702.159]]},{"text":"“","box":[[122.04,717.409],[137.99,717.409],[137.99,733.359],[122.04,733.359]]},{"text":"还有一个问题，现在清理阶级队伍，必要性不讲了","box":[[138,717.409],[489.93950000000024,717.409],[489.93950000000024,733.359],[138,733.359]]},{"text":"。","box":[[489.959,717.409],[505.909,717.409],[505.909,733.359],[489.959,733.359]]},{"text":"同志们知道","box":[[90,748.609],[169.90789999999998,748.609],[169.90789999999998,764.5590000000001],[90,764.5590000000001]]},{"text":"。","box":[[170.04,748.609],[185.98999999999998,748.609],[185.98999999999998,764.5590000000001],[170.04,764.5590000000001]]},{"text":"造反派同志要注意","box":[[185.759,748.609],[313.6654000000001,748.609],[313.6654000000001,764.5590000000001],[185.759,764.5590000000001]]},{"text":"，","box":[[313.8,748.609],[329.75,748.609],[329.75,764.5590000000001],[313.8,764.5590000000001]]},{"text":"要警惕","box":[[329.52,748.609],[377.50910000000005,748.609],[377.50910000000005,764.5590000000001],[329.52,764.5590000000001]]},{"text":"。","box":[[377.52,748.609],[393.46999999999997,748.609],[393.469999999