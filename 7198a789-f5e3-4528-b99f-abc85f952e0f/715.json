95],[185.98760000000001,218.20899999999995],[185.98760000000001,234.15899999999993],[90,234.15899999999993]]},{"text":"。","box":[[186,218.20899999999995],[201.95,218.20899999999995],[201.95,234.15899999999993],[186,234.15899999999993]]},{"text":"”","box":[[194.04,218.20899999999995],[209.98999999999998,218.20899999999995],[209.98999999999998,234.15899999999993],[194.04,234.15899999999993]]},{"text":"这正是周恩来外交的智慧","box":[[122.04,249.409],[297.9449,249.409],[297.9449,265.359],[122.04,265.359]]},{"text":"。","box":[[297.959,249.409],[313.909,249.409],[313.909,265.359],[297.959,265.359]]},{"text":"本来","box":[[313.68,249.409],[345.5894,249.409],[345.5894,265.359],[313.68,265.359]]},{"text":"，","box":[[345.6,249.409],[361.55,249.409],[361.55,265.359],[345.6,265.359]]},{"text":"在送审的新闻公告稿","box":[[361.32,249.409],[505.30610000000013,249.409],[505.30610000000013,265.359],[361.32,265.359]]},{"text":"中","box":[[90,280.60900000000004],[105.95,280.60900000000004],[105.95,296.559],[90,296.559]]},{"text":"，","box":[[105.959,280.60900000000004],[121.909,280.60900000000004],[121.909,296.559],[105.959,296.559]]},{"text":"有","box":[[121.8,280.60900000000004],[137.75,280.60900000000004],[137.75,296.559],[121.8,296.559]]},{"text":"“","box":[[137.759,280.60900000000004],[153.70899999999997,280.60900000000004],[153.70899999999997,296.559],[137.759,296.559]]},{"text":"同志般的","box":[[153.72,280.60900000000004],[217.66850000000002,280.60900000000004],[217.66850000000002,296.559],[153.72,296.559]]},{"text":"”","box":[[217.8,280.60900000000004],[233.75,280.60900000000004],[233.75,296.559],[217.8,296.559]]},{"text":"、","box":[[233.759,280.60900000000004],[249.70899999999997,280.60900000000004],[249.70899999999997,296.559],[233.759,296.559]]},{"text":"“","box":[[249.6,280.60900000000004],[265.55,280.60900000000004],[265.55,296.559],[249.6,296.559]]},{"text":"友好的","box":[[265.559,280.60900000000004],[313.5481000000001,280.60900000000004],[313.5481000000001,296.559],[265.559,296.559]]},{"text":"”","b