00003],[90,140.55900000000003]]},{"text":"，","box":[[105.959,124.60900000000004],[121.909,124.60900000000004],[121.909,140.55900000000003],[105.959,140.55900000000003]]},{"text":"“","box":[[114,124.60900000000004],[129.95,124.60900000000004],[129.95,140.55900000000003],[114,140.55900000000003]]},{"text":"刚才王良恩同志来电话说，这份","box":[[129.959,124.60900000000004],[353.8624000000001,124.60900000000004],[353.8624000000001,140.55900000000003],[129.959,140.55900000000003]]},{"text":"《","box":[[354,124.60900000000004],[369.95,124.60900000000004],[369.95,140.55900000000003],[354,140.55900000000003]]},{"text":"简报","box":[[369.959,124.60900000000004],[401.8684,124.60900000000004],[401.8684,140.55900000000003],[369.959,140.55900000000003]]},{"text":"》","box":[[402,124.60900000000004],[417.95,124.60900000000004],[417.95,140.55900000000003],[402,140.55900000000003]]},{"text":"要收回去","box":[[417.959,124.60900000000004],[481.9075000000001,124.60900000000004],[481.9075000000001,140.55900000000003],[417.959,140.55900000000003]]},{"text":"。","box":[[482.04,124.60900000000004],[497.99,124.60900000000004],[497.99,140.55900000000003],[482.04,140.55900000000003]]},{"text":"”","box":[[489.959,124.60900000000004],[505.909,124.60900000000004],[505.909,140.55900000000003],[489.959,140.55900000000003]]},{"text":"江青看到简报","box":[[122.04,155.80899999999997],[219.58775000000003,155.80899999999997],[219.58775000000003,171.75899999999996],[122.04,171.75899999999996]]},{"text":"，","box":[[219.84,155.80899999999997],[235.79,155.80899999999997],[235.79,171.75899999999996],[219.84,171.75899999999996]]},{"text":"心情平静了一些。她说","box":[[236.159,155.80899999999997],[398.98495,155.80899999999997],[398.98495,171.75899999999996],[236.159,171.75899999999996]]},{"text":"：","box":[[399.359,155.80899999999997],[415.30899999999997,155.80899999999997],[415.30899999999997,171.75899999999996],[399.359,171.75899999999996]]},{"text":"“","box":[[407.64,155.80899999999997],[423.59,155.80899999999997],[423.59,171.75899999999996],[407.64,171.75899999999996]]},{"text":"你给良恩同","box":[[423.959,155.80899999999997],[505.1872,155.80899999999997],[505.1872,171.75899999999996],[423.959,171.75899999999996]]},{"text":"志回电话，就说我还没有看，看完了再交回去","box":[[90,187.00900000000001],[415.54324999999994,187.00900000000001],[415.54324999999994,202.959],[90,202.959]]},{"text":"。","box":[[415.8,187.00900000000001],[431.75,187.00900000000001],[431.75,202.959],[415.8,202.959]]},{"text":"”","box":[[424.08,187.00900000000001],[440.03,187.00900000000001],[440.03,202.959],[424.08,202.959]]},{"text":"然后","box":[[440.4,187.00900000000001],[472.66954999999996,187.00900000000001],[472.66954999999996,202.959],[440.4,202.959]]},{"text":"自言","box":[[473.04,187.00900000000001],[505.30955,187.00900000000001],[505.30955,202.959],[473.04,202.959]]},{"text":"自语地说","box":[[90,218.20899999999995],[153.9485,218.20899999999995],[153.9485,234.15899999999993],[90,234.15899999999993]]},{"text":"：","box":[[153.959,218.20899999999995],[169.909,218.20899999999995],[169.909,234.15899999999993],[153.959,234.15899999999993]]},{"text":"“","box":[[162,218.20899999999995],[177.95,218.20899999999995],[177.95,234.15899999999993],[162,234.15899999999993]]},{"text":"看来是有人做贼心虚了","box":[[177.959,218.20899999999995],[337.9045000000001,218.20899999999995],[337.9045000000001,234.15899999999993],[177.959,234.15899999999993]]},{"text":"。","box":[[338.04,218.20899999999995],[353.99,218.20899999999995],[353.99,234.15899999999993],[338.04,234.15899999999993]]},{"text":"”","box":[[345.959,218.20899999999995],[361.909,218.20899999999995],[361.909,234.15899999999993],[345.959,234.15899999999993]]},{"text":"她转向杨银禄说","box":[[122.04,249.409],[235.90730000000002,249.409],[235.90730000000002,265.359],[122.04,265.359]]},{"text":"：","box":[[236.159,249.409],[252.10899999999998,249.409],[252.10899999999998,265.359],[236.159,265.359]]},{"text":"“","box":[[244.44,249.409],[260.39,249.409],[260.39,265.359],[244.44,265.359]]},{"text":"你给总理打个电话，如果总理有时","box":[[260.88,249.409],[505.30370000000005,249.409],[505.30370000000005,265.359],[260.88,265.359]]},{"text":"间的话，我到总理那里去一下","box":[[90,280.60900000000004],[297.9440000000001,280.60900000000004],[297.9440000000001,296.559],[90,296.559]]},{"text":"。","box":[[297.959,280.60900000000004],[313.909,280.60900000000004],[313.909,296.559],[297.959,296.559]]},{"text":"”","box":[[306,280.60900000000004],[321.95,280.60900000000004],[321.95,296.559],[306,296.559]]},{"text":"杨银禄说","box":[[122.04,311.8090000000001],[185.98850000000002,311.8090000000001],[185.98850000000002,327.75900000000007],[122.04,327.75900000000007]]},{"text":"：","box":[[186,311.8090000000001],[201.95,311.8090000000001],[201.95,327.75900000000007],[186,327.75900000000007]]},{"text":"“","box":[[194.04,311.8090000000001],[209.98999999999998,311.8090000000001],[209.98999999999998,327.75900000000007],[194.04,327.75900000000007]]},{"text":"好，我去给总理打电话","box":[[210,311.8090000000001],[369.94550000000015,311.8090000000001],[369.94550000000015,327.75900000000007],[210,327.75900000000007]]},{"text":"。","box":[[369.959,311.8090000000001],[385.909,311.8090000000001],[385.909,327.75900000000007],[369.959,327.75900000000007]]},{"text":"”","box":[[378,311.8090000000001],[393.95,311.8090000000001],[393.95,327.75900000000007],[378,327.75900000000007]]},{"text":"杨银禄回到自己的办公室","box":[[122.04,343.009],[297.9449,343.009],[297.9449,358.959],[122.04,358.959]]},{"text":"，","box":[[297.959,343.009],[313.909,343.009],[313.909,358.959],[297.959,358.959]]},{"text":"立即给","box":[[313.32,343.009],[361.30910000000006,343.009],[361.30910000000006,358.959],[313.32,358.959]]},{"text":"周恩来","box":[[361.32,343.009],[409.30910000000006,343.009],[409.30910000000006,358.959],[361.32,358.959]]},{"text":"的秘书钱嘉栋","box":[[409.32,343.009],[505.3076000000001,343.009],[505.3076000000001,358.959],[409.32,358.959]]},{"text":"打了电话。等了一分钟，钱嘉栋回电话说","box":[[90,374.209],[382.90414999999996,374.209],[382.90414999999996,390.159],[90,390.159]]},{"text":"：","box":[[383.28,374.209],[399.22999999999996,374.209],[399.22999999999996,390.159],[383.28,390.159]]},{"text":"“","box":[[391.44,374.209],[407.39,374.209],[407.39,390.159],[391.44,390.159]]},{"text":"总理说请江青","box":[[407.759,374.209],[505.1872,374.209],[505.1872,390.159],[407.759,390.159]]},{"text":"同志现在就来","box":[[90,405.409],[185.98760000000001,405.409],[185.98760000000001,421.359],[90,421.359]]},{"text":"。","box":[[186,405.409],[201.95,405.409],[201.95,421.359],[186,421.359]]},{"text":"”","box":[[194.04,405.409],[209.98999999999998,405.409],[209.98999999999998,421.359],[194.04,421.359]]},{"text":"杨银禄放下电话，跑到大院里报告","box":[[122.04,436.609],[366.4637,436.609],[366.4637,452.55899999999997],[122.04,452.55899999999997]]},{"text":"正在散步的","box":[[366.72,436.609],[447.94820000000004,436.609],[447.94820000000004,452.55899999999997],[366.72,452.55899999999997]]},{"text":"江青说","box":[[448.32,436.609],[496.9091,436.609],[496.9091,452.55899999999997],[448.32,452.55899999999997]]},{"text":"：","box":[[497.28,436.609],[513.23,436.609],[513.23,452.55899999999997],[497.28,452.55899999999997]]},{"text":"“","box":[[90,467.809],[105.95,467.809],[105.95,483.759],[90,483.759]]},{"text":"小高那里请您现在就去","box":[[105.959,467.809],[265.90450000000004,467.809],[265.90450000000004,483.759],[105.959,483.759]]},{"text":"。","box":[[266.04,467.809],[281.99,467.809],[281.99,483.759],[266.04,483.759]]},{"text":"”","box":[[273.959,467.809],[289.909,467.809],[289.909,483.759],[273.959,483.759]]},{"text":"江青上车走了。","box":[[122.04,499.009],[233.98700000000002,499.009],[233.98700000000002,514.9590000000001],[122.04,514.9590000000001]]},{"text":"再说上海组这边","box":[[122.04,530.209],[233.98700000000002,530.209],[233.98700000000002,546.159],[122.04,546.159]]},{"text":"，","box":[[234,530.209],[249.95,530.209],[249.95,546.159],[234,546.159]]},{"text":"徐景贤送走严秘书","box":[[245.64,530.209],[373.5464000000002,530.209],[373.5464000000002,546.159],[245.64,546.159]]},{"text":"，","box":[[373.68,530.209],[389.63,530.209],[389.63,546.159],[373.68,546.159]]},{"text":"和王洪文商量后","box":[[385.32,530.209],[497.26700000000017,530.209],[497.26700000000017,546.159],[385.32,546.159]]},{"text":"，","box":[[497.28,530.209],[513.23,530.209],[513.23,546.159],[497.28,546.159]]},{"text":"赶紧把上海的中委们全部找来，紧急讨论上午如何表态。","box":[[90,561.409],[489.93800000000033,561.409],[489.93800000000033,577.359],[90,577.359]]},{"text":"小组的成员们没有到齐","box":[[122.04,592.609],[281.9855,592.609],[281.9855,608.5590000000001],[122.04,608.5590000000001]]},{"text":"，","box":[[282,592.609],[297.95,592.609],[297.95,608.5590000000001],[282,608.5590000000001]]},{"text":"王洪文和徐景贤站在别墅门口","box":[[297.24,592.609],[505.18400000000025,592.609],[505.18400000000025,608.5590000000001],[297.24,608.5590000000001]]},{"text":"的台阶上朝坡下望去，只见张春桥从他的住所急匆匆走出","box":[[90,623.809],[505.1801000000005,623.809],[505.1801000000005,639.759],[90,639.759]]},{"text":"来","box":[[90,655.009],[105.95,655.009],[105.95,670.9590000000001],[90,670.9590000000001]]},{"text":"，","box":[[105.959,655.009],[121.909,655.009],[121.909,670.9590000000001],[105.959,670.9590000000001]]},{"text":"几乎是同时","box":[[121.8,655.009],[201.70790000000002,655.009],[201.70790000000002,670.9590000000001],[121.8,670.9590000000001]]},{"text":"，","box":[[201.72,655.009],[217.67,655.009],[217.67,670.9590000000001],[201.72,670.9590000000001]]},{"text":"左首别墅里的姚文元也出了他的屋门","box":[[217.44,655.009],[473.3825000000003,655.009],[473.3825000000003,670.9590000000001],[217.44,670.9590000000001]]},{"text":"。","box":[[473.52,655.009],[489.46999999999997,655.009],[489.46999999999997,670.9590000000001],[473.52,670.9590000000001]]},{"text":"两","box":[[489.359,655.009],[505.30899999999997,655.009],[505.30899999999997,670.9590000000001],[489.359,670.9590000000001]]},{"text":"个人见了面","box":[[90,686.209],[169.90789999999998,686.209],[169.90789999999998,702.159],[90,702.159]]},{"text":"，","box":[[170.04,686.209],[185.98999999999998,686.209],[185.98999999999998,702.159],[170.04,702.159]]},{"text":"不说一句话","box":[[185.64,686.209],[265.5479,686.209],[265.5479,702.159],[185.64,702.159]]},{"text":"，","box":[[265.68,686.209],[281.63,686.209],[281.63,702.159],[265.68,702.159]]},{"text":"就一起坐进了早已停在院子门外","box":[[281.28,686.209],[505.18340000000023,686.209],[505.18340000000023,702.159],[281.28,702.159]]},{"text":"的一辆轿车里，疾驰而去","box":[[90,717.409],[265.90490000000005,717.409],[265.90490000000005,733.359],[90,733.359]]},{"text":"......","box":[[266.04,717.409],[297.9494,717.409],[297.9494,733.359],[266.04,733.359]]},{"text":"上海的中委们齐集在王洪文的住所","box":[[122.04,748.609],[362.02310000000006,748.609],[362.02310000000006,764.5590000000001],[122.04,764.5590000000001]]},{"text":"，","box":[[362.04,748.609],[377.99,748.609],[377.99,764.5590000000001],[362.04,764.5590000000001]]},{"text":"大家翻看着","box":[[377.64,748.609],[457.5479000000001,748.609],[457.5479000000001,764.5590000000001],[377.64,764.5590000000001]]},{"text":"《","box":[[457.32,748.609],[473.27,748.609],[473.27,764.5590000000001],[457.32,764.5590000000001]]},{"text":"六号","box":[[473.28,748.609],[505.1894,748.609],[505.1894,764.5590000000001],[473.28,764.5590000000001]]}]}