265.359]]},{"text":"：","box":[[497.28,249.409],[513.23,249.409],[513.23,265.359],[497.28,265.359]]},{"text":"“","box":[[122.04,280.60900000000004],[137.99,280.60900000000004],[137.99,296.559],[122.04,296.559]]},{"text":"一个是吴传启","box":[[138,280.60900000000004],[233.98760000000004,280.60900000000004],[233.98760000000004,296.559],[138,296.559]]},{"text":"，","box":[[234,280.60900000000004],[249.95,280.60900000000004],[249.95,296.559],[234,296.559]]},{"text":"湖北人","box":[[249.72,280.60900000000004],[297.7091,280.60900000000004],[297.7091,296.559],[249.72,296.559]]},{"text":"，","box":[[297.72,280.60900000000004],[313.67,280.60900000000004],[313.67,296.559],[297.72,296.559]]},{"text":"是国民党特务","box":[[313.559,280.60900000000004],[409.5466000000001,280.60900000000004],[409.5466000000001,296.559],[313.559,296.559]]},{"text":"，","box":[[409.559,280.60900000000004],[425.509,280.60900000000004],[425.509,296.559],[409.559,296.559]]},{"text":"参加过伪国","box":[[425.28,280.60900000000004],[505.18790000000007,280.60900000000004],[505.18790000000007,296.559],[425.28,296.559]]},{"text":"大竞选","box":[[90,311.8090000000001],[137.9891,311.8090000000001],[137.9891,327.75900000000007],[90,327.75900000000007]]},{"text":"，","box":[[138,311.8090000000001],[153.95,311.8090000000001],[153.95,327.75900000000007],[138,327.75900000000007]]},{"text":"是宋洛介绍入党的","box":[[153.72,311.8090000000001],[281.6264,311.8090000000001],[281.6264,327.75900000000007],[153.72,327.75900000000007]]},{"text":"，","box":[[281.759,311.8090000000001],[297.709,311.8090000000001],[297.709,327.75900000000007],[281.759,327.75900000000007]]},{"text":"宋洛是武汉市宣传部部长","box":[[297.6,311.8090000000001],[473.50490000000025,311.8090000000001],[473.50490000000025,327.75900000000007],[297.6,327.75900000000007]]},{"text":"，","box":[[473.52,311.8090000000001],[489.46999999999997,311.8090000000001],[489.46999999999997,327.75900000000007],[473.52,327.75900000000007]]},{"text":"参","box":[[489.359,311.8090000000001],[505.30899999999997,311.8090000000001],[505.30899999999997,327.75900000000007],[489.359,327.75900000000007]]},{"text":"加了造反派","box":[[90,343.009],[169.90789999999998,343.009],[169.90789999999998,358.959],[90,358.959]]},{"text":"，","box":[[170.04,343.009],[185.98999999999998,343.009],[185.98999999999998,358.959],[170.04,358.959]]},{"text":"他联系的人是国民党特务","box":[[185.759,343.009],[361.6639000000002,343.009],[361.6639000000002,358.959],[185.759,358.959]]},{"text":"，","box":[[361.8,343.009],[377.75,343.009],[377.75,358.959],[361.8,358.959]]},{"text":"托派王明分子","box":[[377.52,343.009],[473.5076000000001,343.009],[473.5076000000001,358.959],[377.52,358.959]]},{"text":"，","box":[[473.52,343.009],[489.46999999999997,343.009],[489.46999999999997,358.959],[473.52,358.959]]},{"text":"还","box":[[489.359,343.009],[505.30899999999997,343.009],[505.30899999999997,358.959],[489.359,358.959]]},{"text":"有大资本家","box":[[90,374.209],[169.90789999999998,374.209],[169.90789999999998,390.159],[90,390.159]]},{"text":"，","box":[[170.04,374.209],[185.98999999999998,374.209],[185.98999999999998,390.159],[170.04,390.159]]},{"text":"他们摇身一变","box":[[183.12,374.209],[279.1076,374.209],[279.1076,390.159],[183.12,390.159]]},{"text":"，","box":[[279.12,374.209],[295.07,374.209],[295.07,390.159],[279.12,390.159]]},{"text":"成了造反派","box":[[292.2,374.209],[372.1079000000001,374.209],[372.1079000000001,390.159],[292.2,390.159]]},{"text":"，","box":[[372.24,374.209],[388.19,374.209],[388.19,390.159],[372.24,390.159]]},{"text":"这些人到处串联","box":[[385.32,374.209],[497.26700000000017,374.209],[497.26700000000017,390.159],[385.32,390.159]]},{"text":"。","box":[[497.28,374.209],[513.23,374.209],[513.23,390.159],[497.28,390.159]]},{"text":"九大前后","box":[[90,405.409],[153.9485,405.409],[153.9485,421.359],[90,421.359]]},{"text":"，","box":[[153.959,405.409],[169.909,405.409],[169.909,421.359],[153.959,421.359]]},{"text":"反复旧他们都搞了","box":[[169.8,405.409],[297.70640000000003,405.409],[297.70640000000003,421.359],[169.8,421.359]]},{"text":"，","box":[[297.72,405.409],[313.67,405.409],[313.67,421.359],[297.72,421.359]]},{"text":"根就是吴传启","box":[[313.559,405.409],[409.5466000000001,405.409],[409.5466000000001,421.359],[313.559,421.359]]},{"text":"。","box":[[409.559,405.409],[425.509,405.409],[425.509,421.359],[409.559,421.359]]},{"text":"吴传启解放","box":[[425.28,405.409],[505.18790000000007,405.409],[505.18790000000007,421.359],[425.28,421.359]]},{"text":"后跑到中央计委，最后到学部。在六十年代，可能在以前","box":[[90,436.609],[489.93800000000033,436.609],[489.93800000000033,452.55899999999997],[90,452.55899999999997]]},{"text":"，","box":[[489.959,436.609],[505.909,436.609],[505.909,452.55899999999997],[489.959,452.55899999999997]]},{"text":"他和关锋","box":[[90,467.809],[153.9485,467.809],[153.9485,483.759],[90,483.759]]},{"text":"、","box":[[153.959,467.809],[169.909,467.809],[169.909,483.759],[153.959,483.759]]},{"text":"林聿时三人","box":[[169.8,467.809],[249.70790000000005,467.809],[249.70790000000005,483.759],[169.8,483.759]]},{"text":"，","box":[[249.84,467.809],[265.79,467.809],[265.79,483.759],[249.84,483.759]]},{"text":"他们在学部写文章","box":[[265.68,467.809],[393.7067000000002,467.809],[393.7067000000002,483.759],[265.68,483.759]]},{"text":"，","box":[[393.72,467.809],[409.67,467.809],[409.67,483.759],[393.72,483.759]]},{"text":"用假名字","box":[[409.559,467.809],[473.5075000000001,467.809],[473.5075000000001,483.759],[409.559,483.759]]},{"text":"，","box":[[473.52,467.809],[489.46999999999997,467.809],[489.46999999999997,483.759],[473.52,483.759]]},{"text":"在","box":[[489.359,467.809],[505.30899999999997,467.809],[505.30899999999997,483.759],[489.359,483.759]]},{"text":"哲学研究杂志上发表叫撒仁兴。","box":[[90,499.009],[313.90340000000003,499.009],[313.90340000000003,514.9590000000001],[90,514.9590000000001]]},{"text":"这条线","box":[[122.04,530.209],[170.02910000000003,530.209],[170.02910000000003,546.159],[122.04,546.159]]},{"text":"，","box":[[170.04,530.209],[185.98999999999998,530.209],[185.98999999999998,546.159],[170.04,546.159]]},{"text":"还有叛徒潘梓年","box":[[185.759,530.209],[297.7060000000001,530.209],[297.7060000000001,546.159],[185.759,546.159]]},{"text":"，","box":[[297.72,530.209],[313.67,530.209],[313.67,546.159],[297.72,546.159]]},{"text":"是潘汉年的哥哥","box":[[313.559,530.209],[425.5060000000002,530.209],[425.5060000000002,546.159],[313.559,546.159]]},{"text":"，","box":[[425.52,530.209],[441.46999999999997,530.209],[441.46999999999997,546.159],[425.52,546.159]]},{"text":"在国民党","box":[[441.24,530.209],[505.1885000000001,530.209],[505.1885000000001,546.159],[441.24,546.159]]},{"text":"反动时期被捕","box":[[90,561.409],[185.98760000000001,561.409],[185.98760000000001,577.359],[90,577.359]]},{"text":"，","box":[[186,561.409],[201.95,561.409],[201.95,577.359],[186,577.359]]},{"text":"现在查明","box":[[200.4,561.409],[264.3485,561.409],[264.3485,577.359],[200.4,577.359]]},{"text":"，","box":[[264.48,561.409],[280.43,561.409],[280.43,577.359],[264.48,577.359]]},{"text":"他和国民党勾结","box":[[278.88,561.409],[390.8270000000001,561.409],[390.8270000000001,577.359],[278.88,577.359]]},{"text":"，","box":[[390.84,561.409],[406.78999999999996,561.409],[406.78999999999996,577.359],[390.84,577.359]]},{"text":"通过国民党","box":[[405.359,561.409],[485.2669000000001,561.409],[485.2669000000001,577.359],[405.359,577.359]]},{"text":"","box":[[485.2669,561.409],[567.1088999999993,561.409],[567.1088999999993,561.409],[485.2669,561.409]]},{"text":"C","box":[[489.359,561.409],[497.334,561.409],[497.334,577.359],[489.359,577.359]]},{"text":"C","box":[[497.4,561.409],[505.375,561.409],[505.375,577.359],[497.4,577.359]]},{"text":"特务，把他放了。因为我们不清楚，他还作了《新华日报","box":[[90,592.609],[489.93800000000033,592.609],[489.93800000000033,608.5590000000001],[90,608.5590000000001]]},{"text":"》","box":[[489.959,592.609],[505.909,592.609],[505.909,608.5590000000001],[489.959,608.5590000000001]]},{"text":"的编辑","box":[[90,623.809],[137.9891,623.809],[137.9891,639.759],[90,639.759]]},{"text":"，","box":[[138,623.809],[153.95,623.809],[153.95,639.759],[138,639.759]]},{"text":"解放后到了学部","box":[[149.64,623.8