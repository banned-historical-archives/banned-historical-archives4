ox":[[473.4,655.009],[505.3094,655.009],[505.3094,670.9590000000001],[473.4,670.9590000000001]]},{"text":"怎么当主席呢？","box":[[90,686.209],[201.947,686.209],[201.947,702.159],[90,702.159]]},{"text":"”","box":[[201.959,686.209],[217.909,686.209],[217.909,702.159],[201.959,702.159]]},{"text":"8","box":[[122.04,717.409],[130.01500000000001,717.409],[130.01500000000001,733.359],[122.04,733.359]]},{"text":"","box":[[130.01500000000001,717.409],[210.51499999999956,717.409],[210.51499999999956,717.409],[130.01500000000001,717.409]]},{"text":"月","box":[[134.04,717.409],[149.98999999999998,717.409],[149.98999999999998,733.359],[134.04,733.359]]},{"text":"","box":[[149.98999999999998,717.409],[229.37000000000043,717.409],[229.37000000000043,717.409],[149.98999999999998,717.409]]},{"text":"22","box":[[153.959,717.409],[169.97385,717.409],[169.97385,733.359],[153.959,733.359]]},{"text":"","box":[[169.97385,717.409],[250.49685000000002,717.409],[250.49685000000002,717.409],[169.97385,717.409]]},{"text":"日一大早，文道宏","box":[[174,717.409],[301.9064,717.409],[301.9064,733.359],[174,733.359]]},{"text":"将汪东兴的话","box":[[302.04,717.409],[398.0276000000001,717.409],[398.0276000000001,733.359],[302.04,733.359]]},{"text":"告诉了程世清","box":[[398.04,717.409],[494.0276000000001,717.409],[494.0276000000001,733.359],[398.04,733.359]]},{"text":"。","box":[[494.04,717.409],[509.99,717.409],[509.99,733.359],[494.04,733.359]]},{"text":"这天","box":[[122.04,748.609],[153.9494,748.609],[153.9494,764.5590000000001],[122.04,764.5590000000001]]},{"text":"上午","box":[[153.959,748.609],[185.8684,748.609],[185.8684,764.5590000000001],[153.959,764.5590000000001]]},{"text":"，","box":[[186,748.609],[201.95,748.609],[201.95,764.5590000000001],[186,764.5590000000001]]},{"text":"陈伯达去看吴法宪","box":[[201.84,748.609],[329.86670000000004,748.609],[329.86670000000004,764.5590000000001],[201.84,764.5590000000001]]},{"text":"，","box":[[329.88,748.609],[345.83,748.609],[345.83,764.5590000000001],[329.88,764.5590000000001]]},{"text":"没