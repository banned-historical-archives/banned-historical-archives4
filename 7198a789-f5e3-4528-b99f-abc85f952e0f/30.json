96]]},{"text":"","box":[[158.14999999999998,155.80899999999997],[239.9500000000006,155.80899999999997],[239.9500000000006,155.80899999999997],[158.14999999999998,155.80899999999997]]},{"text":"6","box":[[162.24,155.80899999999997],[170.215,155.80899999999997],[170.215,171.75899999999996],[162.24,171.75899999999996]]},{"text":"","box":[[170.215,155.80899999999997],[255.51499999999973,155.80899999999997],[255.51499999999973,155.80899999999997],[170.215,155.80899999999997]]},{"text":"日，曾于","box":[[174.48,155.80899999999997],[238.9097,155.80899999999997],[238.9097,171.75899999999996],[174.48,171.75899999999996]]},{"text":"","box":[[238.90969999999996,155.80899999999997],[323.11570000000086,155.80899999999997],[323.11570000000086,155.80899999999997],[238.90969999999996,155.80899999999997]]},{"text":"1927","box":[[243.12,155.80899999999997],[275.09425,155.80899999999997],[275.09425,171.75899999999996],[243.12,171.75899999999996]]},{"text":"","box":[[275.09425000000005,155.80899999999997],[358.0092499999993,155.80899999999997],[358.0092499999993,155.80899999999997],[275.09425000000005,155.80899999999997]]},{"text":"年任湖北总工会劳资斗争委员会","box":[[279.24,155.80899999999997],[505.1885000000001,155.80899999999997],[505.1885000000001,171.75899999999996],[279.24,171.75899999999996]]},{"text":"委员","box":[[90,187.00900000000001],[121.9094,187.00900000000001],[121.9094,202.959],[90,202.959]]},{"text":"、","box":[[122.04,187.00900000000001],[137.99,187.00900000000001],[137.99,202.959],[122.04,202.959]]},{"text":"湖北省","box":[[137.28,187.00900000000001],[185.2691,187.00900000000001],[185.2691,202.959],[137.28,202.959]]},{"text":"劳资仲裁委员会主席","box":[[185.28,187.00900000000001],[329.26610000000016,187.00900000000001],[329.26610000000016,202.959],[185.28,202.959]]},{"text":"的","box":[[329.28,187.00900000000001],[345.22999999999996,187.00900000000001],[345.22999999999996,202.959],[329.28,202.959]]},{"text":"丁觉群","box":[[345.24,187.00900000000001],[393.2291000000001,187.00900000000001],[393.2291000000001,202.959],[345.24,202.959]]},{"text":"写成了","box":[[393.24,187.00900000000001],[441.2291000000001,187.00900000000001],[441.2291000000001,202.959],[393.24,202.959]]},{"text":"关于刘少","box":[[441.24,187.00900000000001],[505.1885000000001,187.00900000000001],[505.1885000000001,202.959],[441.24,202.959]]},{"text":"奇勾结汪精卫、陈公博充当内奸的供词","box":[[90,218.20899999999995],[361.9019000000001,218.20899999999995],[361.9019000000001,234.15899999999993],[90,234.15899999999993]]},{"text":"：","box":[[362.04,218.20899999999995],[377.99,218.20899999999995],[377.99,234.15899999999993],[362.04,234.15899999999993]]},{"text":"对刘少奇一九二七年在汉口、庐山所犯罪行的交待","box":[[137.64,249.409],[489.5795000000002,249.409],[489.5795000000002,265.359],[137.64,265.359]]},{"text":"刘少奇于一九二六年十月中旬由广州到武汉","box":[[122.04,280.60900000000004],[425.9810000000002,280.60900000000004],[425.9810000000002,296.559],[122.04,296.559]]},{"text":"，","box":[[426,280.60900000000004],[441.95,280.60900000000004],[441.95,296.559],[426,296.559]]},{"text":"任湖北省","box":[[441.24,280.60900000000004],[505.1885000000001,280.60900000000004],[505.1885000000001,296.559],[441.24,296.559]]},{"text":"总工会秘书长","box":[[90,311.8090000000001],[185.98760000000001,311.8090000000001],[185.98760000000001,327.75900000000007],[90,327.75900000000007]]},{"text":"。","box":[[186,311.8090000000001],[201.95,311.8090000000001],[201.95,327.75900000000007],[186,327.75900000000007]]},{"text":"当时我是国民党汉口特别市党部的执行委员","box":[[201.24,311.8090000000001],[505.1810000000004,311.8090000000001],[505.1810000000004,327.75900000000007],[201.24,327.75900000000007]]},{"text":"兼工人部长","box":[[90,343.009],[169.90789999999998,343.009],[169.90789999999998,358.959],[90,358.959]]},{"text":"。","box":[[170.04,343.009],[185.98999999999998,343.009],[185.98999999999998,358.959],[170.04,358.959]]},{"text":"总工会有关汉口的工人运动是受汉口市党部工","box":[[185.28,343.009],[505.1804000000004,343.009],[505.1804000000004,358.959],[185.28,358.959]]},{"text":"人部领导的","box":[[90,374.209],[169.90789999999998,374.209],[169.90789999999998,390.159],[90,390.159]]},{"text":"。","box":[[170.04,374.209],[185.98999999999998,374.209],[185.98999999999998,390.159],[170.04,390.159]]},{"text":"十一月上旬总工会成立","box":[[183.12,374.209],[343.06550000000004,374.209],[343.06550000000004,390.159],[183.12,390.159]]},{"text":"“","box":[[340.2,374.209],[356.15,374.209],[356.15,390.159],[340.2,390.159]]},{"text":"劳资斗争委员会","box":[[356.159,374.209],[468.1060000000001,374.209],[468.1060000000001,390.159],[356.159,390.159]]},{"text":"”","box":[[468.24,374.209],[484.19,374.209],[484.19,390.159],[468.24,390.159]]},{"text":"（","box":[[473.28,374.209],[489.22999999999996,374.209],[489.22999999999996,390.159],[473.28,390.159]]},{"text":"对","box":[[489.24,374.209],[505.19,374.209],[505.19,390.159],[489.24,390.159]]},{"text":"外则用","box":[[90,405.409],[137.9891,405.409],[137.9891,421.359],[90,421.359]]},{"text":"“","box":[[137.759,405.409],[153.70899999999997,405.409],[153.70899999999997,421.359],[137.759,421.359]]},{"text":"经济争议委员会","box":[[153.72,405.409],[265.66700000000003,405.409],[265.66700000000003,421.359],[153.72,421.359]]},{"text":"”","box":[[265.8,405.409],[281.75,405.409],[281.75,421.359],[265.8,421.359]]},{"text":"）","box":[[273.72,405.409],[289.67,405.409],[289.67,421.359],[273.72,421.359]]},{"text":"，","box":[[281.759,405.409],[297.709,405.409],[297.709,421.359],[281.759,421.359]]},{"text":"刘少奇兼任主席","box":[[297.6,405.409],[409.54700000000014,405.409],[409.54700000000014,421.359],[297.6,421.359]]},{"text":"，","box":[[409.559,405.409],[425.509,405.409],[425.509,421.359],[409.559,421.359]]},{"text":"我是委员之","box":[[425.28,405.409],[505.18790000000007,405.409],[505.18790000000007,421.359],[425.28,421.359]]},{"text":"一","box":[[90,436.609],[105.95,436.609],[105.95,452.55899999999997],[90,452.55899999999997]]},{"text":"，","box":[[105.959,436.609],[121.909,436.609],[121.909,452.55899999999997],[105.959,452.55899999999997]]},{"text":"在共产党内我是受他领导的","box":[[121.8,436.609],[313.78460000000007,436.609],[313.78460000000007,452.55899999999997],[121.8,452.55899999999997]]},{"text":"。","box":[[313.8,436.609],[329.75,436.609],[329.75,452.55899999999997],[313.8,452.55899999999997]]},{"text":"十一月底成立","box":[[329.52,436.609],[425.5076000000001,436.609],[425.5076000000001,452.55899999999997],[329.52,452.55899999999997]]},{"text":"“","box":[[425.4,436.609],[441.34999999999997,436.609],[441.34999999999997,452.55899999999997],[425.4,452.55899999999997]]},{"text":"湖北省劳","box":[[441.24,436.609],[505.1885000000001,436.609],[505.1885000000001,452.55899999999997],[441.24,452.55899999999997]]},{"text":"资问题仲裁委员会","box":[[90,467.809],[217.90640000000002,467.809],[217.90640000000002,483.759],[90,483.759]]},{"text":"”","box":[[218.04,467.809],[233.98999999999998,467.809],[233.98999999999998,483.759],[218.04,483.759]]},{"text":"我被选为主席","box":[[233.64,467.809],[329.62760000000003,467.809],[329.62760000000003,483.759],[233.64,483.759]]},{"text":"，","box":[[329.64,467.809],[345.59,467.809],[345.59,483.759],[329.64,483.759]]},{"text":"更须与总工会劳资斗争","box":[[345.24,467.809],[505.1855000000002,467.809],[505.1855000000002,483.759],[345.24,483.759]]},{"text":"会密切联系","box":[[90,499.009],[169.90789999999998,499.009],[169.90789999999998,514.9590000000001],[90,514.9590000000001]]},{"text":"，","box":[[170.04,499.009],[185.98999999999998,499.009],[185.98999999999998,514.9590000000001],[170.04,514.9590000000001]]},{"text":"所以我和刘少奇往来更密","box":[[185.64,499.009],[361.5449000000001,499.009],[361.5449000000001,514.9590000000001],[185.64,514.9590000000001]]},{"text":"。","box":[[361.68,499.009],[377.63,499.009],[377.63,514.9590000000001],[361.68,514.9590000000001]]},{"text":"刘少奇在一九二七","box":[[377.28,499.009],[505.18640000000016,499.009],[505.18640000000016,514.9590000000001],[377.28,514.9590000000001]]},{"text":"年七月八日左右离汉到庐山住白鹿洞书院","box":[[90,530.209],[377.9816000000001,530.209],[377.9816000000001,546.159],[90,546.159]]},{"text":"。","box":[[378,530.209],[393.95,530.209],[393.95,546.159],[378,546.159]]},{"text":"我于七月三十一","box":[[393.24,530.209],[505.1870000000001,530.209],[505.1870000000001,546.159],[393.24,546.159]]},{"text":"日到牯岭","box":[[90,561.409],[153.9485,561.409],[153.9485,577.359],[90,577.359]]},{"text":"。","box":[[153.959,561.409],[169.909,561.409],[169.909,577.359],[153.959,577.359]]},{"text":"八月中旬刘少奇","box":[[169.68,561.409],[281.627,561.409],[281.627,577.359],[169.68,577.359]]},{"text":"、","box":[[281.64,561.409],[297.59,561.409],[297.59,577.359],[281.64,577.359]]},{"text":"何宝珍和我同住在牯岭河南路","box":[[297.24,561.409],[505.18400000000025,561.409],[505.18400000000025,577.359],[297.24,577.359]]},{"text":"一所房子里。这房子是九江警备司令部参谋长之妹贺淑租","box":[[90,592.609],[505.1801000000005,592.609],[505.1801000000005,608.5590000000001],[90,608.5590000000001]]},{"text":"的","box":[[90,623.809],[105.95,623.809],[105.95,639.759],[90,639.759]]},{"text":"，","box":[[105.959,623.809],[121.909,623.809],[121.909,639.759],[105.959,639.759]]},{"text":"她下山后","box":[[117.6,623.809],[181.54850000000002,623.809],[181.54850000000002,639.759],[117.6,639.759]]},{"text":"，","box":[[181.68,623.809],[197.63,623.809],[197.63,639.759],[181.68,639.759]]},{"text":"由朱培德公馆的宋嘉晋无代价让给我们住的","box":[[193.32,623.809],[497.26100000000037,623.809],[497.26100000000037,639.759],[193.32,639.759]]},{"text":"，","box":[[497.28,623.809],[513.23,623.809],[513.23,639.759],[497.28,639.759]]},{"text":"直到十月初离开牯岭到上海。因此我对他的许多事是了解","box":[[90,655.009],[505.1801000000005,655.009],[505.1801000000005,670.9590000000001],[90,670.9590000000001]]},{"text":"的。兹就我所知道刘少奇的罪行交代如下：","box":[[90,686.209],[393.9410000000002,686.209],[393.9410000000002,702.159],[90,702.159]]},{"text":"一","box":[[122.04,717.409],[137.99,717.409],[137.99,733.359],[122.04,733.359]]},{"text":"、","box":[[138,717.409],[153.95,717.409],[153.95,733.359],[138,733.359]]},{"text":"受资本家贿赂","box":[[153.72,717.409],[249.70760000000004,717.409],[249.70760000000004,733.359],[153.72,733.359]]},{"text":"，","box":[[249.72,717.409],[265.67,717.409],[265.67,733.359],[249.72,733.359]]},{"text":"出卖工人阶级利益","box":[[265.559,717.409],[393.4654000000002,717.409],[393.4654000000002,733.359],[265.559,733.359]]},{"text":"。","box":[[393.6,717.409],[409.55,717.409],[409.55,733.359],[393.6,733.359]]},{"text":"自一九二六年","box":[[409.32,717.409],[505.3076000000001,717.409],[505.3076000000001,733.359],[409.32,733.359]]},{"text":"十月下旬经市党部介绍汉口总商会请愿代表与刘少奇见面","box":[[90,748.609],[505.1801000000005,748.609],[505.1801000000005,764.5590000000001],[90,764.5590000000001]]}]}