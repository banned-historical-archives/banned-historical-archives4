],[473.26910000000004,436.609],[473.26910000000004,452.55899999999997],[425.28,452.55899999999997]]},{"text":"也","box":[[473.28,436.609],[489.22999999999996,436.609],[489.22999999999996,452.55899999999997],[473.28,452.55899999999997]]},{"text":"有","box":[[489.24,436.609],[505.19,436.609],[505.19,452.55899999999997],[489.24,452.55899999999997]]},{"text":"了","box":[[90,467.809],[105.95,467.809],[105.95,483.759],[90,483.759]]},{"text":"叶群","box":[[105.959,467.809],[137.8684,467.809],[137.8684,483.759],[105.959,483.759]]},{"text":"，","box":[[138,467.809],[153.95,467.809],[153.95,483.759],[138,483.759]]},{"text":"但她","box":[[149.64,467.809],[181.5494,467.809],[181.5494,483.759],[149.64,483.759]]},{"text":"还是","box":[[181.68,467.809],[213.5894,467.809],[213.5894,483.759],[181.68,483.759]]},{"text":"害怕林彪牛脾气一来","box":[[213.6,467.809],[357.58610000000016,467.809],[357.58610000000016,483.759],[213.6,483.759]]},{"text":"，","box":[[357.6,467.809],[373.55,467.809],[373.55,483.759],[357.6,483.759]]},{"text":"把叶群的大名划掉","box":[[369.24,467.809],[497.1464000000002,467.809],[497.1464000000002,483.759],[369.24,483.759]]},{"text":"。","box":[[497.28,467.809],[513.23,467.809],[513.23,483.759],[497.28,483.759]]},{"text":"但是","box":[[90,499.009],[121.9094,499.009],[121.9094,514.9590000000001],[90,514.9590000000001]]},{"text":"几天前","box":[[122.04,499.009],[170.02910000000003,499.009],[170.02910000000003,514.9590000000001],[122.04,514.9590000000001]]},{"text":"，","box":[[170.04,499.009],[185.98999999999998,499.009],[185.98999999999998,514.9590000000001],[170.04,514.9590000000001]]},{"text":"叶群与林彪吵了架","box":[[185.759,499.009],[313.6654000000001,499.009],[313.6654000000001,514.9590000000001],[185.759,514.9590000000001]]},{"text":"，","box":[[313.8,499.009],[329.75,499.009],[329.75,514.9590000000001],[313.8,514.9590000000001]]},{"text":"几天来","box":[[329.52,499.009],[377.50910000000005,499.009],[377.50910000000005,514.9590000000001],[329.52,514.9590000000001]]},{"text":"，","box":[[377.52,499.009],