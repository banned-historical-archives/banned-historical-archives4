59,608.5590000000001]]},{"text":"。","box":[[345.72,592.609],[361.67,592.609],[361.67,608.5590000000001],[345.72,608.5590000000001]]},{"text":"我和叶群商量过","box":[[361.44,592.609],[473.38700000000017,592.609],[473.38700000000017,608.5590000000001],[361.44,608.5590000000001]]},{"text":"，","box":[[473.52,592.609],[489.46999999999997,592.609],[489.46999999999997,608.5590000000001],[473.52,608.5590000000001]]},{"text":"准","box":[[489.24,592.609],[505.19,592.609],[505.19,608.5590000000001],[489.24,608.5590000000001]]},{"text":"备讲六个题目","box":[[90,623.809],[185.98760000000001,623.809],[185.98760000000001,639.759],[90,639.759]]},{"text":"：","box":[[186,623.809],[201.95,623.809],[201.95,639.759],[186,639.759]]},{"text":"第一","box":[[201.84,623.809],[233.7494,623.809],[233.7494,639.759],[201.84,639.759]]},{"text":"，","box":[[233.759,623.809],[249.70899999999997,623.809],[249.70899999999997,639.759],[233.759,639.759]]},{"text":"讲讲文化大革命的意义","box":[[249.6,623.809],[409.5455000000002,623.809],[409.5455000000002,639.759],[249.6,639.759]]},{"text":"；","box":[[409.68,623.809],[425.63,623.809],[425.63,639.759],[409.68,639.759]]},{"text":"第二","box":[[425.4,623.809],[457.3094,623.809],[457.3094,639.759],[425.4,639.759]]},{"text":"，","box":[[457.44,623.809],[473.39,623.809],[473.39,639.759],[457.44,639.759]]},{"text":"讲讲","box":[[473.28,623.809],[505.1894,623.809],[505.1894,639.759],[473.28,639.759]]},{"text":"文化大革命的胜利","box":[[90,655.009],[217.90640000000002,655.009],[217.90640000000002,670.9590000000001],[90,670.9590000000001]]},{"text":"；","box":[[218.04,655.009],[233.98999999999998,655.009],[233.98999999999998,670.9590000000001],[218.04,670.9590000000001]]},{"text":"第三","box":[[233.64,655.009],[265.5494,655.009],[265.5494,670.9590000000001],[233.64,670.9590000000001]]},{"text":"，","box":[[265.68,655.009],[281.63,655.009],[281.63,670.9590000000001],[265.68,670.9590000000001]]},{"text":"讲讲文化大革命中的两条路线斗","box":[[281.28,655.009],[505.18340000000023,655.009],[505.18340000000023,670.9590000000001],[281.28,670.9590000000001]]},{"text":"争","box":[[90,686.209],[105.95,686.209],[105.95,702.159],[90,702.159]]},{"text":"；","box":[[105.959,686.209],[121.909,686.209],[121.909,702.159],[105.959,702.159]]},{"text":"第四","box":[[121.8,686.209],[153.7094,686.209],[153.7094,702.159],[121.8,702.159]]},{"text":"，","box":[[153.84,686.209],[169.79,686.209],[169.79,702.159],[153.84,702.159]]},{"text":"讲讲毛主席对马列主义的发展","box":[[169.68,686.209],[377.62400000000014,686.209],[377.62400000000014,702.159],[169.68,702.159]]},{"text":"；","box":[[377.64,686.209],[393.59,686.209],[393.59,702.159],[377.64,702.159]]},{"text":"第五","box":[[393.48,686.209],[425.3894,686.209],[425.3894,702.159],[393.48,702.159]]},{"text":"，","box":[[425.4,686.209],[441.34999999999997,686.209],[441.34999999999997,702.159],[425.4,702.159]]},{"text":"讲讲中国","box":[[441.24,686.209],[505.1885000000001,686.209],[505.1885000000001,702.159],[441.24,702.159]]},{"text":"革命在世界革命上的作用","box":[[90,717.409],[265.90490000000005,717.409],[265.90490000000005,733.359],[90,733.359]]},{"text":"；","box":[[266.04,717.409],[281.99,717.409],[281.99,733.359],[266.04,733.359]]},{"text":"第六","box":[[281.759,717.409],[313.6684,717.409],[313.6684,733.359],[281.759,733.359]]},{"text":"，","box":[[313.8,717.409],[329.75,717.409],[329.75,733.359],[313.8,733.359]]},{"text":"讲讲今后怎么做","box":[[329.52,717.409],[441.4670000000001,717.409],[441.4670000000001,733.359],[329.52,733.359]]},{"text":"。","box":[[441.48,717.409],[457.43,717.409],[457.43,733.359],[441.48,733.359]]},{"text":"这六个","box":[[457.32,717.409],[505.30910000000006,717.409],[505.30910000000006,733.359],[457.32,733.359]]},{"text":"题目","box":[[90,748.609],[121.9094,748.609],[121.9094,764.5590000000001],[90,764.5590000000001]]},{"text":"，","box":[[122.04,748.609],[137.99,748.609],[137.99,764.5590000000001],[122.04,764.5590000000001]]},{"text":"我都准备信口讲","box":[[137.759,748.609],[249.70600000000002,748.609],[249.70600000000002,764.5590000000001],[137.759,764.5590000000001]]},{"text":"，","box":[[249.84,748.609],[265.79,748.609],[265.79,764.5590000000001],[249.84,764.5590000000001]]},{"text":"不能念现成稿子","box":[[265.68,748.609],[377.6270000000002,748.609],[377.6270000000002,764.5590000000001],[265.68,764.5590000000001]]},{"text":"。","box":[[377.64,748.609],[393.59,748.609],[393.59,764.5590000000001],[377.64,764.5590000000001]]},{"text":"但信口讲","box":[[393.48,748.609],[457.4285000000001,748.609],[457.4285000000001,764.5590000000001],[393.48,764.5590000000001]]},{"text":"，","box":[[457.44,748.609],[473.39,748.609],[473.39,764.5590000000001],[457.44,764.5590000000001]]},{"text":"也还","box":[[473.28,748.609],[505.1894,748.609],[505.1894,764.5590000000001],[473.28,764.5590000000001]]}]}