390.159]]},{"text":"劝他退出","box":[[201.72,374.209],[265.6685,374.209],[265.6685,390.159],[201.72,390.159]]},{"text":"。","box":[[265.8,374.209],[281.75,374.209],[281.75,390.159],[265.8,390.159]]},{"text":"极少数的人","box":[[281.52,374.209],[361.4279000000001,374.209],[361.4279000000001,390.159],[281.52,390.159]]},{"text":"，","box":[[361.559,374.209],[377.509,374.209],[377.509,390.159],[361.559,390.159]]},{"text":"可能要采取纪律的","box":[[377.28,374.209],[505.18640000000016,374.209],[505.18640000000016,390.159],[377.28,390.159]]},{"text":"处分了","box":[[90,405.409],[137.9891,405.409],[137.9891,421.359],[90,421.359]]},{"text":"，","box":[[138,405.409],[153.95,405.409],[153.95,421.359],[138,421.359]]},{"text":"党章规定了的","box":[[153.72,405.409],[249.70760000000004,405.409],[249.70760000000004,421.359],[153.72,421.359]]},{"text":"，","box":[[249.72,405.409],[265.67,405.409],[265.67,421.359],[249.72,421.359]]},{"text":"是不是呀？还要经过支部大会","box":[[265.559,405.409],[473.50300000000027,405.409],[473.50300000000027,421.359],[265.559,421.359]]},{"text":"，","box":[[473.52,405.409],[489.46999999999997,405.409],[489.46999999999997,421.359],[473.52,421.359]]},{"text":"上","box":[[489.359,405.409],[505.30899999999997,405.409],[505.30899999999997,421.359],[489.359,421.359]]},{"text":"级批准。总而言之，要采取谨慎的方法。要做，一定要做","box":[[90,436.609],[489.93800000000033,436.609],[489.93800000000033,452.55899999999997],[90,452.55899999999997]]},{"text":"，","box":[[489.959,436.609],[505.909,436.609],[505.909,452.55899999999997],[489.959,452.55899999999997]]},{"text":"但是要采取谨慎的方法","box":[[90,467.809],[249.94550000000004,467.809],[249.94550000000004,483.759],[90,483.759]]},{"text":"。","box":[[249.959,467.809],[265.909,467.809],[265.909,483.759],[249.959,483.759]]},{"text":"”","box":[[258,467.809],[273.95,467.809],[273.95,483.759],[258,483.759]]},{"text":"“","box":[[122.04,499.009],[137.99,499.009],[137.99,514.9590000000001],[122.04,514.9590000000001]]},{"text":"团结起来，为了一个目标，就是巩固无产阶级专政","box":[[138,499.009],[489.93950000000024,499.009],[489.93950000000024,514.9590000000001],[138,514.9590000000001]]},{"text":"，","box":[[489.959,499.009],[505.909,499.009],[505.909,514.9590000000001],[489.959,514.9590000000001]]},{"text":"要落实到每个工厂、农村、机关、学校","box":[[90,530.209],[361.9019000000001,530.209],[361.9019000000001,546.159],[90,546.159]]},{"text":"。","box":[[362.04,530.209],[377.99,530.209],[377.99,546.159],[362.04,546.159]]},{"text":"”","box":[[369.959,530.209],[385.909,530.209],[385.909,546.159],[369.959,546.159]]},{"text":"“","box":[[122.04,561.409],[137.99,561.409],[137.99,577.359],[122.04,577.359]]},{"text":"此外","box":[[138,561.409],[169.9094,561.409],[169.9094,577.359],[138,577.359]]},{"text":"，","box":[[170.04,561.409],[185.98999999999998,561.409],[185.98999999999998,577.359],[170.04,577.359]]},{"text":"过去讲过的了","box":[[183.12,561.409],[279.1076,561.409],[279.1076,577.359],[183.12,577.359]]},{"text":"，","box":[[279.12,561.409],[295.07,561.409],[295.07,577.359],[279.12,577.359]]},{"text":"就是要准备打仗","box":[[292.2,561.409],[404.14700000000016,561.409],[404.14700000000016,577.359],[292.2,577.359]]},{"text":"。","box":[[404.159,561.409],[420.109,561.409],[420.109,577.359],[404.159,577.359]]},{"text":"”","box":[[412.2,561.409],[428.15,561.409],[428.15,577.359],[412.2,577.359]]},{"text":"“","box":[[417.359,561.409],[433.30899999999997,561.409],[433.30899999999997,577.359],[417.359,577.359]]},{"text":"人家打来","box":[[433.32,561.409],[497.2685000000001,561.409],[497.2685000000001,577.359],[433.32,577.359]]},{"text":"，","box":[[497.28,561.409],[513.23,561.409],[513.23,577.359],[497.28,577.359]]},{"text":"我们不打出去","box":[[90,592.609],[185.98760000000001,592.609],[185.98760000000001,608.5590000000001],[90,608.5590000000001]]},{"text":"。","box":[[186,592.609],[201.95,592.609],[201.95,608.5590000000001],[186,608.5590000000001]]},{"text":"我们是不打出去的","box":[[201.72,592.609],[329.62640000000005,592.609],[329.62640000000005,608.5590000000001],[201.72,608.5590000000001]]},{"text":"。","box":[[329.759,592.609],[345.709,592.609]