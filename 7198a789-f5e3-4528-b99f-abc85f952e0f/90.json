391.08,686.209],[407.03,686.209],[407.03,702.159],[391.08,702.159]]},{"text":"“","box":[[401.4,686.209],[417.34999999999997,686.209],[417.34999999999997,702.159],[401.4,702.159]]},{"text":"介绍一个人","box":[[417.24,686.209],[497.1479000000001,686.209],[497.1479000000001,702.159],[417.24,702.159]]},{"text":"，","box":[[497.28,686.209],[513.23,686.209],[513.23,702.159],[497.28,702.159]]},{"text":"此人叫王洪文，站起来看一看","box":[[90,717.409],[297.9440000000001,717.409],[297.9440000000001,733.359],[90,733.359]]},{"text":"。","box":[[297.959,717.409],[313.909,717.409],[313.909,733.359],[297.959,733.359]]},{"text":"”","box":[[306,717.409],[321.95,717.409],[321.95,733.359],[306,733.359]]},{"text":"王洪文站了起来。","box":[[122.04,748.609],[249.94640000000004,748.609],[249.94640000000004,764.5590000000001],[122.04,764.5590000000001]]}]}