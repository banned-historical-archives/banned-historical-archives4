‘","box":[[449.4,155.80899999999997],[465.34999999999997,155.80899999999997],[465.34999999999997,171.75899999999996],[449.4,171.75899999999996]]},{"text":"九大","box":[[465.24,155.80899999999997],[497.1494,155.80899999999997],[497.1494,171.75899999999996],[465.24,171.75899999999996]]},{"text":"’","box":[[497.28,155.80899999999997],[513.23,155.80899999999997],[513.23,171.75899999999996],[497.28,171.75899999999996]]},{"text":"的精神团结起来","box":[[90,187.00900000000001],[201.947,187.00900000000001],[201.947,202.959],[90,202.959]]},{"text":"，","box":[[201.959,187.00900000000001],[217.909,187.00900000000001],[217.909,202.959],[201.959,202.959]]},{"text":"陈伯达的发言违背了","box":[[215.759,187.00900000000001],[359.74510000000015,187.00900000000001],[359.74510000000015,202.959],[215.759,202.959]]},{"text":"‘","box":[[357.6,187.00900000000001],[373.55,187.00900000000001],[373.55,202.959],[357.6,202.959]]},{"text":"九大","box":[[373.68,187.00900000000001],[405.5894,187.00900000000001],[405.5894,202.959],[373.68,202.959]]},{"text":"’","box":[[405.6,187.00900000000001],[421.55,187.00900000000001],[421.55,202.959],[405.6,202.959]]},{"text":"方针","box":[[419.4,187.00900000000001],[451.3094,187.00900000000001],[451.3094,202.959],[419.4,202.959]]},{"text":"。","box":[[451.44,187.00900000000001],[467.39,187.00900000000001],[467.39,202.959],[451.44,202.959]]},{"text":"......","box":[[465.24,187.00900000000001],[497.1494,187.00900000000001],[497.1494,202.959],[465.24,202.959]]},{"text":"”","box":[[497.28,187.00900000000001],[513.23,187.00900000000001],[513.23,202.959],[497.28,202.959]]},{"text":"下面有人不理解","box":[[122.04,218.20899999999995],[233.98700000000002,218.20899999999995],[233.98700000000002,234.15899999999993],[122.04,234.15899999999993]]},{"text":"，","box":[[234,218.20899999999995],[249.95,218.20899999999995],[249.95,234.15899999999993],[234,234.15899999999993]]},{"text":"小声议论着","box":[[249.84,218.20899999999995],[329.7479000000001,218.20899999999995],[329.7479000000001,234.15899999999993],[249.84,234.15899999999993]]},{"text":"。","box":[[329.759,218.20899999999995],[345.709,218.20899999999995],[345.709,234.15899999999993],[329.759,234.15899999999993]]},{"text":"毛泽东说","box":[[345.6,218.20899999999995],[409.5485000000001,218.20899999999995],[409.5485000000001,234.15899999999993],[345.6,234.15899999999993]]},{"text":"：","box":[[409.68,218.20899999999995],[425.63,218.20899999999995],[425.63,234.15899999999993],[409.68,234.15899999999993]]},{"text":"“","box":[[425.28,218.20899999999995],[441.22999999999996,218.20899999999995],[441.22999999999996,234.15899999999993],[425.28,234.15899999999993]]},{"text":"你们继续","box":[[441.24,218.20899999999995],[505.1885000000001,218.20899999999995],[505.1885000000001,234.15899999999993],[441.24,234.15899999999993]]},{"text":"这样我就下山","box":[[90,249.409],[185.98760000000001,249.409],[185.98760000000001,265.359],[90,265.359]]},{"text":"，","box":[[186,249.409],[201.95,249.409],[201.95,265.359],[186,265.359]]},{"text":"让你们搞","box":[[201.72,249.409],[265.6685,249.409],[265.6685,265.359],[201.72,265.359]]},{"text":"。","box":[[265.8,249.409],[281.75,249.409],[281.75,265.359],[265.8,265.359]]},{"text":"设国家主席问题不要再提了","box":[[281.52,249.409],[473.5046000000002,249.409],[473.5046000000002,265.359],[281.52,265.359]]},{"text":"，","box":[[473.52,249.409],[489.46999999999997,249.409],[489.46999999999997,265.359],[473.52,265.359]]},{"text":"要","box":[[489.24,249.409],[505.19,249.409],[505.19,265.359],[489.24,265.359]]},{"text":"我早点死","box":[[90,280.60900000000004],[153.9485,280.60900000000004],[153.9485,296.559],[90,296.559]]},{"text":"，","box":[[153.959,280.60900000000004],[169.909,280.60900000000004],[169.909,296.559],[153.959,296.559]]},{"text":"就让我当国家主席","box":[[169.8,280.60900000000004],[297.70640000000003,280.60900000000004],[297.70640000000003,296.559],[169.8,296.559]]},{"text":"，","box":[[297.84,280.60900000000004],[313.78999999999996,280.60900000000004],[313.78999999999996,296.559],[297.84,296.559]]},{"text":"谁坚持设","box":[[313.68,280.60900000000004],[377.6285000000001,280.60900000000004],[377.6285000000001,296.559],[313.68,296.559]]},{"text":"，","box":[[377.64,280.60900000000004],[393.59,280.60900000000004],[393.59,296.559],[377.64,296.559]]},{"text":"谁就去当","box":[[393.48,280.60900000000004],[457.4285000000001,280.60900000000004],[457.4285000000001,296.559],[393.48,296.559]]},{"text":"，","box":[[457.44,280.60900000000004],[473.39,280.60900000000004],[473.39,296.559],[457.44,296.559]]},{"text":"反正","box":[[473.28,280.60900000000004],[505.1894,280.60900000000004],[505.1894,296.559],[473.28,296.559]]},{"text":"我不当。","box":[[90,311.8090000000001],[153.9485,311.8090000000001],[153.9485,327.75900000000007],[90,327.75900000000007]]},{"text":"”","box":[[153.959,311.8090000000001],[169.909,311.8090000000001],[169.909,327.75900000000007],[153.959,327.75900000000007]]},{"text":"说到这里","box":[[122.04,343.009],[185.98850000000002,343.009],[185.98850000000002,358.959],[122.04,358.959]]},{"text":"，","box":[[186,343.009],[201.95,343.009],[201.95,358.959],[186,358.959]]},{"text":"毛泽东转向林彪说","box":[[201.72,343.009],[329.62640000000005,343.009],[329.62640000000005,358.959],[201.72,358.959]]},{"text":"：","box":[[329.759,343.009],[345.709,343.009],[345.709,358.959],[329.759,358.959]]},{"text":"“","box":[[345.24,343.009],[361.19,343.009],[361.19,358.959],[345.24,358.959]]},{"text":"我劝你也不要当国家","box":[[361.32,343.009],[505.30610000000013,343.009],[505.30610000000013,358.959],[361.32,358.959]]},{"text":"主席。","box":[[90,374.209],[137.9891,374.209],[137.9891,390.159],[90,390.159]]},{"text":"”","box":[[138,374.209],[153.95,374.209],[153.95,390.159],[138,390.159]]},{"text":"毛泽东的话里透出一种凄凉。这种话是他从未说过的","box":[[122.04,405.409],[489.9389000000003,405.409],[489.9389000000003,421.359],[122.04,421.359]]},{"text":"。","box":[[489.959,405.409],[505.909,405.409],[505.909,421.359],[489.959,421.359]]},{"text":"1959","box":[[90,436.609],[121.97425000000001,436.609],[121.97425000000001,452.55899999999997],[90,452.55899999999997]]},{"text":"年批判彭德怀的那种高屋建瓴","box":[[122.04,436.609],[329.9840000000001,436.609],[329.9840000000001,452.55899999999997],[122.04,452.55899999999997]]},{"text":"，","box":[[330,436.609],[345.95,436.609],[345.95,452.55899999999997],[330,452.55899999999997]]},{"text":"那种嬉笑怒骂的潇洒已","box":[[345.24,436.609],[505.1855000000002,436.609],[505.1855000000002,452.55899999999997],[345.24,452.55899999999997]]},{"text":"经从他身上永远地消失了。","box":[[90,467.809],[281.98460000000006,467.809],[281.98460000000006,483.759],[90,483.759]]},{"text":"他说：","box":[[122.04,499.009],[170.02910000000003,499.009],[170.02910000000003,514.9590000000001],[122.04,514.9590000000001]]},{"text":"“","box":[[170.04,499.009],[185.98999999999998,499.009],[185.98999999999998,514.9590000000001],[170.04,514.9590000000001]]},{"text":"本来我们这个会议，方针嘛，是开好的会议","box":[[186,499.009],[489.94100000000026,499.009],[489.94100000000026,514.9590000000001],[186,514.9590000000001]]},{"text":"、","box":[[489.959,499.009],[505.909,499.009],[505.909,514.9590000000001],[489.959,514.9590000000001]]},{"text":"团结胜利的会议呢","box":[[90,530.209],[217.90640000000002,530.209],[217.90640000000002,546.159],[90,546.159]]},{"text":"，","box":[[218.04,530.209],[233.98999999999998,530.209],[233.98999999999998,546.159],[218.04,546.159]]},{"text":"还是开不好的会议","box":[[233.64,530.209],[361.54640000000006,530.209],[361.54640000000006,546.159],[233.64,546.159]]},{"text":"，","box":[[361.68,530.209],[377.63,530.209],[377.63,546.159],[361.68,546.159]]},{"text":"变成了分裂失败的","box":[[377.28,530.209],[505.18640000000016,530.209],[505.18640000000016,546.159],[377.28,546.159]]},{"text":"会议呢？还是要争取开一个团结的会","box":[[90,561.409],[345.9425000000001,561.409],[345.9425000000001,577.359],[90,577.359]]},{"text":"，","box":[[345.959,561.409],[361.909,561.409],[361.909,577.359],[345.959,577.359]]},{"text":"争取更大的胜利","box":[[361.68,561.409],[473.6270000000002,561.409],[473.6270000000002,577.359],[361.68,577.359]]},{"text":"。","box":[[473.64,561.409],[489.59,561.409],[489.59,577.359],[473.64,577.359]]},{"text":"如","box":[[489.24,561.409],[505.19,561.409],[505.19,577.359],[489.24,577.359]]},{"text":"果开不成","box":[[90,592.609],[153.9485,592.609],[153.9485,608.5590000000001],[90,608.5590000000001]]},{"text":"，","box":[[153.959,592.609],[169.909,592.609],[169.909,608.5590000000001],[153.959,608.5590000000001]]},{"text":"仍要分裂","box":[[169.8,592.609],[233.74850000000004,592.609],[233.74850000000004,608.5590000000001],[169.8,608.5590000000001]]},{"text":"，","box":[[233.759,592.609],[249.70899999999997,592.609],[249.70899999999997,608.5590000000001],[233.759,608.5590000000001]]},{"text":"那也没有什么了不起","box":[[249.48,592.609],[393.4661000000001,592.609],[393.4661000000001,608.5590000000001],[249.48,608.5590000000001]]},{"text":"。","box":[[393.48,592.609],[409.43,592.609],[409.43,608.5590000000001],[393.48,608.5590000000001]]},{"text":"过去党是经过","box":[[409.32,592.609],[505.3076000000001,592.609],[505.3076000000001,608.5590000000001],[409.32,608.5590000000001]]},{"text":"挫折的","box":[[90,623.809],[137.9891,623.809],[137.9891,639.759],[90,639.759]]},{"text":"，","box":[[138,623.809],[153.95,623.809],[153.95,639.759],[138,639.759]]},{"text":"千锤百炼","box":[[153.84,623.809],[217.78850000000003,623.809],[217.78850000000003,639.759],[153.84,639.759]]},{"text":"。","box":[[217.8,623.809],[233.75,623.809],[233.75,639.759],[217.8,639.759]]},{"text":"如果搞不好的话","box":[[233.64,623.809],[345.58700000000005,623.809],[345.58700000000005,639.759],[233.64,639.759]]},{"text":"，","box":[[345.6,623.809],[361.55,623.809],[361.55,639.759],[345.6,639.759]]},{"text":"那我就下山","box":[[361.44,623.809],[441.3479000000001,623.809],[441.3479000000001,639.759],[361.44,639.759]]},{"text":"，","box":[[441.48,623.809],[457.43,623.809],[457.43,639.759],[441.48,639.759]]},{"text":"你们去","box":[[457.32,623.809],[505.30910000000006,623.809],[505.30910000000006,639.759],[457.32,639.759]]},{"text":"开","box":[[90,655.009],[105.95,655.009],[105.95,670.9590000000001],[90,670.9590000000001]]},{"text":"，","box":[[105.959,655.009],[121.909,655.009],[121.909,670.9590000000001],[105.959,670.9590000000001]]},{"text":"开完了会我再上山","box":[[121.8,655.009],[249.70640000000003,655.009],[249.70640000000003,670.9590000000001],[121.8,670.9590000000001]]},{"text":"，","box":[[249.84,655.009],[265.79,655.009],[265.79,670.9590000000001],[249.84,670.9590000000001]]},{"text":"就不下去了","box":[[265.68,655.009],[345.5879000000001,655.009],[345.5879000000001,670.9590000000001],[265.68,670.9590000000001]]},{"text":"。","box":[[345.6,655.009],[361.55,655.009],[361.55,670.9590000000001],[345.6,670.9590000000001]]},{"text":"再不然","box":[[361.44,655.009],[409.42910000000006,655.009],[409.42910000000006,670.9590000000001],[361.44,670.9590000000001]]},{"text":"，","box":[[409.44,655.009],[425.39,655.009],[425.39,670.9590000000001],[409.44,670.9590000000001]]},{"text":"我就辞掉党","box":[[425.28,655.009],[505.18790000000007,655.009],[505.18790000000007,670.9590000000001],[425.28,670.9590000000001]]},{"text":"中央委员会的主席。","box":[[90,686.209],[233.98610000000005,686.209],[233.98610000000005,702.159],[90,702.159]]},{"text":"”","box":[[234,686.209],[249.95,686.209],[249.95,702.159],[234,702.159]]},{"text":"他","box":[[122.04,717.409],[137.99,717.409],[137.99,733.359],[122.04,733.359]]},{"text":"说","box":[[138,717.409],[153.95,717.409],[153.95,733.359],[138,733.359]]},{"text":"陈伯达","box":[[153.959,717.409],[201.9481,717.409],[201.9481,733.359],[153.959,733.359]]},{"text":"：","box":[[201.959,717.409],[217.909,717.409],[217.909,733.359],[201.959,733.359]]},{"text":"“","box":[[217.8,717.409],[233.75,717.409],[233.75,733.359],[217.8,733.359]]},{"text":"人家搞阴谋","box":[[233.759,717.409],[313.66690000000006,717.409],[313.66690000000006,733.359],[233.759,733.359]]},{"text":"，","box":[[313.8,717.409],[329.75,717.409],[329.75,733.359],[313.8,733.359]]},{"text":"就你不搞阴谋？","box":[[329.52,717.409],[441.4670000000001,717.409],[441.4670000000001,733.359],[329.52,733.359]]},{"text":"”","box":[[441.6,717.409],[457.55,717.409],[457.55,733.359],[441.6,733.359]]},{"te