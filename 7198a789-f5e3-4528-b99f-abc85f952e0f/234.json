4.15899999999993]]},{"text":"，","box":[[497.28,218.20899999999995],[513.23,218.20899999999995],[513.23,234.15899999999993],[497.28,234.15899999999993]]},{"text":"有不少经验","box":[[90,249.409],[169.90789999999998,249.409],[169.90789999999998,265.359],[90,265.359]]},{"text":"。","box":[[170.04,249.409],[185.98999999999998,249.409],[185.98999999999998,265.359],[170.04,265.359]]},{"text":"”","box":[[177.959,249.409],[193.909,249.409],[193.909,265.359],[177.959,265.359]]},{"text":"康生接着讲","box":[[122.04,280.60900000000004],[203.26820000000004,280.60900000000004],[203.26820000000004,296.559],[122.04,296.559]]},{"text":"：","box":[[203.52,280.60900000000004],[219.47,280.60900000000004],[219.47,296.559],[203.52,296.559]]},{"text":"“","box":[[211.92,280.60900000000004],[227.86999999999998,280.60900000000004],[227.86999999999998,296.559],[211.92,296.559]]},{"text":"内蒙古的敌人很多，有伪满洲国的，有","box":[[228.24,280.60900000000004],[505.3028,280.60900000000004],[505.3028,296.559],[228.24,296.559]]},{"text":"伪蒙疆的，有傅作义的，还有乌兰夫的","box":[[90,311.8090000000001],[361.9019000000001,311.8090000000001],[361.9019000000001,327.75900000000007],[90,327.75900000000007]]},{"text":"。","box":[[362.04,311.8090000000001],[377.99,311.8090000000001],[377.99,327.75900000000007],[362.04,327.75900000000007]]},{"text":"”","box":[[369.959,311.8090000000001],[385.909,311.8090000000001],[385.909,327.75900000000007],[369.959,327.75900000000007]]},{"text":"周恩来对滕海清说","box":[[122.04,343.009],[252.22685,343.009],[252.22685,358.959],[122.04,358.959]]},{"text":"：","box":[[252.48,343.009],[268.43,343.009],[268.43,358.959],[252.48,358.959]]},{"text":"“","box":[[260.88,343.009],[276.83,343.009],[276.83,358.959],[260.88,358.959]]},{"text":"解决内蒙古问题需要一个过程，","box":[[277.2,343.009],[505.30415000000005,343.009],[505.30415000000005,358.959],[277.2,358.959]]},{"text":"要花时间","box":[[90,374.209],[153.9485,374.209],[153.9485,390.159],[90,390.159]]},{"text":"，","box":[[153.959,374.209],[169.909,374.209],[169.909,390.159],[153.959,390.159]]},{"text":"你们的步子太快了","box":[[169.8,374.209],[297.70640000000003,374.209],[297.70640000000003,390.159],[169.8,390.159]]},{"text":"，","box":[[297.72,374.209],[313.67,374.209],[313.67,390.159],[297.72,390.159]]},{"text":"中央从来没有督促你们","box":[[313.559,374.209],[473.50450000000023,374.209],[473.50450000000023,390.159],[313.559,390.159]]},{"text":"，","box":[[473.52,374.209],[489.46999999999997,374.209],[489.46999999999997,390.159],[473.52,390.159]]},{"text":"你","box":[[489.359,374.209],[505.30899999999997,374.209],[505.30899999999997,390.159],[489.359,390.159]]},{"text":"们不要急嘛","box":[[90,405.409],[169.90789999999998,405.409],[169.90789999999998,421.359],[90,421.359]]},{"text":"。","box":[[170.04,405.409],[185.98999999999998,405.409],[185.98999999999998,421.359],[170.04,421.359]]},{"text":"清理阶级队伍要注意掌握政策","box":[[181.68,405.409],[389.62400000000014,405.409],[389.62400000000014,421.359],[181.68,421.359]]},{"text":"，","box":[[389.64,405.409],[405.59,405.409],[405.59,421.359],[389.64,421.359]]},{"text":"要扩大教育面","box":[[401.28,405.409],[497.2676000000001,405.409],[497.2676000000001,421.359],[401.28,421.359]]},{"text":"，","box":[[497.28,405.409],[513.23,405.409],[513.23,421.359],[497.28,421.359]]},{"text":"要缩小打击面","box":[[90,436.609],[185.98760000000001,436.609],[185.98760000000001,452.55899999999997],[90,452.55899999999997]]},{"text":"。","box":[[186,436.609],[201.95,436.609],[201.95,452.55899999999997],[186,452.55899999999997]]},{"text":"”","box":[[194.04,436.609],[209.98999999999998,436.609],[209.98999999999998,452.55899999999997],[194.04,452.55899999999997]]},{"text":"江青把话题拉回到革委会增加新人的问题上，说","box":[[122.04,467.809],[464.3809999999999,467.809],[464.3809999999999,483.759],[122.04,483.759]]},{"text":"：","box":[[464.64,467.809],[480.59,467.809],[480.59,483.759],[464.64,483.759]]},{"text":"“","box":[[472.919,467.809],[488.86899999999997,467.809],[488.86899999999997,483.759],[472.919,483.759]]},{"text":"革","box":[[489.24,467.809],[505.19,467.809],[505.19,483.759],[489.24,483.759]]},{"text":