"box":[[329.52,686.209],[441.4670000000001,686.209],[441.4670000000001,702.159],[329.52,702.159]]},{"text":"，","box":[[441.6,686.209],[457.55,686.209],[457.55,702.159],[441.6,702.159]]},{"text":"也只能","box":[[457.32,686.209],[505.30910000000006,686.209],[505.30910000000006,702.159],[457.32,702.159]]},{"text":"干体力活","box":[[90,717.409],[153.9485,717.409],[153.9485,733.359],[90,733.359]]},{"text":"，","box":[[153.959,717.409],[169.909,717.409],[169.909,733.359],[153.959,733.359]]},{"text":"而不能从事专业研究和创作","box":[[169.68,717.409],[361.6646000000001,717.409],[361.6646000000001,733.359],[169.68,733.359]]},{"text":"，","box":[[361.68,717.409],[377.63,717.409],[377.63,733.359],[361.68,733.359]]},{"text":"他们的大好年华","box":[[377.28,717.409],[489.2270000000001,717.409],[489.2270000000001,733.359],[377.28,733.359]]},{"text":"似","box":[[489.24,717.409],[505.19,717.409],[505.19,733.359],[489.24,733.359]]},{"text":"乎","box":[[90,748.609],[105.95,748.609],[105.95,764.5590000000001],[90,764.5590000000001]]},{"text":"就在繁重的体力劳动和单调的生活中消逝了。一般来说","box":[[105.959,748.609],[489.9376000000004,748.609],[489.9376000000004,764.5590000000001],[105.959,764.5590000000001]]},{"text":"，","box":[[489.959,748.609],[505.909,748.609],[505.909,764.5590000000001],[489.959,764.5590000000001]]}]}