,[137.759,733.359]]},{"text":"。","box":[[201.84,717.409],[217.79,717.409],[217.79,733.359],[201.84,733.359]]},{"text":"总理的指示很重要","box":[[217.68,717.409],[345.70670000000007,717.409],[345.70670000000007,733.359],[217.68,733.359]]},{"text":"：","box":[[345.72,717.409],[361.67,717.409],[361.67,733.359],[345.72,733.359]]},{"text":"第一","box":[[361.44,717.409],[393.3494,717.409],[393.3494,733.359],[361.44,733.359]]},{"text":"，","box":[[393.6,717.409],[409.55,717.409],[409.55,733.359],[393.6,733.359]]},{"text":"脑袋里不要有","box":[[409.32,717.409],[505.3076000000001,717.409],[505.3076000000001,733.359],[409.32,733.359]]},{"text":"框框","box":[[90,748.609],[121.9094,748.609],[121.9094,764.5590000000001],[90,764.5590000000001]]},{"text":"；","box":[[122.04,748.609],[137.99,748.609],[137.99,764.5590000000001],[122.04,764.5590000000001]]},{"text":"第二","box":[[137.759,748.609],[169.6684,748.609],[169.6684,764.5590000000001],[137.759,764.5590000000001]]},{"text":"，","box":[[169.8,748.609],[185.75,748.609],[185.75,764.5590000000001],[169.8,764.5590000000001]]},{"text":"要密切注意世界战略格局的发展变化","box":[[185.52,748.609],[441.4625000000003,748.609],[441.4625000000003,764.5590000000001],[185.52,764.5590000000001]]},{"text":"。","box":[[441.6,748.609],[457.55,748.609],[457.55,764.5590000000001],[441.6,764.5590000000001]]},{"text":"一次议","box":[[457.32,748.609],[505.30910000000006,748.609],[505.30910000000006,764.5590000000001],[457.32,764.5590000000001]]}]}