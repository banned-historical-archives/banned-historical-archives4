59],[313.8,702.159]]},{"text":"命都可以不要","box":[[329.52,686.209],[425.5076000000001,686.209],[425.5076000000001,702.159],[329.52,702.159]]},{"text":"，","box":[[425.52,686.209],[441.46999999999997,686.209],[441.46999999999997,702.159],[425.52,702.159]]},{"text":"冷点算什","box":[[441.24,686.209],[505.1885000000001,686.209],[505.1885000000001,702.159],[441.24,702.159]]},{"text":"么","box":[[90,717.409],[105.95,717.409],[105.95,733.359],[90,733.359]]},{"text":"？","box":[[105.959,717.409],[121.909,717.409],[121.909,733.359],[105.959,733.359]]},{"text":"可是看样子仗打不起来了","box":[[122.04,717.409],[297.9449,717.409],[297.9449,733.359],[122.04,733.359]]},{"text":"，","box":[[297.959,717.409],[313.909,717.409],[313.909,733.359],[297.959,733.359]]},{"text":"部队还冻在野外干什么","box":[[313.32,717.409],[473.2655000000002,717.409],[473.2655000000002,733.359],[313.32,733.359]]},{"text":"呢？","box":[[473.28,717.409],[505.1894,717.409],[505.1894,733.359],[473.28,733.359]]},{"text":"官兵们受不了","box":[[90,748.609],[185.98760000000001,748.609],[185.98760000000001,764.5590000000001],[90,764.5590000000001]]},{"text":"，","box":[[186,748.609],[201.95,748.609],[201.95,764.5590000000001],[186,764.5590000000001]]},{"text":"重型武器装备也受不了","box":[[201.6,748.609],[361.5455000000001,748.609],[361.5455000000001,764.5590000000001],[201.6,764.5590000000001]]},{"text":"。","box":[[361.68,748.609],[377.63,748.609],[377.63,764.5590000000001],[361.68,764.5590000000001]]},{"text":"上边好像把他们忘","box":[[377.28,748.609],[505.18640000000016,748.609],[505.18640000000016,764.5590000000001],[377.28,764.5590000000001]]}]}