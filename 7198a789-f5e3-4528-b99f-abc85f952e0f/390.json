265.8,639.759]]},{"text":"“","box":[[273.6,623.809],[289.55,623.809],[289.55,639.759],[273.6,639.759]]},{"text":"你们要那就要吧","box":[[289.559,623.809],[401.5060000000002,623.809],[401.5060000000002,639.759],[289.559,639.759]]},{"text":"。","box":[[401.52,623.809],[417.46999999999997,623.809],[417.46999999999997,639.759],[401.52,639.759]]},{"text":"不管怎么说","box":[[417.24,623.809],[497.1479000000001,623.809],[497.1479000000001,639.759],[417.24,639.759]]},{"text":"，","box":[[497.4,623.809],[513.35,623.809],[513.35,639.759],[497.4,639.759]]},{"text":"列上了责任在我身上，就这样吧","box":[[90,655.009],[313.90340000000003,655.009],[313.90340000000003,670.9590000000001],[90,670.9590000000001]]},{"text":"。","box":[[314.04,655.009],[329.99,655.009],[329.99,670.9590000000001],[314.04,670.9590000000001]]},{"text":"”","box":[[321.959,655.009],[337.909,655.009],[337.909,670.9590000000001],[321.959,670.9590000000001]]},{"text":"周恩来","box":[[122.04,686.209],[170.02910000000003,686.209],[170.02910000000003,702.159],[122.04,702.159]]},{"text":"","box":[[170.0291,686.209],[249.4471,686.209],[249.4471,686.209],[170.0291,686.209]]},{"text":"3","box":[[174,686.209],[181.975,686.209],[181.975,702.159],[174,702.159]]},{"text":"","box":[[181.975,686.209],[262.47500000000014,686.209],[262.47500000000014,686.209],[181.975,686.209]]},{"text":"人接着又到林彪家里去了","box":[[186,686.209],[361.9049000000001,686.209],[361.9049000000001,702.159],[186,702.159]]},{"text":"。","box":[[362.04,686.209],[377.99,686.209],[377.99,702.159],[362.04,702.159]]},{"text":"叶群将他们","box":[[377.28,686.209],[457.18790000000007,686.209],[457.18790000000007,702.159],[377.28,702.159]]},{"text":"","box":[[457.1879,686.209],[539.0298999999992,686.209],[539.0298999999992,686.209],[457.1879,686.209]]},{"text":"3","box":[[461.28,686.209],[469.255,686.209],[469.255,702.159],[461.28,702.159]]},{"text":"","box":[[469.255,686.209],[549.7549999999995,686.209],[549.7549999999995,686.209],[469.255,686.209]]},{"text":"人领","box":[[473.28,686.209],[505.1894,686.209],[505.1894,702.159],[473.28,702.159]]},{"text":"进林彪的会客室后，自动回避了。","box":[[90,717.409],[329.98310000000004,717.409],[329.98310000000004,733.359],[90,733.359]]},{"text":"周恩来对林彪说","box":[[122.04,748.609],[235.90730000000002,748.609],[235.90730000000002,764.5590000000001],[122.04,764.5590000000001]]},{"text":"：","box":[[236.159,748.609],[252.10899999999998,748.609],[252.10899999999998,764.5590000000001],[236.159,764.5590000000001]]},{"text":"“","box":[[244.44,748.609],[260.39,748.609],[260.39,764.5590000000001],[244.44,764.5590000000001]]},{"text":"关于叶群同志列入政治局候选人名","box":[[260.88,748.609],[505.30370000000005,748.609],[505.30370000000005,764.5590000000001],[260.88,764.5590000000001]]}]}