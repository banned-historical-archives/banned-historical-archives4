5899999999993],[282,234.15899999999993]]},{"text":"另外","box":[[297.6,218.20899999999995],[329.5094,218.20899999999995],[329.5094,234.15899999999993],[297.6,234.15899999999993]]},{"text":"，","box":[[329.64,218.20899999999995],[345.59,218.20899999999995],[345.59,234.15899999999993],[329.64,234.15899999999993]]},{"text":"因为我们是一起造反的","box":[[345.24,218.20899999999995],[505.1855000000002,218.20899999999995],[505.1855000000002,234.15899999999993],[345.24,234.15899999999993]]},{"text":"战友","box":[[90,249.409],[121.9094,249.409],[121.9094,265.359],[90,265.359]]},{"text":"，","box":[[122.04,249.409],[137.99,249.409],[137.99,265.359],[122.04,265.359]]},{"text":"如果有什么历史问题","box":[[137.759,249.409],[281.74510000000004,249.409],[281.74510000000004,265.359],[137.759,265.359]]},{"text":"，","box":[[281.759,249.409],[297.709,249.409],[297.709,265.359],[281.759,265.359]]},{"text":"清理阶级队伍","box":[[297.6,249.409],[393.5876000000001,249.409],[393.5876000000001,265.359],[297.6,265.359]]},{"text":"，","box":[[393.6,249.409],[409.55,249.409],[409.55,265.359],[393.6,265.359]]},{"text":"有什么要检查","box":[[409.32,249.409],[505.3076000000001,249.409],[505.3076000000001,265.359],[409.32,265.359]]},{"text":"就检查","box":[[90,280.60900000000004],[137.9891,280.60900000000004],[137.9891,296.559],[90,296.559]]},{"text":"。","box":[[138,280.60900000000004],[153.95,280.60900000000004],[153.95,296.559],[138,296.559]]},{"text":"如果检查你","box":[[153.6,280.60900000000004],[233.5079,280.60900000000004],[233.5079,296.559],[153.6,296.559]]},{"text":"，","box":[[233.64,280.60900000000004],[249.58999999999997,280.60900000000004],[249.58999999999997,296.559],[233.64,296.559]]},{"text":"抱什么态度？杨","box":[[249.24,280.60900000000004],[361.1870000000001,280.60900000000004],[361.1870000000001,296.559],[249.24,296.559]]},{"text":"保","box":[[361.32,280.60900000000004],[377.27,280.60900000000004],[377.27,296.559],[361.32,296.559]]},{"text":"华的报告中错在什","box":[[377.28,280.60900000000004],[505.18640000000016,280.60900000000004],[505.18640000000016,296.559],[377.28,296.559]]},{"text":"么地方？一个余金瑞","box":[[90,311.8090000000001],[233.98610000000005,311.8090000000001],[233.98610000000005,327.75900000000007],[90,327.75900000000007]]},{"text":"，","box":[[234,311.8090000000001],[249.95,311.8090000000001],[249.95,327.75900000000007],[234,327.75900000000007]]},{"text":"是一个干部","box":[[249.72,311.8090000000001],[329.62790000000007,311.8090000000001],[329.62790000000007,327.75900000000007],[249.72,327.75900000000007]]},{"text":"，","box":[[329.759,311.8090000000001],[345.709,311.8090000000001],[345.709,327.75900000000007],[329.759,327.75900000000007]]},{"text":"他的祖父","box":[[345.6,311.8090000000001],[409.5485000000001,311.8090000000001],[409.5485000000001,327.75900000000007],[345.6,327.75900000000007]]},{"text":"、","box":[[409.559,311.8090000000001],[425.509,311.8090000000001],[425.509,327.75900000000007],[409.559,327.75900000000007]]},{"text":"父亲均是反","box":[[425.28,311.8090000000001],[505.18790000000007,311.8090000000001],[505.18790000000007,327.75900000000007],[425.28,327.75900000000007]]},{"text":"革命","box":[[90,343.009],[121.9094,343.009],[121.9094,358.959],[90,358.959]]},{"text":"，","box":[[122.04,343.009],[137.99,343.009],[137.99,358.959],[122.04,358.959]]},{"text":"他是造反比较早的","box":[[137.759,343.009],[265.7857,343.009],[265.7857,358.959],[137.759,358.959]]},{"text":"，","box":[[265.8,343.009],[281.75,343.009],[281.75,358.959],[265.8,358.959]]},{"text":"和王效禹一起的","box":[[281.64,343.009],[393.5870000000001,343.009],[393.5870000000001,358.959],[281.64,358.959]]},{"text":"。","box":[[393.72,343.009],[409.67,343.009],[409.67,358.959],[393.72,358.959]]},{"text":"整党","box":[[409.559,343.009],[441.46840000000003,343.009],[441.46840000000003,358.959],[409.559,358.959]]},{"text":"、","box":[[441.6,343.009],[457.55,343.009],[457.55,358.959],[441.6,358.959]]},{"text":"建党时","box":[[457.32,343.009],[505.30910000000006,343.009],[505.30910000000006,358.959],[457.32,358.959]]},{"text":"碰到一个问题","box":[[90,374.209],[185.98760000000001,374.209],[185.98760000000001,390.159],[90,390.159]]},{"text":"：","box":[[186,374.209],[201.95,374.209],[201.95,390.159],[186,390.159]]},{"text":"他是否可以加入党？革委会大多数是不同意","box":[[201.24,374.209],[505.1810000000004,374.209],[505.1810000000004,390.159],[201.24,390.159]]},{"text":"的","box":[[90,405.409],[105.95,405.409],[105.95,421.359],[90,421.359]]},{"text":"，","box":[[105.959,405.409],[121.909,405.409],[121.909,421.359],[105.959,421.359]]},{"text":"当时杨","box":[[121.8,405.409],[169.78910000000002,405.409],[169.78910000000002,421.359],[121.8,421.359]]},{"text":"保","box":[[169.8,405.409],[185.75,405.409],[185.75,421.359],[169.8,421.359]]},{"text":"华还拿不定","box":[[185.759,405.409],[265.6669,405.409],[265.6669,421.359],[185.759,421.359]]},{"text":"。","box":[[265.8,405.409],[281.75,405.409],[281.75,421.359],[265.8,421.359]]},{"text":"王效禹平批评了他","box":[[281.52,405.409],[409.42640000000017,405.409],[409.42640000000017,421.359],[281.52,421.359]]},{"text":"，","box":[[409.559,405.409],[425.509,405.409],[425.509,421.359],[409.559,421.359]]},{"text":"说我可以介","box":[[425.28,405.409],[505.18790000000007,405.409],[505.18790000000007,421.359],[425.28,421.359]]},{"text":"绍","box":[[90,436.609],[105.95,436.609],[105.95,452.55899999999997],[90,452.55899999999997]]},{"text":"，","box":[[105.959,436.609],[121.909,436.609],[121.909,452.55899999999997],[105.959,452.55899999999997]]},{"text":"批评杨","box":[[121.8,436.609],[169.78910000000002,436.609],[169.78910000000002,452.55899999999997],[121.8,452.55899999999997]]},{"text":"保","box":[[169.8,436.609],[185.75,436.609],[185.75,452.55899999999997],[169.8,452.55899999999997]]},{"text":"华对造反派没有感情","box":[[185.759,436.609],[329.7451000000001,436.609],[329.7451000000001,452.55899999999997],[185.759,452.55899999999997]]},{"text":"，","box":[[329.759,436.609],[345.709,436.609],[345.709,452.55899999999997],[329.759,452.55899999999997]]},{"text":"说可以入党","box":[[345.6,436.609],[425.5079000000001,436.609],[425.5079000000001,452.55899999999997],[345.6,452.