899999999993],[90,234.15899999999993]]},{"text":"”","box":[[170.04,218.20899999999995],[185.98999999999998,218.20899999999995],[185.98999999999998,234.15899999999993],[170.04,234.15899999999993]]},{"text":"林彪说：","box":[[122.04,249.409],[185.98850000000002,249.409],[185.98850000000002,265.359],[122.04,265.359]]},{"text":"“","box":[[186,249.409],[201.95,249.409],[201.95,265.359],[186,265.359]]},{"text":"把老虎叫来，再商量一下。","box":[[201.959,249.409],[393.9436000000002,249.409],[393.9436000000002,265.359],[201.959,265.359]]},{"text":"”","box":[[393.959,249.409],[409.909,249.409],[409.909,265.359],[393.959,265.359]]},{"text":"叶群立即让内勤去叫林立果来","box":[[122.04,280.60900000000004],[329.9840000000001,280.60900000000004],[329.9840000000001,296.559],[122.04,296.559]]},{"text":"，","box":[[330,280.60900000000004],[345.95,280.60900000000004],[345.95,296.559],[330,296.559]]},{"text":"一起为","box":[[345.24,280.60900000000004],[393.2291000000001,280.60900000000004],[393.2291000000001,296.559],[345.24,296.559]]},{"text":"5","box":[[393.24,280.60900000000004],[401.21500000000003,280.60900000000004],[401.21500000000003,296.559],[393.24,296.559]]},{"text":"月","box":[[401.28,280.60900000000004],[417.22999999999996,280.60900000000004],[417.22999999999996,296.559],[401.28,296.559]]},{"text":"2","box":[[417.24,280.60900000000004],[425.21500000000003,280.60900000000004],[425.21500000000003,296.559],[417.24,296.559]]},{"text":"日的活动作","box":[[425.28,280.60900000000004],[505.18790000000007,280.60900000000004],[505.18790000000007,296.559],[425.28,296.559]]},{"text":"安排","box":[[90,311.8090000000001],[121.9094,311.8090000000001],[121.9094,327.75900000000007],[90,327.75900000000007]]},{"text":"。","box":[[122.04,311.8090000000001],[137.99,311.8090000000001],[137.99,327.75900000000007],[122.04,327.75900000000007]]},{"text":"在确定名单时","box":[[137.88,311.8090000000001],[233.86760000000004,311.8090000000001],[233.86760000000004,327.75900000000007],[137.88,327.75900000000007]]},{"text":"，","box":[[233.88,311.80900000