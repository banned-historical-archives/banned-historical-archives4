3999999999996],[194.583,158.53999999999996]]},{"text":"依然执着","box":[[328.80449999999996,148.03999999999996],[370.80449999999996,148.03999999999996],[370.80449999999996,158.53999999999996],[328.80449999999996,158.53999999999996]]},{"text":"地","box":[[62.64,165.07999999999998],[73.14,165.07999999999998],[73.14,175.57999999999998],[62.64,175.57999999999998]]},{"text":"寻找革命阵地，","box":[[73.14,165.07999999999998],[146.64,165.07999999999998],[146.64,175.57999999999998],[73.14,175.57999999999998]]},{"text":"甘愿抛头颅洒热血。","box":[[144.36149999999998,165.07999999999998],[238.86149999999998,165.07999999999998],[238.86149999999998,175.57999999999998],[144.36149999999998,175.57999999999998]]},{"text":"进入","box":[[236.58299999999997,165.07999999999998],[257.58299999999997,165.07999999999998],[257.58299999999997,175.57999999999998],[236.58299999999997,175.57999999999998]]},{"text":"","box":[[257.58299999999997,165.07999999999998],[260.26,165.07999999999998],[260.26,165.07999999999998],[257.58299999999997,165.07999999999998]]},{"text":"4","box":[[260.26,165.07999999999998],[265.51,165.07999999999998],[265.51,175.57999999999998],[260.26,175.57999999999998]]},{"text":"","box":[[265.51,165.07999999999998],[268.18,165.07999999999998],[268.18,165.07999999999998],[265.51,165.07999999999998]]},{"text":"月，","box":[[268.18,165.07999999999998],[289.18,165.07999999999998],[289.18,175.57999999999998],[268.18,175.57999999999998]]},{"text":"以反对血统论为基","box":[[286.9015,165.07999999999998],[370.8385,165.07999999999998],[370.8385,175.57999999999998],[286.9015,175.57999999999998]]},{"text":"础兴起的造反派","box":[[62.64,182.06],[136.14,182.06],[136.14,192.56],[62.64,192.56]]},{"text":"（后来被泛称红卫兵）","box":[[133.86149999999998,182.06],[238.86149999999998,182.06],[238.86149999999998,192.56],[133.86149999999998,192.56]]},{"text":"，","box":[[233.57999999999998,182.06],[244.07999999999998,182.06],[244.07999999999998,192.56],[233.57999999999998,192.56]]},{"text":"在如何对待军训、","box":[[241.80149999999998,182.06],[325.8015,182.06],[325.8015,192.56],[241.80149999999998,192.56]]},{"text":"老红卫兵、","box":[[323.52299999999997,182.06],[376.02299999999997,182.06],[376.02299999999997,192.56],[323.52299999999997,192.56]]},{"text":"大联合等重大问题上产生分歧，因中央文革领导讲话而公开分裂。","box":[[62.64,199.10000000000002],[367.14,199.10000000000002],[367.14,209.60000000000002],[62.64,209.60000000000002]]},{"text":"1967","box":[[83.66,216.14],[104.66,216.14],[104.66,226.64],[83.66,226.64]]},{"text":"","box":[[104.66,216.14],[107.54,216.14],[107.54,216.14],[104.66,216.14]]},{"text":"年","box":[[107.54,216.14],[118.04,216.14],[118.04,226.64],[107.54,226.64]]},{"text":"","box":[[118.04,216.14],[120.86,216.14],[120.86,216.14],[118.04,216.14]]},{"text":"4","box":[[120.86,216.14],[126.11,216.14],[126.11,226.64],[120.86,226.64]]},{"text":"","box":[[126.11,216.14],[128.96,216.14],[128.96,216.14],[126.11,216.14]]},{"text":"月","box":[[128.96,216.14],[139.46,216.14],[139.46,226.64],[128.96,226.64]]},{"text":"","box":[[139.46,216.14],[142.22,216.14],[142.22,216.14],[139.46,216.14]]},{"text":"3","box":[[142.22,216.14],[147.47,216.14],[147.47,226.64],[142.22,226.64]]},{"text":"","box":[[147.47,216.14],[150.32,216.14],[150.32,216.14],[147.47,216.14]]},{"text":"日，周恩来、陈伯达、康生、江青在人民大会堂","box":[[150.32,216.14],[370.82,216.14],[370.82,226.64],[150.32,226.64]]},{"text":"接见中学师生代表。北京","box":[[62.64,233.18],[177.363,233.18],[177.363,243.68],[62.64,243.68]]},{"text":"","box":[[177.363,233.18],[180.02,233.18],[180.02,233.18],[177.363,233.18]]},{"text":"28","box":[[180.02,233.18],[190.49,233.18],[190.49,243.68],[180.02,243.68]]},{"text":"","box":[[190.49,233.18],[193.16,233.18],[193.16,233.18],[190.49,233.18]]},{"text":"中的","box":[[193.16,233.18],[214.16,233.18],[214.16,243.68],[193.16,243.68]]},{"text":"学生领袖","box":[[214.16,233.18],[256.097,233.18],[256.097,243.68],[214.16,243.68]]},{"text":"王宇反映","box":[[256.12,233.18],[298.12,233.18],[298.12,243.68],[256.12,243.68]]},{"text":"，","box":[[298.12,233.18],[308.62,233.18],[308.62,243.68],[298.12,243.68]]},{"text":"在中学搞军训","box":[[307.84,233.18],[370.84,233.18],[370.84,243.68],[307.84,243.68]]},{"text":"的解放军支持","box":[[62.64,250.16000000000003],[125.64,250.16000000000003],[125.64,260.66],[62.64,260.66]]},{"text":"“","box":[[125.24,250.16000000000003],[135.74,250.16000000000003],[135.74,260.66],[125.24,260.66]]},{"text":"联动","box":[[135.74,250.16000000000003],[156.74,250.16000000000003],[156.74,260.66],[135.74,260.66]]},{"text":"”","box":[[156.74,250.16000000000003],[167.24,250.16000000000003],[167.24,260.66],[156.74,260.66]]},{"text":"，支持保守派，不支持革命派。","box":[[162.02,250.16000000000003],[308.18000000000006,250.16000000000003],[308.18000000000006,260.66],[162.02,260.66]]},{"text":"与会者对军训","box":[[307.78,250.16000000000003],[370.78,250.16000000000003],[370.78,260.66],[307.78,260.66]]},{"text":"解放军强行解散造反组织的做法表达了强烈不满","box":[[62.64,267.2],[283.14,267.2],[283.14,277.7],[62.64,277.7]]},{"text":"。","box":[[283.18,267.2],[293.68,267.2],[293.68,277.7],[283.18,277.7]]},{"text":"陈伯达、","box":[[290.257,267.2],[332.257,267.2],[332.257,277.7],[290.257,277.7]]},{"text":"江青等听","box":[[328.834,267.2],[370.834,267.2],[370.834,277.7],[328.834,277.7]]},{"text":"了，情绪激动地批评北京卫戍区领导的中学军训团","box":[[62.64,284.24],[293.1045,284.24],[293.1045,294.74],[62.64,294.74]]},{"text":"，","box":[[293.14,284.24],[303.64,284.24],[303.64,294.74],[293.14,294.74]]},{"text":"陈伯达说：","box":[[303.1,284.24],[355.6,284.24],[355.6,294.74],[303.1,294.74]]},{"text":"“军","box":[[349.84,284.24],[370.84,284.24],[370.84,294.74],[349.84,294.74]]},{"text":"训不能干涉、","box":[[62.64,301.22],[125.64,301.22],[125.64,311.72],[62.64,311.72]]},{"text":"不要妨碍学生群众的文化","box":[[123.3615,301.22],[238.8615,301.22],[238.8615,311.72],[123.3615,311.72]]},{"text":"革命运动，","box":[[238.88,301.22],[291.38,301.22],[291.38,311.72],[238.88,311.72]]},{"text":"不要包办代替。","box":[[289.1015,301.22],[362.6015,301.22],[362.6015,311.72],[289.1015,311.72]]},{"text":"大","box":[[360.323,301.22],[370.823,301.22],[370.823,311.72],[360.323,311.72]]},{"text":"联合不能同解散（左派组织）等同起来。","box":[[62.64,318.26],[250.56899999999996,318.26],[250.56899999999996,328.76],[62.64,328.76]]},{"text":"”康生在讲话中直接批评了","box":[[245.35049999999995,318.26],[370.81499999999994,318.26],[370.81499999999994,328.76],[245.35049999999995,328.76]]},{"text":"北京卫戍区副司令员李钟奇，","box":[[62.64,335.3],[199.14,335.3],[199.14,345.8],[62.64,345.8]]},{"text":"他说：","box":[[196.86149999999998,335.3],[228.36149999999998,335.3],[228.36149999999998,345.8],[196.86149999999998,345.8]]},{"text":"“支左不能解散左派组织。","box":[[220.80149999999998,335.3],[346.8015,335.3],[346.8015,345.8],[220.80149999999998,345.8]]},{"text":"”","box":[[341.52,335.3],[352.02,335.3],[352.02,345.8],[341.52,345.8]]},{"text":"江青","box":[[349.7415,335.3],[370.7415,335.3],[370.7415,345.8],[349.7415,345.8]]},{"text":"说：","box":[[62.64,352.28],[83.64,352.28],[83.64,362.78],[62.64,362.78]]},{"text":"“军训的同志应该明确表示支持左派，现在事实上证明把左派组","box":[[77.6445,352.28],[370.8045,352.28],[370.8045,362.78],[77.6445,362.78]]},{"text":"织搞掉了嘛。","box":[[62.64,369.32],[125.64,369.32],[125.64,379.82],[62.64,379.82]]},{"text":"首先左派组织不能解散，","box":[[123.3615,369.32],[238.8615,369.32],[238.8615,379.82],[123.3615,379.82]]},{"text":"解散了怎么支左？”","box":[[236.583,369.32],[331.08299999999997,369.32],[331.08299999999997,379.82],[236.583,379.82]]},{"text":"对军训有","box":[[328.80449999999996,369.32],[370.80449999999996,369.32],[370.80449999999996,379.82],[328.80449999999996,379.82]]},{"text":"意见的中学生认为这些讲话好得很，","box":[[62.64,386.38],[230.64,386.38],[230.64,396.88],[62.64,396.88]]},{"text":"连夜将讲话印成传单散发，","box":[[227.21699999999998,386.38],[353.217,386.38],[353.217,396.88],[227.21699999999998,396.88]]},{"text":"满大","box":[[349.794,386.38],[370.794,386.38],[370.794,396.88],[349.794,396.88]]},{"text":"街贴出了“炮轰李钟奇”的大标语。这部分学生被称为“四三派”","box":[[62.64,403.36],[367.14,403.36],[367.14,413.86],[62.64,413.86]]},{"text":"。","box":[[361.9215,403.36],[372.4215,403.36],[372.4215,413.86],[361.9215,413.86]]},{"text":"4","box":[[83.66,420.4],[88.91,420.4],[88.91,430.9],[83.66,430.9]]},{"text":"","box":[[88.91,420.4],[91.58,420.4],[91.58,420.4],[88.91,420.4]]},{"text":"月","box":[[91.58,420.4],[102.08,420.4],[102.08,430.9],[91.58,430.9]]},{"text":"","box":[[102.08,420.4],[104.66,420.4],[104.66,420.4],[102.08,420.4]]},{"text":"4","box":[[104.66,420.4],[109.91,420.4],[109.91,430.9],[104.66,430.9]]},{"text":"","box":[[109.91,420.4],[112.58,420.4],[112.58,420.4],[109.91,420.4]]},{"text":"日晚，周恩来、陈伯达、康生、江青等再次接见学生造反","box":[[112.58,420.4],[370.81699999999995,420.4],[370.81699999999995,430.9],[112.58,430.9]]},{"text":"派代表。","box":[[62.64,437.44],[104.64,437.44],[104.64,447.94],[62.64,447.94]]},{"text":"“","box":[[99.02,437.44],[109.52,437.44],[109.52,447.94],[99.02,447.94]]},{"text":"中学红代会","box":[[109.52,437.44],[162.01999999999998,437.44],[162.01999999999998,447.94],[109.52,447.94]]},{"text":"”","box":[[162.02,437.44],[172.52,437.44],[172.52,447.94],[162.02,447.94]]},{"text":"的","box":[[172.1,437.44],[182.6,437.44],[182.6,447.94],[172.1,447.94]]},{"text":"头头","box":[[182.6,437.44],[203.6,437.44],[203.6,447.94],[182.6,447.94]]},{"text":"李冬民首先发言说：中学全乱了，没","box":[[203.6,437.44],[370.76,437.44],[370.76,447.94],[203.6,447.94]]},{"text":"法工作","box":[[62.64,454.48],[94.14,454.48],[94.14,464.98],[62.64,464.98]]},{"text":"了，军训没法进行","box":[[94.16,454.48],[178.16,454.48],[178.16,464.98],[94.16,464.98]]},{"text":"了。江青等人开始给“","box":[[178.16,454.48],[283.15999999999997,454.48],[283.15999999999997,464.98],[178.16,464.98]]},{"text":"43","box":[[283.18,454.48],[293.71,454.48],[293.71,464.98],[283.18,464.98]]},{"text":"","box":[[293.71,454.48],[297.4,454.48],[297.4,454.48],[293.71,454.48]]},{"text":"讲话”纠偏和降","box":[[297.4,454.48],[370.837,454.48],[370.837,464.98],[297.4,464.98]]},{"text":"温。江青说：","box":[[62.64,471.46000000000004],[125.22,471.46000000000004],[125.22,481.96000000000004],[62.64,481.96000000000004]]},{"text":"“卫戍区还是好的，是紧跟毛主席的。有些问题是属于","box":[[119.5815,471.46000000000004],[370.7415,471.46000000000004],[370.7415,481.96000000000004],[119.5815,481.96000000000004]]},{"text":"认识问题，不要造成对立。军队有立场问题，也是可以改正的。有责","box":[[62.64,488.5],[370.7205,488.5],[370.7205,499],[62.64,499]]},{"text":"任，中央文革也有责任，没有及时和大家交换意见。","box":[[62.64,505.53999999999996],[303.06899999999996,505.53999999999996],[303.06899999999996,516.04],[62.64,516.04]]},{"text":"”讲话中强调了","box":[[297.85049999999995,505.53999999999996],[370.81499999999994,505.53999999999996],[370.81499999999994,516.04],[297.85049999999995,516.04]]},{"text":"对解放军军训的支持。","box":[[62.64,522.52],[167.64,522.52],[167.64,533.02],[62.64,533.02]]},{"text":"于是，","box":[[164.21699999999998,522.52],[195.71699999999998,522.52],[195.71699999999998,533.02],[164.21699999999998,533.02]]},{"text":"另一部分拥护军训的中学生立即组织反","box":[[192.29399999999998,522.52],[370.794,522.52],[370.794,533.02],[192.29399999999998,533.02]]},{"text":"击，这就是所谓的“四四派”","box":[[62.64,539.56],[199.14,539.56],[199.14,550.06],[62.64,550.06]]},{"text":"。","box":[[193.92149999999998,539.56],[204.42149999999998,539.56],[204.42149999999998,550.06],[193.92149999999998,550.06]]},{"text":"中央文革小组的两次讲话，","box":[[83.66,556.6],[209.66,556.6],[209.66,567.1],[83.66,567.1]]},{"text":"在中学生中造成了极大的混乱，","box":[[206.237,556.6],[353.23699999999997,556.6],[353.23699999999997,567.1],[206.237,567.1]]},{"text":"刚成","box":[[349.81399999999996,556.6],[370.81399999999996,556.6],[370.81399999999996,567.1],[349.81399999999996,567.1]]},{"text":"立的“","box":[[62.64,573.58],[92.7645,573.58],[92.7645,584.08],[62.64,584.08]]},{"text":"中学红代会","box":[[92.78,573.58],[145.28,573.58],[145.28,584.08],[92.78,584.08]]},{"text":"”严重分裂，李冬民站在“四四派”立场上对中学","box":[[145.28,573.58],[370.778,573.58],[370.778,584.08],[145.28,584.08]]},{"text":"红代会立即做了清洗，仅西城区","box":[[62.64,590.62],[208.2645,590.62],[208.2645,601.12],[62.64,601.12]]},{"text":"","box":[[208.2645,590.62],[210.92,590.62],[210.92,590.62],[208.2645,590.62]]},{"text":"10","box":[[210.92,590.62],[221.39,590.62],[221.39,601.12],[210.92,601.12]]},{"text":"","box":[[221.39,590.62],[224,590.62],[224,590.62],[221.39,590.62]]},{"text":"个“四三派”","box":[[224,590.62],[285.6245,590.62],[285.6245,601.12],[224,601.12]]},{"text":"“红代会”成员，即","box":[[279.02000000000004,590.62],[370.76900000000006,590.62],[370.76900000000006,601.12],[279.02000000000004,601.12]]}]}