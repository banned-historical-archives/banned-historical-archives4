6.96000000000004],[204.38,96.96000000000004],[204.38,107.46000000000004],[193.88,107.46000000000004]]},{"text":"，最后一次接见","box":[[204.38,96.96000000000004],[277.88,96.96000000000004],[277.88,107.46000000000004],[204.38,107.46000000000004]]},{"text":"的红卫兵","box":[[277.9,96.96000000000004],[319.9,96.96000000000004],[319.9,107.46000000000004],[277.9,107.46000000000004]]},{"text":"","box":[[319.9,96.96000000000004],[322.54,96.96000000000004],[322.54,96.96000000000004],[319.9,96.96000000000004]]},{"text":"200","box":[[322.54,96.96000000000004],[338.29,96.96000000000004],[338.29,107.46000000000004],[322.54,107.46000000000004]]},{"text":"","box":[[338.29,96.96000000000004],[340.96,96.96000000000004],[340.96,96.96000000000004],[338.29,96.96000000000004]]},{"text":"多万。","box":[[340.96,96.96000000000004],[372.397,96.96000000000004],[372.397,107.46000000000004],[340.96,107.46000000000004]]},{"text":"第一天是","box":[[62.64,114],[105.00000000000001,114],[105.00000000000001,124.5],[62.64,124.5]]},{"text":"乘车环城接见","box":[[105.14,114],[168.72799999999998,114],[168.72799999999998,124.5],[105.14,124.5]]},{"text":"，","box":[[168.98,114],[179.48,114],[179.48,124.5],[168.98,124.5]]},{"text":"路线基本上是现在的二环路，从天安门出","box":[[179.6,114],[370.57400000000007,114],[370.57400000000007,124.5],[179.6,124.5]]},{"text":"发，走东直","box":[[62.64,131],[113.397,131],[113.397,141.5],[62.64,141.5]]},{"text":"门，","box":[[113.42,131],[134.42000000000002,131],[134.42000000000002,141.5],[113.42,141.5]]},{"text":"车速很快，","box":[[132.68,131],[185.18,131],[185.18,141.5],[132.68,141.5]]},{"text":"绕道到钓鱼台休息。第二天的接见","box":[[183.5,131],[339.257,131],[339.257,141.5],[183.5,141.5]]},{"text":"改在西","box":[[339.28,131],[370.78,131],[370.78,141.5],[339.28,141.5]]},{"text":"苑机场，","box":[[62.64,148.03999999999996],[104.64,148.03999999999996],[104.64,158.53999999999996],[62.64,158.53999999999996]]},{"text":"上百万人争睹伟大领袖真容，","box":[[101.217,148.03999999999996],[237.71699999999998,148.03999999999996],[237.71699999999998,158.53999999999996],[101.217,158.53999999999996]]},{"text":"时任北京市代市长的吴德记得","box":[[234.29399999999998,148.03999999999996],[370.794,148.03999999999996],[370.794,158.53999999999996],[234.29399999999998,158.53999999999996]]},{"text":"“当时踩死了两三个人，大家都没命地往前拥，形成不由自主的人","box":[[62.64,165.07999999999998],[370.3950000000001,165.07999999999998],[370.3950000000001,175.57999999999998],[62.64,175.57999999999998]]},{"text":"潮。","box":[[62.64,182.06],[83.64,182.06],[83.64,192.56],[62.64,192.56]]},{"text":"”","box":[[78.4215,182.06],[88.9215,182.06],[88.9215,192.56],[78.4215,192.56]]},{"text":"伟大领袖毛主席以前无古人后无来者的胆魄，决心让天下大乱","box":[[83.66,199.10000000000002],[370.5200000000001,199.10000000000002],[370.5200000000001,209.60000000000002],[83.66,209.60000000000002]]},{"text":"到极点（以达到天下大治）","box":[[62.64,216.14],[188.1045,216.14],[188.1045,226.64],[62.64,226.64]]},{"text":"。现在想来，老人家以","box":[[182.886,216.14],[286.81499999999994,216.14],[286.81499999999994,226.64],[182.886,226.64]]},{"text":"","box":[[286.81499999999994,216.14],[289.48,216.14],[289.48,216.14],[286.81499999999994,216.14]]},{"text":"73","box":[[289.48,216.14],[299.95000000000005,216.14],[299.95000000000005,226.64],[289.48,226.64]]},{"text":"","box":[[299.95000000000005,216.14],[302.62,216.14],[302.62,216.14],[299.95000000000005,216.14]]},{"text":"岁的古稀年龄，","box":[[302.62,216.14],[376.057,216.14],[376.057,226.64],[302.62,226.64]]},{"text":"在极端纷乱喧嚣的环境里，头顶烈日动辄就是几个小时站在行走的","box":[[62.64,233.18],[370.3950000000001,233.18],[370.3950000000001,243.68],[62.64,243.68]]},{"text":"敞篷车里，","box":[[62.64,250.16000000000003],[115.14,250.16000000000003],[115.14,260.66],[62.64,260.66]]},{"text":"或在天安门城楼上从东到西来来回回地走大半天，","box":[[111.717,250.16000000000003],[342.717,250.16000000000003],[342.717,260.66],[111.717,260.66]]},{"text":"最多一","box":[[339.294,250.16000000000003],[370.794,250.16000000000003],[370.794,260.66],[339.294,260.66]]},{"text":"次检阅竟走了九趟！是什么精神鼓舞着他？若要打倒身边的赫鲁晓","box":[[62.64,267.2],[370.3950000000001,267.2],[370.3950000000001,277.7],[62.64,277.7]]},{"text":"夫，","box":[[62.64,284.24],[83.64,284.24],[83.64,294.74],[62.64,294.74]]},{"text":"若要清除党内的资产阶级司令部，","box":[[81.3615,284.24],[238.8615,284.24],[238.8615,294.74],[81.3615,294.74]]},{"text":"在一个没有","box":[[236.583,284.24],[289.08299999999997,284.24],[289.08299999999997,294.74],[236.583,294.74]]},{"text":"法治的帝国，","box":[[289.12,284.24],[352.12,284.24],[352.12,294.74],[289.12,294.74]]},{"text":"非要","box":[[349.8415,284.24],[370.8415,284.24],[370.8415,294.74],[349.8415,294.74]]},{"text":"这样吗？伟大领袖是否也是阿尔茨海默病","box":[[62.64,301.22],[251.64,301.22],[251.64,311.72],[62.64,311.72]]},{"text":"（老年痴呆症）","box":[[248.21699999999998,301.22],[321.717,301.22],[321.717,311.72],[248.21699999999998,311.72]]},{"text":"患者呢？疑","box":[[318.294,301.22],[370.794,301.22],[370.794,311.72],[318.294,311.72]]},{"text":"心、偏执、随意、喜怒无常这些早期症状，放在一个普通市民身上，","box":[[62.64,318.26],[376.02299999999997,318.26],[376.02299999999997,328.76],[62.64,328.76]]},{"text":"至多牵累几个家人，可是最高领袖病态的任何举动，都让几亿人遭","box":[[62.64,335.3],[370.3950000000001,335.3],[370.3950000000001,345.8],[62.64,345.8]]},{"text":"殃。","box":[[62.64,352.28],[83.64,352.28],[83.64,362.78],[62.64,362.78]]},{"text":"大串联就像水库的闸门，","box":[[83.66,369.32],[199.16,369.32],[199.16,379.82],[83.66,379.82]]},{"text":"一经打开就风起浪涌，","box":[[196.8815,369.32],[301.88149999999996,369.32],[301.88149999999996,379.82],[196.8815,379.82]]},{"text":"一泻千里。","box":[[299.60299999999995,369.32],[352.10299999999995,369.32],[352.10299999999995,379.82],[299.60299999999995,379.82]]},{"text":"没有","box":[[349.82449999999994,369.32],[370.82449999999994,369.32],[370.82449999999994,379.82],[349.82449999999994,379.82]]},{"text":"亲身经历者无法想象那种无序和极端混乱。","box":[[62.64,386.38],[262.14,386.38],[262.14,396.88],[62.64,396.88]]},{"text":"吴德在《十年风雨纪事》中回忆，毛主席从","box":[[83.66,403.36],[283.15999999999997,403.36],[283.15999999999997,413.86],[83.66,413.86]]},{"text":"","box":[[283.15999999999997,403.36],[286.42,403.36],[286.42,403.36],[283.15999999999997,403.36]]},{"text":"1966","box":[[286.42,403.36],[307.42,403.36],[307.42,413.86],[286.42,413.86]]},{"text":"","box":[[307.42,403.36],[310.66,403.36],[310.66,403.36],[307.42,403.36]]},{"text":"年","box":[[310.66,403.36],[321.16,403.36],[321.16,413.86],[310.66,413.86]]},{"text":"","box":[[321.16,403.36],[324.4,403.36],[324.4,403.36],[321.16,403.36]]},{"text":"8","box":[[324.4,403.36],[329.65,403.36],[329.65,413.86],[324.4,413.86]]},{"text":"","box":[[329.65,403.36],[332.92,403.36],[332.92,403.36],[329.65,403.36]]},{"text":"月","box":[[332.92,403.36],[343.42,403.36],[343.42,413.86],[332.92,413.86]]},{"text":"","box":[[343.42,403.36],[346.6,403.36],[346.6,403.36],[343.42,403.36]]},{"text":"18","box":[[346.6,403.36],[357.07000000000005,403.36],[357.07000000000005,413.86],[346.6,413.86]]},{"text":"","box":[[357.07000000000005,403.36],[360.28,403.36],[360.28,403.36],[357.07000000000005,403.36]]},{"text":"日","box":[[360.28,403.36],[370.78,403.36],[370.78,413.86],[360.28,413.86]]},{"text":"起，","box":[[62.64,420.4],[83.64,420.4],[83.64,430.9],[62.64,430.9]]},{"text":"接连八次检阅红卫兵，","box":[[81.3615,420.4],[186.3615,420.4],[186.3615,430.9],[81.3615,430.9]]},{"text":"大概接见了","box":[[184.083,420.4],[236.583,420.4],[236.583,430.9],[184.083,430.9]]},{"text":"","box":[[236.583,420.4],[239.24,420.4],[239.24,420.4],[236.583,420.4]]},{"text":"1300","box":[[239.24,420.4],[260.24,420.4],[260.24,430.9],[239.24,430.9]]},{"text":"","box":[[260.24,420.4],[262.9,420.4],[262.9,420.4],[260.24,420.4]]},{"text":"万全国各地红卫兵。","box":[[262.9,420.4],[357.337,420.4],[357.337,430.9],[262.9,430.9]]},{"text":"“全","box":[[349.84,420.4],[370.777,420.4],[370.777,430.9],[349.84,430.9]]},{"text":"国大串联，坐火车不要钱，到处可以吃饭。当时都叫红卫兵，手臂上","box":[[62.64,437.44],[370.7205,437.44],[370.7205,447.94],[62.64,447.94]]},{"text":"套一块红布就行，这给组织接见工作带","box":[[62.64,454.48],[241.14,454.48],[241.14,464.98],[62.64,464.98]]},{"text":"来很大困难。","box":[[241.16,454.48],[304.15999999999997,454.48],[304.15999999999997,464.98],[241.16,464.98]]},{"text":"”","box":[[298.94149999999996,454.48],[309.44149999999996,454.48],[309.44149999999996,464.98],[298.94149999999996,464.98]]},{"text":"当时的负责官员不知被毛泽东的突发奇想弄得怎样狼狈不堪，","box":[[83.66,471.46000000000004],[370.5200000000001,471.46000000000004],[370.5200000000001,481.96000000000004],[83.66,481.96000000000004]]},{"text":"连现场维持秩序的吴德，","box":[[62.64,488.5],[178.14,488.5],[178.14,499],[62.64,499]]},{"text":"也常被挤得前胸贴后背，","box":[[175.86149999999998,488.5],[291.3615,488.5],[291.3615,499],[175.86149999999998,499]]},{"text":"找不到北，","box":[[289.08299999999997,488.5],[341.58299999999997,488.5],[341.58299999999997,499],[289.08299999999997,499]]},{"text":"喘不过","box":[[339.30449999999996,488.5],[370.80449999999996,488.5],[370.80449999999996,499],[339.30449999999996,499]]},{"text":"气。","box":[[62.64,505.53999999999996],[83.64,505.53999999999996],[83.64,516.04],[62.64,516.04]]},{"text":"让伟大领袖坐在车里检阅，","box":[[81.3615,505.53999999999996],[207.3615,505.53999999999996],[207.3615,516.04],[81.3615,516.04]]},{"text":"或让红卫兵坐在车里受阅，","box":[[205.083,505.53999999999996],[331.08299999999997,505.53999999999996],[331.08299999999997,516.04],[205.083,516.04]]},{"text":"各有各的","box":[[328.80449999999996,505.53999999999996],[370.80449999999996,505.53999999999996],[370.80449999999996,516.04],[328.80449999999996,516.04]]},{"text":"麻烦。他说：","box":[[62.64,522.52],[125.22,522.52],[125.22,533.02],[62.64,533.02]]},{"text":"“八次检阅，每次都是周总理亲自布置，当时真是时时","box":[[119.5815,522.52],[370.7415,522.52],[370.7415,533.02],[119.5815,533.02]]},{"text":"提着心，生怕出事。","box":[[62.64,539.56],[157.14,539.56],[157.14,550.06],[62.64,550.06]]},{"text":"”","box":[[151.92149999999998,539.56],[162.42149999999998,539.56],[162.42149999999998,550.06],[151.92149999999998,550.06]]},{"text":"据吴德回忆，当时北京城区市民不过","box":[[83.66,556.6],[251.66,556.6],[251.66,567.1],[83.66,567.1]]},{"text":"","box":[[251.66,556.6],[256.12,556.6],[256.12,556.6],[251.66,556.6]]},{"text":"300","box":[[256.12,556.6],[271.92999999999995,556.6],[271.92999999999995,567.1],[256.12,567.1]]},{"text":"","box":[[271.92999999999995,556.6],[276.4,556.6],[276.4,556.6],[271.92999999999995,556.6]]},{"text":"多万，来京红卫兵最","box":[[276.4,556.6],[370.837,556.6],[370.837,567.1],[276.4,567.1]]},{"text":"多一天竟也","box":[[62.64,573.58],[115.14,573.58],[115.14,584.08],[62.64,584.08]]},{"text":"","box":[[115.14,573.58],[119.6,573.58],[119.6,573.58],[115.14,573.58]]},{"text":"300","box":[[119.6,573.58],[135.41,573.58],[135.41,584.08],[119.6,584.08]]},{"text":"","box":[[135.41,573.58],[139.88,573.58],[139.88,573.58],[135.41,573.58]]},{"text":"多万，包吃住行还不算，大量南方学生还要送御寒","box":[[139.88,573.58],[370.817,573.58],[370.817,584.08],[139.88,584.08]]},{"text":"衣服。","box":[[62.64,590.62],[94.14,590.62],[94.14,601.12],[62.64,601.12]]},{"text":"有一天来的人特别多，","box":[[91.137,590.62],[196.137,590.62],[196.137,601.12],[91.137,601.12]]},{"text":"前门一带","box":[[193.13400000000001,590.62],[235.13400000000001,590.62],[235.13400000000001,601.12],[193.13400000000001,601.12]]},{"text":"（北京市委接待处）","box":[[232.13100000000003,590.62],[326.63100000000003,590.62],[326.63100000000003,601.12],[232.13100000000003,601.12]]},{"text":"都挤满了，","box":[[323.62800000000004,590.62],[376.12800000000004,590.62],[376.12800000000004,601.12],[323.62800000000004,601.12]]}]}