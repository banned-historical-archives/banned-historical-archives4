48.03999999999996],[239.11700000000002,158.53999999999996],[113.18,158.53999999999996]]},{"text":"，记录了","box":[[233.8985,148.03999999999996],[275.8355,148.03999999999996],[275.8355,158.53999999999996],[233.8985,158.53999999999996]]},{"text":"","box":[[275.8355,148.03999999999996],[278.68,148.03999999999996],[278.68,148.03999999999996],[275.8355,148.03999999999996]]},{"text":"5","box":[[278.68,148.03999999999996],[283.93,148.03999999999996],[283.93,158.53999999999996],[278.68,158.53999999999996]]},{"text":"","box":[[283.93,148.03999999999996],[286.78,148.03999999999996],[286.78,148.03999999999996],[283.93,148.03999999999996]]},{"text":"日全市有两个人非","box":[[286.78,148.03999999999996],[370.78,148.03999999999996],[370.78,158.53999999999996],[286.78,158.53999999999996]]},{"text":"正常死亡，","box":[[62.64,165.07999999999998],[115.14,165.07999999999998],[115.14,175.57999999999998],[62.64,175.57999999999998]]},{"text":"其中一个是师大女附中党总支书记、","box":[[111.9585,165.07999999999998],[279.9585,165.07999999999998],[279.9585,175.57999999999998],[111.9585,175.57999999999998]]},{"text":"副校长卞仲耘，","box":[[276.777,165.07999999999998],[350.277,165.07999999999998],[350.277,175.57999999999998],[276.777,175.57999999999998]]},{"text":"“","box":[[341.81399999999996,165.07999999999998],[352.31399999999996,165.07999999999998],[352.31399999999996,175.57999999999998],[341.81399999999996,175.57999999999998]]},{"text":"8","box":[[352.36,165.07999999999998],[357.61,165.07999999999998],[357.61,175.57999999999998],[352.36,175.57999999999998]]},{"text":"","box":[[357.61,165.07999999999998],[360.28,165.07999999999998],[360.28,165.07999999999998],[357.61,165.07999999999998]]},{"text":"月","box":[[360.28,165.07999999999998],[370.78,165.07999999999998],[370.78,175.57999999999998],[360.28,175.57999999999998]]},{"text":"5","box":[[62.64,182.06],[67.89,182.06],[67.89,192.56],[62.64,192.56]]},{"text":"","box":[[67.89,182.06],[71.58,182.06],[71.58,182.06],[67.89,182.06]]},{"text":"日下午，高一（","box":[[71.58,182.06],[145.017,182.06],[145.017,192.56],[71.58,192.56]]},{"text":"3","box":[[145.04,182.06],[150.29,182.06],[150.29,192.56],[145.04,192.56]]},{"text":"）班和其他班一些干部子弟，斗争党总支书记卞","box":[[150.32,182.06],[370.757,182.06],[370.757,192.56],[150.32,192.56]]},{"text":"仲耘等","box":[[62.64,199.10000000000002],[94.14,199.10000000000002],[94.14,209.60000000000002],[62.64,209.60000000000002]]},{"text":"","box":[[94.14,199.10000000000002],[96.8,199.10000000000002],[96.8,199.10000000000002],[94.14,199.10000000000002]]},{"text":"5","box":[[96.8,199.10000000000002],[102.05,199.10000000000002],[102.05,209.60000000000002],[96.8,209.60000000000002]]},{"text":"","box":[[102.05,199.10000000000002],[104.72,199.10000000000002],[104.72,199.10000000000002],[102.05,199.10000000000002]]},{"text":"名有问题的领导干部，","box":[[104.72,199.10000000000002],[209.65699999999998,199.10000000000002],[209.65699999999998,209.60000000000002],[104.72,209.60000000000002]]},{"text":"5","box":[[207.56,199.10000000000002],[212.81,199.10000000000002],[212.81,209.60000000000002],[207.56,209.60000000000002]]},{"text":"","box":[[212.81,199.10000000000002],[215.48,199.10000000000002],[215.48,199.10000000000002],[212.81,199.10000000000002]]},{"text":"人被学生殴打，","box":[[215.48,199.10000000000002],[288.91700000000003,199.10000000000002],[288.91700000000003,209.60000000000002],[215.48,209.60000000000002]]},{"text":"有的学生还用缠铁","box":[[286.817,199.10000000000002],[370.817,199.10000000000002],[370.817,209.60000000000002],[286.817,209.60000000000002]]},{"text":"丝的木棒打，卞仲耘受伤较重，送医院抢救无效已死。","box":[[62.64,216.14],[311.154,216.14],[311.154,226.64],[62.64,226.64]]},{"text":"”","box":[[305.9355,216.14],[316.4355,216.14],[316.4355,226.64],[305.9355,226.64]]},{"text":"（见《南方周","box":[[309.474,216.14],[370.731,216.14],[370.731,226.64],[309.474,226.64]]},{"text":"末》","box":[[62.64,233.18],[83.64,233.18],[83.64,243.68],[62.64,243.68]]},{"text":"2014","box":[[83.66,233.18],[104.66,233.18],[104.66,243.68],[83.66,243.68]]},{"text":"","box":[[104.66,233.18],[107.3,233.18],[107.3,233.18],[104.66,233.18]]},{"text":"年","box":[[107.3,233.18],[117.8,233.18],[117.8,243.68],[107.3,243.68]]},{"text":"","box":[[117.8,233.18],[120.38,233.18],[120.38,233.18],[117.8,233.18]]},{"text":"3","box":[[120.38,233.18],[125.63,233.18],[125.63,243.68],[120.38,243.68]]},{"text":"","box":[[125.63,233.18],[128.3,233.18],[128.3,233.18],[125.63,233.18]]},{"text":"月","box":[[128.3,233.18],[138.8,233.18],[138.8,243.68],[128.3,243.68]]},{"text":"","box":[[138.8,233.18],[141.38,233.18],[141.38,233.18],[138.8,233.18]]},{"text":"13","box":[[141.38,233.18],[151.91,233.18],[151.91,243.68],[141.38,243.68]]},{"text":"","box":[[151.91,233.18],[154.52,233.18],[154.52,233.18],[151.91,233.18]]},{"text":"日","box":[[154.52,233.18],[165.02,233.18],[165.02,243.68],[154.52,243.68]]},{"text":"","box":[[165.02,233.18],[167.66,233.18],[167.66,233.18],[165.02,233.18]]},{"text":"D","box":[[167.66,233.18],[175.24099999999999,233.18],[175.24099999999999,243.68],[167.66,243.68]]},{"text":"27","box":[[175.22,233.18],[185.69,233.18],[185.69,243.68],[175.22,243.68]]},{"text":"）不足百字，对事件的发生时间、人物、","box":[[185.72,233.18],[374.70950000000005,233.18],[374.70950000000005,243.68],[185.72,243.68]]},{"text":"情节、结果记录在案。","box":[[62.64,250.16000000000003],[167.64,250.16000000000003],[167.64,260.66],[62.64,260.66]]},{"text":"高一（","box":[[83.66,267.2],[114.6245,267.2],[114.6245,277.7],[83.66,277.7]]},{"text":"3","box":[[114.62,267.2],[119.87,267.2],[119.87,277.7],[114.62,277.7]]},{"text":"）班起头的学生都是保工作组一派的，她们要用行动表","box":[[119.9,267.2],[370.82899999999995,267.2],[370.82899999999995,277.7],[119.9,277.7]]},{"text":"现革命，","box":[[62.64,284.24],[104.64,284.24],[104.64,294.74],[62.64,294.74]]},{"text":"那就是，","box":[[102.2355,284.24],[144.2355,284.24],[144.2355,294.74],[102.2355,294.74]]},{"text":"把工作组时期","box":[[141.831,284.24],[204.831,284.24],[204.831,294.74],[141.831,294.74]]},{"text":"“包庇”","box":[[202.42649999999998,284.24],[244.42649999999998,284.24],[244.42649999999998,294.74],[202.42649999999998,294.74]]},{"text":"的黑帮校领导，","box":[[241.96949999999998,284.24],[315.4695,284.24],[315.4695,294.74],[241.96949999999998,294.74]]},{"text":"揪出来示众。","box":[[313.065,284.24],[376.065,284.24],[376.065,294.74],[313.065,294.74]]},{"text":"伟大领袖毛主席教导我们：","box":[[62.64,301.22],[188.64,301.22],[188.64,311.72],[62.64,311.72]]},{"text":"“农会权力无上，不许地主说话，把地主","box":[[182.8755,301.22],[370.80449999999996,301.22],[370.80449999999996,311.72],[182.8755,311.72]]},{"text":"的威风扫光。这等于将地主打翻在地，再踏上一只脚。","box":[[62.64,318.26],[313.56899999999996,318.26],[313.56899999999996,328.76],[62.64,328.76]]},{"text":"”在头脑热昏","box":[[308.35049999999995,318.26],[370.81499999999994,318.26],[370.81499999999994,328.76],[308.35049999999995,328.76]]},{"text":"的中","box":[[62.64,335.3],[83.64,335.3],[83.64,345.8],[62.64,345.8]]},{"text":"学生眼里，","box":[[83.66,335.3],[136.16,335.3],[136.16,345.8],[83.66,345.8]]},{"text":"“黑帮”和地主没两样。从前我们的老子干革命，把","box":[[130.5215,335.3],[370.7615,335.3],[370.7615,345.8],[130.5215,345.8]]},{"text":"脑袋别在裤腰带上，","box":[[62.64,352.28],[157.14,352.28],[157.14,362.78],[62.64,362.78]]},{"text":"如今我们红后代也要敢打敢杀。","box":[[154.86149999999998,352.28],[301.8615,352.28],[301.8615,362.78],[154.86149999999998,362.78]]},{"text":"毛主席在","box":[[299.58299999999997,352.28],[341.58299999999997,352.28],[341.58299999999997,362.78],[299.58299999999997,362.78]]},{"text":"《湖南","box":[[339.30449999999996,352.28],[370.80449999999996,352.28],[370.80449999999996,362.78],[339.30449999999996,362.78]]},{"text":"农民运动考察报告》","box":[[62.64,369.32],[157.14,369.32],[157.14,379.82],[62.64,379.82]]},{"text":"里历数的农民造反行动，","box":[[154.86149999999998,369.32],[270.3615,369.32],[270.3615,379.82],[154.86149999999998,379.82]]},{"text":"譬如抄家、","box":[[268.08299999999997,369.32],[320.58299999999997,369.32],[320.58299999999997,379.82],[268.08299999999997,379.82]]},{"text":"动不动捉人","box":[[318.34,369.32],[370.84,369.32],[370.84,379.82],[318.34,379.82]]},{"text":"戴高帽子游街，为所欲为，一切反常，造成一种恐怖现象的事，今天","box":[[62.64,386.38],[370.731,386.38],[370.731,396.88],[62.64,396.88]]},{"text":"都要试一试。","box":[[62.64,403.36],[125.64,403.36],[125.64,413.86],[62.64,413.86]]},{"text":"“革命不是请客吃饭，不是做文章，不是绘画绣花，不","box":[[119.9385,403.36],[370.7415,403.36],[370.7415,413.86],[119.9385,413.86]]},{"text":"能那样雅致，那样从容不迫，文质彬彬，那样","box":[[62.64,420.4],[267.411,420.4],[267.411,430.9],[62.64,430.9]]},{"text":"温良恭俭让","box":[[267.46,420.4],[319.96,420.4],[319.96,430.9],[267.46,430.9]]},{"text":"。革命是暴","box":[[319.96,420.4],[370.7695,420.4],[370.7695,430.9],[319.96,430.9]]},{"text":"动，是一个阶级推翻一个阶级的暴烈的行动。","box":[[62.64,437.44],[272.64,437.44],[272.64,447.94],[62.64,447.94]]},{"text":"”","box":[[267.4215,437.44],[277.9215,437.44],[277.9215,447.94],[267.4215,447.94]]},{"text":"于是，尽管当天下午很多学生不在校，高一部分学生的“革命”","box":[[83.66,454.48],[376.0535,454.48],[376.0535,464.98],[83.66,464.98]]},{"text":"行动还是得到了不小的响应，最狂热拥戴她们的是初二年级的一些","box":[[62.64,471.46000000000004],[370.3950000000001,471.46000000000004],[370.3950000000001,481.96000000000004],[62.64,481.96000000000004]]},{"text":"红五类，","box":[[62.64,488.5],[104.64,488.5],[104.64,499],[62.64,499]]},{"text":"加上围观者，","box":[[102.3615,488.5],[165.3615,488.5],[165.3615,499],[102.3615,499]]},{"text":"游斗现场据说人多时有数百之众。","box":[[163.083,488.5],[320.58299999999997,488.5],[320.58299999999997,499],[163.083,499]]},{"text":"为什么是这","box":[[318.30449999999996,488.5],[370.80449999999996,488.5],[370.80449999999996,499],[318.30449999999996,499]]},{"text":"两个年级里的学生参与最多？至今未见学界从心理学层面去研究。","box":[[62.64,505.53999999999996],[370.3950000000001,505.53999999999996],[370.3950000000001,516.04],[62.64,516.04]]},{"text":"我也写过多篇关于母校文革问题的文章，","box":[[62.64,522.52],[251.64,522.52],[251.64,533.02],[62.64,533.02]]},{"text":"以往我和别人一样，","box":[[248.21699999999998,522.52],[342.717,522.52],[342.717,533.02],[248.21699999999998,533.02]]},{"text":"把目光","box":[[339.294,522.52],[370.794,522.52],[370.794,533.02],[339.294,533.02]]},{"text":"集中在已逝的卞校长，现在让我们看看","box":[[62.64,539.56],[241.14,539.56],[241.14,550.06],[62.64,550.06]]},{"text":"","box":[[241.14,539.56],[244.7,539.56],[244.7,539.56],[241.14,539.56]]},{"text":"8","box":[[244.7,539.56],[249.95,539.56],[249.95,550.06],[244.7,550.06]]},{"text":"","box":[[249.95,539.56],[253.54,539.56],[253.54,539.56],[249.95,539.56]]},{"text":"月","box":[[253.54,539.56],[264.03999999999996,539.56],[264.03999999999996,550.06],[253.54,550.06]]},{"text":"","box":[[264.03999999999996,539.56],[267.58,539.56],[267.58,539.56],[264.03999999999996,539.56]]},{"text":"5","box":[[267.58,539.56],[272.83,539.56],[272.83,550.06],[267.58,550.06]]},{"text":"","box":[[272.83,539.56],[276.34,539.56],[276.34,539.56],[272.83,539.56]]},{"text":"日当天同样遭受殴打","box":[[276.34,539.56],[370.84,539.56],[370.84,550.06],[276.34,550.06]]},{"text":"折磨幸存的教导处主任汪玉冰老师的回忆：","box":[[62.64,556.6],[262.14,556.6],[262.14,567.1],[62.64,567.1]]},{"text":"八月四日下午","box":[[83.66,579.58],[146.66,579.58],[146.66,590.08],[83.66,590.08]]},{"text":"，","box":[[146.66,579.58],[157.16,579.58],[157.16,590.08],[146.66,590.08]]},{"text":"几个学生把我推到办公室","box":[[154.88,579.58],[270.38,579.58],[270.38,590.08],[154.88,590.08]]},{"text":"，","box":[[270.4,579.58],[280.9,579.58],[280.9,590.08],[270.4,590.08]]},{"text":"让我写东西。","box":[[278.62,579.58],[341.62,579.58],[341.62,590.08],[278.62,590.08]]},{"text":"一进办","box":[[339.3415,579.58],[370.8415,579.58],[370.8415,590.08],[339.3415,590.08]]}]}