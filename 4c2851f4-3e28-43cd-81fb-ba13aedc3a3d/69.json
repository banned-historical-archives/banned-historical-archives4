14],[138.1955,114]]},{"text":"老师是有夫之妇，也是两地分居。我的班主任赵老师","box":[[140.84,114],[378.749,114],[378.749,124.5],[140.84,124.5]]},{"text":"曾经对我讲过，","box":[[70.56,131],[144.06,131],[144.06,141.5],[70.56,141.5]]},{"text":"当时学校里有两对婚外恋，","box":[[141.7815,131],[267.7815,131],[267.7815,141.5],[141.7815,141.5]]},{"text":"他是最早发现的。","box":[[265.503,131],[349.503,131],[349.503,141.5],[265.503,141.5]]},{"text":"一对是","box":[[347.2245,131],[378.7245,131],[378.7245,141.5],[347.2245,141.5]]},{"text":"外语组的老师某某和某某，","box":[[70.56,148.03999999999996],[196.56,148.03999999999996],[196.56,158.53999999999996],[70.56,158.53999999999996]]},{"text":"他们在北海公园约会被他看见过。","box":[[193.137,148.03999999999996],[350.637,148.03999999999996],[350.637,158.53999999999996],[193.137,158.53999999999996]]},{"text":"另一对","box":[[347.214,148.03999999999996],[378.714,148.03999999999996],[378.714,158.53999999999996],[347.214,158.53999999999996]]},{"text":"就是","box":[[70.56,165.07999999999998],[91.56,165.07999999999998],[91.56,175.57999999999998],[70.56,175.57999999999998]]},{"text":"","box":[[91.56,165.07999999999998],[95,165.07999999999998],[95,165.07999999999998],[91.56,165.07999999999998]]},{"text":"DZ","box":[[95,165.07999999999998],[108.9755,165.07999999999998],[108.9755,175.57999999999998],[95,175.57999999999998]]},{"text":"","box":[[108.9755,165.07999999999998],[112.4,165.07999999999998],[112.4,165.07999999999998],[108.9755,165.07999999999998]]},{"text":"二位老师，那时教工宿舍夜里要关大门，他看见过","box":[[112.4,165.07999999999998],[343.337,165.07999999999998],[343.337,175.57999999999998],[112.4,175.57999999999998]]},{"text":"","box":[[343.337,165.07999999999998],[346.78,165.07999999999998],[346.78,165.07999999999998],[343.337,165.07999999999998]]},{"text":"D","box":[[346.78,165.07999999999998],[354.361,165.07999999999998],[354.361,175.57999999999998],[346.78,175.57999999999998]]},{"text":"","box":[[354.361,165.07999999999998],[357.76,165.07999999999998],[357.76,165.07999999999998],[354.361,165.07999999999998]]},{"text":"老师","box":[[357.76,165.07999999999998],[378.76,165.07999999999998],[378.76,175.57999999999998],[357.76,175.57999999999998]]},{"text":"翻墙去","box":[[70.56,182.06],[102.06,182.06],[102.06,192.56],[70.56,192.56]]},{"text":"","box":[[102.06,182.06],[105.02,182.06],[105.02,182.06],[102.06,182.06]]},{"text":"Z","box":[[105.02,182.06],[111.43549999999999,182.06],[111.43549999999999,192.56],[105.02,192.56]]},{"text":"","box":[[111.43549999999999,182.06],[114.38,182.06],[114.38,182.06],[111.43549999999999,182.06]]},{"text":"家。","box":[[114.38,182.06],[135.38,182.06],[135.38,192.56],[114.38,192.56]]},{"text":"Z","box":[[135.38,182.06],[141.7955,182.06],[141.7955,192.56],[135.38,192.56]]},{"text":"","box":[[141.7955,182.06],[144.74,182.06],[144.74,182.06],[141.7955,182.06]]},{"text":"老师毕业于北师大历史系，","box":[[144.74,182.06],[270.74,182.06],[270.74,192.56],[144.74,192.56]]},{"text":"1950","box":[[270.76,182.06],[291.76,182.06],[291.76,192.56],[270.76,192.56]]},{"text":"","box":[[291.76,182.06],[294.7,182.06],[294.7,182.06],[291.76,182.06]]},{"text":"年来校","box":[[294.7,182.06],[326.2,182.06],[326.2,192.56],[294.7,192.56]]},{"text":"，","box":[[326.2,182.06],[336.7,182.06],[336.7,192.56],[326.2,192.56]]},{"text":"业务好，","box":[[336.7,182.06],[378.7,182.06],[378.7,192.56],[336.7,192.56]]},{"text":"先后担任过语文和历史教师","box":[[70.56,199.10000000000002],[196.56,199.10000000000002],[196.56,209.60000000000002],[70.56,209.60000000000002]]},{"text":"，","box":[[196.58,199.10000000000002],[207.08,199.10000000000002],[207.08,209.60000000000002],[196.58,209.60000000000002]]},{"text":"性格热情活泼","box":[[204.08,199.10000000000002],[267.08000000000004,199.10000000000002],[267.08000000000004,209.60000000000002],[204.08,209.60000000000002]]},{"text":"，","box":[[267.1,199.10000000000002],[277.6,199.10000000000002],[277.6,209.60000000000002],[267.1,209.60000000000002]]},{"text":"喜欢唱歌、","box":[[274.6,199.10000000000002],[327.1,199.10000000000002],[327.1,209.60000000000002],[274.6,209.60000000000002]]},{"text":"跳舞、","box":[[324.09700000000004,199.10000000000002],[355.59700000000004,199.10000000000002],[355.59700000000004,209.60000000000002],[324.09700000000004,209.60000000000002]]},{"text":"打球。","box":[[352.54150000000004,199.10000000000002],[384.04150000000004,199.10000000000002],[384.04150000000004,209.60000000000002],[352.54150000000004,209.60000000000002]]},{"text":"因这段婚外恋，被学校党支部取消了入党积极分子的培养资格。","box":[[70.56,216.14],[364.56,216.14],[364.56,226.64],[70.56,226.64]]},{"text":"D","box":[[91.58,233.18],[99.161,233.18],[99.161,243.68],[91.58,243.68]]},{"text":"","box":[[99.161,233.18],[101.78,233.18],[101.78,233.18],[99.161,233.18]]},{"text":"老师已经不能回头，主动提出离婚，","box":[[101.78,233.18],[269.36,233.18],[269.36,243.68],[101.78,243.68]]},{"text":"“袁氏”的态度是宁拖不","box":[[263.7215,233.18],[378.7385,233.18],[378.7385,243.68],[263.7215,243.68]]},{"text":"离。","box":[[70.56,250.16000000000003],[91.56,250.16000000000003],[91.56,260.66],[70.56,260.66]]},{"text":"D","box":[[91.58,250.16000000000003],[99.161,250.16000000000003],[99.161,260.66],[91.58,260.66]]},{"text":"","box":[[99.161,250.16000000000003],[102.08,250.16000000000003],[102.08,250.16000000000003],[99.161,250.16000000000003]]},{"text":"起诉到法院，","box":[[102.08,250.16000000000003],[165.1325,250.16000000000003],[165.1325,260.66],[102.08,260.66]]},{"text":"1964","box":[[165.14,250.16000000000003],[186.14,250.16000000000003],[186.14,260.66],[165.14,260.66]]},{"text":"","box":[[186.14,250.16000000000003],[189.08,250.16000000000003],[189.08,250.16000000000003],[186.14,250.16000000000003]]},{"text":"年","box":[[189.08,250.16000000000003],[199.58,250.16000000000003],[199.58,260.66],[189.08,260.66]]},{"text":"","box":[[199.58,250.16000000000003],[202.52,250.16000000000003],[202.52,250.16000000000003],[199.58,250.16000000000003]]},{"text":"8","box":[[202.52,250.16000000000003],[207.77,250.16000000000003],[207.77,260.66],[202.52,260.66]]},{"text":"","box":[[207.77,250.16000000000003],[210.74,250.16000000000003],[210.74,250.16000000000003],[207.77,250.16000000000003]]},{"text":"月被判决离婚。袁向女","box":[[210.74,250.16000000000003],[315.74,250.16000000000003],[315.74,260.66],[210.74,260.66]]},{"text":"附中领导提出","box":[[315.76,250.16000000000003],[378.76,250.16000000000003],[378.76,260.66],[315.76,260.66]]},{"text":"要求，希望多扣","box":[[70.56,267.2],[142.44299999999998,267.2],[142.44299999999998,277.7],[70.56,277.7]]},{"text":"","box":[[142.44299999999998,267.2],[145.1,267.2],[145.1,267.2],[142.44299999999998,267.2]]},{"text":"D","box":[[145.1,267.2],[152.68099999999998,267.2],[152.68099999999998,277.7],[145.1,277.7]]},{"text":"","box":[[152.68099999999998,267.2],[155.3,267.2],[155.3,267.2],[152.68099999999998,267.2]]},{"text":"的工资（","box":[[155.3,267.2],[195.68300000000002,267.2],[195.68300000000002,277.7],[155.3,277.7]]},{"text":"92","box":[[195.68,267.2],[206.15,267.2],[206.15,277.7],[195.68,277.7]]},{"text":"","box":[[206.15,267.2],[208.82,267.2],[208.82,267.2],[206.15,267.2]]},{"text":"元","box":[[208.82,267.2],[219.32,267.2],[219.32,277.7],[208.82,277.7]]},{"text":"/","box":[[219.32,267.2],[224.57,267.2],[224.57,277.7],[219.32,277.7]]},{"text":"月）补贴她和孩子的家用，卞校长","box":[[224.6,267.2],[378.803,267.2],[378.803,277.7],[224.6,277.7]]},{"text":"认为应该按照法院判决书执行，学校不能擅自更改。","box":[[70.56,284.24],[312.06,284.24],[312.06,294.74],[70.56,294.74]]},{"text":"“袁氏”认为，","box":[[306.8415,284.24],[380.2785,284.24],[380.2785,294.74],[306.8415,294.74]]},{"text":"卞明显地站在男方一边，","box":[[70.56,301.22],[186.06,301.22],[186.06,311.72],[70.56,311.72]]},{"text":"因此一再被激怒。","box":[[183.897,301.22],[267.897,301.22],[267.897,311.72],[183.897,311.72]]},{"text":"随后，","box":[[265.734,301.22],[297.234,301.22],[297.234,311.72],[265.734,311.72]]},{"text":"D","box":[[295.06,301.22],[302.641,301.22],[302.641,311.72],[295.06,311.72]]},{"text":"","box":[[302.641,301.22],[305.26,301.22],[305.26,301.22],[302.641,301.22]]},{"text":"老师被调出女附","box":[[305.26,301.22],[378.76,301.22],[378.76,311.72],[305.26,311.72]]},{"text":"中，至此袁淑娥应该并不知晓","box":[[70.56,318.26],[205.6845,318.26],[205.6845,328.76],[70.56,328.76]]},{"text":"","box":[[205.6845,318.26],[208.34,318.26],[208.34,318.26],[205.6845,318.26]]},{"text":"Z","box":[[208.34,318.26],[214.7555,318.26],[214.7555,328.76],[208.34,328.76]]},{"text":"","box":[[214.7555,318.26],[217.4,318.26],[217.4,318.26],[214.7555,318.26]]},{"text":"才是她婚姻中的第三者。文革初起，","box":[[217.4,318.26],[383.9615,318.26],[383.9615,328.76],[217.4,328.76]]},{"text":"袁开始向有关方面控告，无中生有、断章取义、上纲上线，上告信有","box":[[70.56,335.3],[378.64050000000003,335.3],[378.64050000000003,345.8],[70.56,345.8]]},{"text":"两三万字之多，于","box":[[70.56,352.28],[154.56,352.28],[154.56,362.78],[70.56,362.78]]},{"text":"","box":[[154.56,352.28],[157.22,352.28],[157.22,352.28],[154.56,352.28]]},{"text":"1966","box":[[157.22,352.28],[178.2725,352.28],[178.2725,362.78],[157.22,362.78]]},{"text":"","box":[[178.2725,352.28],[180.86,352.28],[180.86,352.28],[178.2725,352.28]]},{"text":"年","box":[[180.86,352.28],[191.36,352.28],[191.36,362.78],[180.86,362.78]]},{"text":"","box":[[191.36,352.28],[194,352.28],[194,352.28],[191.36,352.28]]},{"text":"5","box":[[194,352.28],[199.25,352.28],[199.25,362.78],[194,362.78]]},{"text":"","box":[[199.25,352.28],[201.92,352.28],[201.92,352.28],[199.25,352.28]]},{"text":"月分别寄出。而","box":[[201.92,352.28],[275.41999999999996,352.28],[275.41999999999996,362.78],[201.92,362.78]]},{"text":"","box":[[275.41999999999996,352.28],[278.08,352.28],[278.08,352.28],[275.41999999999996,352.28]]},{"text":"DZ","box":[[278.08,352.28],[292.0555,352.28],[292.0555,362.78],[278.08,362.78]]},{"text":"","box":[[292.0555,352.28],[294.76,352.28],[294.76,352.28],[292.0555,352.28]]},{"text":"二位几年后结为夫","box":[[294.76,352.28],[378.76,352.28],[378.76,362.78],[294.76,362.78]]},{"text":"妇，育有一女，白头偕老。","box":[[70.56,369.32],[196.56,369.32],[196.56,379.82],[70.56,379.82]]},{"text":"胡志涛校长对袁淑娥参加学校的批斗会十分不满，为此曾质问","box":[[91.58,386.38],[378.4400000000001,386.38],[378.4400000000001,396.88],[91.58,396.88]]},{"text":"过工作组。","box":[[70.56,403.36],[123.06,403.36],[123.06,413.86],[70.56,413.86]]},{"text":"她表示，","box":[[120.78150000000001,403.36],[162.7815,403.36],[162.7815,413.86],[120.78150000000001,413.86]]},{"text":"卞仲耘可能有这样那样的问题，","box":[[160.503,403.36],[307.503,403.36],[307.503,413.86],[160.503,413.86]]},{"text":"但生活上没","box":[[305.2245,403.36],[357.7245,403.36],[357.7245,413.86],[305.2245,413.86]]},{"text":"有问","box":[[357.76,403.36],[378.76,403.36],[378.76,413.86],[357.76,413.86]]},{"text":"题。","box":[[70.56,420.4],[91.56,420.4],[91.56,430.9],[70.56,430.9]]},{"text":"反对工作组的老师和学生则认为，","box":[[89.28150000000001,420.4],[246.7815,420.4],[246.7815,430.9],[89.28150000000001,430.9]]},{"text":"让校外身份不明、","box":[[244.503,420.4],[328.503,420.4],[328.503,430.9],[244.503,430.9]]},{"text":"政治面目不","box":[[326.2245,420.4],[378.7245,420.4],[378.7245,430.9],[326.2245,430.9]]},{"text":"清的袁淑娥几乎独占了女附中革命群众发言的讲台，而李黎黎等同","box":[[70.56,437.44],[378.3150000000001,437.44],[378.3150000000001,447.94],[70.56,447.94]]},{"text":"学的发言则数次被强行制止，","box":[[70.56,454.48],[207.06,454.48],[207.06,464.98],[70.56,464.98]]},{"text":"迫使李黎黎等退出会场。","box":[[203.637,454.48],[319.137,454.48],[319.137,464.98],[203.637,464.98]]},{"text":"工作组迅速把","box":[[315.714,454.48],[378.714,454.48],[378.714,464.98],[315.714,464.98]]},{"text":"斗争矛头转向卞仲耘是为了掩盖斗争群众的罪行，欺骗群众。","box":[[70.56,471.46000000000004],[354.06,471.46000000000004],[354.06,481.96000000000004],[70.56,481.96000000000004]]},{"text":"1979","box":[[91.58,494.5],[112.58,494.5],[112.58,505],[91.58,505]]},{"text":"","box":[[112.58,494.5],[116.3,494.5],[116.3,494.5],[112.58,494.5]]},{"text":"年","box":[[116.3,494.5],[126.8,494.5],[126.8,505],[116.3,505]]},{"text":"，","box":[[126.8,494.5],[137.3,494.5],[137.3,505],[126.8,505]]},{"text":"袁淑娥被北京市公安局西城分局以挟嫌报复罪逮捕","box":[[137.3,494.5],[368.237,494.5],[368.237,505],[137.3,505]]},{"text":"，","box":[[368.26,494.5],[378.76,494.5],[378.76,505],[368.26,505]]},{"text":"后西城区检察院认为袁借文化大革命捏造事实","box":[[70.56,511.53999999999996],[280.56,511.53999999999996],[280.56,522.04],[70.56,522.04]]},{"text":"、","box":[[280.6,511.53999999999996],[291.1,511.53999999999996],[291.1,522.04],[280.6,522.04]]},{"text":"诽谤他人","box":[[287.68,511.53999999999996],[329.68,511.53999999999996],[329.68,522.04],[287.68,522.04]]},{"text":"，","box":[[329.68,511.53999999999996],[340.18,511.53999999999996],[340.18,522.04],[329.68,522.04]]},{"text":"已构成诽","box":[[336.76,511.53999999999996],[378.76,511.53999999999996],[378.76,522.04],[336.76,522.04]]},{"text":"谤罪","box":[[70.56,528.52],[91.68,528.52],[91.68,539.02],[70.56,539.02]]},{"text":"，","box":[[91.82,528.52],[102.32,528.52],[102.32,539.02],[91.82,539.02]]},{"text":"但已过追诉时效","box":[[102.44,528.52],[176.63299999999998,528.52],[176.63299999999998,539.02],[102.44,539.02]]},{"text":"，","box":[[176.9,528.52],[187.4,528.52],[187.4,539.02],[176.9,539.02]]},{"text":"决定不予起诉。卞校长丈夫王晶垚先生不","box":[[187.52,528.52],[378.49400000000014,528.52],[378.49400000000014,539.02],[187.52,539.02]]},{"text":"服","box":[[70.56,545.56],[81.06,545.56],[81.06,556.06],[70.56,556.06]]},{"text":"，","box":[[81.06,545.56],[91.56,545.56],[91.56,556.06],[81.06,556.06]]},{"text":"多方奔走申诉","box":[[89.24,545.56],[152.17700000000002,545.56],[152.17700000000002,556.06],[89.24,556.06]]},{"text":"，","box":[[152.18,545.56],[162.68,545.56],[162.68,556.06],[152.18,556.06]]},{"text":"同时也获得很多支持","box":[[160.34,545.56],[254.77700000000002,545.56],[254.77700000000002,556.06],[160.34,556.06]]},{"text":"，","box":[[254.8,545.56],[265.3,545.56],[265.3,556.06],[254.8,556.06]]},{"text":"但始终没有结果。","box":[[262.96,545.56],[346.897,545.56],[346.897,556.06],[262.96,556.06]]},{"text":"1989","box":[[344.56,545.56],[365.56,545.56],[365.56,556.06],[344.56,556.06]]},{"text":"","box":[[365.56,545.56],[368.26,545.56],[368.26,545.56],[365.56,545.56]]},{"text":"年","box":[[368.26,545.56],[378.76,545.56],[378.76,556.06],[368.26,556.06]]},{"text":"全国最高检察院明确表态","box":[[70.56,562.6],[186.06,562.6],[186.06,573.1],[70.56,573.1]]},{"text":"，","box":[[186.08,562.6],[196.58,562.6],[196.58,573.1],[186.08,573.1]]},{"text":"支持西城区","bo