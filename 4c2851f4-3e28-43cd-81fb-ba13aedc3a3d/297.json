.66,454.48],[343.66,454.48],[340.097,454.48]]},{"text":"1","box":[[343.66,454.48],[348.91,454.48],[348.91,464.98],[343.66,464.98]]},{"text":"996","box":[[348.94,454.48],[364.63000000000005,454.48],[364.63000000000005,464.98],[348.94,464.98]]},{"text":"","box":[[364.63000000000005,454.48],[368.26,454.48],[368.26,454.48],[364.63000000000005,454.48]]},{"text":"年","box":[[368.26,454.48],[378.76,454.48],[378.76,464.98],[368.26,464.98]]},{"text":"胡校长的逝世周年座谈会","box":[[70.56,471.46000000000004],[186.06,471.46000000000004],[186.06,481.96000000000004],[70.56,481.96000000000004]]},{"text":"，","box":[[186.08,471.46000000000004],[196.58,471.46000000000004],[196.58,481.96000000000004],[186.08,481.96000000000004]]},{"text":"从","box":[[194.06,471.46000000000004],[204.56,471.46000000000004],[204.56,481.96000000000004],[194.06,481.96000000000004]]},{"text":"","box":[[204.56,471.46000000000004],[207.2,471.46000000000004],[207.2,471.46000000000004],[204.56,471.46000000000004]]},{"text":"2","box":[[207.2,471.46000000000004],[212.45,471.46000000000004],[212.45,481.96000000000004],[207.2,481.96000000000004]]},{"text":"002","box":[[212.42,471.46000000000004],[228.17,471.46000000000004],[228.17,481.96000000000004],[212.42,481.96000000000004]]},{"text":"","box":[[228.17,471.46000000000004],[230.78,471.46000000000004],[230.78,471.46000000000004],[228.17,471.46000000000004]]},{"text":"年开始牵头调查８","box":[[230.78,471.46000000000004],[314.78,471.46000000000004],[314.78,481.96000000000004],[230.78,481.96000000000004]]},{"text":"-","box":[[314.8,471.46000000000004],[318.29650000000004,471.46000000000004],[318.29650000000004,481.96000000000004],[314.8,481.96000000000004]]},{"text":"５事件","box":[[318.28,471.46000000000004],[349.78,471.46000000000004],[349.78,481.96000000000004],[318.28,481.96000000000004]]},{"text":"，","box":[[349.78,471.46000000000004],[360.28,471.46000000000004],[360.28,481.96000000000004],[349.78,481.96000000000004]]},{"text":"走访","box":[[357.76,471.46000000000004],[378.76,471.460000