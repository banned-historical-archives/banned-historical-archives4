354976],[174.91540191801602,255.577948354976],[174.91540191801602,265.281452354976],[65.26577760750399,265.281452354976]]},{"text":"史克也在大会上，","box":[[170.16068495801602,255.577948354976],[249.82648190852797,255.577948354976],[249.82648190852797,265.281452354976],[170.16068495801602,265.281452354976]]},{"text":"我都上当了。","box":[[245.07174554152002,255.577948354976],[304.74833395553594,255.577948354976],[304.74833395553594,265.281452354976],[245.07174554152002,265.281452354976]]},{"text":"”","box":[[299.993616995536,255.577948354976],[309.697120995536,255.577948354976],[309.697120995536,265.281452354976],[299.993616995536,265.281452354976]]},{"text":"周恩来详细叙述了","box":[[85.25503466152,270.812449634976],[164.920841315536,270.812449634976],[164.920841315536,280.51595363497603],[85.25503466152,280.51595363497603]]},{"text":"","box":[[164.92084131553602,270.812449634976],[165.140834315536,270.812449634976],[165.140834315536,270.812449634976],[164.92084131553602,270.812449634976]]},{"text":"１","box":[[167.05554427100802,270.812439931472],[176.75904827100803,270.812439931472],[176.75904827100803,280.515943931472],[167.05554427100802,280.515943931472]]},{"text":"９","box":[[171.90729627100802,270.812439931472],[181.61080027100803,270.812439931472],[181.61080027100803,280.515943931472],[171.90729627100802,280.515943931472]]},{"text":"６","box":[[176.75904827100803,270.812439931472],[186.46255227100804,270.812439931472],[186.46255227100804,280.515943931472],[176.75904827100803,280.515943931472]]},{"text":"７","box":[[181.61080027100803,270.812439931472],[191.31430427100804,270.812439931472],[191.31430427100804,280.515943931472],[181.61080027100803,280.515943931472]]},{"text":"年","box":[[189.47062880750403,270.812439931472],[199.17413280750404,270.812439931472],[199.17413280750404,280.515943931472],[189.47062880750403,280.515943931472]]},{"text":"","box":[[199.17413280750404,270.812439931472],[199.39413180750404,270.812439931472],[199.39413180750404,270.812439931472],[199.17413280750404,270.812439931472]]},{"text":"８","box":[[201.308893984,270.812439931472],[211.01239798400002,270.812439931472],[211.01239798400002,280.515943931472],[201.308893984,280.515943931472]]},{"text":"月","box":[[209.16873222400002,270.812439931472],[218.87223622400003,270.812439931472],[218.87223622400003,280.515943931472],[209.16873222400002,280.515943931472]]},{"text":"","box":[[218.87223622400003,270.812439931472],[219.09223722400003,270.812439931472],[219.09223722400003,270.812439931472],[218.87223622400003,270.812439931472]]},{"text":"１","box":[[221.00701680750402,270.812439931472],[230.71052080750403,270.812439931472],[230.71052080750403,280.515943931472],[221.00701680750402,280.515943931472]]},{"text":"１","box":[[225.858788214512,270.812439931472],[235.562292214512,270.812439931472],[235.562292214512,280.515943931472],[225.858788214512,280.515943931472]]},{"text":"日大会的情况，","box":[[233.71861675100803,270.812439931472],[303.389814285024,270.812439931472],[303.389814285024,280.515943931472],[233.71861675100803,280.515943931472]]},{"text":"接着说：","box":[[298.635058511008,270.812439931472],[338.32239957451196,270.812439931472],[338.32239957451196,280.515943931472],[298.635058511008,280.515943931472]]},{"text":"“","box":[[85.255005551008,286.144034472496],[94.95850955100799,286.144034472496],[94.95850955100799,295.847538472496],[85.255005551008,295.847538472496]]},{"text":"今年五一，","box":[[91.174152694512,286.144034472496],[140.85609317451204,286.144034472496],[140.85609317451204,295.847538472496],[91.174152694512,295.847538472496]]},{"text":"当陈毅出现在天安门的城楼上，","box":[[136.10138591801604,286.144034472496],[275.9289076685279,286.144034472496],[275.9289076685279,295.847538472496],[136.10138591801604,295.847538472496]]},{"text":"美国的情报机构大吃","box":[[271.368270492032,286.144034472496],[362.581305127072,286.144034472496],[362.581305127072,295.847538472496],[271.368270492032,295.847538472496]]},{"text":"一惊。陈毅有病，","box":[[65.26582612502409,301.37849693848],[144.83457833203215,301.37849693848],[144.83457833203215,311.08200093848],[65.26582612502409,311.08200093848]]},{"text":"他是从医院去的天安门，","box":[[140.0798516685281,301.37849693848],[250.11760643553598,301.37849693848],[250.11760643553598,311.08200093848],[140.0798516685281,311.08200093848]]},{"text":"他是新一届的中央委员，","box":[[245.55696925904007,301.37849693848],[357.3413450425441,301.37849693848],[357.3413450425441,311.08200093848],[245.55696925904007,311.08200093848]]},{"text":"中","box":[[352.8777429060481,301.37849693848],[362.58124690604814,301.37849693848],[362.58124690604814,311.08200093848],[352.8777429060481,311.08200093848]]},{"text":"央军委副主席。不光是美国的","box":[[65.26583582852811,316.61299821848],[194.80767244955211,316.61299821848],[194.80767244955211,326.31650221848],[65.26583582852811,326.31650221848]]},{"text":"","box":[[194.80767244955214,316.61299821848],[195.08766044955212,316.61299821848],[195.08766044955212,316.61299821848],[194.80767244955214,316.61299821848]]},{"text":"Ｃ","box":[[197.524537127504,316.613007921984],[207.228041127504,316.613007921984],[207.228041127504,326.316511921984],[197.524537127504,326.316511921984]]},{"text":"Ｉ","box":[[203.443684271008,316.613007921984],[213.14718827100802,316.613007921984],[213.14718827100802,326.316511921984],[203.443684271008,326.316511921984]]},{"text":"Ａ","box":[[206.64585999801602,316.613007921984],[216.34936399801603,316.613007921984],[216.34936399801603,326.316511921984],[206.64585999801602,326.316511921984]]},{"text":"摸不清，","box":[[215.767134351008,316.613007921984],[255.45447541451196,316.613007921984],[255.45447541451196,326.316511921984],[215.767134351008,326.316511921984]]},{"text":"蒋介石的情报组织也摸不","box":[[250.99085387100797,316.613007921984],[362.5812857200638,316.613007921984],[362.5812857200638,326.316511921984],[250.99085387100797,326.316511921984]]},{"text":"清。当蒋介石得知陈毅上了天安门，","box":[[65.26576790399992,331.847528608992],[224.79141247801584,331.847528608992],[224.79141247801584,341.551032608992],[65.26576790399992,341.551032608992]]},{"text":"他把他的情报组织大骂了一通。","box":[[220.03671492502392,331.847528608992],[359.67015689203174,331.847528608992],[359.67015689203174,341.551032608992],[220.03671492502392,341.551032608992]]},{"text":"据说，","box":[[85.25504436502382,347.179045521984],[114.94777630852784,347.179045521984],[114.94777630852784,356.882549521984],[85.25504436502382,356.882549521984]]},{"text":"造反派里还有人企图抓我，","box":[[110.19305934852783,347.179045521984],[229.93434722604778,347.179045521984],[229.93434722604778,356.882549521984],[110.19305934852783,356.882549521984]]},{"text":"我不信。他们连陈毅都没抓成，","box":[[225.37367123553585,347.179045521984],[367.62704957903986,347.179045521984],[367.62704957903986,356.882549521984],[225.37367123553585,356.882549521984]]},{"text":"还想抓我？当然不行，","box":[[65.26585523553587,362.413546801984],[166.5704369955359,362.413546801984],[166.5704369955359,372.117050801984],[65.26585523553587,372.117050801984]]},{"text":"那里解放军很多。这可能是吹牛，","box":[[162.4949847225439,362.413546801984],[319.88580019553575,362.413546801984],[319.88580019553575,372.117050801984],[162.4949847225439,372.117050801984]]},{"text":"瞎说罢了。","box":[[315.90738296254386,362.413546801984],[367.62703987553584,362.413546801984],[367.62703987553584,372.117050801984],[315.90738296254386,372.117050801984]]},{"text":"在这光天化日之下，","box":[[65.26584553203185,377.648086896],[154.92626130604788,377.648086896],[154.92626130604788,387.351590896],[65.26584553203185,387.351590896]]},{"text":"他们不敢露头。只有在夜里，","box":[[150.17152493903987,377.648086896],[280.6836634425438,377.648086896],[280.6836634425438,387.351590896],[150.17152493903987,387.351590896]]},{"text":"在无人之处，","box":[[276.12304567305586,377.648086896],[336.76992626604783,377.648086896],[336.76992626604783,387.351590896],[276.12304567305586,387.351590896]]},{"text":"他们才","box":[[332.30632412955185,377.648086896],[362.5812760165599,377.648086896],[362.5812760165599,387.351590896],[332.30632412955185,387.351590896]]},{"text":"会搞阴谋。外文出版社就有这种事情发生。去年才发现有几个好的","box":[[65.26586493903989,392.882588176],[362.5813730515998,392.882588176],[362.5813730515998,402.586092176],[65.26586493903989,402.586092176]]},{"text":"同志被暗杀了。","box":[[65.26593286356791,408.21412449599995],[134.93710128707198,408.21412449599995],[134.93710128707198,417.91762849599996],[65.26593286356791,417.91762849599996]]},{"text":"８","box":[[84.67277590400002,423.448606368992],[94.37627990400001,423.448606368992],[94.37627990400001,433.152110368992],[84.67277590400002,433.152110368992]]},{"text":"月","box":[[92.53261414400001,423.448606368992],[102.236118144,423.448606368992],[102.236118144,433.152110368992],[92.53261414400001,433.152110368992]]},{"text":"","box":[[102.236118144,423.448606368992],[102.456119144,423.448606368992],[102.456119144,423.448606368992],[102.236118144,423.448606368992]]},{"text":"７","box":[[104.370898727504,423.448606368992],[114.074402727504,423.448606368992],[114.074402727504,433.152110368992],[104.370898727504,433.152110368992]]},{"text":"日王力大放厥词，","box":[[112.23072726400001,423.448606368992],[193.64314523100796,423.448606368992],[193.64314523100796,433.152110368992],[112.23072726400001,433.152110368992]]},{"text":"支部书记要当外交部长，","box":[[189.47063851100802,423.448606368992],[304.06903045451196,423.448606368992],[304.06903045451196,433.152110368992],[189.47063851100802,433.152110368992]]},{"text":"姚登山要当外","box":[[299.993539367504,423.448606368992],[362.581140167504,423.448606368992],[362.581140167504,433.152110368992],[299.993539367504,433.152110368992]]},{"text":"长，","box":[[65.26576790399997,438.68310764899195],[84.96388102399999,438.68310764899195],[84.96388102399999,448.38661164899196],[65.26576790399997,448.38661164899196]]},{"text":"王力要当总理或副总理，","box":[[80.20916406399998,438.68310764899195],[191.41136842152002,438.68310764899195],[191.41136842152002,448.38661164899196],[80.20916406399998,448.38661164899196]]},{"text":"因为他后面有人要当总理。这事一直发","box":[[187.044781918016,438.68310764899195],[362.5812469060479,438.68310764899195],[362.5812469060479,448.38661164899196],[187.044781918016,448.38661164899196]]},{"text":"展下来，","box":[[65.265835828528,453.917628336],[104.95316718852803,453.917628336],[104.95316718852803,463.621132336],[65.265835828528,463.621132336]]},{"text":"主席发现了这事。姚文元发表评陶铸的两本书，","box":[[100.19845993203202,453.917628336],[315.32521153655995,453.917628336],[315.32521153655995,463.621132336],[100.19845993203202,463.621132336]]},{"text":"指出他们揪","box":[[310.95862503305614,453.917628336],[362.5813051270721,453.917628336],[362.5813051270721,463.621132336],[310.95862503305614,463.621132336]]},{"text":"军内一小撮是错误的。军内虽然有坏人，","box":[[65.26589404955212,469.15212961599997],[244.78074716006407,469.15212961599997],[244.78074716006407,478.855633616],[65.26589404955212,478.855633616]]},{"text":"但不能乱军，","box":[[240.0260496070721,469.15212961599997],[299.70260891057603,469.15212961599997],[299.70260891057603,478.855633616],[240.0260496070721,478.855633616]]},{"text":"那就不好了。","box":[[294.94787254356805,469.15212961599997],[354.62448036459205,469.15212961599997],[354.62448036459205,478.855633616],[294.94787254356805,478.855633616]]},{"text":"军队是个集体力量，","box":[[85.25516080707206,484.483646528992],[175.30371674108807,484.483646528992],[175.30371674108807,494.187150528992],[85.25516080707206,494.187150528992]]},{"text":"党领导军队，","box":[[170.93713023758406,484.483646528992],[232.16625018108806,484.483646528992],[232.16625018108806,494.187150528992],[170.93713023758406,494.187150528992]]},{"text":"进行抗日战争，","box":[[227.79968308459212,484.483646528992],[299.7997118751041,484.483646528992],[299.7997118751041,494.187150528992],[227.79968308459212,494.187150528992]]},{"text":"土地革命，","box":[[295.53016041160015,484.483646528992],[346.6675779740801,484.483646528992],[346.6675779740801,494.187150528992],[295.53016041160015,494.187150528992]]},{"text":"解放","box":[[342.39808473160014,484.483646528992],[362.58139245860815,484.483646528992],[362.58139245860815,494.187150528992],[342.39808473160014,494.187150528992]]},{"text":"战争，","box":[[65.26598138108812,499.718167216],[94.95870362108815,499.718167216],[94.95870362108815,509.421671216],[65.26598138108812,509.421671216]]},{"text":"抗美援朝，","box":[[90.20399636459214,499.718167216],[139.88593684459218,499.718167216],[139.88593684459218,509.421671216],[90.20399636459214,509.421671216]]},{"text":"在文化革命中，","box":[[135.13122958809618,499.718167216],[204.80239801160013,499.718167216],[204.80239801160013,509.421671216],[135.13122958809618,509.421671216]]},{"text":"军队保卫文化革命，","box":[[200.04770045860818,499.718167216],[290.87247849160013,499.718167216],[290.87247849160013,509.421671216],[200.04770045860818,509.421671216]]},{"text":"不管那些错误，","box":[[286.31186072211216,499.718167216],[357.34151000211216,499.718167216],[357.34151000211216,509.421671216],[286.31186072211216,509.421671216]]},{"text":"但","box":[[352.8779078656162,499.718167216],[362.5814118656162,499.718167216],[362.5814118656162,509.421671216],[352.8779078656162,509.421671216]]},{"text":"是保卫了文化革命。军队的“","box":[[65.26600078809616,514.952668496],[194.71077325860819,514.952668496],[194.71077325860819,524.656172496],[65.26600078809616,524.656172496]]},{"text":"五不”","box":[[190.92639699510417,514.952668496],[220.61913864211215,514.952668496],[220.61913864211215,524.656172496],[190.92639699510417,524.656172496]]},{"text":"就是从三大纪律八大注意里来的，","box":[[216.83476237860816,514.952668496],[367.627233945616,514.952668496],[367.627233945616,524.656172496],[216.83476237860816,524.656172496]]},{"text":"有纪律的军队，","box":[[65.26603960211207,530.1871794795039],[134.93720802561614,530.1871794795039],[134.93720802561614,539.8906834795039],[65.26603960211207,539.8906834795039]]},{"text":"说不打就不打，","box":[[130.18249106561612,530.1871794795039],[201.6002999126241,530.1871794795039],[201.6002999126241,539.8906834795039],[130.18249106561612,539.8906834795039]]},{"text":"人家骂就骂，","box":[[197.23373281612817,530.1871794795039],[258.4628721666402,530.1871794795039],[258.4628721666402,539.8906834795039],[197.23373281612817,539.8906834795039]]},{"text":"还有被打死的，","box":[[254.09626625612822,530.1871794795039],[325.7081548866402,530.1871794795039],[325.7081548866402,539.8906834795039],[254.09626625612822,539.8906834795039]]},{"text":"这样还是","box":[[321.43862283014425,530.1871794795039],[362.5814991971522,530.1871794795039],[362.5814991971522,539.8906834795039],[321.43862283014425,539.8906834795039]]},{"text":"保卫文化大革命，","box":[[65.26608811963223,545.518715799504],[146.48440689612823,545.518715799504],[146.48440689612823,555.2222197995039],[65.26608811963223,555.2222197995039]]},{"text":"这样的军队在世界上还找不出第二个，","box":[[142.11783009612822,545.518715799504],[316.1016277056162,545.518715799504],[316.1016277056162,555.2222197995039],[142.11783009612822,555.2222197995039]]},{"text":"当然希望有","box":[[311.73506060912035,545.518715799504],[362.58147008664037,545.518715799504],[362.58147008664037,555.2222197995039],[311.73506060912035,555.2222197995039]]}]}