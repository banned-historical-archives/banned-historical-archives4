221847997]]},{"text":"应该进行一次思想和政治路线方面的教育。周、","box":[[150.94781496254413,316.61299821847996],[367.627117503568,316.61299821847996],[367.627117503568,326.31650221847997],[150.94781496254413,326.31650221847997]]},{"text":"康阅，","box":[[65.26592316006412,331.84751890548796],[94.95864540006414,331.84751890548796],[94.95864540006414,341.55102290548797],[65.26592316006412,341.55102290548797]]},{"text":"退林办。","box":[[90.20393814356814,331.84751890548796],[129.89126950356817,331.84751890548796],[129.89126950356817,341.55102290548797],[90.20393814356814,341.55102290548797]]},{"text":"”","box":[[125.13655254356816,331.84751890548796],[134.84005654356815,331.84751890548796],[134.84005654356815,341.55102290548797],[125.13655254356816,341.55102290548797]]},{"text":"当天下午，","box":[[85.25514140006416,347.17903581848],[134.9370915835682,347.17903581848],[134.9370915835682,356.88253981848],[85.25514140006416,356.88253981848]]},{"text":"毛泽东在人民大会堂会见巴基斯坦海军司令哈桑。他","box":[[130.1823746235682,347.17903581848],[362.581343941088,347.17903581848],[362.581343941088,356.88253981848],[130.1823746235682,356.88253981848]]},{"text":"说：","box":[[65.26593286356814,362.41353709848],[84.96404598356816,362.41353709848],[84.96404598356816,372.11704109848],[65.26593286356814,372.11704109848]]},{"text":"“","box":[[80.20932902356815,362.41353709848],[89.91283302356814,362.41353709848],[89.91283302356814,372.11704109848],[80.20932902356815,372.11704109848]]},{"text":"有人说我们修地道花钱太多了，","box":[[86.12847616707215,362.41353709848],[227.50856826108816,362.41353709848],[227.50856826108816,372.11704109848],[86.12847616707215,372.11704109848]]},{"text":"我们根本没有花钱，","box":[[223.2390362045922,362.41353709848],[315.6164233951042,362.41353709848],[315.6164233951042,372.11704109848],[223.2390362045922,372.11704109848]]},{"text":"国家预算没","box":[[311.3468428210883,362.41353709848],[362.5814215691202,362.41353709848],[362.5814215691202,372.11704109848],[311.3468428210883,372.11704109848]]},{"text":"有这笔钱，","box":[[65.26601049160024