174300794]]},{"text":"输不到自己头上”","box":[[110.19306905203253,453.91764774300793],[189.85884659553648,453.91764774300793],[189.85884659553648,463.62115174300794],[110.19306905203253,463.62115174300794]]},{"text":"。","box":[[186.07448973904053,453.91764774300793],[195.77799373904054,453.91764774300793],[195.77799373904054,463.62115174300794],[186.07448973904053,463.62115174300794]]},{"text":"这又引起一阵哄笑。","box":[[85.25508317904053,469.15214902300795],[174.9154892495525,469.15214902300795],[174.9154892495525,478.85565302300796],[85.25508317904053,478.85565302300796]]},{"text":"基辛格又挖苦洛德说，","box":[[85.25510258604852,484.4836659359999],[184.9101080730565,484.4836659359999],[184.9101080730565,494.1871699359999],[85.25510258604852,494.1871699359999]]},{"text":"“","box":[[180.15539111305657,484.4836659359999],[189.85889511305658,484.4836659359999],[189.85889511305658,494.1871699359999],[180.15539111305657,494.1871699359999]]},{"text":"洛德先生这次随尼克松总统访华，","box":[[186.07453825656057,484.4836659359999],[337.0610993105764,484.4836659359999],[337.0610993105764,494.1871699359999],[186.07453825656057,494.1871699359999]]},{"text":"把头发","box":[[332.50044272707254,484.4836659359999],[362.5813245340806,484.4836659359999],[362.5813245340806,494.1871699359999],[332.50044272707254,494.1871699359999]]},{"text":"都剪短了，","box":[[65.26591345656055,499.7181866230079],[114.85081889656058,499.7181866230079],[114.85081889656058,509.4216906230079],[65.26591345656055,509.4216906230079]]},{"text":"以便给中方一个好印象”","box":[[110.09610193656056,499.7181866230079],[219.7457359505765,499.7181866230079],[219.7457359505765,509.4216906230079],[110.09610193656056,509.4216906230079]]},{"text":"。其实这是一句反话，","box":[[215.96137909408057,499.7181866230079],[316.68371180006443,499.7181866230079],[316.68371180006443,509.4216906230079],[215.96137909408057,509.4216906230079]]},{"text":"因为在美国","box":[[312.12311343758455,499.7181866230079],[362.58137305160056,499.7181866230079],[362.58137305160056,509.4216906230079],[312.1231134375845