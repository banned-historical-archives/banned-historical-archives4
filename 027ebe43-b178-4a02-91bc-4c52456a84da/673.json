.62115174300794],[65.26617545116824,463.62115174300794]]},{"text":"”","box":[[100.19878985116826,453.91764774300793],[109.90229385116825,453.91764774300793],[109.90229385116825,463.62115174300794],[100.19878985116826,463.62115174300794]]},{"text":"当张玉凤回答他是","box":[[85.25539369116825,469.15214902300795],[164.92120034518425,469.15214902300795],[164.92120034518425,478.85565302300796],[85.25539369116825,478.85565302300796]]},{"text":"","box":[[164.92120034518427,469.15214902300795],[165.17115634518424,469.15214902300795],[165.17115634518424,469.15214902300795],[164.92120034518427,469.15214902300795]]},{"text":"１","box":[[167.34664939100801,469.152119912496],[177.05015339100802,469.152119912496],[177.05015339100802,478.855623912496],[167.34664939100801,478.855623912496]]},{"text":"点","box":[[175.691653127504,469.152119912496],[185.395157127504,469.152119912496],[185.395157127504,478.855623912496],[175.691653127504,478.855623912496]]},{"text":"","box":[[185.395157127504,469.152119912496],[185.66515712750402,469.152119912496],[185.66515712750402,469.152119912496],[185.395157127504,469.152119912496]]},{"text":"３","box":[[188.015103207504,469.152119912496],[197.71860720750402,469.152119912496],[197.71860720750402,478.855623912496],[188.015103207504,478.855623912496]]},{"text":"０","box":[[192.86685520750402,469.152119912496],[202.57035920750403,469.152119912496],[202.57035920750403,478.855623912496],[192.86685520750402,478.855623912496]]},{"text":"分的时候，","box":[[201.211878351008,469.152119912496],[252.25231909451196,469.152119912496],[252.25231909451196,478.855623912496],[201.211878351008,478.855623912496]]},{"text":"毛泽东拍打了一下沙发的","box":[[248.07981237451202,469.152119912496],[362.581198388528,469.152119912496],[362.581198388528,478.855623912496],[248.07981237451202,478.855623912496]]},{"text":"扶手：","box":[[65.26578731100801,484.483636825488],[94.95850955100804,484.483636825488],[94.95850955100804,494.18714082548803],[65.26578731100801,494.18714082548803]]},{"text":"“","box":[[