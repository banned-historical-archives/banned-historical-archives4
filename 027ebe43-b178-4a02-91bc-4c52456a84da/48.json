73110081,362.41354680198395],[362.5811886850241,362.41354680198395],[362.5811886850241,372.11705080198396],[332.1121473110081,372.11705080198396]]},{"text":"级部长以上和保卫、","box":[[65.26577760750405,377.648086896],[155.3143238380161,377.648086896],[155.3143238380161,387.351590896],[65.26577760750405,387.351590896]]},{"text":"作战部门配用。保卫部除干部配用外，","box":[[150.94773733451208,377.648086896],[325.70790259553604,377.648086896],[325.70790259553604,387.351590896],[150.94773733451208,387.351590896]]},{"text":"还有一部","box":[[321.43837053904014,377.648086896],[362.5812469060481,377.648086896],[362.5812469060481,387.351590896],[321.43837053904014,387.351590896]]},{"text":"分枪支存在保卫部保密室的保险柜中，","box":[[65.26583582852811,392.88258817599996],[239.0555730615201,392.88258817599996],[239.0555730615201,402.58609217599997],[65.26583582852811,402.58609217599997]]},{"text":"而且枪弹混存，","box":[[234.68898655801618,392.88258817599996],[305.81568058152016,392.88258817599996],[305.81568058152016,402.58609217599997],[234.68898655801618,402.58609217599997]]},{"text":"由秘书袁孔祥","box":[[301.44909407801623,392.88258817599996],[362.58122749904027,392.88258817599996],[362.58122749904027,402.58609217599997],[301.44909407801623,402.58609217599997]]},{"text":"负责保管。景儒林没有给袁孔祥布置清查枪支的任务。","box":[[65.2657873110083,408.21412449599995],[308.7267220780162,408.21412449599995],[308.7267220780162,417.91762849599996],[65.2657873110083,417.91762849599996]]},{"text":"３","box":[[308.629687038016,408.214114792496],[318.333191038016,408.214114792496],[318.333191038016,417.917618792496],[308.629687038016,417.917618792496]]},{"text":"２","box":[[313.481439038016,408.214114792496],[323.184943038016,408.214114792496],[323.184943038016,417.917618792496],[313.481439038016,417.917618792496]]},{"text":"号院血案","box":[[321.72939803100803,408.214114792496],[362.581188685024,408.214114792496],[362.581188685024,417.917618792496],[321.72939803100803,417.917618792496]]},{"text":"发生后多日，","box":[[65.26577760750399,423.44861607249595],[125.91267760750404,423.44861607249595],[125.91267760750404,433.15212007249596],[65.26577760750399,433.15212007249596]]},{"text":"保卫部人员整天忙忙碌碌地去检查别人，","box":[[121.546100807504,423.44861607249595],[305.81561265699196,423.44861607249595],[305.81561265699196,433.15212007249596],[121.546100807504,433.15212007249596]]},{"text":"却偏偏没有检","box":[[301.44902615348815,423.44861607249595],[362.5811595745122,423.44861607249595],[362.5811595745122,433.15212007249596],[301.44902615348815,433.15212007249596]]},{"text":"查自己的枪。也许他们认为，","box":[[65.26571938648021,438.68311735249597],[194.8075366004962,438.68311735249597],[194.8075366004962,448.386621352496],[65.26571938648021,448.386621352496]]},{"text":"我们保卫部的枪难道还会出问题？","box":[[190.05281964049624,438.68311735249597],[339.6808610240001,438.68311735249597],[339.6808610240001,448.386621352496],[190.05281964049624,448.386621352496]]},{"text":"然而，","box":[[85.25498614400018,453.91763803950397],[114.9477180875042,453.91763803950397],[114.9477180875042,463.621142039504],[85.25498614400018,463.621142039504]]},{"text":"意料不到的事还真发生了：","box":[[110.19300112750419,453.91763803950397],[229.83723455801612,453.91763803950397],[229.83723455801612,463.621142039504],[110.19300112750419,463.621142039504]]},{"text":"在全军区检验枪支后的第","box":[[225.1795526380162,453.91763803950397],[337.3521365060483,453.91763803950397],[337.3521365060483,463.621142039504],[225.1795526380162,463.621142039504]]},{"text":"","box":[[337.3521365060483,453.91763803950397],[337.60212350604826,453.91763803950397],[337.60212350604826,453.91763803950397],[337.3521365060483,453.91763803950397]]},{"text":"９","box":[[339.77788636049604,453.917628336],[349.48139036049605,453.917628336],[349.48139036049605,463.621132336],[339.77788636049604,463.621132336]]},{"text":"天，","box":[[347.92886853451205,453.917628336],[367.62697195100804,453.917628336],[367.62697195100804,463.621132336],[347.92886853451205,463.621132336]]},{"text":"１","box":[[64.683557664,469.152119912496],[74.387061664,469.152119912496],[74.387061664,478.855623912496],[64.683557664,478.855623912496]]},{"text":"２","box":[[69.53530966400001,469.152119912496],[79.238813664,469.152119912496],[79.238813664,478.855623912496],[69.53530966400001,478.855623912496]]},{"text":"月","box":[[77.395147904,469.152119912496],[87.098651904,469.152119912496],[87.098651904,478.855623912496],[77.395147904,478.855623912496]]},{"text":"","box":[[87.098651904,469.152119912496],[87.31865190399999,469.152119912496],[87.31865190399999,469.152119912496],[87.098651904,469.152119912496]]},{"text":"２","box":[[89.233422784,469.152119912496],[98.936926784,469.152119912496],[98.936926784,478.855623912496],[89.233422784,478.855623912496]]},{"text":"５","box":[[94.085174784,469.152119912496],[103.788678784,469.152119912496],[103.788678784,478.855623912496],[94.085174784,478.855623912496]]},{"text":"日，","box":[[101.945022727504,469.152119912496],[121.64313584750401,469.152119912496],[121.64313584750401,478.855623912496],[101.945022727504,478.855623912496]]},{"text":"当保卫部副部长王庆和要用枪时，","box":[[116.888418887504,469.152119912496],[269.233431687504,469.152119912496],[269.233431687504,478.855623912496],[116.888418887504,478.855623912496]]},{"text":"袁孔祥打开保险柜，","box":[[264.8668645910081,469.152119912496],[357.24429059553603,469.152119912496],[357.24429059553603,478.855623912496],[264.8668645910081,478.855623912496]]},{"text":"发","box":[[352.8777040920321,469.152119912496],[362.5812080920321,469.152119912496],[362.5812080920321,478.855623912496],[352.8777040920321,478.855623912496]]},{"text":"现锁在里面的","box":[[65.26579701451209,484.483636825488],[124.94234661451215,484.483636825488],[124.94234661451215,494.18714082548803],[65.26579701451209,494.18714082548803]]},{"text":"","box":[[124.94234661451212,484.483636825488],[125.16234461451211,484.483636825488],[125.16234461451211,484.483636825488],[124.94234661451212,484.483636825488]]},{"text":"５","box":[[127.077098087504,484.483636825488],[136.780602087504,484.483636825488],[136.780602087504,494.18714082548803],[127.077098087504,494.18714082548803]]},{"text":"９","box":[[131.928859791008,484.483636825488],[141.632363791008,484.483636825488],[141.632363791008,494.18714082548803],[131.928859791008,494.18714082548803]]},{"text":"式手枪竟少了两支，","box":[[139.78868832750402,484.483636825488],[229.83721515100797,484.483636825488],[229.83721515100797,494.18714082548803],[139.78868832750402,494.18714082548803]]},{"text":"并且还丢了","box":[[225.373593607504,484.483636825488],[276.21999338151994,484.483636825488],[276.21999338151994,494.18714082548803],[225.373593607504,494.18714082548803]]},{"text":"","box":[[276.21999338152,484.483636825488],[276.46999138152,484.483636825488],[276.46999138152,484.483636825488],[276.21999338152,484.483636825488]]},{"text":"２","box":[[278.64584997451203,484.483636825488],[288.34935397451204,484.483636825488],[288.34935397451204,494.18714082548803],[278.64584997451203,494.18714082548803]]},{"text":"０","box":[[283.497601974512,484.483636825488],[293.201105974512,484.483636825488],[293.201105974512,494.18714082548803],[283.497601974512,494.18714082548803]]},{"text":"发子弹！再把其","box":[[291.64853563100803,484.483636825488],[362.58116927801603,484.483636825488],[362.58116927801603,494.18714082548803],[291.64853563100803,494.18714082548803]]},{"text":"它柜子打开寻找，","box":[[65.26575820049607,499.718157512496],[144.93155515100813,499.718157512496],[144.93155515100813,509.42166151249603],[65.26575820049607,509.42166151249603]]},{"text":"仍无枪的踪影，","box":[[140.1768187840001,499.718157512496],[209.84800661451206,499.718157512496],[209.84800661451206,509.42166151249603],[140.1768187840001,509.42166151249603]]},{"text":"小袁被吓的五魂出壳。他想，","box":[[205.1903149910081,499.718157512496],[336.769829231008,499.718157512496],[336.769829231008,509.42166151249603],[205.1903149910081,509.42166151249603]]},{"text":"办公楼","box":[[332.30622709451205,499.718157512496],[362.5811789815201,499.718157512496],[362.5811789815201,509.42166151249603],[332.30622709451205,509.42166151249603]]},{"text":"门口有哨兵，","box":[[65.26576790400009,514.952658792496],[124.94232720750414,514.952658792496],[124.94232720750414,524.6561627924959],[65.26576790400009,524.6561627924959]]},{"text":"保密室装有子弹锁的铁门，","box":[[120.18761024750411,514.952658792496],[240.12295850152003,514.952658792496],[240.12295850152003,524.6561627924959],[120.18761024750411,524.6561627924959]]},{"text":"保险柜有密码，","box":[[235.56230191801612,514.952658792496],[306.20381103801606,514.952658792496],[306.20381103801606,524.6561627924959],[235.56230191801612,524.6561627924959]]},{"text":"只有他知道密","box":[[301.6431544545121,514.952658792496],[362.5812372025441,514.952658792496],[362.5812372025441,524.6561627924959],[301.6431544545121,524.6561627924959]]},{"text":"码才能打开保险柜，","box":[[65.26579701451215,530.187169776],[156.76983003100818,530.187169776],[156.76983003100818,539.890673776],[65.26579701451215,539.890673776]]},{"text":"保密室的门窗没有撬动痕迹，","box":[[152.40326293451218,530.187169776],[285.24419388049614,530.187169776],[285.24419388049614,539.890673776],[152.40326293451218,539.890673776]]},{"text":"保险柜的暗锁也完","box":[[280.8776267840003,530.187169776],[362.58117898152034,530.187169776],[362.58117898152034,539.890673776],[280.8776267840003,539.890673776]]},{"text":"好无损，","box":[[65.26573879348837,545.518706096],[104.9530701534884,545.518706096],[104.9530701534884,555.2222100959999],[65.26573879348837,555.2222100959999]]},{"text":"而里面的枪弹却不翼而飞。","box":[[100.19836289699239,545.518706096],[219.84260603100836,545.518706096],[219.84260603100836,555.2222100959999],[100.19836289699239,555.2222100959999]]}]}