74300806]]},{"text":"有人应了一声。","box":[[122.03180970022441,453.91764774300805],[191.70300723424037,453.91764774300805],[191.70300723424037,463.62115174300806],[122.03180970022441,463.62115174300806]]},{"text":"“","box":[[85.25554894723243,469.152149023008],[94.95905294723242,469.152149023008],[94.95905294723242,478.855653023008],[85.25554894723243,478.855653023008]]},{"text":"那好，","box":[[91.17469609073643,469.152149023008],[120.86741833073646,469.152149023008],[120.86741833073646,478.855653023008],[91.17469609073643,478.855653023008]]},{"text":"叫黄道霞来。","box":[[116.11271107424045,469.152149023008],[175.7892703777444,469.152149023008],[175.7892703777444,478.855653023008],[116.11271107424045,478.855653023008]]},{"text":"”","box":[[171.03455341774446,469.152149023008],[180.73805741774447,469.152149023008],[180.73805741774447,478.855653023008],[171.03455341774446,478.855653023008]]},{"text":"江青说。","box":[[176.95370056124847,469.152149023008],[216.64104162475243,469.152149023008],[216.64104162475243,478.855653023008],[176.95370056124847,478.855653023008]]},{"text":"可是，","box":[[85.25557805774444,484.483665936],[114.94831000124846,484.483665936],[114.94831000124846,494.18716993600003],[85.25557805774444,494.18716993600003]]},{"text":"会场里哪有黄道霞的影子。","box":[[110.19359304124845,484.483665936],[229.83782647176037,484.483665936],[229.83782647176037,494.18716993600003],[110.19359304124845,494.18716993600003]]},{"text":"江青尖声质问：","box":[[85.25560716825646,499.718186623008],[154.92680470227248,499.718186623008],[154.92680470227248,509.42169062300803],[85.25560716825646,509.42169062300803]]},{"text":"“","box":[[150.17206833526447,499.718186623008],[159.87557233526448,499.718186623008],[159.87557233526448,509.42169062300803],[150.17206833526447,509.42169062300803]]},{"text":"没来？","box":[[156.09120577526448,499.718186623008],[185.78394742227246,499.718186623008],[185.78394742227246,509.42169062300803],[156.09120577526448,509.42169062300803]]},{"text":"！怎么没来？","box":[[181.0292304622725,4