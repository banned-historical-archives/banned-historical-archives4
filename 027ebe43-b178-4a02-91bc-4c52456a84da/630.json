[326.484891271328,392.88260758300805],[326.484891271328,402.58611158300806],[65.26650537030406,402.58611158300806]]},{"text":"让熊向晖","box":[[322.0212891348321,392.88260758300805],[362.5819746688481,392.88260758300805],[362.5819746688481,402.58611158300806],[322.0212891348321,402.58611158300806]]},{"text":"带几个人先去联合国，","box":[[65.26653448081612,408.214143903008],[164.92154967132814,408.214143903008],[164.92154967132814,417.917647903008],[65.26653448081612,417.917647903008]]},{"text":"作为先遣人员，","box":[[160.16682300782412,408.214143903008],[229.8380011348321,408.214143903008],[229.8380011348321,417.917647903008],[160.16682300782412,417.917647903008]]},{"text":"就地了解情况，","box":[[225.08328417483216,408.214143903008],[294.7544623018401,408.214143903008],[294.7544623018401,417.917647903008],[225.08328417483216,417.917647903008]]},{"text":"进行准备。","box":[[289.99974534184014,408.214143903008],[339.68168582184006,408.214143903008],[339.68168582184006,417.917647903008],[289.99974534184014,417.917647903008]]},{"text":"”","box":[[334.9269882688481,408.214143903008],[344.6304922688481,408.214143903008],[344.6304922688481,417.917647903008],[334.9269882688481,417.917647903008]]},{"text":"毛泽东说：","box":[[85.25581094184011,423.448645183008],[134.93776112534414,423.448645183008],[134.93776112534414,433.152149183008],[85.25581094184011,433.152149183008]]},{"text":"“","box":[[130.18304416534414,423.448645183008],[139.88654816534415,423.448645183008],[139.88654816534415,433.152149183008],[130.18304416534414,433.152149183008]]},{"text":"那倒不必喽。联合国秘书长不是来了电报吗？我们","box":[[136.10219130884815,423.448645183008],[362.582071703888,423.448645183008],[362.582071703888,433.152149183008],[136.10219130884815,433.152149183008]]},{"text":"就派代表团去。让乔老爷当团长，","box":[[65.26666062636815,438.68314646300803],[214.79769608038413,438.68314646300803],[214.79769608038413,448.38665046300804],[65.26666062636815,448.38665046300804]]},{"text":"熊向晖当代表，","box":[[210.0429791203842,438.68314646300803],[280.6844688333762,438.68314646300803],[280.6844688333762,448.38665046300804],[210.0429791203842,448.38665046300804]]},{"text":"开完会就回来，","box":[[276.1238510638882,438.68314646300803],[347.05646530388816,438.68314646300803],[347.05646530388816,448.38665046300804],[276.1238510638882,448.38665046300804]]},{"text":"还要","box":[[342.5928631673922,438.68314646300803],[362.5820911108962,438.68314646300803],[362.5820911108962,448.38665046300804],[342.5928631673922,448.38665046300804]]},{"text":"接待尼克松嘛。派谁参加安理会，","box":[[65.26668003337619,453.917667150016],[219.45537800038423,453.917667150016],[219.45537800038423,463.621171150016],[65.26668003337619,463.621171150016]]},{"text":"你们再研究。那么多国家欢迎我","box":[[215.37988691337623,453.917667150016],[362.58197466884815,453.917667150016],[362.58197466884815,463.621171150016],[215.37988691337623,463.621171150016]]},{"text":"们，","box":[[65.26664121936017,469.152168430016],[84.96475433936018,469.152168430016],[84.96475433936018,478.855672430016],[65.26664121936017,478.855672430016]]},{"text":"再不派代表团，","box":[[80.21003737936017,469.152168430016],[149.88121550636822,469.152168430016],[149.88121550636822,478.855672430016],[80.21003737936017,478.855672430016]]},{"text":"那就没有道理了。不高兴的人也有，","box":[[145.12650824987222,469.152168430016],[306.0106336803841,469.152168430016],[306.0106336803841,478.855672430016],[145.12650824987222,478.855672430016]]},{"text":"蒋委员长就是","box":[[301.4499770968802,469.152168430016],[362.58211051790425,469.152168430016],[362.58211051790425,478.855672430016],[301.4499770968802,478.855672430016]]},{"text":"头一个。美国国务院说要发表声明，","box":[[65.26667032987228,484.483685343008],[229.15885288987232,484.483685343008],[229.15885288987232,494.187189343008],[65.26667032987228,494.187189343008]]},{"text":"还没有看到，","box":[[225.08338120987233,484.483685343008],[287.0887620663683,484.483685343008],[287.0887620663683,494.187189343008],[225.08338120987233,494.187189343008]]},{"text":"不过是一篇‘","box":[[283.01329038636834,484.483685343008],[345.6979068193603,484.483685343008],[345.6979068193603,494.187189343008],[283.01329038636834,494.187189343008]]},{"text":"吊丧","box":[[342.30172893688035,484.483685343008],[362.58205229688036,484.483685343008],[362.58205229688036,494.187189343008],[342.30172893688035,494.187189343008]]},{"text":"文’","box":[[65.26664121936034,499.718206030016],[84.96475433936035,499.718206030016],[84.96475433936035,509.421710030016],[65.26664121936034,509.421710030016]]},{"text":"。","box":[[81.18039748286435,499.718206030016],[90.88390148286435,499.718206030016],[90.88390148286435,509.421710030016],[81.18039748286435,509.421710030016]]},{"text":"”","box":[[86.12917481936034,499.718206030016],[95.83267881936034,499.718206030016],[95.83267881936034,509.421710030016],[86.12917481936034,509.421710030016]]},{"text":"１","box":[[84.67277590400002,514.952658792496],[94.37627990400001,514.952658792496],[94.37627990400001,524.6561627924959],[84.67277590400002,524.6561627924959]]},{"text":"０","box":[[89.52453760750402,514.952658792496],[99.22804160750401,514.952658792496],[99.22804160750401,524.6561627924959],[89.52453760750402,524.6561627924959]]},{"text":"月","box":[[97.384366144,514.952658792496],[107.087870144,514.952658792496],[107.087870144,524.6561627924959],[97.384366144,524.6561627924959]]},{"text":"","box":[[107.087870144,514.952658792496],[107.30787114399999,514.952658792496],[107.30787114399999,514.952658792496],[107.087870144,514.952658792496]]},{"text":"２","box":[[109.222650727504,514.952658792496],[118.926154727504,514.952658792496],[118.926154727504,524.6561627924959],[109.222650727504,524.6561627924959]]},{"text":"９","box":[[114.07440272750401,514.952658792496],[123.777906727504,514.952658792496],[123.777906727504,524.6561627924959],[114.07440272750401,524.6561627924959]]},{"text":"日，","box":[[121.93424096750401,514.952658792496],[141.63236379100803,514.952658792496],[141.63236379100803,524.6561627924959],[121.93424096750401,524.6561627924959]]},{"text":"外交部代部长姬鹏飞致电联合国秘书长吴丹，","box":[[136.87762742400002,514.952658792496],[337.06099257203186,514.952658792496],[337.06099257203186,524.6561627924959],[136.87762742400002,524.6561627924959]]},{"text":"通知中","box":[[332.50033598852804,514.952658792496],[362.5812177955361,514.952658792496],[362.5812177955361,524.6561627924959],[332.50033598852804,524.6561627924959]]},{"text":"国政府将在近期内派团出席本届联大。在此之前，","box":[[65.26580671801605,530.187169776],[285.82651085903996,530.187169776],[285.82651085903996,539.890673776],[65.26580671801605,539.890673776]]},{"text":"联合国秘书长吴丹","box":[[281.26585427553607,530.187169776],[362.5812760165601,530.187169776],[362.5812760165601,539.890673776],[281.26585427553607,539.890673776]]},{"text":"又多次来电邀请中国政府派团出席本届联大。","box":[[65.26583582852811,545.518706096],[264.866971329552,545.518706096],[264.866971329552,555.2222100959999],[65.26583582852811,555.2222100959999]]}]}