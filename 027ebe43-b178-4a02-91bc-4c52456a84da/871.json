396082548797],[357.1472652590403,360.76396082548797],[357.1472652590403,370.467464825488],[100.19843082152042,370.467464825488]]},{"text":"林","box":[[352.87777201656047,360.76396082548797],[362.5812760165605,360.76396082548797],[362.5812760165605,370.467464825488],[352.87777201656047,370.467464825488]]},{"text":"彪也有他自己的政府嘛！一个时候，","box":[[65.26586493904045,376.192532185488],[226.15000007305642,376.192532185488],[226.15000007305642,385.896036185488],[65.26586493904045,385.896036185488]]},{"text":"群众中有极‘","box":[[221.68637852955248,376.192532185488],[282.9154693625445,376.192532185488],[282.9154693625445,385.896036185488],[221.68637852955248,385.896036185488]]},{"text":"左’","box":[[279.4222370330565,376.192532185488],[299.4114358660485,376.192532185488],[299.4114358660485,385.896036185488],[279.4222370330565,385.896036185488]]},{"text":"思潮、","box":[[296.0152288730565,376.192532185488],[326.09610097656054,376.192532185488],[326.09610097656054,385.896036185488],[296.0152288730565,385.896036185488]]},{"text":"无政府主","box":[[321.72951447305655,376.192532185488],[362.5813051270725,376.192532185488],[362.5813051270725,385.896036185488],[321.72951447305655,385.896036185488]]},{"text":"义，","box":[[65.26589404955251,391.52404909848],[84.96400716955253,391.52404909848],[84.96400716955253,401.22755309848003],[65.26589404955251,401.22755309848003]]},{"text":"是个别的。有些地方说批林就是批极‘","box":[[80.20929020955252,391.52404909848],[249.72957301408042,391.52404909848],[249.72957301408042,401.22755309848003],[80.20929020955252,401.22755309848003]]},{"text":"左’","box":[[245.94521615758453,391.52404909848],[265.6433195740805,391.52404909848],[265.6433195740805,401.22755309848003],[245.94521615758453,401.22755309848003]]},{"text":"思潮，","box":[[261.8589627175845,391.52404909848],[291.5517043645925,391.52404909848],[291.5517043645925,401.22755309848003],[261.8589627175845,401.22755309848003]]},{"text":"这就批错了，","box":[[286.7969874045925,391.52404909848],[347.2498076210885,391.52404909848],[347.2498076