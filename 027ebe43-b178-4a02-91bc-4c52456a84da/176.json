332732439504],[336.47883084955237,450.036236439504],[275.2496914990404,450.036236439504]]},{"text":"我和他","box":[[332.11224434604844,440.332732439504],[362.58128572006444,440.332732439504],[362.58128572006444,450.036236439504],[332.11224434604844,450.036236439504]]},{"text":"们一起措措辞。","box":[[65.26587464254442,455.66424935249603],[134.9370430660485,455.66424935249603],[134.9370430660485,465.36775335249604],[65.26587464254442,465.36775335249604]]},{"text":"”","box":[[130.18232610604846,455.66424935249603],[139.88583010604847,455.66424935249603],[139.88583010604847,465.36775335249604],[130.18232610604846,465.36775335249604]]},{"text":"江青说：","box":[[85.25510258604847,470.995785672496],[124.9424436495525,470.995785672496],[124.9424436495525,480.69928967249604],[85.25510258604847,480.69928967249604]]},{"text":"“","box":[[120.18772668955248,470.995785672496],[129.8912306895525,470.995785672496],[129.8912306895525,480.69928967249604],[120.18772668955248,480.69928967249604]]},{"text":"我看文元同志的意见好。叶群同志，","box":[[126.10687383305648,470.995785672496],[285.63252811057635,470.995785672496],[285.63252811057635,480.69928967249604],[126.10687383305648,480.69928967249604]]},{"text":"你看呢？","box":[[280.87781115057646,470.995785672496],[320.56515221408046,470.995785672496],[320.56515221408046,480.69928967249604],[280.87781115057646,480.69928967249604]]},{"text":"”","box":[[315.8104546610885,470.995785672496],[325.5139586610885,470.995785672496],[325.5139586610885,480.69928967249604],[315.8104546610885,480.69928967249604]]},{"text":"叶群高兴地说：","box":[[85.2551511035685,486.32733169600004],[154.9263486375845,486.32733169600004],[154.9263486375845,496.03083569600005],[85.2551511035685,496.03083569600005]]},{"text":"“","box":[[150.5597524305765,486.32733169600004],[160.26325643057652,486.32733169600004],[160.26325643057652,496.03083569600005],[150.5597524305765,496.03083569600005]]},{"text":"好好好，","box":[[156.67295024707252,486.32733169600004],[197.1365910375845,486.32733169600004],[197.13659103