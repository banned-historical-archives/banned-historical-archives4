2015,387.351590896]]},{"text":"。陈士印试","box":[[311.54076734852816,377.648086896],[362.5812274990401,377.648086896],[362.5812274990401,387.351590896],[311.54076734852816,387.351590896]]},{"text":"探性地向正前方推了两次驾驶杆，","box":[[65.26581642152013,392.882588176],[218.48412517451212,392.882588176],[218.48412517451212,402.586092176],[65.26581642152013,402.586092176]]},{"text":"都没有将飞机改出来，","box":[[214.1175580780162,392.882588176],[316.1013754145122,392.882588176],[316.1013754145122,402.586092176],[214.1175580780162,402.586092176]]},{"text":"第三次重复","box":[[311.73480831801623,392.882588176],[362.58121779553625,392.882588176],[362.58121779553625,402.586092176],[311.73480831801623,402.586092176]]},{"text":"操纵，","box":[[65.26577760750428,408.21412449599995],[94.9584998475043,408.21412449599995],[94.9584998475043,417.91762849599996],[65.26577760750428,417.91762849599996]]},{"text":"终于缓解了飞机的下降速度。但直升机最终还是尾部和右机轮","box":[[90.2037925910083,408.21412449599995],[362.5812760165601,408.21412449599995],[362.5812760165601,417.91762849599996],[90.2037925910083,417.91762849599996]]},{"text":"先着地，","box":[[65.26579701451237,423.448625776],[104.9531283745124,423.448625776],[104.9531283745124,433.152129776],[65.26579701451237,433.152129776]]},{"text":"陈士印迅速关闭发动机及点火开关。","box":[[100.1984211180164,423.448625776],[259.8211201390403,423.448625776],[259.8211201390403,433.152129776],[100.1984211180164,433.152129776]]},{"text":"６","box":[[84.67277590400002,438.683107648992],[94.37627990400001,438.683107648992],[94.37627990400001,448.386611648992],[84.67277590400002,448.386611648992]]},{"text":"时","box":[[92.53261414400001,438.683107648992],[102.236118144,438.683107648992],[102.236118144,448.386611648992],[92.53261414400001,448.386611648992]]},{"text":"","box":[[102.236118144,438.683107648992],[102.456119144,438.683107648992],[102.456119144,438.683107648992],[102.236118144,438.683107648992]]},{"text":"４","box":[[104.370898727504,438.683107648992],[114.074402727504,438.683107648992],[114.074402727504,448.386611648992],[104.370898727504,448.386611648992]]},{"text":"７","box":[[109.222650727504,438.683107648992],[118.926154727504,438.683107648992],[118.926154727504,448.386611648992],[109.222650727504,448.386611648992]]},{"text":"分，","box":[[117.082488967504,438.683107648992],[136.780602087504,438.683107648992],[136.780602087504,448.386611648992],[117.082488967504,448.386611648992]]},{"text":"直升飞机歪歪扭扭地落在北京怀柔县沙峪公社景峪村","box":[[132.025885127504,438.683107648992],[362.5812566095518,438.683107648992],[362.5812566095518,448.386611648992],[132.025885127504,448.386611648992]]},{"text":"荒草地上。于新野未待落稳，","box":[[65.26581642152001,453.917628336],[194.807633635536,453.917628336],[194.807633635536,463.621132336],[65.26581642152001,463.621132336]]},{"text":"就从客舱中跳出，","box":[[190.05291667553604,453.917628336],[270.494955132032,453.917628336],[270.494955132032,463.621132336],[190.05291667553604,463.621132336]]},{"text":"爬到前舱外面，","box":[[265.934317955536,453.917628336],[336.76987774852796,453.917628336],[336.76987774852796,463.621132336],[265.934317955536,463.621132336]]},{"text":"将枪口","box":[[332.306275612032,453.917628336],[362.58122749904004,453.917628336],[362.58122749904004,463.621132336],[332.306275612032,463.621132336]]},{"text":"紧贴窗口玻璃向陈士印开枪。陈士印凭感觉迅速躲了一下。于新野","box":[[65.26581642152001,469.152129616],[362.5812857200638,469.152129616],[362.5812857200638,478.85563361600003],[65.26581642152001,478.85563361600003]]},{"text":"的子弹在陈士印的两件飞行服中穿了四个洞，","box":[[65.26587464254402,484.483646528992],[269.912735188528,484.483646528992],[269.912735188528,494.187150528992],[65.26587464254402,494.187150528992]]},{"text":"击中周宇驰的手腕，","box":[[265.5461292780161,484.483646528992],[357.24429059553614,484.483646528992],[357.24429059553614,494.187150528992],[265.5461292780161,494.187150528992]]},{"text":"鲜","box":[[352.8777040920322,484.483646528992],[362.5812080920322,484.483646528992],[362.5812080920322,494.187150528992],[352.8777040920322,494.187150528992]]},{"text":"血溅到陈士印的脸上，","box":[[65.2657970145122,499.718167216],[164.92081220502422,499.718167216],[164.92081220502422,509.421671216],[65.2657970145122,509.421671216]]},{"text":"他就势躺下装死。","box":[[160.1660855415202,499.718167216],[239.83188249203215,499.718167216],[239.83188249203215,509.421671216],[160.1660855415202,509.421671216]]},{"text":"一会儿，","box":[[85.25505406852821,514.952668496],[124.94239513203223,514.952668496],[124.94239513203223,524.656172496],[85.25505406852821,524.656172496]]},{"text":"周宇驰浑身是血走下飞机，","box":[[120.18767817203222,514.952668496],[242.5489121295522,514.952668496],[242.5489121295522,524.656172496],[120.18767817203222,524.656172496]]},{"text":"骂于新野为什么乱开枪，","box":[[238.18230621904024,514.952668496],[352.1985461435682,514.952668496],[352.1985461435682,524.656172496],[238.18230621904024,524.656172496]]},{"text":"说：","box":[[347.92898497656023,514.952668496],[367.6270883930562,514.952668496],[367.6270883930562,524.656172496],[347.92898497656023,524.656172496]]},{"text":"“","box":[[65.26589404955217,530.187179479504],[74.96939804955217,530.187179479504],[74.96939804955217,539.890683479504],[65.26589404955217,539.890683479504]]},{"text":"那个飞行员故意制造罗盘故障，","box":[[71.18503148955217,530.187179479504],[210.81850256707213,530.187179479504],[210.81850256707213,539.890683479504],[71.18503148955217,539.890683479504]]},{"text":"实际离北京没多远，","box":[[206.06376620006418,530.187179479504],[295.7242013810881,530.187179479504],[295.7242013810881,539.890683479504],[206.06376620006418,539.890683479504]]},{"text":"我把他打死了！","box":[[290.96944560707215,530.187179479504],[360.64065284459207,530.187179479504],[360.64065284459207,539.890683479504],[290.96944560707215,539.890683479504]]},{"text":"”","box":[[355.8859164775841,530.187179479504],[365.5894204775841,530.187179479504],[365.5894204775841,539.890683479504],[355.8859164775841,539.890683479504]]},{"text":"接着，","box":[[85.25517051057608,545.518715799504],[114.9479024540801,545.518715799504],[114.9479024540801,555.2222197995039],[85.25517051057608,555.2222197995039]]},{"text":"李伟信也从直升机上爬下来。周宇驰、","box":[[110.19318549408008,545.518715799504],[280.6837701810881,545.518715799504],[280.6837701810881,555.2222197995039],[110.19318549408008,555.2222197995039]]},{"text":"于新野、","box":[[276.22014863758415,545.518715799504],[316.4896999410881,545.518715799504],[316.4896999410881,555.2222197995039],[276.22014863758415,555.2222197995039]]},{"text":"李伟信","box":[[312.0260978045921,545.518715799504],[342.3010205810881,545.518715799504],[342.3010205810881,555.2222197995039],[312.0260978045921,555.2222197995039]]},{"text":"","box":[[342.30102058108815,545.518715799504],[342.55100358108814,545.518715799504],[342.55100358108814,545.518715799504],[342.30102058108815,545.518715799504]]},{"text":"３","box":[[344.72673162152,545.518696392496],[354.43023562152,545.518696392496],[354.43023562152,555.222200392496],[344.72673162152,555.222200392496]]},{"text":"人","box":[[352.87767498152004,545.518696392496],[362.58117898152005,545.518696392496],[362.58117898152005,555.222200392496],[352.87767498152004,555.222200392496]]}]}