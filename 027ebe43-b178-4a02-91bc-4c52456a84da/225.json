67504,362.41354680198395],[181.70783531100798,362.41354680198395],[181.70783531100798,372.11705080198396],[152.015103367504,372.11705080198396]]},{"text":"中央政治局成员在人民大会堂接见广州军","box":[[176.95311835100802,362.41354680198395],[362.58120809203183,362.41354680198395],[362.58120809203183,372.11705080198396],[176.95311835100802,372.11705080198396]]},{"text":"区和湖南省军政负责人，","box":[[65.26579701451197,377.648086896],[175.88575231801605,377.648086896],[175.88575231801605,387.351590896],[65.26579701451197,387.351590896]]},{"text":"到场的中央领导人有周恩来、","box":[[171.51920462852803,377.648086896],[305.42758893904,377.648086896],[305.42758893904,387.351590896],[171.51920462852803,387.351590896]]},{"text":"康生、","box":[[301.0610412495521,377.648086896],[331.2389483930561,377.648086896],[331.2389483930561,387.351590896],[301.0610412495521,387.351590896]]},{"text":"张春桥、","box":[[326.9693484120321,377.648086896],[367.6270592825441,377.648086896],[367.6270592825441,387.351590896],[326.9693484120321,387.351590896]]},{"text":"江青等人。周恩来代表政治局讲话。他说，","box":[[65.26586493904006,392.88258817599996],[259.530024722544,392.88258817599996],[259.530024722544,402.58609217599997],[65.26586493904006,402.58609217599997]]},{"text":"为了加强“","box":[[255.45457244955205,392.88258817599996],[307.56236952254403,392.88258817599996],[307.56236952254403,402.58609217599997],[255.45457244955205,402.58609217599997]]},{"text":"三北”","box":[[304.166162529552,392.88258817599996],[334.82925457656006,392.88258817599996],[334.82925457656006,402.58609217599997],[304.166162529552,402.58609217599997]]},{"text":"地区战","box":[[331.72409448254405,392.88258817599996],[362.581237202544,392.88258817599996],[362.581237202544,402.58609217599997],[331.72409448254405,402.58609217599997]]},{"text":"备，","box":[[65.26586493904,408.21412449599995],[84.96397805904002,408.21412449599995],[84.96397805904002,417.91762849599996],[65.26586493904,417.91762849599996]]},{"text":"决定","box":[[80.20926109904,408.21412449599995],[99.90738392254401,408.21412449599995],[99.90738392254401,417.91762849599996],[80.20926109904,417.91762849599996]]},{"text":"","box":[[99.907383922544,408.21412449599995],[100.127373922544,408.21412449599995],[100.127373922544,408.21412449599995],[99.907383922544,408.21412449599995]]},{"text":"４","box":[[102.042057767504,408.214114792496],[111.745561767504,408.214114792496],[111.745561767504,417.917618792496],[102.042057767504,417.917618792496]]},{"text":"７","box":[[106.89380976750401,408.214114792496],[116.597313767504,408.214114792496],[116.597313767504,417.917618792496],[106.89380976750401,417.917618792496]]},{"text":"军调往兰州军区。黎原随部队北调，","box":[[114.753638304,408.214114792496],[275.05554379100784,408.214114792496],[275.05554379100784,417.917618792496],[114.753638304,417.917618792496]]},{"text":"但他的革委会主任职","box":[[270.5919416545119,408.214114792496],[362.58121779553596,408.214114792496],[362.58121779553596,417.917618792496],[270.5919416545119,417.917618792496]]},{"text":"务不免，","box":[[65.26577760750399,423.44861607249595],[104.95310896750402,423.44861607249595],[104.95310896750402,433.15212007249596],[65.26577760750399,433.15212007249596]]},{"text":"以防人事变动频繁，","box":[[100.19840171100802,423.44861607249595],[191.41136842152002,423.44861607249595],[191.41136842152002,433.15212007249596],[100.19840171100802,433.15212007249596]]},{"text":"引起外界猜疑。黎走后由华国锋代理省","box":[[187.04478191801607,423.44861607249595],[362.581285720064,423.44861607249595],[362.581285720064,433.15212007249596],[187.04478191801607,433.15212007249596]]},{"text":"革委会主任。为了不削弱省革委会领导力量，","box":[[65.26584553203207,438.68311735249597],[265.449162162544,438.68311735249597],[265.449162162544,448.386621352496],[65.26584553203207,448.386621352496]]},{"text":"另调卜占亚任省革委会","box":[[260.88854439305607,438.68311735249597],[362.58134394108794,438.68311735249597],[362.58134394108794,448.386621352496],[260.88854439305607,448.386621352496]]},{"text":"副主任。接见过程不过半个小时。","box":[[65.26586493904006,453.91763803950397],[214.79690039305603,453.91763803950397],[214.79690039305603,463.621142039504],[65.26586493904006,463.621142039504]]},{"text":"就这样，","box":[[85.25511228955207,469.15213931950393],[124.9424533530561,469.15213931950393],[124.9424533530561,478.85564331950394],[85.25511228955207,478.85564331950394]]},{"text":"卜占亚来到了湖南。华国锋调中央后，","box":[[120.18773639305608,469.15213931950393],[294.171611630576,469.15213931950393],[294.171611630576,478.85564331950394],[120.18773639305608,478.85564331950394]]},{"text":"卜占亚负责湖南","box":[[289.99910491057614,469.15213931950393],[362.58135364459207,469.15213931950393],[362.58135364459207,478.85564331950394],[289.99910491057614,478.85564331950394]]},{"text":"的全面工作。","box":[[65.2659425670721,484.48365623249595],[124.94250187057615,484.48365623249595],[124.94250187057615,494.18716023249596],[65.2659425670721,494.18716023249596]]},{"text":"再说","box":[[85.25517051057614,499.71817691950395],[104.85624859057614,499.71817691950395],[104.85624859057614,509.42168091950396],[85.25517051057614,509.42168091950396]]},{"text":"","box":[[104.85624859057613,499.71817691950395],[105.07623059057612,499.71817691950395],[105.07623059057612,499.71817691950395],[104.85624859057613,499.71817691950395]]},{"text":"１","box":[[106.99084480750402,499.71815751249596],[116.69434880750401,499.71815751249596],[116.69434880750401,509.421661512496],[106.99084480750402,509.421661512496]]},{"text":"９","box":[[111.84259680750402,499.71815751249596],[121.54610080750402,499.71815751249596],[121.54610080750402,509.421661512496],[111.84259680750402,509.421661512496]]},{"text":"７","box":[[116.69434880750403,499.71815751249596],[126.39785280750402,499.71815751249596],[126.39785280750402,509.421661512496],[116.69434880750403,509.421661512496]]},{"text":"１","box":[[121.54610080750403,499.71815751249596],[131.24960480750403,499.71815751249596],[131.24960480750403,509.421661512496],[121.54610080750403,509.421661512496]]},{"text":"年","box":[[129.40592934400001,499.71815751249596],[139.10943334400002,499.71815751249596],[139.10943334400002,50