[[347.92886853451245,423.448625776],[367.62697195100844,423.448625776],[367.62697195100844,433.152129776],[347.92886853451245,433.152129776]]},{"text":"“","box":[[65.26577760750439,438.683127056],[74.96928160750439,438.683127056],[74.96928160750439,448.386631056],[65.26577760750439,448.386631056]]},{"text":"那就不加了，","box":[[71.18491504750439,438.683127056],[130.86148405451243,438.683127056],[130.86148405451243,448.386631056],[71.18491504750439,448.386631056]]},{"text":"明早再说。","box":[[126.1067670945124,438.683127056],[175.78871727801635,438.683127056],[175.78871727801635,448.386631056],[126.1067670945124,448.386631056]]},{"text":"”","box":[[171.0340003180164,438.683127056],[180.73750431801642,438.683127056],[180.73750431801642,448.386631056],[171.0340003180164,448.386631056]]},{"text":"２","box":[[84.67277590400002,453.917628336],[94.37627990400001,453.917628336],[94.37627990400001,463.621132336],[84.67277590400002,463.621132336]]},{"text":"１","box":[[89.52453760750402,453.917628336],[99.22804160750401,453.917628336],[99.22804160750401,463.621132336],[89.52453760750402,463.621132336]]},{"text":"时多，","box":[[97.384366144,453.917628336],[127.07708838400002,453.917628336],[127.07708838400002,463.621132336],[97.384366144,463.621132336]]},{"text":"机组","box":[[122.32238112750402,453.917628336],[142.02049424750402,453.917628336],[142.02049424750402,463.621132336],[122.32238112750402,463.621132336]]},{"text":"","box":[[142.02049424750402,453.917628336],[142.24049424750402,453.917628336],[142.24049424750402,453.917628336],[142.02049424750402,453.917628336]]},{"text":"９","box":[[144.155265127504,453.917628336],[153.85876912750402,453.917628336],[153.85876912750402,463.621132336],[144.155265127504,463.621132336]]},{"text":"人塞好了飞机的“","box":[[152.015103367504,453.917628336],[235.1741423510079,453.917628336],[235.1741423510079,463.621132336],[152.015103367504,463.621132336]]},{"text":"眼”","box":[[231.777896544,453.917628336],[251.86416923100796,453.917628336],[251.86416923100796,463.621132336],[231.777896544,463.621132336]]},{"text":"，","box":[[248.079812374512,453.917628336],[257.78331637451197,453.917628336],[257.78331637451197,463.621132336],[248.079812374512,463.621132336]]},{"text":"最后由机械师锁上飞机，","box":[[253.61079024750399,453.917628336],[367.6269913580159,453.917628336],[367.6269913580159,463.621132336],[253.61079024750399,463.621132336]]},{"text":"将飞机交给山海关机场的警卫人员，","box":[[65.26579701451192,469.15212961599997],[230.61350517451194,469.15212961599997],[230.61350517451194,478.855633616],[65.26579701451192,478.855633616]]},{"text":"然后一起到空勤灶吃饭。食堂","box":[[226.53803349451198,469.15212961599997],[362.58114016750403,469.15212961599997],[362.58114016750403,478.855633616],[226.53803349451198,478.855633616]]},{"text":"里，","box":[[65.26576790400003,484.483646528992],[84.96388102400005,484.483646528992],[84.96388102400005,494.187150528992],[65.26576790400003,494.187150528992]]},{"text":"程洪珍身边放着一个精致的手提皮箱，","box":[[80.20916406400003,484.483646528992],[250.31163770151994,484.483646528992],[250.31163770151994,494.187150528992],[80.20916406400003,494.187150528992]]},{"text":"此时，","box":[[245.75100052502407,484.483646528992],[275.6377928450241,484.483646528992],[275.6377928450241,494.187150528992],[245.75100052502407,494.187150528992]]},{"text":"他和两个女兵已经快","box":[[271.07717507553605,484.483646528992],[362.581285720064,484.483646528992],[362.581285720064,494.187150528992],[271.07717507553605,494.187150528992]]},{"text":"吃完饭了。","box":[[65.26580671801605,499.718167216],[114.9477569015201,499.718167216],[114.9477569015201,509.421671216],[65.26580671801605,509.421671216]]},{"text":"吃完饭，","box":[[85.25503466152007,514.952668496],[124.9423757250241,514.952668496],[124.9423757250241,524.656172496],[85.25503466152007,524.656172496]]},{"text":"中队长康廷梓分配房间。潘景寅是师级干部，","box":[[120.18765876502408,514.952668496],[325.6109063695521,514.952668496],[325.6109063695521,524.656172496],[120.18765876502408,524.656172496]]},{"text":"被安排在","box":[[321.4383996495522,514.952668496],[