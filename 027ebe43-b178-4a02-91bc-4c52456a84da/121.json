06368992],[362.58122749904004,433.152110368992],[249.34129700502407,433.152110368992]]},{"text":"子一家都是当兵的，","box":[[65.26581642152007,438.68310764899195],[155.31436265203212,438.68310764899195],[155.31436265203212,448.38661164899196],[65.26581642152007,448.38661164899196]]},{"text":"没有一个像你们这么混蛋的！你们别放我出去，","box":[[150.9477761485281,438.68310764899195],[367.62707868955204,438.68310764899195],[367.62707868955204,448.38661164899196],[150.9477761485281,448.38661164899196]]},{"text":"否则我同你一块见阎王！","box":[[65.26588434604815,453.917628336],[174.91550865656018,453.917628336],[174.91550865656018,463.621132336],[65.26588434604815,463.621132336]]},{"text":"”","box":[[170.16079169656018,453.917628336],[179.8642956965602,453.917628336],[179.8642956965602,463.621132336],[170.16079169656018,463.621132336]]},{"text":"他的嚣张气焰惹火了军管会，","box":[[85.25512199305618,469.15212961599997],[218.7753564400642,469.15212961599997],[218.7753564400642,478.855633616],[85.25512199305618,478.855633616]]},{"text":"第二天，","box":[[214.79691980006422,469.15212961599997],[255.8427320165602,469.15212961599997],[255.8427320165602,478.855633616],[214.79691980006422,478.855633616]]},{"text":"被押到厂里批斗了一场。","box":[[251.86431478356823,469.15212961599997],[367.62707868955215,469.15212961599997],[367.62707868955215,478.855633616],[251.86431478356823,478.855633616]]},{"text":"批斗完后，","box":[[65.26588434604815,484.483646528992],[114.9478345295522,484.483646528992],[114.9478345295522,494.187150528992],[65.26588434604815,494.187150528992]]},{"text":"军管会副主任来到关押他的房间训话：","box":[[110.19311756955217,484.483646528992],[279.81042571057606,484.483646528992],[279.81042571057606,494.187150528992],[110.19311756955217,494.187150528992]]},{"text":"“","box":[[85.25515110356815,499.718167216],[94.95865510356815,499.718167216],[94.95865510356815,509.421671216],[85.25515110356815,509.421671216]]},{"text":"戴维堤，","box":[[91.17429824707216,499.718167216],[130.86163931057618,499.718167216],[130.86163931057618,509.421671216],[91.17429824707216,509.421671216]]},{"text":"我告诉你，","box":[[126.10692235057617,499.718167216],[176.37107307057616,499.718167216],[176.37107307057616,509.421671216],[126.10692235057617,509.421671216]]},{"text":"你给我放老实点。老子管过监狱，","box":[[172.0045059740802,499.718167216],[325.9991823786081,499.718167216],[325.9991823786081,509.421671216],[172.0045059740802,509.421671216]]},{"text":"像你这样","box":[[321.63259587510424,499.718167216],[362.5814021621122,499.718167216],[362.5814021621122,509.421671216],[321.63259587510424,509.421671216]]},{"text":"的，","box":[[65.2659910845922,514.952668496],[84.96410420459222,514.952668496],[84.96410420459222,524.656172496],[65.2659910845922,524.656172496]]},{"text":"老子在监狱里见的多了。你的问题很严重，","box":[[80.20938724459221,514.952668496],[277.1905572586082,514.952668496],[277.1905572586082,524.656172496],[80.20938724459221,524.656172496]]},{"text":"我们有充足的材料。","box":[[273.21208180459223,514.952668496],[367.62716602108816,514.952668496],[367.62716602108816,524.656172496],[273.21208180459223,524.656172496]]},{"text":"坦白从宽，","box":[[65.26597167758416,530.1871794795039],[114.9479218610882,530.1871794795039],[114.9479218610882,539.8906834795039],[65.26597167758416,539.8906834795039]]},{"text":"抗拒从严，","box":[[110.19320490108818,530.1871794795039],[160.3603205810882,530.1871794795039],[160.3603205810882,539.8906834795039],[110.19320490108818,539.8906834795039]]},{"text":"顽固不化，","box":[[155.9937437810882,530.1871794795039],[206.8401338516002,530.1871794795039],[206.8401338516002,539.8906834795039],[155.9937437810882,539.8906834795039]]},{"text":"死路一条。你不要认为你出身好，","box":[[202.47354734809622,530.1871794795039],[357.14741081160014,530.1871794795039],[357.14741081160014,539.8906834795039],[202.47354734809622,539.8906834795039]]},{"text":"我","box":[[352.8779175691202,530.1871794795039],[362.5814215691202,530.1871794795039],[362.5814215691202,539.8906834795039],[352.8779175691202,539.8906834795039]]},{"text":"们就不敢怎么着你，","box":[[65.26601049160018,545.518715799504],[155.31455672211223,545.518715799504],[155.31455672211223,555.2222197995039],[65.26601049160018,555.2222197995039]]},{"text":"你的出身我们也要调查，","box":[[150.94797021860822,545.518715799504],[264.09084626561616,545.518715799504],[264.09084626561616,555.2222197995039],[150.94797021860822,555.2222197995039]]},{"text":"烈士当中也有叛徒。我","box":[[259.72429857612826,545.518715799504],[362.58147979014416,545.518715799504],[362.58147979014416,555.2222197995039],[259.72429857612826,555.2222197995039]]}]}