93599997],[275.6379869151042,484.48366593599997],[275.6379869151042,494.187169936],[195.09889401160024,494.187169936]]},{"text":"可能引起不必要的猜","box":[[271.0773691456162,484.48366593599997],[362.58147979014416,484.48366593599997],[362.58147979014416,494.187169936],[271.0773691456162,494.187169936]]},{"text":"想。究如何为好，","box":[[65.26600078809622,499.71818662300797],[144.83476269860824,499.71818662300797],[144.83476269860824,509.421690623008],[65.26600078809622,509.421690623008]]},{"text":"请主席指示。","box":[[140.08003603510423,499.71818662300797],[199.75661474561616,499.71818662300797],[199.75661474561616,509.421690623008],[140.08003603510423,509.421690623008]]},{"text":"周恩来","box":[[332.8886993291202,514.9526879030079],[362.5814021621121,514.9526879030079],[362.5814021621121,524.6561919030079],[332.8886993291202,524.6561919030079]]},{"text":"一九七二年一月八日夜","box":[[262.92641608211215,530.187198886512],[362.581353644592,530.187198886512],[362.581353644592,539.8907028865119],[262.92641608211215,539.8907028865119]]},{"text":"因为悼词中已有“","box":[[85.254986144,545.518696392496],[164.92078309451205,545.518696392496],[164.92078309451205,555.222200392496],[85.254986144,555.222200392496]]},{"text":"陈毅同志是中国共产党的优秀党员，","box":[[161.13641653451202,545.518696392496],[325.999027122544,545.518696392496],[325.999027122544,555.222200392496],[161.13641653451202,555.222200392496]]},{"text":"是中国人","box":[[321.7294659555361,545.518696392496],[362.58125660955204,545.518696392496],[362.58125660955204,555.222200392496],[321.7294659555361,555.222200392496]]}]}