":[[65.26599108459209,408.21411479249593],[154.92640685860812,408.21411479249593],[154.92640685860812,417.91761879249594],[65.26599108459209,417.91761879249594]]},{"text":"报告一级一级，","box":[[85.25522873160011,423.44861607249595],[155.60564243510413,423.44861607249595],[155.60564243510413,433.15212007249596],[85.25522873160011,433.15212007249596]]},{"text":"从县到省，","box":[[151.62722520211213,423.44861607249595],[203.3469015221121,423.44861607249595],[203.3469015221121,433.15212007249596],[151.62722520211213,433.15212007249596]]},{"text":"不敢延迟。省里派人去看，","box":[[199.36844547510415,423.44861607249595],[324.8347327880961,423.44861607249595],[324.8347327880961,433.15212007249596],[199.36844547510415,433.15212007249596]]},{"text":"觉得没问","box":[[320.8562961480962,423.44861607249595],[362.5813536445922,423.44861607249595],[362.5813536445922,433.15212007249596],[320.8562961480962,433.15212007249596]]},{"text":"题，","box":[[65.26598138108818,438.68311735249597],[84.9640945010882,438.68311735249597],[84.9640945010882,448.386621352496],[65.26598138108818,448.386621352496]]},{"text":"６","box":[[79.626953824,438.683107648992],[89.33045782399999,438.683107648992],[89.33045782399999,448.386611648992],[79.626953824,448.386611648992]]},{"text":"３","box":[[84.478705824,438.683107648992],[94.182209824,438.683107648992],[94.182209824,448.386611648992],[84.478705824,448.386611648992]]},{"text":"军军长看后说就是余洪信，","box":[[92.338553767504,438.683107648992],[211.98278719801598,438.683107648992],[211.98278719801598,448.386611648992],[92.338553767504,448.386611648992]]},{"text":"公安部报给中央。中央说：","box":[[207.22807023801604,438.683107648992],[326.775278332032,438.683107648992],[326.775278332032,448.386611648992],[207.22807023801604,448.386611648992]]},{"text":"“","box":[[322.0205807790401,438.683107648992],[331.7240847790401,438.683107648992],[331.7240847790401,448.386611648992],[322.0205807790401,448.386611648992]]},{"text":"那不行，","box":[[327.93968910852806,438.683107648992],[367.627030172032,438.683107648992],[367.627030172032,448.386611648992],[327.93968910852806,448.386611648992]]},{"text":"不能说就是，","box":[[65.265835828528,453.917628336],[124.94239513203205,453.917628336],[124.94239513203205,463.621132336],[65.265835828528,463.621132336]]},{"text":"得拿证据说话，","box":[[120.18767817203202,453.917628336],[191.21734685904,453.917628336],[191.21734685904,463.621132336],[120.18767817203202,463.621132336]]},{"text":"首先要弄清是不是余洪信，","box":[[186.85077976254405,453.917628336],[310.37641479305603,453.917628336],[310.37641479305603,463.621132336],[186.85077976254405,463.621132336]]},{"text":"如果是，","box":[[306.00982828955216,453.917628336],[346.6674809390401,453.917628336],[346.6674809390401,463.621132336],[306.00982828955216,463.621132336]]},{"text":"怎么","box":[[342.39798769656016,453.917628336],[362.5812954235682,453.917628336],[362.5812954235682,463.621132336],[342.39798769656016,463.621132336]]},{"text":"死的，","box":[[65.26588434604815,469.152129616],[94.95860658604818,469.152129616],[94.95860658604818,478.85563361600003],[65.26588434604815,478.85563361600003]]},{"text":"自杀还是他杀。","box":[[90.20389932955217,469.152129616],[159.8750677530562,469.152129616],[159.8750677530562,478.85563361600003],[90.20389932955217,478.85563361600003]]},{"text":"”","box":[[155.12035079305622,469.152129616],[164.82385479305623,469.152129616],[164.82385479305623,478.85563361600003],[155.12035079305622,478.85563361600003]]},{"text":"奉命抵达山西榆次的乌国庆看见了那片麦田，","box":[[85.25511228955222,484.483646528992],[286.02068767758414,484.483646528992],[286.02068767758414,494.187150528992],[85.25511228955222,494.187150528992]]},{"text":"麦子倒伏。一个人","box":[[281.4599922800642,484.483646528992],[362.5813148305762,484.483646528992],[362.5813148305762,494.187150528992],[281.4599922800642,494.187150528992]]},{"text":"就躺在一片麦子的倒伏里。","box":[[65.26590375305625,499.718167216],[184.91012748006426,499.718167216],[184.91012748006426,509.421671216],[65.26590375305625,509.421671216]]},{"text":"这个人到底是不是余洪信？从中央到地方，","box":[[85.25514140006428,514.952668496],[275.4438392070722,514.952668496],[275.4438392070722,524.656172496],[85.25514140006428,524.656172496]]},{"text":"直至这一片麦田四周","box":[[270.88322143758427,514.952668496],[362.5813924586082,514.952668496],[362.5813924586082,524.656172496],[270.88322143758427,524.656172496]]},{"text":"站满的人群，","box":[[65.26595227057629,530.187179479504],[124.94251157408034,530.187179479504],[124.94251157408034,539.890683479504],[65.26595227057629,539.890683479504]]},{"text":"远的近的，","box":[[120.18779461408032,530.187179479504],[169.8697447975843,530.187179479504],[169.8697447975843,539.890683479504],[120.18779461408032,539.890683479504]]},{"text":"看得见的看不见的，","box":[[165.11504724459232,530.187179479504],[255.26059940459223,530.187179479504],[255.26059940459223,539.890683479504],[165.11504724459232,539.890683479504]]},{"text":"一层一层的人都等着乌国","box":[[250.6999428210883,530.187179479504],[362.5814506796322,530.187179479504],[362.5814506796322,539.890683479504],[250.6999428210883,539.890683479504]]},{"text":"庆看后给出一个结果。","box":[[65.26597167758433,545.518715799504],[164.92098686809635,545.518715799504],[164.92098686809635,555.2222197995039],[65.26597167758433,555.2222197995039]]}]}