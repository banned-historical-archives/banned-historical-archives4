4999999999994],[90.024,280.24999999999994]]},{"text":"/","box":[[132.02,266.2099999999999],[139.04000000000002,266.2099999999999],[139.04000000000002,280.24999999999994],[132.02,280.24999999999994]]},{"text":"我们的车是早上","box":[[139.1,266.2099999999999],[237.12727999999996,266.2099999999999],[237.12727999999996,280.24999999999994],[139.1,280.24999999999994]]},{"text":"","box":[[237.12727999999993,266.2099999999999],[241.97,266.2099999999999],[241.97,266.2099999999999],[237.12727999999993,266.2099999999999]]},{"text":"8","box":[[241.97,266.2099999999999],[248.55476,266.2099999999999],[248.55476,280.24999999999994],[241.97,280.24999999999994]]},{"text":"","box":[[248.55476,266.2099999999999],[253.49,266.2099999999999],[253.49,266.2099999999999],[248.55476,266.2099999999999]]},{"text":"点左右出发的。从重庆至北碚的公路，一","box":[[253.49,266.2099999999999],[505.45184000000006,266.2099999999999],[505.45184000000006,280.24999999999994],[253.49,280.24999999999994]]},{"text":"直都十分平静。到同兴乡，开始有第一道关卡，三道交错的石墙拦住","box":[[90.024,286.61],[505.2850800000002,286.61],[505.2850800000002,300.65000000000003],[90.024,300.65000000000003]]},{"text":"去路，","box":[[90.024,306.89],[132.144,306.89],[132.144,320.93],[90.024,320.93]]},{"text":"只留出一条很窄的","box":[[127.46867999999998,306.89],[239.53595999999993,306.89],[239.53595999999993,320.93],[127.46867999999998,320.93]]},{"text":"","box":[[239.5359599999999,306.89],[243.05,306.89],[243.05,306.89],[239.5359599999999,306.89]]},{"text":"S","box":[[243.05,306.89],[249.77516,306.89],[249.77516,320.93],[243.05,320.93]]},{"text":"","box":[[249.77516,306.89],[253.25,306.89],[253.25,306.89],[249.77516,306.89]]},{"text":"型通道。","box":[[253.25,306.89],[309.28364,306.89],[309.28364,320.93],[253.25,320.93]]},{"text":"车停下了，","box":[[304.7206400000001,306.89],[374.66792000000015,306.89],[374.66792000000015,320.93],[304.7206400000001,320.93]]},{"text":"一个手持","box":[[369.99260000000027,306.89],[426.02624000000026,306.89],[426.02624000000026,320.93],[369.99260000000027,320.93]]},{"text":"“","box":[[421.51,306.89],[435.55,306.89],[435.55,320.93],[421.51,320.93]]},{"text":"三八大盖","box":[[435.43,306.89],[491.46364000000005,306.89],[491.46364000000005,320.93],[435.43,320.93]]},{"text":"”","box":[[491.5,306.89],[505.54,306.89],[505.54,320.93],[491.5,320.93]]},{"text":"的","box":[[90.024,327.28999999999996],[104.064,327.28999999999996],[104.064,341.33],[90.024,341.33]]},{"text":"“","box":[[102.14,327.28999999999996],[116.18,327.28999999999996],[116.18,341.33],[102.14,341.33]]},{"text":"兵","box":[[116.18,327.28999999999996],[130.22,327.28999999999996],[130.22,341.33],[116.18,341.33]]},{"text":"”","box":[[130.1,327.28999999999996],[144.14,327.28999999999996],[144.14,341.33],[130.1,341.33]]},{"text":"走上前检查验明，然后放行。","box":[[142.22,327.28999999999996],[322.32511999999997,327.28999999999996],[322.32511999999997,341.33],[142.22,341.33]]},{"text":"/","box":[[320.35,327.28999999999996],[327.37,327.28999999999996],[327.37,341.33],[320.35,341.33]]},{"text":"一会儿，车过施家梁，沿着嘉","box":[[327.31,327.28999999999996],[505.47760000000005,327.28999999999996],[505.47760000000005,341.33],[327.31,341.33]]},{"text":"陵江奔跑一段路，过龙凤桥，便到北碚了。迎接我们的，又是一道检","box":[[90.024,347.68999999999994],[505.5658800000002,347.68999999999994],[505.5658800000002,361.72999999999996],[90.024,361.72999999999996]]},{"text":"查介绍信的关卡，又是一道有","box":[[90.024,368.09],[270.0167999999999,368.09],[270.0167999999999,382.13],[90.024,382.13]]},{"text":"","box":[[270.0167999999999,368.09],[273.53,368.09],[273.53,368.09],[270.0167999999999,368.09]]},{"text":"S","box":[[273.53,368.09],[280.25516,368.09],[280.25516,382.13],[273.53,382.13]]},{"text":"","box":[[280.25516,368.09],[283.61,368.09],[283.61,368.09],[280.25516,368.09]]},{"text":"通道的墙，再绕过一道用灭火机改装","box":[[283.61,368.09],[505.47008000000005,368.09],[505.47008000000005,382.13],[283.61,382.13]]},{"text":"的炸弹网，便进入了北碚城。","box":[[90.024,388.48999999999995],[273.1056,388.48999999999995],[273.1056,402.53],[90.024,402.53]]},{"text":"/","box":[[273.17,388.48999999999995],[280.19,388.48999999999995],[280.19,402.53],[273.17,402.53]]},{"text":"祝捷会已经开完。大街上人声鼎沸，","box":[[280.25,388.48999999999995],[505.56391999999994,388.48999999999995],[505.56391999999994,402.53],[280.25,402.53]]},{"text":"车水马龙，热闹非凡。其热闹程度，大概只有节日的解放碑可以与之","box":[[90.024,408.89],[505.2850800000002,408.89],[505.2850800000002,422.93],[90.024,422.93]]},{"text":"相匹。","box":[[90.024,429.28999999999996],[132.144,429.28999999999996],[132.144,443.33],[90.024,443.33]]},{"text":"大字报非常多，","box":[[127.46867999999998,429.28999999999996],[225.36959999999993,429.28999999999996],[225.36959999999993,443.33],[127.46867999999998,443.33]]},{"text":"大标语也稠密至极。","box":[[220.6942799999999,429.28999999999996],[346.6751999999999,429.28999999999996],[346.6751999999999,443.33],[220.6942799999999,443.33]]},{"text":"一会儿，","box":[[341.9998800000001,429.28999999999996],[398.03352000000007,429.28999999999996],[398.03352000000007,443.33],[341.9998800000001,443.33]]},{"text":"锣鼓声响过来了。","box":[[393.3582000000002,429.28999999999996],[505.4254800000002,429.28999999999996],[505.4254800000002,443.33],[393.3582000000002,443.33]]},{"text":"红旗和欢呼声簇拥着一队队英雄队伍。他们是在八二八大战中，用生","box":[[90.024,449.71],[505.2850800000002,449.71],[505.2850800000002,463.75],[90.024,463.75]]},{"text":"命换取胜利的英雄。他们都扛着枪，握着最高的奖赏：毛主席著作，","box":[[90.024,469.98999999999995],[512.4595200000002,469.98999999999995],[512.4595200000002,484.03],[90.024,484.03]]},{"text":"骄傲地在大街上走过","box":[[90.024,490.39],[216.72096,490.39],[216.72096,504.43],[90.024,504.43]]},{"text":"/","box":[[216.77,490.39],[223.79000000000002,490.39],[223.79000000000002,504.43],[216.77,504.43]]},{"text":"街道的墙壁上，宣传画向他们欢呼，写着喜报","box":[[223.97,490.39],[505.55624,490.39],[505.55624,504.43],[223.97,504.43]]},{"text":"的大红纸向他们欢呼，林荫道中幡飞的标语向他们欢呼：","box":[[90.024,510.78999999999996],[442.1472000000001,510.78999999999996],[442.1472000000001,524.8299999999999],[90.024,524.8299999999999]]},{"text":"“","box":[[435.19,510.78999999999996],[449.23,510.78999999999996],[449.23,524.8299999999999],[435.19,524.8299999999999]]},{"text":"庆祝八二","box":[[449.38,510.78999999999996],[505.65232000000003,510.78999999999996],[505.65232000000003,524.8299999999999],[449.38,524.8299999999999]]},{"text":"八大捷！","box":[[90.024,531.1899999999999],[146.05764,531.1899999999999],[146.05764,545.2299999999999],[90.024,545.2299999999999]]},{"text":"”","box":[[139.1,531.1899999999999],[153.14,531.1899999999999],[153.14,545.2299999999999],[139.1,545.2299999999999]]},{"text":"“","box":[[146.05999999999997,531.1899999999999],[160.09999999999997,531.1899999999999],[160.09999999999997,545.2299999999999],[146.05999999999997,545.2299999999999]]},{"text":"文攻武卫好得很！","box":[[160.1,531.1899999999999],[272.04091999999997,531.1899999999999],[272.04091999999997,545.2299999999999],[160.1,545.2299999999999]]},{"text":"”","box":[[265.01,531.1899999999999],[279.05,531.1899999999999],[279.05,545.2299999999999],[265.01,545.2299999999999]]},{"text":"“","box":[[272.09000000000003,531.1899999999999],[286.13000000000005,531.1899999999999],[286.13000000000005,545.2299999999999],[272.09000000000003,545.2299999999999]]},{"text":"穷追砸匪！","box":[[286.13,531.1899999999999],[356.07728000000003,531.1899999999999],[356.07728000000003,545.2299999999999],[286.13,545.2299999999999]]},{"text":"”","box":[[349.03,531.1899999999999],[363.07,531.1899999999999],[363.07,545.2299999999999],[349.03,545.2299999999999]]},{"text":"......","box":[[363.07,531.1899999999999],[391.15,531.1899999999999],[391.15,545.2299999999999],[363.07,545.2299999999999]]},{"text":"祝捷大会的第二天，即关于收缴“武器、装备和各种兵种军用物","box":[[117.98,578.71],[505.2874400000002,578.71],[505.2874400000002,592.75],[117.98,592.75]]},{"text":"资”的“九五命令”正式下达前夜，九月四日，重大","box":[[90.024,602.7099999999999],[403.78992,602.7099999999999],[403.78992,616.7499999999999],[90.024,616.7499999999999]]},{"text":"","box":[[403.78992000000017,602.7099999999999],[407.35,602.7099999999999],[407.35,602.7099999999999],[403.78992000000017,602.7099999999999]]},{"text":"301","box":[[407.35,602.7099999999999],[432.00424000000004,602.7099999999999],[432.00424000000004,616.7499999999999],[407.35,616.7499999999999]]},{"text":"","box":[[432.00424000000004,602.7099999999999],[435.43,602.7099999999999],[435.43,602.7099999999999],[432.00424000000004,602.7099999999999]]},{"text":"纵队以军训","box":[[435.43,602.7099999999999],[505.50364,602.7099999999999],[505.50364,616.7499999999999],[435.43,616.7499999999999]]},{"text":"为名，私匿武器，神不知鬼不觉向华蓥山进发了：按照总团的部署，","box":[[90.024,626.74],[512.4595200000002,626.74],[512.4595200000002,640.78],[90.024,640.78]]},{"text":"此行是去建立根据地，以保存实力为持久战作准备。","box":[[90.024,650.74],[412.05948000000006,650.74],[412.05948000000006,664.78],[90.024,664.78]]},{"text":"那是一个月黑风高之夜，田野和山峦都肃杀无声，很利于实施军","box":[[117.98,674.8599999999999],[505.41380000000015,674.8599999999999],[505.41380000000015,688.8999999999999],[117.98,688.8999999999999]]},{"text":"事行动。此前，我们《","box":[[90.024,698.8599999999999],[225.13091999999995,698.8599999999999],[225.13091999999995,712.8999999999999],[90.024,712.8999999999999]]},{"text":"8","box":[[225.17,698.8599999999999],[233.39744,698.8599999999999],[233.39744,712.8999999999999],[225.17,712.8999999999999]]},{"text":"·","box":[[233.45,698.8599999999999],[236.83364,698.8599999999999],[236.83364,712.8999999999999],[233.45,712.8999999999999]]},{"text":"15","box":[[236.81,698.8599999999999],[253.31748000000002,698.8599999999999],[253.31748000000002,712.8999999999999],[236.81,712.8999999999999]]},{"text":"","box":[[253.31748000000002,698.8599999999999],[256.73,698.8599999999999],[256.73,698.8599999999999],[253.31748000000002,698.8599999999999]]},{"text":"战报》编辑部已开过会，决定要用实际行","box":[[256.73,698.8599999999999],[505.44860000000006,698.8599999999999],[505.44860000000006,712.8999999999999],[256.73,712.8999999999999]]},{"text":"动向","box":[[90.024,722.8599999999999],[118.104,722.8599999999999],[118.104,736.8999999999999],[90.024,736.8999999999999]]},{"text":"","box":[[118.10399999999998,722.8599999999999],[122.06,722.8599999999999],[122.06,722.8599999999999],[118.10399999999998,722.8599999999999]]},{"text":"301","box":[[122.06,722.8599999999999],[146.71424000000002,722.8599999999999],[146.71424000000002,736.8999999999999],[122.06,736.8999999999999]]},{"text":"","box":[[146.71424000000002,722.8599999999999],[150.74,722.8599999999999],[150.74,722.8599999999999],[146.71424000000002,722.8599999999999]]},{"text":"学习。实际行动之一就是派出人员分批跟队","box":[[150.74,722.8599999999999],[416.74183999999997,722.8599999999999],[416.74183999999997,736.8999999999999],[150.74,736.8999999999999]]},{"text":"","box":[[416.74184000000025,722.8599999999999],[420.79,722.8599999999999],[420.79,722.8599999999999],[416.74184000000025,722.8599999999999]]},{"text":"301","box":[[420.79,722.8599999999999],[445.33752,722.8599999999999],[445.33752,736.8999999999999],[420.79,736.8999999999999]]},{"text":"","box":[[445.33752,722.8599999999999],[449.38,722.8599999999999],[449.38,722.8599999999999],[445.33752,722.8599999999999]]},{"text":"做战地采","box":[[449.38,722.8599999999999],[505.53999999999996,722.8599999999999],[505.53999999999996,736.8999999999999],[449.38,736.8999999999999]]},{"text":"访。","box":[[90.024,746.856],[118.104,746.856],[118.104,760.896],[90.024,760.896]]},{"text":"霍晓林和张宗云作为第一批成员跟队出发","box":[[111.14015999999998,746.856],[363.1019999999999,746.856],[363.1019999999999,760.896],[111.14015999999998,760.896]]},{"text":"——","box":[[363.19,746.856],[393.47319999999996,746.856],[393.47319999999996,760.896],[363.19,760.896]]},{"text":"在华蓥山采访数日","box":[[393.43,746.856],[505.37092000000007,746.856],[505.37092000000007,760.896],[393.43,760.896]]}]}